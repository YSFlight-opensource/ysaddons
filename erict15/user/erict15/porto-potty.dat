IDENTIFY "[KMN]FLYING_PORTO-POTTY"
CATEGORY NORMAL
AFTBURNR FALSE                #HAVE AFTERBURNER?
THRAFTBN  0.0t                #THRUST WITH AFTERBURNER
THRMILIT 50.0t                #THRUST AT MILITARY POWER
WEIGHCLN  1.0t                #WEIGHT CLEAN
WEIGFUEL 15.0t                #WEIGHT OF FUEL
WEIGLOAD  1.0t                #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 10.0kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP 0m  0m  0m   #COCKPIT POSITION
LEFTGEAR -1.05m -6.5m -0.85m #LEFT LANDING GEAR POSITION
RIGHGEAR  1.05m -6.5m -0.85m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -3.5m -1.00m  #WHEEL POSITION
ARRESTER  0.0m -3.5m -1.50m
MACHNGUN  0.00m 0.00m  0.00m  #MACHINE GUN POSITION
SMOKEGEN  0.00m  0.00m  0.00m  #SMOKE GENERATOR POSITION
VAPORPO0 0.00m  0.00m 0.00m  #VAPOR POSITION (Wings Swept Back or no-VGW)
HTRADIUS  110.00m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 20

CRITAOAP  25deg               #CRITICAL AOA POSITIVE
CRITAOAM -20deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 1160km/h             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR FALSE                #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m 0.0m  1.00m     #Thrust vector 0
TRSTDIR1 0.0m 1.0m -0.11m     #Thrust vector 1
PSTMPTCH 10deg                #Post-Stall Maneuver VPitch
PSTMYAW_ 10deg                #Post-Stall Maneuver VYaw
PSTMROLL 10deg                #Post-Stall Maneuver VRoll

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 35m^2                #WING AREA

MXIPTAOA 28.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 80kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 100kt                #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE TRUE                 #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3m 0m            #POSITION
ATTITUDE 0deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

HRDPOINT 0m 0m 0m AIM9 B250
HRDPOINT 0m 0m 0m AIM9 B250
HRDPOINT 0m 0m 0m AIM9 B250
HRDPOINT 0m 0m 0m AIM9 B250
HRDPOINT 0m 0m 0m AIM9 B250
HRDPOINT 0m 0m 0m AIM9 B250
HRDPOINT 0m 0m 0m AIM9 B250
HRDPOINT 0m 0m 0m AIM9 B250
HRDPOINT 0m 0m 0m AIM9 B250
HRDPOINT 0m 0m 0m AIM9 B250
HRDPOINT 0m 0m 0m AIM120 B250
HRDPOINT 0m 0m 0m AIM120 B250
HRDPOINT 0m 0m 0m AIM120 B250
HRDPOINT 0m 0m 0m AIM120 B250
HRDPOINT 0m 0m 0m AIM120 B250
HRDPOINT 0m 0m 0m AIM120 B250
HRDPOINT 0m 0m 0m AIM120 B250
HRDPOINT 0m 0m 0m AIM120 B250
HRDPOINT 0m 0m 0m AIM120 B250
HRDPOINT 0m 0m 0m AIM120 B250
LMTBYHDP TRUE
INITIGUN 5000                    #INITIAL GUN
LOADWEPN AIM9 10
LOADWEPN AIM120 10
LOADWEPN B250 20
AAMVISIB FALSE
BOMVISIB FALSE

RADARCRS 0.01                  #RADAR CROSS SECTION


REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.8MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REM REFACRUS 37500ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 110kt                #LANDING SPEED
REFAOALD 15deg                #LANDING AOA
REFLNRWY 2000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
