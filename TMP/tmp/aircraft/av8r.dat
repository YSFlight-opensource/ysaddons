REM AV-8B_Harrier2+
IDENTIFY "AV-8B_Harrier2+"
CATEGORY ATTACKER
AFTBURNR FALSE                #HAVE AFTERBURNER?
THRAFTBN  0.0t                #THRUST WITH AFTERBURNER
THRMILIT 10.07t               #THRUST AT MILITARY POWER
WEIGHCLN  7.05t               #WEIGHT CLEAN
WEIGFUEL  3.519t              #WEIGHT OF FUEL
WEIGLOAD  4.2t                #WEIGHT OF PAYLOAD
FUELABRN  0.0kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.51kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  0.45m  2.75m  #COCKPIT POSITION
LEFTGEAR -2.21m -1.6m -1.40m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.21m -1.6m -1.40m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.90m  1.5m   #WHEEL POSITION
MACHNGUN -0.5m  -1.0m  2.85m  #MACHINE GUN POSITION
SMOKEGEN -0.0m  0.30m -6.00m  #SMOKE GENERATOR POSITION
VAPORPO0  3.75m -0.35m -2.30m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  3.75m  0.35m -2.30m #VAPOR POSITION (Wings Spread)
HTRADIUS  6.0m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  25deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 1041km/h             #CRITICAL SPEED
MAXSPEED 1065km/h             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 1.0                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 21.37m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 60kt                 #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 4
INITIGUN 450                  #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB
INITRCKT 0                    #INITIAL ROCKET
WEAPONCH GUN

MAXNMAAM 4
MAXNMAGM 6
MAXNBOMB 6
MAXNMRKT 36

AAMSLOT_  3.60m -0.44m -1.00m
AAMSLOT_ -3.60m -0.44m -1.00m
AAMSLOT_  3.00m -0.23m -0.80m
AAMSLOT_ -3.00m -0.23m -0.80m

AGMSLOT_  1.20m -0.30m -0.15m
AGMSLOT_ -1.20m -0.30m -0.15m
AGMSLOT_  1.80m -0.30m -0.15m
AGMSLOT_ -1.80m -0.30m -0.15m
AGMSLOT_  3.00m -0.43m -1.00m
AGMSLOT_ -3.00m -0.43m -1.00m

BOMBSLOT -3.00m -0.43m -1.00m
BOMBSLOT  3.00m -0.43m -1.00m
BOMBSLOT -1.80m -0.30m -0.15m
BOMBSLOT  1.80m -0.30m -0.15m
BOMBSLOT -1.20m -0.30m -0.15m
BOMBSLOT  1.20m -0.30m -0.15m

RKTSLOT_  1.50m -0.30m -0.15m
RKTSLOT_ -1.50m -0.30m -0.15m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.8MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 90kt                 #LANDING SPEED
REFAOALD 5deg                 #LANDING AOA
REFLNRWY 1500m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.2                  #LANDING THROTTLE SETTING

AUTOCALC
