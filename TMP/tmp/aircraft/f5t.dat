IDENTIFY "F-5E_Tiger2(T)"
CATEGORY FIGHTER
AFTBURNR TRUE                  #HAVE AFTERBURNER?
THRAFTBN  4.54t                #THRUST WITH AFTERBURNER
THRMILIT  3.00t                #THRUST AT MILITARY POWER
WEIGHCLN  4.40t                #WEIGHT CLEAN
WEIGFUEL  2.00t                #WEIGHT OF FUEL
WEIGLOAD  0.50t                #WEIGHT OF PAYLOAD
FUELABRN  4.00kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.40kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  1.05m  2.20m  #COCKPIT POSITION
LEFTGEAR -1.80m -1.22m -1.40m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.80m -1.22m -1.40m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.22m  3.70m  #WHEEL POSITION
MACHNGUN -0.27m  0.36m  5.00m  #MACHINE GUN POSITION
MACHNGN2  0.27m  0.36m  5.00m  #MACHINE GUN POSITION
SMOKEGEN  0.00m  0.55m -5.65m  #SMOKE GENERATOR POSITION
VAPORPO0  3.90m  0.10m -2.50m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  3.90m  0.10m -2.50m  #VAPOR POSITION (Wings Spread)
HTRADIUS  6.5m                 #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  22deg                #CRITICAL AOA POSITIVE
CRITAOAM -15deg                #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH               #CRITICAL SPEED
MAXSPEED 1.6MACH               #MAXIMUM SPEED

HASSPOIL TRUE                  #HAS SPOILER?
RETRGEAR TRUE                  #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                  #HAS VARIABLE GEOMETRY WING?
VGWSPED1 -1kt                #VARIABLE GEOMETRY WING SPEED
VGWSPED2 -1kt                #VARIABLE GEOMETRY WING SPEED

CLVARGEO 0.0                   #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                   #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                   #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.5                   #EFFECT OF FLAP FOR Cd
CDBYGEAR 1.5                   #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                   #EFFECT OF SPOILER FOR Cd

WINGAREA 17.28m^2                 #WING AREA

MXIPTAOA 23.0deg               #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 180.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                  #MINIMUM MANEUVABLE SPEED
MANESPD2 130kt                 #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                  #CONTROL LANDINGGEAR
CTLBRAKE FALSE                 #CONTROL BRAKE
CTLSPOIL 0.0                   #CONTROL SPOILER
CTLABRNR FALSE                 #CONTROL AFTERBURNER
CTLTHROT 0.0                   #CONTROL THRUST
CTLIFLAP 0.0                   #CONTROL FLAP
CTLINVGW 0.0                   #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                  #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m             #POSITION
ATTITUDE 45deg 0deg 0deg       #ATTITUDE
INITFUEL 75%                   #INITIAL FUEL
INITLOAD 0.0t                  #INITIAL PAYLOAD
INITSPED 0.0MACH               #INITIAL SPEED

INITIGUN 3000                  #INITIAL GUN
INITIAAM 2                     #INITIAL AAM
INITIAGM 0                     #INITIAL AGM
INITBOMB 0                     #INITIAL AGM
MAXNMAAM 6
MAXNMAGM 4
MAXNBOMB 4

AAMSLOT_ -4.13m  0.10m -1.5m
AAMSLOT_  4.13m  0.10m -1.5m
AAMSLOT_ -3.10m -0.37m -1.0m
AAMSLOT_  3.10m -0.37m -1.0m
AAMSLOT_ -2.30m -0.37m -0.8m
AAMSLOT_  2.30m -0.37m -0.8m

AGMSLOT_ -3.10m -0.37m -1.0m
AGMSLOT_  3.10m -0.37m -1.0m
AGMSLOT_ -2.30m -0.37m -0.8m
AGMSLOT_  2.30m -0.37m -0.8m

BOMBSLOT -3.10m -0.37m -1.0m
BOMBSLOT  3.10m -0.37m -1.0m
BOMBSLOT -2.30m -0.37m -0.8m
BOMBSLOT  2.30m -0.37m -0.8m


REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.5MACH               #CRUISING SPEED
REFACRUS 30000ft               #CRUISING ALTITUDE
REFTCRUS 0.8                   #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 110kt                 #LANDING SPEED
REFAOALD 10deg                 #LANDING AOA
REFLNRWY 1000m                  #RUNWAY REQUIRED TO STOP
REFTHRLD 0.30                  #LANDING THROTTLE SETTING

AUTOCALC
