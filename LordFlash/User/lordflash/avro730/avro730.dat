IDENTIFY "AVRO_730"
CATEGORY NORMAL
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 77600lb                #THRUST WITH AFTERBURNER
THRMILIT 49733lb                #THRUST AT MILITARY POWER
THRSTREV 0.3                 #EFFICIENCY OF THRUST REVERSER
WEIGHCLN 47.0t                #WEIGHT CLEAN
WEIGFUEL 28.0t                #WEIGHT OF FUEL
WEIGLOAD  15.0t                #WEIGHT OF PAYLOAD
FUELABRN  8.0kg               #FUEL CONSUMPTION WHEN USINGAFTERBURNER
FUELMILI  0.9kg               #FUEL CONSUMPTION ATMILITARY POWER

COCKPITP  0.00m  1.15m 12.0m #COCKPIT POSITION
EXCAMERA "Pilot" -0.4m 0.33m 12.8m  0deg 0deg 0deg INSIDE
EXCAMERA "Co-pilot" 0.4m 0.33m 12.8m  0deg 0deg 0deg INSIDE
EXCAMERA "Cockpit" 0m 0.33m 12.0m  0deg 0deg 0deg INSIDE
EXCAMERA "Tail" 0m 9.0m -20.0m  0deg 0deg 0deg INSIDE
LEFTGEAR -6.35m -3.05m -5.5m #LEFT LANDING GEAR POSITION
RIGHGEAR  6.35m -3.05m -5.5m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -4.0m  12.70m #WHEEL POSITION
MACHNGUN  0.00m  0.00m  0.00m #MACHINE GUN POSITION
SMOKEGEN  0.00m  0.00m -26.0m #SMOKE GENERATOR POSITION
VAPORPO0  10.3m  0.40m -14.5m #VAPOR POSITION (Wings SweptBack or no-VGW)
VAPORPO1  10.3m  0.40m -14.5m #VAPOR POSITION (WingsSpread)
HTRADIUS 20.00m               #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 20

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 3.0MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDINGGEAR?
VARGEOMW FALSE                 #HAS VARIABLE GEOMET
CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRYWING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRYWING FOR Cd
CLBYFLAP 0.6                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 2.0                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 3.0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 3.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 185.8m^2            #WING AREA

MXIPTAOA 10.0deg              #MAX INPUT AOA
MXIPTSSA 7.5deg               #MAX INPUT SSA
MXIPTROL 90.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 110kt                #MINIMUM MANEUVABLE SPEED
MANESPD2 150kt                #FULLY MANEUVABLE SPEED
CPITMANE 5.0                  #PITCH MANEUVERBILITYCONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 4.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 5.0                  #YAW STABILITY CONSTANT
CROLLMAN 2.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP FALSE                #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRYWING
CTLATVGW TRUE                #CONTROL AUTO VARIABLEGEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

BOMVISIB FALSE
AGMVISIB FALSE
BOMINBAY TRUE                 #Bombs in Bomb Bay?
LMTBYHDP FALSE 

HRDPOINT -0.4m 1.23m -1.40m B500
HRDPOINT  0.4m 1.23m -1.40m B500
HRDPOINT -0.4m 1.23m -2.90m B500
HRDPOINT  0.4m 1.23m -2.90m B500
HRDPOINT -0.4m 1.23m -4.40m B500
HRDPOINT  0.4m 1.23m -4.40m B500
HRDPOINT -0.3m 1.33m -6.60m B500
HRDPOINT  0.3m 1.33m -6.60m B500
HRDPOINT -0.3m 1.33m -8.10m B500
HRDPOINT  0.3m 1.33m -8.10m B500
HRDPOINT -0.3m 1.33m -9.70m B500
HRDPOINT  0.3m 1.33m -9.70m B500
HRDPOINT -0.4m -1.4m -2.90m AGM65
HRDPOINT  0.4m -1.4m -2.90m AGM65


INITIGUN 0                    #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 8                    #INITIAL AGM
INITBOMB 40                   #INITIAL BOMB
MAXNMAGM 20                   #MAXIMUM AGM
MAXNBOMB 60                   #MAXIMUM BOM

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 3.0MACH              #CRUISING SPEED
REFACRUS 30000ft              #CRUISING ALTITUDE
REFTCRUS 0.7                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 180kt                #LANDING SPEED
REFAOALD 8deg                #LANDING AOA
REFLNRWY 2200m                #RUNWAY REQUIRED TO STOP
REFTHRLD 1.0                  #LANDING THROTTLE SETTING

AUTOCALC
