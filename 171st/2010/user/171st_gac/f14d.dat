IDENTIFY "[VT-171]F-14D_BOMBCAT"
SUBSTNAM F-14_TOMCAT
CATEGORY ATTACKER
AFTBURNR TRUE #HAVE AFTERBURNER?
THRAFTBN 27.0t #THRUST WITH AFTERBURNER
THRMILIT 16.1t #THRUST AT MILITARY POWER
WEIGHCLN 21.0t #WEIGHT CLEAN
WEIGFUEL 7.0t #WEIGHT OF FUEL
WEIGLOAD 10.0t #WEIGHT OF PAYLOAD
FUELABRN 6.5kg #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.45kg #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  1.40m   6.25m  #COCKPIT POSITION
LEFTGEAR -2.76m -2.075m -2.00m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.76m -2.075m -2.00m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.98m  6.1m  #WHEEL POSITION
ARRESTER  0.0m -1.40m -7.00m #Arresting hook position
MACHNGUN -0.63m -0.18m  8.40m  #MACHINE GUN POSITION
SMOKEGEN  0.00m 0.00m -8.00m  #SMOKE GENERATOR POSITION
VAPORPO0  5.719m  0.692m -5.882m  #VAPOR POSITION (Wings Swept back)
VAPORPO1  9.715m  0.694m -3.190m  #VAPOR POSITION (Wings Spread)
HTRADIUS  13.0m                #OUTSIDE SPHERE RADIUS
GUNINTVL 0.03

EXCAMERA "CO-PILOT" 0.0m 1.50m 4.55m 0deg 0deg 0deg INSIDE

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 13

CRITAOAP 22deg #CRITICAL AOA POSITIVE
CRITAOAM -15deg #CRITICAL AOA NEGATIVE

CRITSPED 1.3MACH #CRITICAL SPEED
MAXSPEED 2.4MACH #MAXIMUM SPEED

HASSPOIL TRUE #HAS SPOILER?
RETRGEAR TRUE #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.6 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 1.0 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.2 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5 #EFFECT OF GEAR FOR Cd
CDSPOILR 1.0 #EFFECT OF SPOILER FOR Cd

WINGAREA 56.5m^2 #WING AREA

MXIPTAOA 24.0deg #MAX INPUT AOA
MXIPTSSA 5.0deg #MAX INPUT SSA
MXIPTROL 210.0deg #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt #MINIMUM MANEUVABLE SPEED
MANESPD2 120kt #FULLY MANEUVABLE SPEED
CPITMANE 10.0 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2 #PITCH STABILITY CONSTANT
CYAWMANE 5.0 #YAW MANEUVABILITY CONSTANT
CYAWSTAB 4.0 #YAW STABILITY CONSTANT
CROLLMAN 3.5 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE #CONTROL LANDINGGEAR
CTLBRAKE FALSE #CONTROL BRAKE
CTLSPOIL 0.0 #CONTROL SPOILER
CTLABRNR FALSE #CONTROL AFTERBURNER
CTLTHROT 0.0 #CONTROL THRUST
CTLIFLAP 0.0 #CONTROL FLAP
CTLINVGW 1.0 #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m #POSITION
ATTITUDE 45deg 0deg 0deg #ATTITUDE
INITFUEL 75% #INITIAL FUEL
INITLOAD 0.0t #INITIAL PAYLOAD
INITSPED 0.0MACH #INITIAL SPEED'

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT 3.2m 0.25m 0.15m AIM9
HRDPOINT -3.2m 0.25m 0.15m AIM9

HRDPOINT 2.85m -0.25m 0.0m AIM120 AIM9 AGM65 RKT
HRDPOINT -2.85m -0.25m 0.0m AIM120 AIM9 AGM65 RKT

HRDPOINT 0.35m -0.6m 2.1m B500 B250 AIM120
HRDPOINT -0.35m -0.6m 2.1m B500 B250 AIM120
HRDPOINT 0.35m -0.6m -1.8m B500 B250 AIM120
HRDPOINT -0.35m -0.6m -1.8m B500 B250 AIM120


HRDPOINT 0.0m -0.6m 2.4m B500 B250 
HRDPOINT 0.3m -0.6m -0.1m B500 B250 
HRDPOINT -0.3m -0.6m -0.1m B500 B250 
HRDPOINT 0.0m -0.6m -0.1m B500 B250 

HRDPOINT 0.0m -0.6m -2.7m B500 B250 


LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 2500                 #INITIAL GUN
LOADWEPN AIM120 2
LOADWEPN AIM9 2
INITIAGM 0                    #INITIAL AGM
INITBOMB 4

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.5MACH #CRUISING SPEED
REFACRUS 27000ft #CRUISING ALTITUDE
REFTCRUS 1.0 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 100kt #LANDING SPEED
REFAOALD 9deg #LANDING AOA
REFLNRWY 1000m #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3 #LANDING THROTTLE SETTING
THRSTREV 0.05 #REVERS THRUST POWER

AUTOCALC
