REM 171st F-4 - modified UWM dat - fused with stock F-14
IDENTIFY "[CVW-171]F-4S_PHANTOMII(VFA-42)"
CATEGORY ATTACKER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 17.9t                #THRUST WITH AFTERBURNER
THRMILIT 11.8t                #THRUST AT MILITARY POWER
WEIGHCLN 14.2t                #WEIGHT CLEAN
WEIGFUEL  6.0t                #WEIGHT OF FUEL
WEIGLOAD 10.0t                #WEIGHT OF PAYLOAD
FUELABRN 6.5kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.45kg               #FUEL CONSUMPTION AT MILITARY POWER


COCKPITP  0.0m  1.33m   5.75m  #COCKPIT POSITION
EXCAMERA "CO_P" 0.0m 1.56m 4.5m 0deg 0deg 0deg INSIDE
LEFTGEAR -2.75m -1.62m -0.22m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.75m -1.62m -0.22m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.62m  6.93m  #WHEEL POSITION
MACHNGUN  0m  -0.46m  2.886m  #MACHINE GUN POSITION
MACHNGN2  2.17m  -0.66m  1.914m  #MACHINE GUN POSITION
MACHNGN3  -2.17m  -0.66m  1.914m  #MACHINE GUN POSITION
GUNINTVL 0.025
SMOKEGEN 0m 1.52m -7.8m  #SMOKE GENERATOR POSITION
VAPORPO0  5.85m  0.38m -3.15m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.85m  0.38m -3.15m  #VAPOR POSITION (Wings Spread)
HTRADIUS  11.0m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.2MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.3                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd
VGWSPED1 100kt
VGWSPED2 1200kt

WINGAREA 49.2m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 7.0deg               #MAX INPUT SSA
MXIPTROL 150.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 100kt                #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED


REM Belly hardpoints
HRDPOINT -0.57m -0.17m 4.61m AIM120 AIM9
HRDPOINT 0.57m -0.17m 4.61m AIM120 AIM9
HRDPOINT -1.09m -0.17m -0.7m AIM120 AIM9
HRDPOINT 1.09m -0.17m -0.7m AIM120 AIM9

REM Bottom HPs
HRDPOINT -3.45m -1.2m 1m B250 B500 RKT
HRDPOINT 3.45m -1.2m 1m B250 B500 RKT
HRDPOINT -3.45m -1.2m -1.3m B250 B500
HRDPOINT 3.45m -1.2m -1.3m B250 B500

REM Left HPs
HRDPOINT -3.8m -0.7m 1m B250 B500
HRDPOINT 3.8m -0.7m 1m B250 B500
HRDPOINT -3.8m -0.7m -1.3m B250 B500
HRDPOINT 3.8m -0.7m -1.3m B250 B500

REM Right HPs
HRDPOINT -3.1m -0.7m 1m B250 B500
HRDPOINT 3.1m -0.7m 1m B250 B500
HRDPOINT -3.1m -0.7m -1.3m B250 B500
HRDPOINT 3.1m -0.7m -1.3m B250 B500


MAXNMRKT 36
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
GUNPOWER 3
INITIGUN 1059               #INITIAL GUN
LOADWEPN AIM120 4
LOADWEPN AIM9 4

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 110kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1600m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
