REM F-14 TOMCAT
IDENTIFY "[CVW-171]F-14D_Bombcat"
CATEGORY FIGHTER
AFTBURNR TRUE #HAVE AFTERBURNER?
THRAFTBN 27.0t #THRUST WITH AFTERBURNER
THRMILIT 16.1t #THRUST AT MILITARY POWER
WEIGHCLN 21.0t #WEIGHT CLEAN
WEIGFUEL 7.0t #WEIGHT OF FUEL
WEIGLOAD 10.0t #WEIGHT OF PAYLOAD
FUELABRN 6.5kg #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.45kg #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  0.73m   6.08m  #COCKPIT POSITION
EXCAMERA  "RIO" 0.0m  1.03m 4.34m 0deg 0deg 0deg INSIDE
LEFTGEAR -2.7m -2.23m -1.51m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.7m -2.23m -1.51m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.48m  5.31m  #WHEEL POSITION
MACHNGUN -0.64m -0.81m  7.9m  #MACHINE GUN POSITION
ARRESTER  0.00m -1.73m -8.17m  #ARRESTING HOOK
SMOKEGEN  0.0m -0.2m -8.84m  #SMOKE GENERATOR POSITION
VAPORPO0  6.05m  0.46m -6.77m  #VAPOR POSITION (Wings Swept back)
VAPORPO1  9.92m  0.43m -3.99m  #VAPOR POSITION (Wings Spread)
HTRADIUS  12.00m                #OUTSIDE SPHERE RADIUS
GUNINTVL 0.03

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 13

CRITAOAP 22deg #CRITICAL AOA POSITIVE
CRITAOAM -15deg #CRITICAL AOA NEGATIVE

CRITSPED 1.3MACH #CRITICAL SPEED
MAXSPEED 2.4MACH #MAXIMUM SPEED

HASSPOIL TRUE #HAS SPOILER?
RETRGEAR TRUE #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.6 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 1.0 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.2 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5 #EFFECT OF GEAR FOR Cd
CDSPOILR 1.0 #EFFECT OF SPOILER FOR Cd

WINGAREA 56.5m^2 #WING AREA

MXIPTAOA 24.0deg #MAX INPUT AOA
MXIPTSSA 5.0deg #MAX INPUT SSA
MXIPTROL 210.0deg #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt #MINIMUM MANEUVABLE SPEED
MANESPD2 120kt #FULLY MANEUVABLE SPEED
CPITMANE 10.0 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2 #PITCH STABILITY CONSTANT
CYAWMANE 5.0 #YAW MANEUVABILITY CONSTANT
CYAWSTAB 4.0 #YAW STABILITY CONSTANT
CROLLMAN 3.5 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE #CONTROL LANDINGGEAR
CTLBRAKE FALSE #CONTROL BRAKE
CTLSPOIL 0.0 #CONTROL SPOILER
CTLABRNR FALSE #CONTROL AFTERBURNER
CTLTHROT 0.0 #CONTROL THRUST
CTLIFLAP 0.0 #CONTROL FLAP
CTLINVGW 1.0 #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m #POSITION
ATTITUDE 45deg 0deg 0deg #ATTITUDE
INITFUEL 75% #INITIAL FUEL
INITLOAD 0.0t #INITIAL PAYLOAD
INITSPED 0.0MACH #INITIAL SPEED'

MAXNMRKT 24

HRDPOINT -3.36m -0.07m 0.20m AIM9
HRDPOINT 3.36m -0.07m 0.20m AIM9
HRDPOINT -2.98m -0.50m 0.10m AIM120 AIM9 AGM65 RKT
HRDPOINT 2.98m -0.50m 0.10m AIM120 AIM9 AGM65 RKT
HRDPOINT -0.35m -1.00m 1.18m B500 B250 AIM120 
HRDPOINT 0.35m -1.00m 1.18m B500 B250 AIM120 
HRDPOINT -0.35m -0.75m -1.9m B500 B250 AIM120
HRDPOINT 0.35m -0.75m -1.9m B500 B250 AIM120 
HRDPOINT -0.35m -0.65m -4.85m B500 B250
HRDPOINT 0.35m -0.65m -4.85m B500 B250
HRDPOINT 0.00m -1.00m 1.18m B500 B250
HRDPOINT 0.00m -0.75m -1.9m B500 B250
HRDPOINT 0.00m -0.65m -4.85m B500 B250

LMTBYHDP TRUE #Limit Max Num Weapon by Hardpoint

INITIGUN 2500 #INITIAL GUN
LOADWEPN AIM9 2
LOADWEPN AGM65 2
LOADWEPN B500 4

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.5MACH #CRUISING SPEED
REFACRUS 27000ft #CRUISING ALTITUDE
REFTCRUS 1.0 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 100kt #LANDING SPEED
REFAOALD 9deg #LANDING AOA
REFLNRWY 1000m #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3 #LANDING THROTTLE SETTING
THRSTREV 0.05 #REVERS THRUST POWER

AUTOCALC

