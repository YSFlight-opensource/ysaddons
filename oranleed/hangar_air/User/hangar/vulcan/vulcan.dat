REM Data file for Vulcan by Bluebox
REM Final Release 1.1 of 2007/07/14
REM Terms Of Use: GPL

IDENTIFY "AVRO Vulcan Mk.B.2 XM606"
CATEGORY HEAVYBOMBER

AFTBURNR FALSE                #HAVE AFTERBURNER?
THRAFTBN  0.00t               #THRUST WITH AFTERBURNER
THRMILIT 33.00t               #THRUST AT MILITARY POWER
WEIGHCLN 61.50t               #WEIGHT CLEAN
WEIGFUEL 42.00t               #WEIGHT OF FUEL
WEIGLOAD  9.50t               #WEIGHT OF PAYLOAD
FUELABRN  0.00kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  6.00kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP -0.43m  1.63m  10.32m #COCKPIT POSITION
LEFTGEAR -4.68m -3.35m -2.90m  #LEFT LANDING GEAR POSITION
RIGHGEAR  4.68m -3.35m -2.90m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -3.73m  6.98m  #WHEEL POSITION
ARRESTER  0.00m -3.62m -12.25m #ARRESTING HOOK POSITION
SMOKEGEN  0.15m  0.05m -6.55m  #SMOKE GENERATOR POSITION
VAPORPO0  16.81m -0.38m -13.05m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  16.81m -0.38m -13.05m #VAPOR POSITION (Wings Spread)
HTRADIUS  18.00m              #OUTSIDE SPHERE RADIUS

STRENGTH 12

CRITAOAP  13.5deg             #CRITICAL AOA POSITIVE
CRITAOAM  -8.0deg             #CRITICAL AOA NEGATIVE

CRITSPED 0.88MACH             #CRITICAL SPEED
MAXSPEED 0.95MACH             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.2                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.1                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.6                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.2                  #EFFECT OF SPOILER FOR Cd

WINGAREA 368m^2               #WING AREA

MXIPTAOA 22.0deg              #MAX INPUT AOA
MXIPTSSA  8.0deg              #MAX INPUT SSA
MXIPTROL 40.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                #MINIMUM MANEUVABLE SPEED
MANESPD2 150kt                #FULLY MANEUVABLE SPEED
CPITMANE 7.4                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 6.6                  #PITCH STABILITY CONSTANT
CYAWMANE 4.4                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.6                  #YAW STABILITY CONSTANT
CROLLMAN 3.5                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 0ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

REM Slots must come before Inits.
HRDPOINT 0.65m -0.30m -0.20m B500
HRDPOINT -0.65m -0.30m -0.20m B500
HRDPOINT 0.00m 0.20m -0.20m B500

HRDPOINT 0.65m -0.10m -2.40m B500
HRDPOINT -0.65m -0.10m -2.40m B500
HRDPOINT 0.00m 0.10m -2.40m B500

HRDPOINT 0.65m -0.00m -4.60m B500
HRDPOINT -0.65m -0.00m -4.60m B500
HRDPOINT 0.00m 0.00m -4.60m B500


HRDPOINT 1.25m 0.00m 0.90m B500
HRDPOINT -1.25m 0.00m 0.90m B500
HRDPOINT 0.00m 0.50m 0.90m B500

HRDPOINT 1.25m 0.00m -1.30m B500
HRDPOINT -1.25m 0.00m -1.30m B500
HRDPOINT 0.00m 0.50m -1.30m B500

HRDPOINT 1.25m 0.00m -3.50m B500
HRDPOINT -1.25m 0.00m -3.50m B500
HRDPOINT 0.00m 0.50m -3.50m B500

HRDPOINT 1.25m -0.14m -5.70m B500
HRDPOINT -1.25m -0.14m -5.70m B500
HRDPOINT 0.00m 0.30m -5.70m B500

BOMVISIB FALSE
BOMINBAY TRUE
LMTBYHDP TRUE

INITIGUN 0                    #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 21
WEAPONCH BOM

RADARCRS 0.3                  #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.76MACH             #CRUISING SPEED
REFACRUS 10000ft              #CRUISING ALTITUDE
REFTCRUS 0.85                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 145kt                #LANDING SPEED
REFAOALD 10.0deg              #LANDING AOA
REFLNRWY 1200m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
