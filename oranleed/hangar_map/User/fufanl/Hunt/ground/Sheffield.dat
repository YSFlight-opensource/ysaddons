REM shp
IDENTIFY "Cruiser SHEFFIELD"
CATEGORY NORMAL
AFTBURNR FALSE 
WEIGHCLN 12000.0t                #WEIGHT CLEAN
WEIGFUEL 1750.0t                #WEIGHT OF FUEL
WEIGLOAD 1200.0t               #WEIGHT OF PAYLOAD
PROPELLR 152000hp
FUELMILI 1.0kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.56m  16.57m  53.70m  #COCKPIT POSITION
LEFTGEAR  -8.0m -0.1m -99.00m  #LEFT LANDING GEAR POSITION
RIGHGEAR  8.0m -0.1m -99.00m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -0.1m 90.00m  #WHEEL POSITION
ARRESTER  0.0m -1.62m -7.28m
MACHNGUN  0.00m 12.41m 70.31m  #MACHINE GUN POSITION
SMOKEGEN  0.0m -0.0m  -90.00m  #SMOKE GENERATOR POSITION
VAPORPO0  6.3m  0.60m -5.60m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  6.3m  0.60m -5.60m  #VAPOR POSITION (Wings Spread)
HTRADIUS  60.0m               #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 SMK
TRIGGER3 NON
TRIGGER4 NON

STRENGTH 200

CRITAOAP 0.0deg
CRITAOAM -0.0deg

CRITSPED 22kt
MAXSPEED 35kt

HASSPOIL TRUE
RETRGEAR FALSE
VARGEOMW FALSE

CLVARGEO 0.0
CDVARGEO 0.0
CLBYFLAP 0.0
CDBYFLAP 0.0
CDBYGEAR 0.0
CDSPOILR 5.0

WINGAREA 2.0m^2

MXIPTAOA 0.0deg
MXIPTSSA 5.0deg
MXIPTROL 0.0deg

MANESPD1 4kt
MANESPD2 7kt
CPITMANE 0.0
CPITSTAB 99999.0
CYAWMANE 11.0
CYAWSTAB 11.0
CROLLMAN 0.0

CTLLDGEA TRUE
CTLBRAKE FALSE
CTLSPOIL 0.0
CTLABRNR FALSE
CTLTHROT 0.0
CTLIFLAP 0.0
CTLINVGW 1.0
CTLATVGW TRUE

POSITION 0m 3ft 0m
ATTITUDE 45deg 0deg 0deg
INITFUEL 100%
INITLOAD 0.0t
INITSPED 0.0MACH

GUNPOWER 8

RKTVISIB  FALSE  
AAMVISIB FALSE
AGMVISIB FALSE


INITIGUN 0                 #INITIAL GUN
INITRCKT 0

SMOKEOIL 1000.0kg

NMTURRET 3

TURRETPO 0 0m 11.83m 61.42m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 0 -2deg 20deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 -90deg 90deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 5000                              # Ammo
TURRETIV 0 0.10sec                            # Number ShootingInterval
TURRETAR 0                                  # Anti Air Capable
REM TURRETGD 0                                   # Anti Ground Capable
TURRETCT 0 PILOT                            # Controlled by PILOT or GUNNER
TURRETRG 0 8000m

TURRETPO 1 0m 11.83m 61.42m 0deg 0deg 0deg     # Number x y z h p b
TURRETPT 1 0deg 0deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 1 -90deg 90deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 1 0                              # Ammo
TURRETIV 1 5.0sec                            # Number ShootingInterval
TURRETNM 1 0002                           # DNM Node Name
TURRETAR 1                                   # Anti Air Capable
REM TURRETGD 1                                   # Anti Ground Capable
TURRETCT 1 PILOT                            # Controlled by PILOT or GUNNER
TURRETRG 1 8000m

TURRETPO 2 0m 11.83m 61.42m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 2 -2deg 15deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 2 0deg 0deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 2 0                              # Ammo
TURRETIV 2 5.0sec                            # Number ShootingInterval
TURRETNM 2 0003                            # DNM Node Name
TURRETAR 2                                   # Anti Air Capable
REM TURRETGD 2                                   # Anti Ground Capable
TURRETCT 2 PILOT                            # Controlled by PILOT or GUNNER
TURRETRG 2 8000m

RADARCRS 1                  #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 28kt                 #CRUISING SPEED
REFACRUS 1000ft
REFTCRUS 0.6

REFVLAND 10kt
REFAOALD 5deg
REFLNRWY 70m
REFTHRLD 0.4

AUTOCALC