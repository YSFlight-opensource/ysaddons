REM f8f
IDENTIFY "F8f-1b(OCP)"
CATEGORY WW2FIGHTER
AFTBURNR FALSE                  #HAVE AFTERBURNER?
PROPELLR  2100HP                #PROPELLER ENGINE
WEIGHCLN  3.2t                  #WEIGHT CLEAN
WEIGFUEL  0.5t                  #WEIGHT OF FUEL
WEIGLOAD  1.2t                  #WEIGHT OF PAYLOAD
FUELMILI  0.15kg                 #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  1.13m -1.37m    #COCKPIT POSITION
INSTPANL user/oranleed/f8f/f8f.ist
LEFTGEAR -1.85m -1.98m  0.11m   #LEFT LANDING GEAR POSITION
RIGHGEAR  1.85m -1.98m  0.11m   #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -0.97m -4.83m   #WHEEL POSITION
ARRESTER  0.00m -0.88m -4.88m   #Arresting hook position
MACHNGUN  2.30m -0.12m  0.76m   #MACHINE GUN POSITION
MACHNGN2 -2.30m -0.12m  0.76m   #MACHINE2 GUN POSITION
MACHNGN3 -2.59m -0.07m  0.73m
MACHNGN4  2.59m -0.07m  0.73m
GUNINTVL 0.07
GUNSIGHT FALSE
SMOKEGEN  0.00m  0.00m -3.50m   #SMOKE GENERATOR POSITION
VAPORPO0  5.43m  0.18m -0.33m   #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.43m  0.18m -0.33m   #VAPOR POSITION (Wings Spread)
HTRADIUS     7m                 #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  15deg                #CRITICAL AOA POSITIVE
CRITAOAM -10deg                #CRITICAL AOA NEGATIVE

CRITSPED 770km/h               #CRITICAL SPEED
MAXSPEED 678km/h               #MAXIMUM SPEED

HASSPOIL FALSE                 #HAS SPOILER?
RETRGEAR TRUE                  #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                  #HAS VARIABLE GEOMETRY WING?

VGWSPED1 150kt
VGWSPED2 250kt

CLVARGEO 0.0                   #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                   #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                   #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.3                   #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.7                   #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                   #EFFECT OF SPOILER FOR Cd

WINGAREA 22.67m^2              #WING AREA

MXIPTAOA 19.0deg               #MAX INPUT AOA
MXIPTSSA 9.0deg                #MAX INPUT SSA
MXIPTROL 140.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 35kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 60kt                 #FULLY MANEUVABLE SPEED
CPITMANE 7.5                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED
WEAPONCH GUN


HRDPOINT -2.44m -0.48m 0.15m B250
HRDPOINT 2.44m -0.48m 0.15m B250
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 1750                 #INITIAL GUN
INITBOMB 0                    #INITIAL BOMB
MAXNBOMB 2                    #MAXIMUM BOMB

RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 630km/h              #CRUISING SPEED
REFACRUS 13000ft              #CRUISING ALTITUDE
REFTCRUS 0.6                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 78kt                 #LANDING SPEED
REFAOALD 8.0deg                 #LANDING AOA
REFLNRWY 500m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
