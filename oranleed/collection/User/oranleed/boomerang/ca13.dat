REM ca13boomerang
IDENTIFY "ca13boomerang"
CATEGORY WW2FIGHTER
AFTBURNR FALSE                 #HAVE AFTERBURNER?
PROPELLR 1200HP                #PROPELLER ENGINE
WEIGHCLN  2.437t               #WEIGHT CLEAN
WEIGFUEL  0.5t                 #WEIGHT OF FUEL
WEIGLOAD  0.5t                 #WEIGHT OF PAYLOAD
FUELMILI  0.03kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  0.925m  -0.62m #COCKPIT POSITION
INSTPANL user/oranleed/boomerang/ca13.ist
LEFTGEAR -1.21m -2.11m  0.65m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.21m -2.11m  0.65m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m  -0.63m -4.57m  #WHEEL POSITION
MACHNGUN  2.0m  -0.74m  1.16m  #MACHINE GUN POSITION
MACHNGN2 -2.0m  -0.74m  1.16m  #MACHINE2 GUN POSITION
MACHNGN3 -2.58m -0.63m  0.0m
MACHNGN4  2.58m -0.63m  0.0m
MACHNGN5 -2.72m -0.61m  0.0m
MACHNGN6  2.72m -0.61m  0.0m
GUNINTVL  0.1
SMOKEGEN  0.0m  -0.30m -3.50m  #SMOKE GENERATOR POSITION
VAPORPO0  5.44m -0.32m -0.97m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.44m -0.32m -0.97m  #VAPOR POSITION (Wings Spread)
HTRADIUS  7m                   #OUTSIDE SPHERE RADIUS
GUNSIGHT FALSE

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 6

CRITAOAP  15deg               #CRITICAL AOA POSITIVE
CRITAOAM -10deg               #CRITICAL AOA NEGATIVE

CRITSPED 500km/h              #CRITICAL SPEED
MAXSPEED 491km/h              #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

VGWSPED1 0kt
VGWSPED2 0.01kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 20.9m^2              #WING AREA

MXIPTAOA 16.5deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 45.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 60kt                 #FULLY MANEUVABLE SPEED
CPITMANE 6.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 2.0                  #YAW STABILITY CONSTANT
CROLLMAN 2.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED
WEAPONCH GUN
GUNPOWER 1

INITIGUN 2000                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB

RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 450km/h              #CRUISING SPEED
REFACRUS 13000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 80kt                 #LANDING SPEED
REFAOALD 8deg                 #LANDING AOA
REFLNRWY 500m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.5                  #LANDING THROTTLE SETTING

AUTOCALC
