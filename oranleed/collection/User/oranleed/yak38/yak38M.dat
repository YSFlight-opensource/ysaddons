REM YAK38
IDENTIFY "Yak-38M Forger"
CATEGORY FIGHTER
AFTBURNR TRUE                #HAVE AFTERBURNER?
THRAFTBN  12.04t                #THRUST WITH AFTERBURNER
THRMILIT  6.93t               #THRUST AT MILITARY POWER10.5
WEIGHCLN  7.38t               #WEIGHT CLEAN
WEIGFUEL  2.2t               #WEIGHT OF FUEL
WEIGLOAD  2.0t                #WEIGHT OF PAYLOAD
FUELABRN  1.6kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.8kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.72m   5.49m  #COCKPIT POSITION
LEFTGEAR -1.53m -1.62m  -0.56m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.53m -1.62m  -0.56m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.88m   5.68m  #WHEEL POSITION
MACHNGUN  1.41m -0.23m   1.65m  #MACHINE GUN POSITION
GUNINTVL 0.04
SMOKEGEN  1.99m  -0.41m   -1.03m  #SMOKE GENERATOR POSITION
VAPORPO0  3.47m -0.12m  -2.19m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  3.47m -0.12m  -2.19m  #VAPOR POSITION (Wings Spread)
HTRADIUS  9.00m                 #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -13deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.95MACH             #CRITICAL SPEED
MAXSPEED 1.1MACH             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE               #HAS VARIABLE GEOMETRY WING?

TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m 0.0m 1.0m       #Thrust vector 0
TRSTDIR1 0.0m 1.0m 0.0m       #Thrust vector 1
PSTMPTCH 10deg                #Post-Stall Maneuver VPitch
PSTMYAW_ 10deg                #Post-Stall Maneuver VYaw
PSTMROLL 10deg                #Post-Stall Maneuver VRoll


CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 18.5m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 3.0deg               #MAX INPUT SSA
MXIPTROL 240.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 120kt                 #FULLY MANEUVABLE SPEED
CPITMANE 7.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1 39kt
VGWSPED2 40kt

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                  #INITIAL FUEL
INITLOAD 0.3t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

HRDPOINT -1.42m -0.22m 0.55m  B250 RKT AIM9
HRDPOINT -2.01m -0.35m 0.1m B500 AGM65 RKT AIM9 B250
HRDPOINT 2.01m -0.35m 0.1m B500 AGM65 RKT AIM9 B250

MAXNMRKT 24
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
GUNPOWER 1
INITIGUN 1500                 #INITIAL GUN
LOADWEPN AIM9 1
LOADWEPN AGM65 2

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.85MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.7                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 140kt                 #LANDING SPEED
REFAOALD 12deg                 #LANDING AOA
REFLNRWY 1200m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
