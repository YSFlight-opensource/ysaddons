REM Sopwith Camel F1
IDENTIFY "Sopwith Camel F1(130)"
CATEGORY WW2FIGHTER
AFTBURNR FALSE                
REM THRAFTBN  0.0t               
REM THRMILIT  1.4t               
PROPELLR 130HP                 
WEIGHCLN  0.43t                 
WEIGFUEL  0.11t                 
WEIGLOAD  0.11t                
REM FUELABRN  0.0kg               
FUELMILI  0.05kg               

COCKPITP  0.00m  0.30m  -0.34m  
INSTPANL user/oranleed/camel/camel.ist
LEFTGEAR  -0.72m  -1.95m 0.35m  
RIGHGEAR  0.72m  -1.95m 0.35m  
WHELGEAR  0.00m  -0.77m -4.09m  
MACHNGUN  0.12m  0.11m  1.05m  
MACHNGN2 -0.12m  0.11m  1.05m  
SMOKEGEN  0.00m  0.0m -1.14m  
VAPORPO0  4.27m  0.70m -0.28m  
VAPORPO1  4.27m  0.70m -0.28m
HTRADIUS  7.00m
GUNINTVL  0.13   
GUNSIGHT FALSE             

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 6                     

CRITAOAP  15.5deg                
CRITAOAM -10deg                

CRITSPED 190km/h               
MAXSPEED 181km/h               

HASSPOIL FALSE                 
RETRGEAR FALSE                 
VARGEOMW FALSE                 

CLVARGEO 0.0                   #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                   #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.0                   
CDBYFLAP 0.0                   
CDBYGEAR 0.0                   
CDSPOILR 0.0                   

WINGAREA 21.46m^2              

MXIPTAOA  16.5deg              
MXIPTSSA  7.0deg              
MXIPTROL 50.0deg              

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 30km/h                
MANESPD2 70km/h                
CPITMANE 5.5                   
CPITSTAB 2.0                   
CYAWMANE 4.0                   
CYAWSTAB 2.5                   
CROLLMAN 2.5                   

CTLLDGEA FALSE                 
CTLBRAKE FALSE                 
CTLSPOIL FALSE                 
CTLABRNR FALSE                 
CTLTHROT 0.0                   
CTLIFLAP 0.0                   
CTLINVGW 0.0                   
CTLATVGW FALSE                 

POSITION 0m 3ft 0m             
ATTITUDE 45deg 0deg 0deg       
INITFUEL 75%                   
INITLOAD 0.0t                  
INITSPED 0.0MACH               

WEAPONCH GUN
REM GUNPOWER 2

INITIGUN 1000                   GUM
INITIAAM 0                      AAM
INITIAGM 0                      AGM
INITBOMB 0                      BOMB

RADARCRS 0.001                 

REM REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 170km/h               
REFACRUS 6000ft               
REFTCRUS 0.8                   

REM REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 68km/h                
REFAOALD 7deg                 
REFLNRWY 800m                 
REFTHRLD 0.5                   

REM SMOKEOIL 0
REM MAXNMGUN 1000
REM MAXNMAAM 0
REM MAXNMAGM 0
REM MAXNBOMB 0
REM MAXNMRKT 0
REM AAMSLOT  0.0m 0.0m 0.0m
REM AGMSLOT  0.0m 0.0m 0.0m
REM BOMBSLOT 0.0m 0.0m 0.0m
REM RKTSLOT  0.0m 0.0m 0.0m
REM AAMVISIB FALSE
REM AGMVISIB FALSE
REM BOMVISIB FALSE
REM RKTVISIB FALSE

AUTOCALC
