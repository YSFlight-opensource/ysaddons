REM FIAT G50 Freccia
IDENTIFY "FIAT G50 Freccia"
CATEGORY WW2FIGHTER
AFTBURNR FALSE                 #HAVE AFTERBURNER?
PROPELLR 840HP              #PROPELLER ENGINE
WEIGHCLN  1.975t                #WEIGHT CLEAN
WEIGFUEL  0.2t                #WEIGHT OF FUEL
WEIGLOAD  0.2t                #WEIGHT OF PAYLOAD
FUELMILI 0.1kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  1.1m  -1.40m  #COCKPIT POSITION
LEFTGEAR -1.18m -1.43m  0.33m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.18m -1.43m  0.33m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -0.27m -4.47m  #WHEEL POSITION
SMOKEGEN  0.00m  -0.33m -0.60m  #SMOKE GENERATOR POSITION
VAPORPO0  5.49m  0.17m  -0.02m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.49m  0.17m  -0.02m   #VAPOR POSITION (Wings Spread)
HTRADIUS  8m                #OUTSIDE SPHERE RADIUS
GUNSIGHT FALSE
INSTPANL user/oranleed/g50/g50.ist
MACHNGUN  0.27m 0.75m  0.68m  #MACHINE GUN POSITION
MACHNGN2 -0.27m 0.75m  0.68m  #MACHINE GUN POSITION

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 6

CRITAOAP  15deg               #CRITICAL AOA POSITIVE
CRITAOAM -10deg               #CRITICAL AOA NEGATIVE

CRITSPED 513km/h              #CRITICAL SPEED
MAXSPEED 473km/h             #MAXIMUM SPEED

HASSPOIL FALSE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.9                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 18.15m^2                #WING AREA

MXIPTAOA 16.5deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 50.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 30kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 55kt                 #FULLY MANEUVABLE SPEED
CPITMANE 6.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3                  #YAW STABILITY CONSTANT
CROLLMAN 2.7                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED


HRDPOINT 2.25m -0.45m -0.01m B250
HRDPOINT -2.25m -0.45m -0.01m B250
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
GUNPOWER 1
INITIGUN 2000                  #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0

MAXNBOMB 2
RADARCRS 0.001
REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 420km/h              #CRUISING SPEED(*1.2
REFACRUS 13000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 65kt                 #LANDING SPEED
REFAOALD 7.0deg                #LANDING AOA
REFLNRWY 650m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.65                  #LANDING THROTTLE SETTING

AUTOCALC
