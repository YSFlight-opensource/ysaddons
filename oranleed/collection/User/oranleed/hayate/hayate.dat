REM hayate
IDENTIFY "hayate(OCP Type4)"
CATEGORY WW2FIGHTER
AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 2000HP               #PROPELLER ENGINE
WEIGHCLN  2.698t                #WEIGHT CLEAN
WEIGFUEL  0.5t                #WEIGHT OF FUEL
WEIGLOAD  0.4t                #WEIGHT OF PAYLOAD
FUELMILI  0.15kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m   0.95m  -1.62m  #COCKPIT POSITION
INSTPANL user/oranleed/hayate/hayate.ist
LEFTGEAR -1.78m -1.93m  0.49m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.78m -1.93m  0.49m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m  -0.48m  -5.80m  #WHEEL POSITION
ARRESTER  0.0m  -0.45m  -5.83m
MACHNGUN  1.98m  -0.30m  0.64m  #MACHINE GUN POSITION
MACHNGN2 -1.98m  -0.30m  0.64m  #MACHINE2 GUN POSITION
MACHNGN3  0.25m  0.69m  1.68m  #MACHINE GUN POSITION
MACHNGN4 -0.25m  0.69m  1.68m  #MACHINE2 GUN POSITION
GUNINTVL 0.08
SMOKEGEN  0.0m   0.00m -7.12m  #SMOKE GENERATOR POSITION
VAPORPO0  5.62m  0.06m  -0.47m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.62m  0.06m  -0.47m  #VAPOR POSITION (Wings Spread)
HTRADIUS  7.0m                 #OUTSIDE SPHERE RADIUS

GUNSIGHT FALSE

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  15deg               #CRITICAL AOA POSITIVE
CRITAOAM -10deg               #CRITICAL AOA NEGATIVE

CRITSPED 750km/h              #CRITICAL SPEED
MAXSPEED 624km/h              #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

VGWSPED1 130kt
VGWSPED2 220km/h

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.2                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.7                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 21.0m^2                #WING AREA

MXIPTAOA 18.5deg              #MAX INPUT AOA
MXIPTSSA 8.0deg               #MAX INPUT SSA
MXIPTROL 80.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 30kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 55kt                 #FULLY MANEUVABLE SPEED
CPITMANE 7.0                #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED
WEAPONCH GUN

INITIGUN 2000                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB
MAXNBOMB 2

BOMBSLOT -2.68m -0.68m -0.48m
BOMBSLOT 2.68m -0.68m -0.48m

RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 550km/h              #CRUISING SPEED
REFACRUS 7000ft             #CRUISING ALTITUDE
REFTCRUS 0.6               #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 75kt                 #LANDING SPEED
REFAOALD 8.0deg                #LANDING AOA
REFLNRWY 600m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.35                 #LANDING THROTTLE SETTING

AUTOCALC
