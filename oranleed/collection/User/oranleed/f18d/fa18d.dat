REM F-18 HORNET
IDENTIFY "F/A-18D_HORNET_[ocp]"
CATEGORY ATTACKER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 16.0t                #THRUST WITH AFTERBURNER
THRMILIT  9.6t                #THRUST AT MILITARY POWER
WEIGHCLN 10.8t                #WEIGHT CLEAN
WEIGFUEL  4.9t                #WEIGHT OF FUEL
WEIGLOAD  9.7t                #WEIGHT OF PAYLOAD
FUELABRN 12.6kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.95kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  1.34m  3.00m  #COCKPIT POSITION
LEFTGEAR -1.78m -1.75m -2.92m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.78m -1.75m -2.92m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.52m  3.46m  #WHEEL POSITION
MACHNGUN  0.00m  0.67m  6.20m  #MACHINE GUN POSITION
ARRESTER  0.00m -1.12m -8.70m  #ARRESTING HOOK
SMOKEGEN -0.30m -0.35m -7.75m  #SMOKE GENERATOR POSITION
VAPORPO0  5.65m  0.26m -3.94m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.65m  0.26m -3.94m  #VAPOR POSITION (Wings Spread)
HTRADIUS 10.0m                  #OUTSIDE SPHERE RADIUS
GUNINTVL 0.03

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.8MACH              #CRITICAL SPEED
MAXSPEED 1.8MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.2                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.3                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 37m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 320.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 100kt                #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 110%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED



HRDPOINT 3.5m -0.6m -2m B500 
HRDPOINT -3.5m -0.6m -2m B500 
HRDPOINT 3.7m -0.3m -2m B500
HRDPOINT -3.7m -0.3m -2m B500
HRDPOINT 3.3m -0.3m -2m B500
HRDPOINT -3.3m -0.3m -2m B500
HRDPOINT -2.5m -0.6m -1.5m B500
HRDPOINT 2.5m -0.6m -1.5m B500
HRDPOINT -2.7m -0.3m -1.5m B500 AGM65
HRDPOINT 2.7m -0.3m -1.5m B500 AGM65
HRDPOINT -2.3m -0.3m -1.5m B500 AGM65
HRDPOINT 2.3m -0.3m -1.5m B500 AGM65
HRDPOINT -5.8m 0.24m -2.8m AIM9
HRDPOINT 5.8m 0.24m -2.8m AIM9
HRDPOINT -1.2m -0.4m -2.8m AIM120
HRDPOINT 1.2m -0.4m -2.8m AIM120


LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 1500                  #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITBOMB 6                    #INITIAL BOMB
INITIAGM 4

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.8MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.7                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 130kt               #LANDING SPEED
REFAOALD 10.5deg               #LANDING AOA
REFLNRWY 800m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                 #LANDING THROTTLE SETTING

AUTOCALC
