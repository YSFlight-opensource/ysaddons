REM shyoki
IDENTIFY "shyoki (OCP type2 II)"
CATEGORY WW2FIGHTER
AFTBURNR FALSE                 #HAVE AFTERBURNER?
PROPELLR 1400HP              #PROPELLER ENGINE
WEIGHCLN  2.1t                #WEIGHT CLEAN
WEIGFUEL  0.5t                #WEIGHT OF FUEL
WEIGLOAD  0.4t                #WEIGHT OF PAYLOAD
FUELMILI  0.15kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.86m -1.69m  #COCKPIT POSITION
INSTPANL user/oranleed/shyoki/shyoki.ist
LEFTGEAR  -1.56m -1.80m  -0.52m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.56m -1.80m  -0.52m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -0.18m -7.17m  #WHEEL POSITION
ARRESTER  0.0m  -0.15m  -7.14m
MACHNGUN  1.98m  -0.40m  0.83m  #MACHINE GUN POSITION
MACHNGN2 -1.98m  -0.40m  0.83m  #MACHINE GUN POSITION
MACHNGN3  0.29m  0.57m  1.45m  #MACHINE GUN POSITION
MACHNGN4 -0.29m  0.57m  1.45m  #MACHINE2 GUN POSITION
GUNINTVL 0.08
SMOKEGEN  0.0m   0.10m -6.00m  #SMOKE GENERATOR POSITION
VAPORPO0 -4.72m   -0.12m  0.00m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.72m   -0.12m  0.00m  #VAPOR POSITION (Wings Spread)
HTRADIUS  6.5m                #OUTSIDE SPHERE RADIUS

GUNSIGHT FALSE


TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 6

CRITAOAP 14deg               #CRITICAL AOA POSITIVE
CRITAOAM -10deg               #CRITICAL AOA NEGATIVE

CRITSPED 650km/h            #CRITICAL SPEED
MAXSPEED 615km/h             #MAXIMUM SPEED

HASSPOIL FALSE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE               #HAS VARIABLE GEOMETRY WING?

VGWSPED1 100kt
VGWSPED2 220kt

CLVARGEO 0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.6                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 15m^2                #WING AREA

MXIPTAOA 18.0deg              #MAX INPUT AOA
MXIPTSSA 6.0deg               #MAX INPUT SSA
MXIPTROL 85.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 60kt                 #FULLY MANEUVABLE SPEED
CPITMANE 7.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.5                #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE               #CONTROL AUTO VARIABLE GEOMETRY WING



POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 15deg 0deg      #ATTITUDE
INITFUEL 75%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED
WEAPONCH GUN

INITIGUN 2000                  #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOM
MAXNBOMB 2

BOMBSLOT -2.17m -0.71m -0.82m
BOMBSLOT 2.17m -0.71m -0.82m

RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 565km/h              #CRUISING SPEED(*1.2
REFACRUS 13123ft              #CRUISING ALTITUDE
REFTCRUS 0.7               #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 78kt                 #LANDING SPEED
REFAOALD 8deg                #LANDING AOA
REFLNRWY 700m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.4                #LANDING THROTTLE SETTING

AUTOCALC
