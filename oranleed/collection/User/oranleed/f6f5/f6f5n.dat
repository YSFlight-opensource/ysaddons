REM f6f5
IDENTIFY "f6f5n"
CATEGORY WW2FIGHTER
AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 2200HP               #PROPELLER ENGINE
WEIGHCLN  4.190t                #WEIGHT CLEAN
WEIGFUEL  0.6t                #WEIGHT OF FUEL
WEIGLOAD  0.5t                #WEIGHT OF PAYLOAD
FUELMILI  0.15kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m   1.52m  -1.10m  #COCKPIT POSITION
INSTPANL user/oranleed/f6f5/f6f.ist
LEFTGEAR -1.85m -1.83m  0.29m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.85m -1.83m  0.29m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m  -0.55m -5.96m  #WHEEL POSITION
MACHNGUN  2.52m  0.08m  0.61m  #MACHINE GUN POSITION
MACHNGN2 -2.52m  0.08m  0.61m  #MACHINE2 GUN POSITION
MACHNGN3 -2.54m  0.10m  0.61m
MACHNGN4  2.54m  0.10m  0.61m
MACHNGN5 -2.79m  0.14m  0.45m
MACHNGN6  2.79m  0.14m  0.45m
GUNINTVL 0.09
SMOKEGEN  0.0m   0.00m -5.50m  #SMOKE GENERATOR POSITION
VAPORPO0  6.53m 0.65m -0.68m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  6.53m 0.65m -0.68m  #VAPOR POSITION (Wings Spread)
HTRADIUS  7m                 #OUTSIDE SPHERE RADIUS
ARRESTER  0.0m -0.55m -1.14m

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 11

CRITAOAP  15deg               #CRITICAL AOA POSITIVE
CRITAOAM -10deg               #CRITICAL AOA NEGATIVE

CRITSPED 700km/h              #CRITICAL SPEED
MAXSPEED 612km/h              #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                #HAS VARIABLE GEOMETRY WING?

VGWSPED1 100kt
VGWSPED2 350km/h

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                 #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 31.03m^2                #WING AREA

MXIPTAOA 18.0deg              #MAX INPUT AOA
MXIPTSSA 3.0deg               #MAX INPUT SSA
MXIPTROL 70.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 30kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 60kt                 #FULLY MANEUVABLE SPEED
CPITMANE 7.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 2.0                  #YAW STABILITY CONSTANT
CROLLMAN 2.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED
WEAPONCH GUN
GUNPOWER 1
INITIGUN 2000                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB
MAXNBOMB 2

BOMBSLOT -0.92m -0.31m 0.02m
BOMBSLOT 0.92m -0.31m 0.02m

RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 560km/h              #CRUISING SPEED
REFACRUS 13000ft              #CRUISING ALTITUDE
REFTCRUS 0.7                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 77kt                 #LANDING SPEED
REFAOALD 8deg                #LANDING AOA
REFLNRWY 600m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.4                  #LANDING THROTTLE SETTING

AUTOCALC
