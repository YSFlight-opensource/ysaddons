REM hien
IDENTIFY "hien (Army Type3,OCP)"
CATEGORY WW2FIGHTER
AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 1180HP               #PROPELLER ENGINE
WEIGHCLN  2.380t                #WEIGHT CLEAN
WEIGFUEL  0.5t                #WEIGHT OF FUEL
WEIGLOAD  0.4t                #WEIGHT OF PAYLOAD
FUELMILI  0.12kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m   0.74m  -0.67m  #COCKPIT POSITION
INSTPANL user/oranleed/hien/hien.ist
LEFTGEAR -1.54m -1.88m  0.09m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.54m -1.88m  0.09m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m  -0.40m -6.27m  #WHEEL POSITION
ARRESTER  0.0m  -0.37m -6.24m
MACHNGUN  0.18m  0.62m  1.1m  #MACHINE GUN POSITION
MACHNGN2 -0.18m  0.62m  1.1m  #MACHINE2 GUN POSITION
MACHNGN3  1.98m  -0.30m  0.64m  #MACHINE GUN POSITION
MACHNGN4 -1.98m  -0.30m  0.64m  #MACHINE2 GUN POSITION
GUNINTVL 0.08

SMOKEGEN  0.0m   -0.33m -6.00m  #SMOKE GENERATOR POSITION
VAPORPO0  6.00m  0.03m  0.00m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  6.00m  0.03m  0.00m  #VAPOR POSITION (Wings Spread)
HTRADIUS  6.0m                 #OUTSIDE SPHERE RADIUS

GUNSIGHT FALSE

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 6

CRITAOAP  15deg               #CRITICAL AOA POSITIVE
CRITAOAM -10deg               #CRITICAL AOA NEGATIVE

CRITSPED 800km/h              #CRITICAL SPEED
MAXSPEED 590km/h              #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE             #HAS VARIABLE GEOMETRY WING?

VGWSPED1 110kt
VGWSPED2 200km/h

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.1                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.8                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 22.0m^2                #WING AREA

MXIPTAOA 18.0deg              #MAX INPUT AOA
MXIPTSSA 8.0deg               #MAX INPUT SSA
MXIPTROL 80.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 30kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 60kt                 #FULLY MANEUVABLE SPEED
CPITMANE 7.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED
WEAPONCH GUN

HRDPOINT 2.35m -0.46m -0.16m B250
HRDPOINT -2.35m -0.46m -0.16m B250
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 2000                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB
MAXNBOMB 2


RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 550km/h              #CRUISING SPEED
REFACRUS 13123ft              #CRUISING ALTITUDE
REFTCRUS 0.6                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 75kt                 #LANDING SPEED
REFAOALD 8deg                #LANDING AOA
REFLNRWY 600m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.37                 #LANDING THROTTLE SETTING

AUTOCALC
