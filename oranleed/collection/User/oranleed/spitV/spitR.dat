REM Spitfire
IDENTIFY "Spitfire Vb R"
CATEGORY WW2FIGHTER
AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 1440HP               #PROPELLER ENGINE
WEIGHCLN  2.299t                #WEIGHT CLEAN
WEIGFUEL  0.5t                #WEIGHT OF FUEL
WEIGLOAD  0.4t                #WEIGHT OF PAYLOAD
FUELMILI  0.1kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m   0.58m  -1.53m  #COCKPIT POSITION
INSTPANL user/oranleed/spitv/spit.ist
LEFTGEAR -0.72m -1.89m  0.15m  #LEFT LANDING GEAR POSITION
RIGHGEAR  0.72m -1.89m  0.15m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m  -0.47m -6.06m  #WHEEL POSITION
ARRESTER  0.0m  -0.47m -6.06m
MACHNGUN  2.02m  -0.43m  1.34m  #MACHINE GUN POSITION
MACHNGN2 -2.02m  -0.43m  1.34m  #MACHINE2 GUN POSITION
MACHNGN3 -2.86m  -0.34m  0.56m
MACHNGN4  2.86m  -0.34m  0.56m
MACHNGN5 -3.64m  -0.26m  0.51m
MACHNGN6  3.64m  -0.26m  0.51m
GUNINTVL 0.08
SMOKEGEN  0.0m   -0.33m -6.00m  #SMOKE GENERATOR POSITION
VAPORPO0  5.61m  -0.09m  -0.24m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.61m  -0.09m  -0.24m  #VAPOR POSITION (Wings Spread)
HTRADIUS  6.0m                 #OUTSIDE SPHERE RADIUS

GUNSIGHT FALSE

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  15deg               #CRITICAL AOA POSITIVE
CRITAOAM -10deg               #CRITICAL AOA NEGATIVE

CRITSPED 850km/h              #CRITICAL SPEED
MAXSPEED 602km/h              #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE             #HAS VARIABLE GEOMETRY WING?

VGWSPED1 150kt
VGWSPED2 250kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.2                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.6                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 22.5m^2                #WING AREA

MXIPTAOA 18.0deg              #MAX INPUT AOA
MXIPTSSA 8.0deg               #MAX INPUT SSA
MXIPTROL 66.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 30kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 60kt                 #FULLY MANEUVABLE SPEED
CPITMANE 7.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED
WEAPONCH GUN
GUNPOWER 1

HRDPOINT 2.19m -0.78m -0.53m B250
HRDPOINT -2.19m -0.78m -0.53m B250
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 3000                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB
MAXNBOMB 2

RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 550km/h              #CRUISING SPEED
REFACRUS 13123ft              #CRUISING ALTITUDE
REFTCRUS 0.7                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 68kt                 #LANDING SPEED
REFAOALD 8deg                #LANDING AOA
REFLNRWY 600m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.37                 #LANDING THROTTLE SETTING

AUTOCALC
