REM DASSAULT MIRAGE F1cmg)
IDENTIFY "MIRAGE_F1(cmg2)"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  7.2t                #THRUST WITH AFTERBURNER
THRMILIT  5t                #THRUST AT MILITARY POWER
WEIGHCLN  7.4t                #WEIGHT CLEAN
WEIGFUEL  3.5t                #WEIGHT OF FUEL
WEIGLOAD  3.5t                #WEIGHT OF PAYLOAD
FUELABRN 3.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.45kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  0.75m  4.35m  #COCKPIT POSITION
LEFTGEAR -1.47m -1.94m -1.19m   #LEFT LANDING GEAR POSITION
RIGHGEAR  1.47m -1.94m -1.19m   #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.96m  3.92m  #WHEEL POSITION
MACHNGUN  0.29m -0.45m  2.44m  #MACHINE GUN POSITION
MACHNGN2 -0.29m -0.45m  2.44m  #MACHINE GUN POSITION
GUNINTVL 0.04
SMOKEGEN -0.0m  0.65m -7.30m  #SMOKE GENERATOR POSITION
VAPORPO0  4.34m 0.17m -3.50m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.34m 0.17m -3.50m  #VAPOR POSITION (Wings Spread)
HTRADIUS  9.0m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  21deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.2MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.1                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.1                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.10                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.6                 #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 25m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 300.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 80kt                #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1 150kt
VGWSPED2 350kt

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED


HRDPOINT 1.6m -0.1m -0.62m AIM9 AGM65 B250 AIM120
HRDPOINT -1.6m -0.1m -0.62m AIM9 AGM65 B250 AIM120
HRDPOINT -4.5m 0.21m -2.4m AIM9
HRDPOINT 4.5m 0.21m -2.4m AIM9
HRDPOINT 3.12m -0.05m -1.45m AIM9 RKT
HRDPOINT 0m -0.95m -0.14m AGM65 B500
HRDPOINT -3.12m -0.05m -1.45m AIM9 RKT
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

GUNPOWER 1
INITIGUN 2000                 #INITIAL GUN
LOADWEPN AIM120 2                    #INITIAL AAM
LOADWEPN AIM9 4                    #INITIAL AGM
INITRCKT 24
INITBOMB 0
MAXNBOMB 3
MAXNMAAM 6
MAXNMAGM 3


REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.7                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 122kt                #LANDING SPEED
REFAOALD 12deg                #LANDING AOA
REFLNRWY 700m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
