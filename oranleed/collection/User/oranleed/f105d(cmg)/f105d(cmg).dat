REM F105D
IDENTIFY "F105D(cmg) Thunderchief"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 12.3t                #THRUST WITH AFTERBURNER
THRMILIT  7.3t                #THRUST AT MILITARY POWER
WEIGHCLN 12.7t                #WEIGHT CLEAN
WEIGFUEL  6.5t                #WEIGHT OF FUEL
WEIGLOAD  5.9t                #WEIGHT OF PAYLOAD
FUELABRN 9.5kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.45kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  1.15m  4.95m  #COCKPIT POSITION
LEFTGEAR -2.6m -2.40m -0.82m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.6m -2.40m -0.82m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.60m  5.50m  #WHEEL POSITION
MACHNGUN  -0.47m -0.43m  8.41m  #MACHINE GUN POSITION
SMOKEGEN -0.0m -0.75m -5.70m  #SMOKE GENERATOR POSITION
VAPORPO0  5.0m  0.00m -4.00m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.0m  0.00m -4.00m  #VAPOR POSITION (Wings Spread)
HTRADIUS  11.0m                #OUTSIDE SPHERE RADIUS
GUNINTVL 0.03
TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.03MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.15                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.3                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 35.8m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 3.0deg               #MAX INPUT SSA
MXIPTROL 220.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 120kt                #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 2.5                  #YAW STABILITY CONSTANT
CROLLMAN 2.5                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 1
INITIGUN 2000                 #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 6
MAXNMRKT 36
MAXNMAAM 4
MAXNMAGM 2
MAXNBOMB 8

AAMSLOT_ 4.03m -0.75m -1.48m
AAMSLOT_ -4.03m -0.75m -1.48m

AGMSLOT_ -4.03m -0.83m -1.48m
AGMSLOT_ 4.03m -0.83m -1.48m

BOMBSLOT 0m -2.1m 1.54m
BOMBSLOT -0.45m -1.55m 1.54m
BOMBSLOT 0.45m -1.55m 1.54m
BOMBSLOT 0m -2.1m -0.46m
BOMBSLOT -0.45m -1.55m -0.46m
BOMBSLOT 0.45m -1.55m -0.46m
BOMBSLOT 4.03m -1m -1.48m
BOMBSLOT -4.03m -1m -1.48m

RKTSLOT_ -4.03m -0.86m -1.48m
RKTSLOT_ 4.03m -0.86m -1.48m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 150kt                #LANDING SPEED
REFAOALD 13deg                #LANDING AOA
REFLNRWY 1000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
