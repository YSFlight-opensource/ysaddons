REM SAAB JAS39 GRIPEN
IDENTIFY "JAS39(hv) Gripen"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  8.2t                #THRUST WITH AFTERBURNER
THRMILIT  5.5t                #THRUST AT MILITARY POWER
WEIGHCLN  6.6t                #WEIGHT CLEAN
WEIGFUEL  1.9t                #WEIGHT OF FUEL
WEIGLOAD  4.0t                #WEIGHT OF PAYLOAD
FUELABRN 2.5kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.45kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  0.58m  3.76m  #COCKPIT POSITION
LEFTGEAR -1.17m -1.83m -1.15m   #LEFT LANDING GEAR POSITION
RIGHGEAR  1.17m -1.83m -1.15m   #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.83m  3.92m  #WHEEL POSITION
MACHNGUN  -0.34m -0.65m  2.60m  #MACHINE GUN POSITION
GUNINTVL 0.04
SMOKEGEN -0.0m  0.65m -7.30m  #SMOKE GENERATOR POSITION
VAPORPO0  4.06m -0.13m -3.73m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.06m -0.13m -3.73m  #VAPOR POSITION (Wings Spread)
HTRADIUS  9.0m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  24deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.0MACH              #MAXIMUM SPEED

TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m 0.0m 1.0m       #Thrust vector 0
TRSTDIR1 0.0m -0.8m -0.2m       #Thrust vector 1
PSTMPTCH 1deg                #Post-Stall Maneuver VPitch
PSTMYAW_ 1deg                #Post-Stall Maneuver VYaw
PSTMROLL 1deg                #Post-Stall Maneuver VRoll


HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.1                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.1                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 30m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 80kt                #FULLY MANEUVABLE SPEED
CPITMANE 9.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.5                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1 200kt
VGWSPED2 400kt

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED


HRDPOINT 4.27m -0.11m -2.71m AIM9
HRDPOINT -4.27m -0.11m -2.71m AIM9
HRDPOINT 0m -1.4m -0.4m B500
HRDPOINT -0.24m -1.07m -0.4m B500
HRDPOINT 0.24m -1.07m -0.4m B500
HRDPOINT 1.95m -0.6m -1.1m AIM9 AIM120 AGM65 B500 
HRDPOINT -1.95m -0.6m -1.1m AIM9 AIM120 AGM65 B500 
HRDPOINT 2.73m -0.7m -1.71m AIM120 AGM65 AIM9
HRDPOINT -2.73m -0.7m -1.71m AIM120 AGM65 AIM9
HRDPOINT 0.62m -0.96m 1.73m RKT



LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

GUNPOWER 1
INITIGUN 2000                 #INITIAL GUN
LOADWEPN AIM9 4                    #INITIAL AAM
LOADWEPN AIM120 2                    #INITIAL AGM
MAXNMRKT 24


REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.7                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 125kt                #LANDING SPEED
REFAOALD 12deg                #LANDING AOA
REFLNRWY 700m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
