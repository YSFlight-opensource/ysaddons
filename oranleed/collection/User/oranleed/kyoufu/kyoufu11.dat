REM KYOUFU
IDENTIFY "kyoufu11(N1K1)"
CATEGORY WW2FIGHTER
AFTBURNR FALSE                  #HAVE AFTERBURNER?
PROPELLR 1480HP                 #PROPELLER ENGINE
WEIGHCLN  2.7t                  #WEIGHT CLEAN
WEIGFUEL  0.4t                  #WEIGHT OF FUEL
WEIGLOAD  0.4t                  #WEIGHT OF PAYLOAD
FUELMILI  0.1kg                #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m   0.98m -1.09m   #COCKPIT POSITION
INSTPANL user/oranleed/kyoufu/kyoufu.ist
LEFTGEAR -4.73m -1.92m -0.60m   #LEFT LANDING GEAR POSITION
RIGHGEAR  4.73m -1.92m -0.60m   #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m  -2.27m  3.64m   #WHEEL POSITION
MACHNGUN  2.52m -0.03m  0.58m   #MACHINE GUN POSITION
MACHNGN2 -2.52m -0.03m  0.58m   #MACHINE2 GUN POSITION
MACHNGN3  0.56m  0.33m  1.65m
MACHNGN4 -0.56m  0.33m  1.65m
GUNINTVL  0.08
SMOKEGEN  0.0m   0.00m -5.50m   #SMOKE GENERATOR POSITION
VAPORPO0  6.00m  0.26m -0.39m   #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  6.00m  0.26m -0.39m   #VAPOR POSITION (Wings Spread)
HTRADIUS  7.0m                  #OUTSIDE SPHERE RADIUS
GUNSIGHT FALSE

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  15deg               #CRITICAL AOA POSITIVE
CRITAOAM -10deg               #CRITICAL AOA NEGATIVE

CRITSPED 550km/h              #CRITICAL SPEED
MAXSPEED 485km/h              #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR FALSE                #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

VGWSPED1 100kt
VGWSPED2 170kt

CLVARGEO 0.20                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.1                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.3                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 23.5m^2              #WING AREA

MXIPTAOA 17.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 50.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 30kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 60kt                 #FULLY MANEUVABLE SPEED
CPITMANE 7.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 2.0                  #YAW STABILITY CONSTANT
CROLLMAN 2.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED
WEAPONCH GUN

HRDPOINT -2.75m -0.30m -0.10m  B250
HRDPOINT  2.75m -0.30m -0.10m  B250
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 2000                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB
MAXNBOMB 2

RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 430km/h              #CRUISING SPEED
REFACRUS 6000ft              #CRUISING ALTITUDE
REFTCRUS 0.7                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 75kt                 #LANDING SPEED
REFAOALD 7.5deg               #LANDING AOA
REFLNRWY 400m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.30                 #LANDING THROTTLE SETTING

AUTOCALC
