REM REISEN
IDENTIFY "Reisen32(A6M3)"
CATEGORY WW2FIGHTER
AFTBURNR FALSE                     #HAVE AFTERBURNER?
PROPELLR 1100HP                    #PROPELLER ENGINE
WEIGHCLN  1.81t                    #WEIGHT CLEAN
WEIGFUEL  0.5t                     #WEIGHT OF FUEL
WEIGLOAD  0.4t                     #WEIGHT OF PAYLOAD
FUELMILI  0.1kg                    #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.87m -0.73m      #COCKPIT POSITION
INSTPANL user/oranleed/reisen/reisen.ist
LEFTGEAR -1.70m -1.97m  0.34m      #LEFT LANDING GEAR POSITION
RIGHGEAR  1.70m -1.97m  0.34m      #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -0.37m -6.16m      #WHEEL POSITION
ARRESTER  0.00m -0.54m -5.43m      #ARRESTING HOCK POSITION
MACHNGUN  0.23m  0.53m  0.91m      #MACHINE GUN POSITION
MACHNGN2 -0.23m  0.53m  0.91m      #MACHINE2 GUN POSITION
MACHNGN3  1.90m -0.27m  0.63m      #MACHINE GUN POSITION
MACHNGN4 -1.90m -0.27m  0.63m      #MACHINE GUN POSITION
GUNINTVL 0.08
SMOKEGEN  0.00m -0.02m -5.00m      #SMOKE GENERATOR POSITION
VAPORPO0 -5.46m  0.01m -0.58m      #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.46m  0.01m -0.58m      #VAPOR POSITION (Wings Spread)
HTRADIUS  7.0m                     #OUTSIDE SPHERE RADIUS
GUNSIGHT FALSE

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 6

CRITAOAP  16deg                    #CRITICAL AOA POSITIVE
CRITAOAM -10deg                    #CRITICAL AOA NEGATIVE

CRITSPED 600km/h                   #CRITICAL SPEED
MAXSPEED 544km/h                   #MAXIMUM SPEED

HASSPOIL FALSE                     #HAS SPOILER?
RETRGEAR TRUE                      #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                     #HAS VARIABLE GEOMETRY WING?
VGWSPED1 170kt                     #VARIABLE GEOMETRY WING SPEED
VGWSPED2 190kt                     #VARIABLE GEOMETRY WING SPEED

CLVARGEO 0.0                       #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                       #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                       #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                       #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                       #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                       #EFFECT OF SPOILER FOR Cd

WINGAREA 21.50m^2                  #WING AREA

MXIPTAOA 18.0deg                   #MAX INPUT AOA
MXIPTSSA 10.0deg                   #MAX INPUT SSA
MXIPTROL 75.0deg                   #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 30kt                      #MINIMUM MANEUVABLE SPEED
MANESPD2 60kt                      #FULLY MANEUVABLE SPEED
CPITMANE 7.0                       #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                       #PITCH STABILITY CONSTANT
CYAWMANE 5.0                       #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                       #YAW STABILITY CONSTANT
CROLLMAN 3.0                       #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                      #CONTROL LANDINGGEAR
CTLBRAKE FALSE                     #CONTROL BRAKE
CTLSPOIL 0.0                       #CONTROL SPOILER
CTLABRNR FALSE                     #CONTROL AFTERBURNER
CTLTHROT 0.0                       #CONTROL THRUST
CTLIFLAP 0.0                       #CONTROL FLAP
CTLINVGW 0.0                       #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                     #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m                 #POSITION
ATTITUDE 45deg 15deg 0deg          #ATTITUDE
INITFUEL 75%                       #INITIAL FUEL
INITLOAD 0.0t                      #INITIAL PAYLOAD
INITSPED 0.0MACH                   #INITIAL SPEED
WEAPONCH GUN

HRDPOINT 0.00m -0.84m -0.54m B250
LMTBYHDP TRUE                      #Limit Max Num Weapon by Hardpoint

INITIGUN 2000                      #INITIAL GUN
INITIAAM 0                         #INITIAL AAM
INITIAGM 0                         #INITIAL AGM
INITBOMB 0                         #INITIAL BOMB
MAXNBOMB 1

RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 480km/h                   #CRUISING SPEED(*1.2
REFACRUS 13000ft                   #CRUISING ALTITUDE
REFTCRUS 0.8                       #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 67kt                      #LANDING SPEED
REFAOALD 7.5deg                    #LANDING AOA
REFLNRWY 500m                      #RUNWAY REQUIRED TO STOP
REFTHRLD 0.4                       #LANDING THROTTLE SETTING

AUTOCALC
