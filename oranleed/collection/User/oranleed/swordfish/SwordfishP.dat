REM swordfish
IDENTIFY "Swordfish Mk II P"
CATEGORY WW2ATTACKER
AFTBURNR FALSE                 #HAVE AFTERBURNER?
PROPELLR 820HP              #PROPELLER ENGINE
WEIGHCLN  2.135t                #WEIGHT CLEAN
WEIGFUEL  0.8t                #WEIGHT OF FUEL
WEIGLOAD  1.1t                #WEIGHT OF PAYLOAD
FUELMILI 0.2kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.19m -0.90m    #COCKPIT POSITION
EXCAMERA "navi" 0m 0.05m  -1.66m  0deg 0deg 0deg INSIDE
EXCAMERA "gunner" 0m  0.08m  -2.2m  180deg 0deg 0deg INSIDE
INSTPANL user/oranleed/swordfish/sf.ist
LEFTGEAR  -1.72m -3.26m  0.56m   #LEFT LANDING GEAR POSITION
RIGHGEAR  1.72m -3.26m  0.56m    #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.73m -6.76m    #WHEEL POSITION
MACHNGUN  0.45m -0.30m  0.58m  #MACHINE GUN POSITION
ARRESTER  0.00m -2.30m -5.11m    #Arresting hook position
SMOKEGEN  0.0m   -1.97m -2.64m    #SMOKE GENERATOR POSITION
VAPORPO0  6.89m   0.58m  -0.60m   #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  6.89m   0.58m  -0.60m   #VAPOR POSITION (Wings Spread)
HTRADIUS  8.0m                   #OUTSIDE SPHERE RADIUS

GUNSIGHT FALSE

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 12

CRITAOAP 15deg               #CRITICAL AOA POSITIVE
CRITAOAM -10deg               #CRITICAL AOA NEGATIVE

CRITSPED 120kt            #CRITICAL SPEED
MAXSPEED 125kt             #MAXIMUM SPEED

HASSPOIL FALSE                 #HAS SPOILER?
RETRGEAR FALSE                #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE               #HAS VARIABLE GEOMETRY WING?

VGWSPED1 0kt
VGWSPED2 0.01kt


CLVARGEO 0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.1                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 56.39m^2                #WING AREA

MXIPTAOA 16.0deg              #MAX INPUT AOA
MXIPTSSA 4.0deg               #MAX INPUT SSA
MXIPTROL 55.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 25kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 50kt                 #FULLY MANEUVABLE SPEED
CPITMANE 6.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.5                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 2.5                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE               #CONTROL AUTO VARIABLE GEOMETRY WING



POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 15deg 0deg      #ATTITUDE
INITFUEL 75%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED


HRDPOINT 0m -2.2m 0.4m AGM65
HRDPOINT -3.24m -1.89m -0.36m B250 
HRDPOINT 3.24m -1.89m -0.36m B250 
HRDPOINT 3.76m -1.89m -0.36m B250
HRDPOINT -3.76m -1.89m -0.36m B250
LMTBYHDP TRUE

INITIGUN 1000                  #INITIAL GUN
LOADWEPN AGM65 1
LOADWEPN B250 4


NMTURRET 1

TURRETPO 0 0m -0.07m -2.655m 180deg 0deg 0deg      # Number x y z h p b
TURRETPT 0 0deg 60deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 -40deg 40deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 1000                              # Ammo
TURRETIV 0 0.2sec                            # Number ShootingInterval
TURRETNM 0 0032                            # DNM Node Name
TURRETAR 0                                   # Anti Air Capable
TURRETGD 0                                   # Anti Ground Capable
TURRETCT 0 GUNNER                            # Controlled by PILOT or GUNNER
TURRETRG 0 1500m



RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 100kt              #CRUISING SPEED(*1.2
REFACRUS 13000ft              #CRUISING ALTITUDE
REFTCRUS 0.6               #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 60kt                 #LANDING SPEED
REFAOALD 7deg                #LANDING AOA
REFLNRWY 500m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.4                 #LANDING THROTTLE SETTING

AUTOCALC
