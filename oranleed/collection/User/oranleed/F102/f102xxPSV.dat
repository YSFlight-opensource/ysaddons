REM Convair F-102 Delta Dagger
IDENTIFY "F-102A Delta Dagger xxPSV"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  7.8t                #THRUST WITH AFTERBURNER
THRMILIT  5.08t                #THRUST AT MILITARY POWER
WEIGHCLN  8.78t                #WEIGHT CLEAN
WEIGFUEL  3.7t                #WEIGHT OF FUEL
WEIGLOAD  3.0t                #WEIGHT OF PAYLOAD
FUELABRN 3.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.45kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  1.18m  6.03m  #COCKPIT POSITION
LEFTGEAR -2.36m -1.79m -0.94m   #LEFT LANDING GEAR POSITION
RIGHGEAR  2.36m -1.79m -0.94m   #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.70m  5.88m  #WHEEL POSITION
MACHNGUN  0.51m -0.85m  5.01m  #MACHINE GUN POSITION
MACHNGN2 -0.51m -0.85m  5.01m  #MACHINE GUN POSITION
MACHNGN3 -0.42m -0.85m  5.01m  #MACHINE GUN POSITION
MACHNGN4 0.42m -0.85m  5.01m  #MACHINE GUN POSITION
GUNINTVL 0.3
SMOKEGEN 0m  -1.15m  0.55m  #SMOKE GENERATOR POSITION
VAPORPO0  5.81m -0.00m -4.90m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.81m -0.00m -4.90m  #VAPOR POSITION (Wings Spread)-013
HTRADIUS  11.0m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  24deg               #CRITICAL AOA POSITIVE
CRITAOAM -17deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 1.25MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.0                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.0                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 64.57m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 3.0deg               #MAX INPUT SSA
MXIPTROL 150.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 100kt                #FULLY MANEUVABLE SPEED
CPITMANE 7.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 4.5                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED


AAMVISIB FALSE
HRDPOINT 0m -1.15m 2.7m AIM120 AIM9
HRDPOINT 0m -1.15m 0.55m AIM120 AIM9
HRDPOINT -1.06m -0.82m 2.70m AIM9 AIM120
HRDPOINT 1.06m -0.82m 2.70m AIM9 AIM120
HRDPOINT -1.06m -0.82m 0.55m AIM9 AIM120
HRDPOINT 1.06m -0.82m 0.55m AIM9 AIM120



LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
GUNPOWER 3
INITIGUN 240                 #INITIAL GUN
LOADWEPN AIM120 2
LOADWEPN AIM9 4



REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 150kt                #LANDING SPEED
REFAOALD 14deg                #LANDING AOA
REFLNRWY 1200m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.35                  #LANDING THROTTLE SETTING

AUTOCALC
