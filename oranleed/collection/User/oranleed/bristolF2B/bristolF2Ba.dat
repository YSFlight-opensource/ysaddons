REM Bristol F2B Fighter
IDENTIFY "Bristol F2B Fighter a"
CATEGORY WW2ATTACKER
AFTBURNR FALSE                
REM THRAFTBN  0.0t               
REM THRMILIT  1.4t               
PROPELLR 275HP                 
WEIGHCLN  0.98t                 
WEIGFUEL  0.15t                 
WEIGLOAD  0.35t                
REM FUELABRN  0.0kg               
FUELMILI  0.05kg               

COCKPITP  0.00m  0.71m  -1.03m  
INSTPANL user/oranleed/BristolF2B/BristolF2B.ist
LEFTGEAR  -0.75m -1.71m -0.00m  
RIGHGEAR  0.75m -1.71m -0.00m
WHELGEAR  0.00m  -0.92m -4.53m  
REM MACHNGUN  0.00m  0.96m  -0.18m  
REM MACHNGN2 -0.00m  0m  0m  
SMOKEGEN  0.00m  0.53m -1.14m  
VAPORPO0  5.94m  1.08m -0.95m  
VAPORPO1  5.94m  1.08m -0.95m  
GUNSIGHT FALSE
HTRADIUS  8.00m                

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10                     

CRITAOAP  14deg                
CRITAOAM -10deg                

CRITSPED 190km/h               
MAXSPEED 198km/h               

HASSPOIL FALSE                 
RETRGEAR FALSE                 
VARGEOMW FALSE                 

CLVARGEO 0.0                   #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                   #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.0                   
CDBYFLAP 0.0                   
CDBYGEAR 0.0                   
CDSPOILR 0.0                   

WINGAREA 37.62.0m^2              

MXIPTAOA  15.0deg              
MXIPTSSA  5.0deg              
MXIPTROL 45.0deg              

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 30km/h                
MANESPD2 70km/h                
CPITMANE 6.0                   
CPITSTAB 4.0                   
CYAWMANE 5.0                   
CYAWSTAB 3.0                   
CROLLMAN 3.0                   

CTLLDGEA FALSE                 
CTLBRAKE FALSE                 
CTLSPOIL FALSE                 
CTLABRNR FALSE                 
CTLTHROT 0.0                   
CTLIFLAP 0.0                   
CTLINVGW 0.0                   
CTLATVGW FALSE                 

POSITION 0m 3ft 0m             
ATTITUDE 45deg 0deg 0deg       
INITFUEL 75%                   
INITLOAD 0.0t                  
INITSPED 0.0MACH               

NMTURRET 1
TURRETPO 0 0m 0.575m -1.694m 180deg 0deg 0deg      # Number x y z h p b
TURRETPT 0 -10deg 40deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 -90deg 90deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 1000                              # Ammo
TURRETIV 0 0.25sec                           # Number ShootingInterval
TURRETNM 0 0008                               # DNM Node Name
TURRETAR 0                                   # Anti Air Capable
REM TURRETGD 0                                   # Anti Ground Capable
TURRETCT 0 GUNNER                             # Controlled by the pilot
TURRETRG 0 1500m

HRDPOINT 1.3m -1.03m -0.24m B250
HRDPOINT -1.3m -1.03m -0.24m B250
HRDPOINT 1.52m -1.03m -0.24m B250
HRDPOINT -1.52m -1.03m -0.24m B250
LMTBYHDP TRUE
LOADWEPN B250 4
REM GUNPOWER 2

INITIGUN 0                      GUM
INITIAAM 0                      AAM
INITIAGM 0                      AGM
INITBOMB 0                      BOMB

RADARCRS 0.001                 

REM REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 188km/h               
REFACRUS 6000ft               
REFTCRUS 0.8                   

REM REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 80km/h                
REFAOALD 7.5deg                 
REFLNRWY 500m                 
REFTHRLD 0.5                   

REM SMOKEOIL 0
REM MAXNMGUN 1000
REM MAXNMAAM 0
REM MAXNMAGM 0
REM MAXNBOMB 0
REM MAXNMRKT 0
REM AAMSLOT  0.0m 0.0m 0.0m
REM AGMSLOT  0.0m 0.0m 0.0m
REM BOMBSLOT 0.0m 0.0m 0.0m
REM RKTSLOT  0.0m 0.0m 0.0m
REM AAMVISIB FALSE
REM AGMVISIB FALSE
REM BOMVISIB FALSE
REM RKTVISIB FALSE

AUTOCALC
