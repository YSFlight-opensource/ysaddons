REM A1H Skyraider
IDENTIFY "A1H Skyraider"
CATEGORY ATTACKER
AFTBURNR FALSE                 #HAVE AFTERBURNER?
PROPELLR 2700HP              #PROPELLER ENGINE
WEIGHCLN  5.429t                #WEIGHT CLEAN
WEIGFUEL  1t                #WEIGHT OF FUEL
WEIGLOAD  4t                #WEIGHT OF PAYLOAD
FUELMILI 0.1kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  1.15m  0.38m  #COCKPIT POSITION
INSTPANL user/oranleed/A1H/A1H.ist
LEFTGEAR -2.35m -2.46m  1.03m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.35m -2.46m  1.03m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -0.99m -5.84m  #WHEEL POSITION
ARRESTER  0.00m -1.40m -7.65m  #ARRESTING HOOK
SMOKEGEN  0.00m  -0.12m -2.815m  #SMOKE GENERATOR POSITION
VAPORPO0  7.57m  -0.13m  -1.03m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  7.57m  -0.13m  -1.03m  #VAPOR POSITION (Wings Spread)
HTRADIUS  9m                #OUTSIDE SPHERE RADIUS
GUNSIGHT FALSE
GUNINTVL 0.07

MACHNGUN  3.31m -0.54m  1.18m  #MACHINE GUN POSITION
MACHNGN2 -3.31m -0.54m  1.18m  #MACHINE GUN POSITION
MACHNGN3  3.83m -0.48m  1.10m
MACHNGN4 -3.83m -0.48m  1.10m

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 11

CRITAOAP  15deg               #CRITICAL AOA POSITIVE
CRITAOAM -10deg               #CRITICAL AOA NEGATIVE

CRITSPED 500km/h              #CRITICAL SPEED
MAXSPEED 518km/h             #MAXIMUM SPEED

HASSPOIL FALSE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

VGWSPED1 80kt
VGWSPED2 200kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.9                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 37.19m^2                #WING AREA

MXIPTAOA 18.0deg              #MAX INPUT AOA
MXIPTSSA 4.0deg               #MAX INPUT SSA
MXIPTROL 85.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 60kt                 #FULLY MANEUVABLE SPEED
CPITMANE 7.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3                  #YAW STABILITY CONSTANT
CROLLMAN 3                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED


HRDPOINT 0.0m -1.42m 1.04m B500
HRDPOINT -2.97m -1.30m 0.36m B500 RKT
HRDPOINT 2.97m -1.30m 0.36m B500 RKT
HRDPOINT -3.82m -0.92m 0.19m B250
HRDPOINT 3.82m -0.92m 0.19m B250
HRDPOINT -4.26m -0.87m 0.16m B250
HRDPOINT 4.26m -0.87m 0.16m B250
HRDPOINT -4.71m -0.82m 0.13m B250
HRDPOINT 4.71m -0.82m 0.13m B250
HRDPOINT -5.11m -0.77m 0.10m B250
HRDPOINT 5.11m -0.77m 0.10m B250
HRDPOINT -5.51m -0.72m 0.07m B250
HRDPOINT 5.51m -0.72m 0.07m B250
HRDPOINT -5.91m -0.67m 0.04m B250
HRDPOINT 5.91m -0.67m 0.04m B250

MAXNMRKT 38
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
GUNPOWER 1
INITIGUN 2000                  #INITIAL GUN
LOADWEPN B500 1
LOADWEPN B250 12
LOADWEPN RKT 36

RADARCRS 0.8
REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 450km/h              #CRUISING SPEED(*1.2
REFACRUS 13000ft              #CRUISING ALTITUDE
REFTCRUS 0.80                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 85kt                 #LANDING SPEED
REFAOALD 8deg                #LANDING AOA
REFLNRWY 600m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.60                  #LANDING THROTTLE SETTING

AUTOCALC
