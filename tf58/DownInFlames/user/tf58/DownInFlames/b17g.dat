REM B17
IDENTIFY "B-17G_{tf58}"
CATEGORY WW2BOMBER
AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 4800HP               #PROPELLER ENGINE
PROPEFCY 0.65                 #Propeller efficiency
WEIGHCLN  32000lb                #WEIGHT CLEAN
WEIGFUEL  16730lb                #WEIGHT OF FUEL       21852lb/3630gal?
WEIGLOAD  12000lb                #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.8lb               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP -0.34m  1.14m  3.43m   #COCKPIT POSITION
EXCAMERA "CO-PILOT" 0.34m  1.14m  3.43m 0deg 0deg 0deg INSIDE
EXCAMERA "BOMBSIGHT" 0.00m 0.01m 6.96m 0deg -64deg 0deg INSIDE
INSTPANL user/tf58/DownInFlames/b17.ist
LEFTGEAR -3.11m -2.79m 1.17m   #LEFT LANDING GEAR POSITION
RIGHGEAR  3.11m -2.79m 1.17m   #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.55m -8.14m   #WHEEL POSITION
SMOKEGEN -3.00m  0.30m  3.80m  #SMOKE GENERATOR POSITION
VAPORPO0  15.96m 0.97m -1.22m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  15.96m 0.97m -1.22m  #VAPOR POSITION (Wings Swept Back or no-VGW)
HTRADIUS  19.5m                #OUTSIDE SPHERE RADIUS
GUNSIGHT FALSE

STRENGTH 70

CRITAOAP   16deg               #CRITICAL AOA POSITIVE
CRITAOAM  -12deg               #CRITICAL AOA NEGATIVE

CRITSPED 330kt               #CRITICAL SPEED
MAXSPEED 400kt               #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                #HAS VARIABLE GEOMETRY WING?

REM The following is for the bomb sight. 
VGWSPED1 110kt
VGWSPED2 250kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.25                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.5                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.7                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 131.92m^2                #WING AREA

MXIPTAOA 20.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 40.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 70kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 120kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 10.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 6.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 2500lb                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

WEAPONCH GUN

GUNPOWER 1
INITIGUN 0                 #INITIAL GUN
GUNINTVL 0.08
HRDPOINT -0.50m 0.00m 0.00m B250 B500
HRDPOINT 0.50m 0.00m 0.00m B250 B500
MAXNB250 8
MAXNBOMB 8
LOADWEPN B500 8
WEAPONCH BOMB
BOMVISIB TRUE
BOMINBAY TRUE                 #Bombs in Bomb Bay?
BMBAYRCS 1.0                  #Effect on Bomb Bay Opening on Radar Cross Section

RADARCRS 0.8






REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 249kt              #CRUISING SPEED
REFACRUS 25000ft              #CRUISING ALTITUDE
REFTCRUS 0.85                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 95kt                 #LANDING SPEED
REFAOALD 6.0deg                 #LANDING AOA
REFLNRWY 600m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.75                  #LANDING THROTTLE SETTING



NMTURRET 6
TURRETPO 0 0m 1.56m 2.08m 0deg 0deg 0deg
TURRETPT 0 0deg 90deg 0deg
TURRETHD 0 -180deg 180deg 0deg
TURRETAM 0 750
TURRETIV 0 0.08sec
TURRETAR 0
TURRETNP 0 "topturretguns"
TURRETNH 0 "topturret"
TURRETCT 0 GUNNER
TURRETRG 0 500m
TURRETPO 1 0.00m -1.07m 6.10m 0deg 0deg 0deg
TURRETPT 1 -50deg 20deg 10deg
TURRETHD 1 -40deg 40deg 0deg
TURRETAM 1 750
TURRETIV 1 0.08sec
TURRETAR 1
TURRETNP 1 "chinguns"
TURRETNH 1 "chinturret"
TURRETCT 1 GUNNER
TURRETRG 1 500m
TURRETPO 2 -0.88m 0.22m -6.65m 120deg 0deg 0deg
TURRETPT 2 -20deg 30deg -10deg
TURRETHD 2 -25deg 25deg 0deg
TURRETAM 2 350
TURRETIV 2 0.16sec
TURRETAR 2
TURRETNM 2 "waistgun_l"
TURRETCT 2 GUNNER
TURRETRG 2 500m
TURRETPO 3 0.97m 0.22m -5.09m -120deg 0deg 0deg
TURRETPT 3 -20deg 30deg -10deg
TURRETHD 3 -25deg 25deg 0deg
TURRETAM 3 350
TURRETIV 3 0.16sec
TURRETAR 3
TURRETNM 3 "waistgun_r"
TURRETCT 3 GUNNER
TURRETRG 3 500m
TURRETPO 4 0m -1.19m -3.93m -180deg 0deg 0deg
TURRETPT 4 -90deg 0deg 0deg
TURRETHD 4 -180deg 180deg 0deg
TURRETAM 4 750
TURRETIV 4 0.08sec
TURRETAR 4
TURRETNM 4 "ballturret"
TURRETCT 4 GUNNER
TURRETRG 4 500m
TURRETPO 5 0m 0.36m -14.90m -180deg 0deg 0deg
TURRETPT 5 -20deg 20deg 0deg
TURRETHD 5 -30deg 30deg 0deg
TURRETAM 5 750
TURRETIV 5 0.08sec
TURRETAR 5
TURRETNM 5 "tailguns"
TURRETCT 5 GUNNER
TURRETRG 5 500m
AUTOCALC
