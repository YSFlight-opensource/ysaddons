REM B17
IDENTIFY "Do_217_E-4_{tf58}"
CATEGORY WW2BOMBER
AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 3160HP               #PROPELLER ENGINE
PROPEFCY 0.65                 #Propeller efficiency
WEIGHCLN  19500lb                #WEIGHT CLEAN
WEIGFUEL  4687lb                #WEIGHT OF FUEL       21852lb/3630gal?
WEIGLOAD  8000lb                #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.4lb               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP -0.23m  0.84m  4.03m   #COCKPIT POSITION
EXCAMERA "BOMBSIGHT" 0.13m -0.78m 5.00m 0deg -64deg 0deg INSIDE
INSTPANL user/tf58/DownInFlames/do217.ist
LEFTGEAR -2.92m -2.52m 1.41m   #LEFT LANDING GEAR POSITION
RIGHGEAR  2.92m -2.52m 1.41m   #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.00m -8.27m   #WHEEL POSITION
SMOKEGEN  2.37m -0.11m 1.48m  #SMOKE GENERATOR POSITION
VAPORPO0  9.45m 0.45m -1.10m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  9.45m 0.45m -1.10m  #VAPOR POSITION (Wings Swept Back or no-VGW)
HTRADIUS  15.0m                #OUTSIDE SPHERE RADIUS
GUNSIGHT FALSE

STRENGTH 40

CRITAOAP   16deg               #CRITICAL AOA POSITIVE
CRITAOAM  -12deg               #CRITICAL AOA NEGATIVE

CRITSPED 330kt               #CRITICAL SPEED
MAXSPEED 400kt               #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                #HAS VARIABLE GEOMETRY WING?

REM The following is for the bomb sight. 
VGWSPED1 110kt
VGWSPED2 250kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.25                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.5                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.7                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 57.10m^2                #WING AREA

MXIPTAOA 20.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 40.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 70kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 120kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 10.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 6.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 2500lb                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

WEAPONCH GUN

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
GUNPOWER 1
INITIGUN 0                 #INITIAL GUN
GUNINTVL 0.08
HRDPOINT -0.29m -0.55m -0.50m B500
HRDPOINT 0.29m -0.55m -0.50m B500
HRDPOINT -0.29m -0.55m -1.83m B500
HRDPOINT 0.29m -0.55m -1.83m B500
HRDPOINT -0.29m -0.55m 0.83m B500
HRDPOINT 0.29m -0.55m 0.83m B500
HRDPOINT -0.29m -0.55m -3.16m B500
HRDPOINT 0.29m -0.55m -3.16m B500
HRDPOINT 5.60m -0.30m 0.00m AGM65
HRDPOINT -5.60m -0.30m 0.00m AGM65
MAXNBOMB 8
LOADWEPN B500 8
LOADWEPN AGM65 0
WEAPONCH BOMB
BOMVISIB TRUE
BOMINBAY TRUE                 #Bombs in Bomb Bay?
BMBAYRCS 1.0                  #Effect on Bomb Bay Opening on Radar Cross Section

RADARCRS 0.8






REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 275kt              #CRUISING SPEED
REFACRUS 17000ft              #CRUISING ALTITUDE
REFTCRUS 0.88                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 95kt                 #LANDING SPEED
REFAOALD 6.0deg                 #LANDING AOA
REFLNRWY 600m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.75                  #LANDING THROTTLE SETTING



NMTURRET 3
TURRETPO 0 0.12m -0.32m 5.82m 0deg 0deg 0deg
TURRETPT 0 -30deg 20deg 0deg
TURRETHD 0 -40deg 30deg 0deg
TURRETAM 0 250
TURRETIV 0 0.11sec
TURRETAR 0
TURRETNM 0 "nosegun"
TURRETCT 0 GUNNER
TURRETRG 0 700m
TURRETPO 1 0.00m 1.08m 2.00m 180deg 0deg 0deg
TURRETPT 1 0deg 45deg 0deg
TURRETHD 1 -160deg 160deg 0deg
TURRETAM 1 250
TURRETIV 1 0.133sec
TURRETAR 1
TURRETNM 1 "dorsalgun"
TURRETCT 1 GUNNER
TURRETRG 1 500m
TURRETPO 2 0.00m -1.26m 2.22m -180deg -10deg 0deg
TURRETPT 2 -35deg 5deg 0deg
TURRETHD 2 -30deg 30deg 0deg
TURRETAM 2 500
TURRETIV 2 0.133sec
TURRETAR 2
TURRETNM 2 "ventralgun"
TURRETCT 2 GUNNER
TURRETRG 2 500m
AUTOCALC
