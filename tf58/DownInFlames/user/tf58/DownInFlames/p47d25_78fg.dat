REM P47
IDENTIFY "P-47D25_78FG_{tf58}"
CATEGORY WW2FIGHTER
AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 2300HP               #PROPELLER ENGINE
PROPEFCY 0.65                 #Propeller efficiency
WEIGHCLN  9950lb                #WEIGHT CLEAN
WEIGFUEL  1848lb                #WEIGHT OF FUEL
WEIGLOAD  2800lb                #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.2kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP 0.0m  1.08m  -0.76m   #COCKPIT POSITION
INSTPANL user/tf58/DownInFlames/p47.ist
LEFTGEAR -2.29m -2.11m 1.08m   #LEFT LANDING GEAR POSITION
RIGHGEAR  2.29m -2.11m 1.08m   #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.09m -5.35m   #WHEEL POSITION
MACHNGUN  2.74m  -0.21m  1.58m  #MACHINE GUN POSITION
MACHNGN2 -2.74m  -0.21m  1.58m  #MACHINE GUN POSITION
MACHNGN3  2.90m  -0.21m  1.45m  #MACHINE GUN POSITION
MACHNGN4 -2.90m  -0.21m  1.45m  #MACHINE GUN POSITION
MACHNGN5  3.06m  -0.21m  1.32m  #MACHINE GUN POSITION
MACHNGN6 -3.06m  -0.21m  1.32m  #MACHINE GUN POSITION
MACHNGN7  3.22m  -0.21m  1.19m  #MACHINE GUN POSITION
MACHNGN8 -3.22m  -0.21m  1.19m  #MACHINE GUN POSITION
SMOKEGEN -0.53m  -0.87m  1.68m  #SMOKE GENERATOR POSITION
VAPORPO0  6.21m  0.13m   0.43m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  6.21m  0.13m   0.43m  #VAPOR POSITION (Wings Swept Back or no-VGW)
HTRADIUS  11.0m                #OUTSIDE SPHERE RADIUS
GUNSIGHT FALSE

STRENGTH 30

CRITAOAP   20deg               #CRITICAL AOA POSITIVE
CRITAOAM  -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 400kt               #CRITICAL SPEED
MAXSPEED 500kt               #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                #HAS VARIABLE GEOMETRY WING?

VGWSPED1 100kt
VGWSPED2 300kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.25                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.6                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.7                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 27.87m^2                #WING AREA

MXIPTAOA 20.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 75.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 70kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 120kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 6.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 6.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 250lb                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

WEAPONCH GUN

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
GUNPOWER 1
INITIGUN 3400                 #INITIAL GUN
GUNINTVL 0.08
HRDPOINT -3.36m -0.78m -0.32m B250 B500
HRDPOINT 3.36m -0.78m -0.32m B250 B500
LOADWEPN B500 0

RADARCRS 0.9

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 373kt              #CRUISING SPEED
REFACRUS 27800ft              #CRUISING ALTITUDE
REFTCRUS 1.0                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 95kt                 #LANDING SPEED
REFAOALD 6.0deg                 #LANDING AOA
REFLNRWY 300m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.75                  #LANDING THROTTLE SETTING



AUTOCALC
