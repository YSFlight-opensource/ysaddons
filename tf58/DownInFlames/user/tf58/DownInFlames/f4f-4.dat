REM F4F Wildcat
IDENTIFY "F4F-4_{tf58}"
CATEGORY WW2FIGHTER
AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 1200HP               #PROPELLER ENGINE
PROPEFCY 0.65                 #Propeller efficiency
WEIGHCLN  4649lb                #WEIGHT CLEAN
WEIGFUEL  864lb                #WEIGHT OF FUEL
WEIGLOAD  1500lb                #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.1kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP 0.0m  1.04m  -1.02m   #COCKPIT POSITION
INSTPANL user/tf58/DownInFlames/f4f.ist
LEFTGEAR -0.99m -1.61m 0.55m   #LEFT LANDING GEAR POSITION
RIGHGEAR  0.99m -1.61m 0.55m   #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -0.645m -4.80m   #WHEEL POSITION
ARRESTER  0.0m -0.46m -6.86m
MACHNGUN  2.00m   0.11m  0.70m  #MACHINE GUN POSITION
MACHNGN2 -2.00m   0.11m  0.70m  #MACHINE GUN POSITION
MACHNGN3  2.20m   0.125m 0.69m  #MACHINE GUN POSITION
MACHNGN4 -2.20m   0.125m 0.69m  #MACHINE GUN POSITION
MACHNGN5  3.10m   0.195m 0.64m  #MACHINE GUN POSITION
MACHNGN6 -3.10m   0.195m 0.64m  #MACHINE GUN POSITION
SMOKEGEN -0.20m  -0.68m  1.00m  #SMOKE GENERATOR POSITION
VAPORPO0  5.65m  0.39m  -1.04m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.65m  0.39m  -1.04m  #VAPOR POSITION (Wings Swept Back or no-VGW)
HTRADIUS  7.50m                #OUTSIDE SPHERE RADIUS
GUNSIGHT FALSE

STRENGTH 25

CRITAOAP   20deg               #CRITICAL AOA POSITIVE
CRITAOAM  -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 350kt               #CRITICAL SPEED
MAXSPEED 450kt               #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                #HAS VARIABLE GEOMETRY WING?

VGWSPED1 100kt
VGWSPED2 300kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.25                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.6                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.7                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 24.20m^2                #WING AREA

MXIPTAOA 20.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 75.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 110kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 6.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 6.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 250lb                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

WEAPONCH GUN

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
GUNPOWER 1
INITIGUN 1440                 #INITIAL GUN
GUNINTVL 0.08
REM HRDPOINT -3.36m -0.78m -0.32m B250 B500
REM HRDPOINT 3.36m -0.78m -0.32m B250 B500
LOADWEPN B500 0

RADARCRS 0.9

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 276kt              #CRUISING SPEED
REFACRUS 19400ft              #CRUISING ALTITUDE
REFTCRUS 1.0                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 80kt                 #LANDING SPEED
REFAOALD 6.0deg                 #LANDING AOA
REFLNRWY 300m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.75                  #LANDING THROTTLE SETTING



AUTOCALC
