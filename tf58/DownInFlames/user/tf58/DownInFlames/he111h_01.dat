REM B17
IDENTIFY "He111-H2_{tf58}"
CATEGORY WW2BOMBER
AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 2200HP               #PROPELLER ENGINE
PROPEFCY 0.65                 #Propeller efficiency
WEIGHCLN  17000lb                #WEIGHT CLEAN
WEIGFUEL  4500lb                #WEIGHT OF FUEL       21852lb/3630gal?
WEIGLOAD  8000lb                #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.4lb               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP -0.35m  0.37m  1.85m   #COCKPIT POSITION
EXCAMERA "CO-PILOT" 0.35m  0.37m  1.85m 0deg 0deg 0deg INSIDE
EXCAMERA "BOMBSIGHT" 0.145m -0.94m 2.94m 0deg -64deg 0deg INSIDE
INSTPANL user/tf58/DownInFlames/he111.ist
LEFTGEAR -2.63m -3.01m 0.76m   #LEFT LANDING GEAR POSITION
RIGHGEAR  2.63m -3.01m 0.76m   #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.14m -10.39m  #WHEEL POSITION
SMOKEGEN  3.03m -0.40m  2.86m  #SMOKE GENERATOR POSITION
VAPORPO0  11.30m 0.56m -2.12m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  11.30m 0.56m -2.12m  #VAPOR POSITION (Wings Swept Back or no-VGW)
HTRADIUS  19.5m                #OUTSIDE SPHERE RADIUS
GUNSIGHT FALSE

STRENGTH 45

CRITAOAP   20deg               #CRITICAL AOA POSITIVE
CRITAOAM  -12deg               #CRITICAL AOA NEGATIVE

CRITSPED 270kt               #CRITICAL SPEED
MAXSPEED 350kt               #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                #HAS VARIABLE GEOMETRY WING?

REM The following is for the bomb sight. 
VGWSPED1 100kt
VGWSPED2 220kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.25                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.5                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.7                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 87.6m^2                #WING AREA

MXIPTAOA 20.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 45.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 70kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 100kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 10.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 6.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 1500lb                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

WEAPONCH GUN

GUNPOWER 1
INITIGUN 0                 #INITIAL GUN
GUNINTVL 0.16
HRDPOINT -0.38m 0.00m 0.00m B250 B500
HRDPOINT 0.38m 0.00m 0.00m B250 B500
MAXNB250 8
MAXNBOMB 8
LOADWEPN B500 4
WEAPONCH BOMB
BOMVISIB TRUE
BOMINBAY TRUE                 #Bombs in Bomb Bay?
BMBAYRCS 1.0                  #Effect on Bomb Bay Opening on Radar Cross Section

RADARCRS 0.8






REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 224kt              #CRUISING SPEED
REFACRUS 16000ft              #CRUISING ALTITUDE
REFTCRUS 0.80                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 80kt                 #LANDING SPEED
REFAOALD 6.0deg                 #LANDING AOA
REFLNRWY 600m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.70                  #LANDING THROTTLE SETTING



NMTURRET 3
TURRETPO 0 0.12m -0.448m 4.026m 0deg 0deg 0deg
TURRETPT 0 -35deg 20deg 0deg
TURRETHD 0 -25deg 25deg 0deg
TURRETAM 0 250
TURRETIV 0 0.24sec
TURRETAR 0
TURRETNM 0 "nosegun"
TURRETCT 0 GUNNER
TURRETRG 0 500m
TURRETPO 1 0.00m 1.01m -2.03m -180deg 0deg 0deg
TURRETPT 1 0deg 40deg 0deg
TURRETHD 1 -30deg 30deg 0deg
TURRETAM 1 250
TURRETIV 1 0.24sec
TURRETAR 1
TURRETNM 1 "dorsalgun"
TURRETCT 1 GUNNER
TURRETRG 1 500m
TURRETPO 2 0.00m -1.29m -4.30m -180deg -15deg 0deg
TURRETPT 2 -35deg 10deg 0deg
TURRETHD 2 -30deg 30deg 0deg
TURRETAM 2 250
TURRETIV 2 0.24sec
TURRETAR 2
TURRETNM 2 "ventralgun"
TURRETCT 2 GUNNER
TURRETRG 2 500m
AUTOCALC
