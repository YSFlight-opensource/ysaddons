REM Spitfire
IDENTIFY "Spitfire_Mk.I_{tf58}"
CATEGORY WW2FIGHTER
AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 1030HP               #PROPELLER ENGINE
PROPEFCY 0.65                 #Propeller efficiency
WEIGHCLN  4810lb                #WEIGHT CLEAN
WEIGFUEL  511lb                #WEIGHT OF FUEL
WEIGLOAD  700lb                #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.2kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP 0.0m  0.55m  -1.02m   #COCKPIT POSITION
INSTPANL user/tf58/DownInFlames/spitfire.ist
LEFTGEAR -0.98m  -1.93m  0.81m  #LEFT LANDING GEAR POSITION
RIGHGEAR  0.98m  -1.93m  0.81m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m   -0.64m -5.61m  #WHEEL POSITION
MACHNGUN  2.13m  -0.53m  1.14m  #MACHINE GUN POSITION
MACHNGN2 -2.13m  -0.53m  1.14m  #MACHINE GUN POSITION
MACHNGN3  2.70m  -0.48m  1.10m  #MACHINE GUN POSITION
MACHNGN4 -2.70m  -0.48m  1.10m  #MACHINE GUN POSITION
MACHNGN5  2.88m  -0.465m 1.09m  #MACHINE GUN POSITION
MACHNGN6 -2.88m  -0.465m 1.09m  #MACHINE GUN POSITION
MACHNGN7  3.81m  -0.383m 1.00m  #MACHINE GUN POSITION
MACHNGN8 -3.81m  -0.383m 1.00m  #MACHINE GUN POSITION
SMOKEGEN -1.10m  -0.91m -0.45m  #SMOKE GENERATOR POSITION
VAPORPO0  5.62m  -0.20m  0.16m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.62m  -0.20m  0.16m  #VAPOR POSITION (Wings Swept Back or no-VGW)
HTRADIUS  9.0m                #OUTSIDE SPHERE RADIUS
GUNSIGHT FALSE

STRENGTH 10

CRITAOAP   20deg               #CRITICAL AOA POSITIVE
CRITAOAM  -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 350kt               #CRITICAL SPEED
MAXSPEED 450kt               #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                #HAS VARIABLE GEOMETRY WING?

VGWSPED1 100kt
VGWSPED2 300kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.25                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.6                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.7                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 22.30m^2                #WING AREA

MXIPTAOA 25.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 116.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 120kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 6.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 6.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 250lb                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

WEAPONCH GUN

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
GUNPOWER 1
INITIGUN 344                 #INITIAL GUN
GUNINTVL 0.4

RADARCRS 0.8

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 318kt              #CRUISING SPEED
REFACRUS 18600ft              #CRUISING ALTITUDE
REFTCRUS 0.85                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 70kt                 #LANDING SPEED
REFAOALD 6.0deg                 #LANDING AOA
REFLNRWY 300m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.75                  #LANDING THROTTLE SETTING



AUTOCALC
