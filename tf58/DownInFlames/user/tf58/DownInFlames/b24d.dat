REM B17
IDENTIFY "B-24D_{tf58}"
CATEGORY WW2BOMBER
AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 4800HP               #PROPELLER ENGINE
PROPEFCY 0.65                 #Propeller efficiency
WEIGHCLN  33980lb                #WEIGHT CLEAN
WEIGFUEL  16800lb                #WEIGHT OF FUEL
WEIGLOAD  12000lb                #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.8lb               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP -0.38m  1.13m  3.25m   #COCKPIT POSITION
EXCAMERA "CO-PILOT" 0.38m  1.13m  3.25m 0deg 0deg 0deg INSIDE
EXCAMERA "BOMBSIGHT" 0.00m 0.00m 6.70m 0deg -64deg 0deg INSIDE
INSTPANL user/tf58/DownInFlames/b24.ist
LEFTGEAR -4.00m -2.34m -0.41m   #LEFT LANDING GEAR POSITION
RIGHGEAR  4.00m -2.34m -0.41m   #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.345m 4.22m   #WHEEL POSITION
SMOKEGEN  7.92m  0.23m  0.49m  #SMOKE GENERATOR POSITION
VAPORPO0  16.69m 1.26m -0.88m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  16.69m 1.26m -0.88m  #VAPOR POSITION (Wings Swept Back or no-VGW)
HTRADIUS  20.00m                #OUTSIDE SPHERE RADIUS
GUNSIGHT FALSE

STRENGTH 50

CRITAOAP   16deg               #CRITICAL AOA POSITIVE
CRITAOAM  -12deg               #CRITICAL AOA NEGATIVE

CRITSPED 330kt               #CRITICAL SPEED
MAXSPEED 400kt               #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                #HAS VARIABLE GEOMETRY WING?

REM The following is for the bomb sight. 
VGWSPED1 110kt
VGWSPED2 250kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.25                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.5                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.7                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 97.40m^2                #WING AREA

MXIPTAOA 20.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 30.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 70kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 120kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 10.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 5.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 2500lb                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

WEAPONCH GUN

GUNPOWER 1
INITIGUN 0                 #INITIAL GUN
GUNINTVL 0.08
HRDPOINT -0.50m 0.00m 0.00m B250 B500
HRDPOINT 0.50m 0.00m 0.00m B250 B500
MAXNB250 8
MAXNBOMB 8
LOADWEPN B500 8
WEAPONCH BOMB
BOMVISIB TRUE
BOMINBAY TRUE                 #Bombs in Bomb Bay?
BMBAYRCS 1.0                  #Effect on Bomb Bay Opening on Radar Cross Section

RADARCRS 0.8






REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 261kt              #CRUISING SPEED
REFACRUS 30000ft              #CRUISING ALTITUDE
REFTCRUS 0.85                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 100kt                 #LANDING SPEED
REFAOALD 6.0deg                 #LANDING AOA
REFLNRWY 600m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.75                  #LANDING THROTTLE SETTING



NMTURRET 6
TURRETPO 0 0.00m 1.59m 1.80m 0deg 0deg 0deg
TURRETPT 0 0deg 90deg 0deg
TURRETHD 0 -180deg 180deg 0deg
TURRETAM 0 750
TURRETIV 0 0.08sec
TURRETAR 0
TURRETNP 0 "topturretguns"
TURRETNH 0 "topturret"
TURRETCT 0 GUNNER
TURRETRG 0 500m
TURRETPO 1 0.00m 0.20m 7.270m 0deg 0deg 0deg
TURRETPT 1 -45deg 5deg 0deg
TURRETHD 1 -30deg 30deg 0deg
TURRETAM 1 350
TURRETIV 1 0.16sec
TURRETAR 1
TURRETNM 1 "nosegun"
TURRETCT 1 GUNNER
TURRETRG 1 500m
TURRETPO 2 -0.87m 0.20m -6.60m 150deg 0deg 0deg
TURRETPT 2 -20deg 30deg 0deg
TURRETHD 2 -70deg 10deg 0deg
TURRETAM 2 350
TURRETIV 2 0.16sec
TURRETAR 2
TURRETNM 2 "waistgun_l"
TURRETCT 2 GUNNER
TURRETRG 2 500m
TURRETPO 3 0.87m 0.20m -6.60m -150deg 0deg 0deg
TURRETPT 3 -20deg 30deg 0deg
TURRETHD 3 -10deg 70deg 0deg
TURRETAM 3 350
TURRETIV 3 0.16sec
TURRETAR 3
TURRETNM 3 "waistgun_r"
TURRETCT 3 GUNNER
TURRETRG 3 500m
TURRETPO 4 0m -1.46m -4.86m -180deg 0deg 0deg
TURRETPT 4 -90deg 0deg 0deg
TURRETHD 4 -180deg 180deg 0deg
TURRETAM 4 750
TURRETIV 4 0.08sec
TURRETAR 4
TURRETNM 4 "ballturret"
TURRETCT 4 GUNNER
TURRETRG 4 500m
TURRETPO 5 0m 0.74m -12.35m -180deg 0deg 0deg
TURRETPT 5 -20deg 40deg 0deg
TURRETHD 5 -70deg 70deg 0deg
TURRETAM 5 750
TURRETIV 5 0.08sec
TURRETAR 5
TURRETNP 5 "tailturretguns"
TURRETNH 5 "tailturret"
TURRETCT 5 GUNNER
TURRETRG 5 500m
AUTOCALC
