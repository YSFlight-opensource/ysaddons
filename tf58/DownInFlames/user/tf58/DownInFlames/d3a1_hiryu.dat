REM D3A1 Type-99
IDENTIFY "D3A1_HIRYU_{tf58}"
CATEGORY WW2DIVEBOMBER
AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 1070HP               #PROPELLER ENGINE
PROPEFCY 0.65                 #Propeller efficiency
WEIGHCLN  5309lb                #WEIGHT CLEAN
WEIGFUEL  1500lb                #WEIGHT OF FUEL
WEIGLOAD  1600lb                #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.1kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP 0.0m  0.92m  -0.36m   #COCKPIT POSITION
INSTPANL user/tf58/DownInFlames/d3a.ist
LEFTGEAR -1.57m -2.12m 0.58m   #LEFT LANDING GEAR POSITION
RIGHGEAR  1.57m -2.12m 0.58m   #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -0.61m -6.77m   #WHEEL POSITION
ARRESTER  0.0m -1.02m -5.76m
MACHNGUN  0.22m   0.54m  2.22m  #MACHINE GUN POSITION
MACHNGN2 -0.22m   0.54m  2.22m  #MACHINE GUN POSITION
SMOKEGEN  0.55m  -0.59m  1.34m  #SMOKE GENERATOR POSITION
VAPORPO0  7.16m  0.09m  -0.65m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  7.16m  0.09m  -0.65m  #VAPOR POSITION (Wings Swept Back or no-VGW)
HTRADIUS  9.00m                #OUTSIDE SPHERE RADIUS
GUNSIGHT FALSE

STRENGTH 15

CRITAOAP   20deg               #CRITICAL AOA POSITIVE
CRITAOAM  -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 280kt               #CRITICAL SPEED
MAXSPEED 350kt               #MAXIMUM SPEED

HASSPOIL TRUE                #HAS SPOILER?
RETRGEAR FALSE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                #HAS VARIABLE GEOMETRY WING?

VGWSPED1 80kt
VGWSPED2 170kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.25                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.6                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 34.90m^2                #WING AREA

MXIPTAOA 20.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 75.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 100kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 6.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 6.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA FALSE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 500lb                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

WEAPONCH GUN

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
GUNPOWER 1
INITIGUN 320                 #INITIAL GUN
GUNINTVL 0.3
HRDPOINT 0.00m -0.99m -0.62m B500
LOADWEPN B500 1

RADARCRS 0.9

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 205kt              #CRUISING SPEED
REFACRUS 12000ft              #CRUISING ALTITUDE
REFTCRUS 0.90                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 70kt                 #LANDING SPEED
REFAOALD 6.0deg                 #LANDING AOA
REFLNRWY 300m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.75                  #LANDING THROTTLE SETTING

NMTURRET 1
TURRETPO 0 0.00m 0.76m -2.40m 180deg 0deg 0deg
TURRETPT 0 0deg 40deg 0deg
TURRETHD 0 -40deg 40deg 0deg
TURRETAM 0 480
TURRETIV 0 0.3sec
TURRETAR 0
TURRETNM 0 "gun"
TURRETCT 0 GUNNER
TURRETRG 0 500m


AUTOCALC
