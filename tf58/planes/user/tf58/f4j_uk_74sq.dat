REM F-4J(UK) PHANTOM
IDENTIFY "F-4J(UK)_No74_Squadron"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 16.1t                #THRUST WITH AFTERBURNER
THRMILIT  10.4t                #THRUST AT MILITARY POWER
WEIGHCLN 14.2t                #WEIGHT CLEAN
WEIGFUEL  7.5t                #WEIGHT OF FUEL
WEIGLOAD  7.2t                #WEIGHT OF PAYLOAD
FUELABRN 8kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 1.00kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.01

COCKPITP  0.0m  0.505m   4.25m  #COCKPIT POSITION
EXCAMERA "RIO" 0.00m  0.80m  2.90m 0deg 0deg 0deg INSIDE
LEFTGEAR -2.51m -2.275m -1.655m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.51m -2.275m -1.655m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.34m  5.315m  #WHEEL POSITION
ARRESTER  0.0m -1.71m -7.275m
MACHNGUN  0.0m -1.25m  7.865m  #MACHINE GUN POSITION
SMOKEGEN -0.0m -0.365m -9.05m  #SMOKE GENERATOR POSITION
VAPORPO0  5.825m  -0.35m -4.59m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.825m  -0.35m -4.59m  #VAPOR POSITION (Wings Spread)
HTRADIUS  15.0m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 16

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.3MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 49m^2                #WING AREA

MXIPTAOA 22.0deg              #MAX INPUT AOA
MXIPTSSA 8.0deg               #MAX INPUT SSA
MXIPTROL 160.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 110kt                #FULLY MANEUVABLE SPEED
CPITMANE 6.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 1.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

HRDPOINT 0.65m -1m 2.4m AIM120
HRDPOINT -0.65m -1m 2.4m AIM120
HRDPOINT 1.0m -1m -2.4m AIM120
HRDPOINT -1.0m -1m -2.4m AIM120

HRDPOINT 2.0m -1.1m 0.2m AIM9
HRDPOINT -2.0m -1.1m 0.2m AIM9
HRDPOINT 2.7m -1.1m 0.2m AIM9
HRDPOINT -2.7m -1.1m 0.2m AIM9

HRDPOINT -2.35m -1.75m 0.2m B500 B250
HRDPOINT 2.35m -1.75m 0.2m B500 B250
HRDPOINT -2.00m -1.45m 0.2m B500 B250
HRDPOINT 2.00m -1.45m 0.2m B500 B250
HRDPOINT -2.70m -1.45m 0.2m B500 B250
HRDPOINT 2.70m -1.45m 0.2m B500 B250

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
INITIGUN 0                 #INITIAL GUN
LOADWEPN AIM120 4
LOADWEPN AIM9 4

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 30000ft              #CRUISING ALTITUDE
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 130kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1500m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.4                  #LANDING THROTTLE SETTING

AUTOCALC
