REM F-104G STARFIGHTER
IDENTIFY "F-104G_Belgian_Air_Force_interceptor"
CATEGORY ATTACKER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
REM              Mil     AB
REM J79-GE-3A   9600  14800
REM J79-GE-7   10000  15800
REM J79-GE-11  10000  15800
REM J79-OEL-7  10000  15800
REM J79-GE-19  11800  17900
THRAFTBN  15800lb                #THRUST WITH AFTERBURNER
THRMILIT  10000lb                #THRUST AT MILITARY POWER
WEIGHCLN  13966lb                #WEIGHT CLEAN
REM JP-4 / Jet-B weight is 2.9753lb per US gal
REM Basic fuel weight (896 US gal) - 2665lb
REM each tip tank (340gal) - 1011lb
REM each wing ext tank (390gal) - 1160lb
WEIGFUEL  5185lb                #WEIGHT OF FUEL
WEIGLOAD  200lb                #WEIGHT OF PAYLOAD
FUELABRN 1.60kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.80kg               #FUEL CONSUMPTION AT MILITARY POWER
THRSTREV  0.18                 #THRUST REVERSER EFFICIENCY (for brake chute)

COCKPITP  0.0m  0.87m  5.12m  #COCKPIT POSITION
LEFTGEAR -1.3m -1.30m -1.20m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.3m -1.30m -1.20m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.30m  3.58m  #WHEEL POSITION
MACHNGUN -0.435m -0.25m 4.95m  #MACHINE GUN POSITION
SMOKEGEN  0.0m  0.90m -7.10m  #SMOKE GENERATOR POSITION
VAPORPO0  3.35m -0.22m -3.15m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  3.35m -0.22m -3.15m #VAPOR POSITION (Wings Spread)
HTRADIUS  9.0m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  27deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 2.26MACH              #CRITICAL SPEED
MAXSPEED 2.40MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.7                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.25                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 18m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 130kt                #FULLY MANEUVABLE SPEED
CPITMANE 7.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

VGWSPED1 1.74MACH
VGWSPED2 1.75MACH

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

HRDPOINT -3.45m -0.24m -1.54m AIM9
HRDPOINT 3.45m -0.24m -1.54m AIM9
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
INITIGUN 725                 #INITIAL GUN
GUNINTVL 0.04
GUNPOWER 2
LOADWEPN AIM9 2


REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 2.30mach             #CRUISING SPEED
REFACRUS 35000ft              #CRUISING ALTITUDE
REFTCRUS 1.00                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 175kt                #LANDING SPEED
REFAOALD 8deg                #LANDING AOA
REFLNRWY 1500m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.45                  #LANDING THROTTLE SETTING

AUTOCALC
