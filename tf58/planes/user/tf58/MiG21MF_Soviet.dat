IDENTIFY "MIG-21MF_Soviet_Airforce"
CATEGORY FIGHTER
THRAFTBN  6490kg  #R25-300: 7100, R13-300: 6490, R11F2S-300: 6175
THRMILIT  4070kg  #R25-300: 4100, R13-300: 4070, R11F2S-300: 3900
THRSTREV  0.2     #THRUST REVERSER EFFICIENCY (for brake chute)
WEIGHCLN  5350kg  #bis: 5895, MF: 5350, PFM: 5383
WEIGFUEL  3010kg  #bis:2880l/2390kg, MF:2200kg, PFM:2650l/2200kg. 405kg per 130gal tank
WEIGLOAD  400kg   #WEIGHT OF PAYLOAD
FUELABRN 2.25kg   #R25: 2.25, R11:1.96
FUELMILI 0.96kg   #R25: 0.96, R11:0.94

COCKPITP  0.0m  0.95m  3.3m  #COCKPIT POSITION
LEFTGEAR -1.4m -1.45m -0.70m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.4m -1.45m -0.70m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.45m  4.0m  #WHEEL POSITION
ARRESTER  0.0m -1.14m -5.88m
MACHNGUN  0.0m -0.55m  2.60m  #MACHINE GUN POSITION
SMOKEGEN  0.0m  0.40m -6.00m  #SMOKE GENERATOR POSITION
VAPORPO0  3.58m -0.08m -2.70m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  3.58m -0.08m -2.70m  #VAPOR POSITION (Wings Spread)
HTRADIUS  8.0m                #OUTSIDE SPHERE RADIUS


STRENGTH 8

CRITAOAP  32deg               #CRITICAL AOA POSITIVE
CRITAOAM -10deg               #CRITICAL AOA NEGATIVE

CRITSPED 1.5MACH              #CRITICAL SPEED
MAXSPEED 2.0MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                #HAS VARIABLE GEOMETRY WING?

VGWSPED1 1.5mach
VGWSPED2 1.8mach

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd (2.0 for all 3, 1.2 for forwards only)

WINGAREA 23m^2                #WING AREA

MXIPTAOA 28.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 270.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 80kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 150kt                #FULLY MANEUVABLE SPEED
REM CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT  8.0/.6
REM CPITSTAB 2.0                  #PITCH STABILITY CONSTANT       2.0/.5
REM CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT     5.0/.5
REM CYAWSTAB 3.0                  #YAW STABILITY CONSTANT         3.0/.5
REM CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT    3.0/.6
CPITMANE 5.0
CPITSTAB 1.5
CYAWMANE 4.0
CYAWSTAB 2.0
CROLLMAN 2.0

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 150kg                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

REM Slots must come before Inits.
HRDPOINT -2.0m -0.47m -0.12m AIM9
HRDPOINT 2.0m -0.47m -0.12m AIM9

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 200                 #INITIAL GUN
LOADWEPN AIM9 2
GUNPOWER 2

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 900kt              #CRUISING SPEED
REFACRUS 35000ft              #CRUISING ALTITUDE
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 140kt                #LANDING SPEED
REFAOALD 8deg                #LANDING AOA
REFLNRWY 1100m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.4                  #LANDING THROTTLE SETTING

AUTOCALC
