REM Canadian Armed Forces CF-18 Hornet, repainted from F-18C by Oranleed/S.C.
REM 441 "Silverfox" Squadron
IDENTIFY "CF-18 441Sq. {TF58-OCP-SC}"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  32000lb             #THRUST WITH AFTERBURNER
THRMILIT  21400lb                #THRUST AT MILITARY POWER
WEIGHCLN  23400lb                #WEIGHT CLEAN
WEIGFUEL  13600LB                #WEIGHT OF FUEL
WEIGLOAD  1800lb                #WEIGHT OF PAYLOAD
FUELABRN 6.6kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.45kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  1.31m   6.18m  #COCKPIT POSITION
LEFTGEAR -1.58m -1.49m 0.00m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.58m -1.49m 0.00m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.49m  5.73m  #WHEEL POSITION
ARRESTER  0.0m -1.48m -5.90m #Arresting hook position
MACHNGUN  0.00m  0.66m  9.28m  #MACHINE GUN POSITION
SMOKEGEN  0.00m 0.14m -5.07m  #SMOKE GENERATOR POSITION
VAPORPO0  6.04m  0.14m -1.52m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  6.04m  0.14m -1.52m  #VAPOR POSITION (Wings Spread)
HTRADIUS 10.0m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 12

CRITAOAP  25deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.98MACH              #CRITICAL SPEED
MAXSPEED 1.8MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

VGWSPED1 -1kt
VGWSPED2 0kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.2                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 37m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 330.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 70kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 110kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED




HRDPOINT -1.2m -0.57m -0.06m AIM120
HRDPOINT 1.2m -0.57m -0.06m AIM120
HRDPOINT -6.11m 0.15m -0.17m AIM9
HRDPOINT 6.11m 0.15m -0.17m AIM9
HRDPOINT 3.41m -0.3m 0.21m AIM9
HRDPOINT 3.90m -0.3m 0.21m AIM9
HRDPOINT -3.655m -0.35m 0.21m AIM120
HRDPOINT -3.41m -0.3m 0.21m AIM9
HRDPOINT -3.90m -0.3m 0.21m AIM9
HRDPOINT 3.655m -0.35m 0.21m AIM120
HRDPOINT -2.8m -0.35m 0.33m AGM65 B250 B500
HRDPOINT 2.8m -0.35m 0.33m AGM65 B250 B500
HRDPOINT -2.06m -0.35m 0.33m AGM65 B250 B500
HRDPOINT 2.06m -0.35m 0.33m AGM65 B250 B500
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
GUNINTVL 0.01
GUNPOWER 4
INITIGUN 520                 #INITIAL GUN
LOADWEPN AIM120 2
LOADWEPN AIM9 2



REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.85MACH              #CRUISING SPEED
REFACRUS 25000ft              #CRUISING ALTITUDE
REFTCRUS 0.75                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 120kt                #LANDING SPEED
REFAOALD 08deg                 #LANDING AOA
REFLNRWY 1500m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.45                #LANDING THROTTLE SETTING

AUTOCALC
