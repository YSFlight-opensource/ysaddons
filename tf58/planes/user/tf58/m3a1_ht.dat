IDENTIFY "M3A1_HALFTRACK"
CATEGORY UTILITY
AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 147HP               #PROPELLER ENGINE
WEIGHCLN 9.3t                #WEIGHT CLEAN
WEIGFUEL 80kg                #WEIGHT OF FUEL
WEIGLOAD  1500kg                #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.0010kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNSIGHT FALSE
THRSTREV 0.6                 #EFFICIENCY OF THRUST REVERSER

GUNDIREC  0.0m -0.01m 1.0m     #MACHINE GUN DIRECTION
PROPEFCY 2                 #Propeller efficiency
PROPVMIN 20m/s                #Minimum speed that T=P/V becomes valid

COCKPITP  -0.61m 0.70m 0.30m  #COCKPIT POSITION
INSTPANL user/tf58/m3_ht.ist
LEFTGEAR -1.06m -1.20m -1.38m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.06m -1.20m -1.38m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.20m  2.57m  #WHEEL POSITION
ARRESTER  0.0m 0m 0m
MACHNGUN  0.00m 0.00m  0.00m  #MACHINE GUN POSITION
SMOKEGEN 0.00m 0m -0m  #SMOKE GENERATOR POSITION
VAPORPO0  0m  0m 0m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  0m  0m 0m  #VAPOR POSITION (Wings Spread)
HTRADIUS  4.5m                #OUTSIDE SPHERE RADIUS

RADARCRS 0.25                  #RADAR CROSS SECTION

TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m -0.5m 1.0m       #Thrust vector 0
TRSTDIR1 0.0m -0.5m 1.0m       #Thrust vector 1
PSTMPTCH 1deg                #Post-Stall Maneuver VPitch
PSTMYAW_ 13deg                #Post-Stall Maneuver VYaw
PSTMROLL 1deg                #Post-Stall Maneuver VRoll

TRIGGER1 RKT
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  1deg               #CRITICAL AOA POSITIVE
CRITAOAM -1deg               #CRITICAL AOA NEGATIVE

CRITSPED 48km/h              #CRITICAL SPEED
MAXSPEED 71km/h              #MAXIMUM SPEED

HASSPOIL FALSE                 #HAS SPOILER?
RETRGEAR FALSE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 10.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 1.0m^2                #WING AREA

MXIPTAOA 0.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 0.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 70km/h                 #MINIMUM MANEUVABLE SPEED
MANESPD2 71km/h                #FULLY MANEUVABLE SPEED
CPITMANE 0.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 4.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 2.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 1ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

NMTURRET 8
TURRETPO 0 0.43m 1.59m 0.22m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 0 -10deg 20deg 5deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 -180deg 180deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 0                              # Ammo
TURRETIV 0 0.12sec                           # Number ShootingInterval
TURRETAR 0                                   # Anti Air Capable
TURRETCT 0 PILOT                             # Controlled by the pilot

TURRETPO 1 0.43m 0.00m 0.22m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 1 0deg 0deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 1 -180deg 180deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 1 0                              # Ammo
TURRETIV 1 0.12sec                           # Number ShootingInterval
TURRETNM 1 gunner                               # DNM Node Name
TURRETAR 1                                   # Anti Air Capable
TURRETCT 1 PILOT                             # Controlled by the pilot

TURRETPO 2 0.43m 1.32m 0.22m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 2 -10deg 20deg 5deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 2 -180deg 180deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 2 500                              # Ammo
TURRETIV 2 0.12sec                           # Number ShootingInterval
TURRETAR 2                                   # Anti Air Capable
TURRETCT 2 PILOT                             # Controlled by the pilot
TURRETRG 2 1800m

TURRETPO 3 0.43m 1.14m 0.22m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 3 -10deg 20deg 5deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 3 0deg 0deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 3 0                              # Ammo
TURRETIV 3 0.12sec                           # Number ShootingInterval
TURRETNM 3 mg                               # DNM Node Name
TURRETAR 3                                   # Anti Air Capable
TURRETCT 3 PILOT                             # Controlled by the pilot

TURRETPO 4 0.43m 1.14m 0.22m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 4 0deg 0deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 4 -180deg 180deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 4 0                              # Ammo
TURRETIV 4 0.12sec                           # Number ShootingInterval
TURRETNM 4 mg_base                               # DNM Node Name
TURRETAR 4                                   # Anti Air Capable
TURRETCT 4 PILOT                             # Controlled by the pilot

TURRETPO 5 0.43m 1.59m 0.22m 180deg 0deg 0deg      # Number x y z h p b
TURRETPT 5 -23deg 21deg -1deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 5 0deg 0deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 5 0                              # Ammo
TURRETIV 5 0.12sec                           # Number ShootingInterval
TURRETNM 5 bicep                               # DNM Node Name
TURRETAR 5                                   # Anti Air Capable
TURRETCT 5 PILOT                             # Controlled by the pilot

TURRETPO 6 0.43m 1.59m 0.22m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 6 -1deg 10deg -1deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 6 0deg 0deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 6 0                              # Ammo
TURRETIV 6 0.12sec                           # Number ShootingInterval
TURRETNM 6 forearm                               # DNM Node Name
TURRETAR 6                                   # Anti Air Capable
TURRETCT 6 PILOT                             # Controlled by the pilot 

TURRETPO 7 0.43m 1.59m 0.22m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 7 0deg 60deg 30deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 7 0deg 0deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 7 0                              # Ammo
TURRETIV 7 0.12sec                           # Number ShootingInterval
TURRETNM 7 hands                               # DNM Node Name
TURRETAR 7                                   # Anti Air Capable
TURRETCT 7 PILOT                             # Controlled by the pilot

REM TURRETPO 1 0m 1.10m -1.96m 180deg 0deg 0deg  # Number x y z h p b
REM TURRETPT 1 -2deg 60deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
REM TURRETHD 1 -180deg 180deg 0deg               # Number MinHdg MaxHdg NeutralHdg
REM TURRETAM 1 5000                              # Ammo
REM TURRETIV 1 0.1sec                            # Number ShootingInterval
REM TURRETNP 1 MIDGUN                            # DNM Node Name
REM TURRETNH 1 MIDGUN_BASE                       # DNM Node Name (Heading Rotation)
REM TURRETAR 1                                   # Anti Air Capable
REM TURRETCT 1 GUNNER                            # Controlled by PILOT or GUNNER
REM TURRETRG 1 1500m

INITIGUN 0                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 71km/h              #CRUISING SPEED
REFACRUS 100ft              #CRUISING ALTITUDE
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 28km/h                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 10m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.8                  #LANDING THROTTLE SETTING

AUTOCALC
