REM Cessna Model-152 Trainer

REM "c152a_kz.dat/c152a_kz.dnm/c152_kz_cockpit.srf/c152_kz_coll.srf/c152a_kz_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2004 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.
REM This version with transparent canopy was by taskforce 58 (taskforce58@gmail.com), modified from 
REM the originals by Kzs.

IDENTIFY "C152_kzs/tf58_flash_blue"
CATEGORY TRAINER               #[NORMAL]

AFTBURNR FALSE                 #HAVE AFTERBURNER?
PROPEFCY 0.65                 #Propeller efficiency
PROPELLR 108HP                 #PROPELLER ENGINE [108HP]
WEIGHCLN 1132lb                 #WEIGHT CLEAN
WEIGFUEL 135lb                 #WEIGHT OF FUEL
WEIGLOAD 180lb                 #WEIGHT OF PAYLOAD
FUELMILI 0.022lb                #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP -0.23m  0.54m  0.11m  #COCKPIT POSITION
EXCAMERA "INSTRUCTOR" 0.23m  0.54m  0.24m  0deg 0deg 0deg INSIDE
INSTPANL user/tf58/c152.ist
LEFTGEAR -1.34m -1.15m -0.20m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.34m -1.15m -0.20m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.20m  1.37m  #WHEEL POSITION
ARRESTER  0.00m -0.75m -3.40m  #ARRESTING HOOK POSITION
MACHNGUN -0.27m -0.34m  1.99m  #MACHINE GUN POSITION
MACHNGN2  0.27m -0.34m  1.99m  #MACHINE GUN POSITION
SMOKEGEN -4.70m  0.70m -0.37m  #SMOKE GENERATOR POSITION
VAPORPO0  5.15m  0.72m  0.16m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.15m  0.72m  0.16m  #VAPOR POSITION (Wings Spread)
HTRADIUS  6.50m                #OUTSIDE SPHERE RADIUS

STRENGTH 4

CRITAOAP  16deg                #CRITICAL AOA POSITIVE
CRITAOAM -10deg                #CRITICAL AOA NEGATIVE

CRITSPED  94kt                 #CRITICAL SPEED
MAXSPEED 115kt                 #MAXIMUM SPEED  [106kt/109kt]

HASSPOIL FALSE                 #HAS SPOILER?
RETRGEAR FALSE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                 #HAS VARIABLE GEOMETRY WING?

REM VGWSPED1 0kt
REM VGWSPED2 90kt

CLVARGEO 0.0                   #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                   #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.25                   #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.60                   #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.0                   #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                   #EFFECT OF SPOILER FOR Cd

WINGAREA 14.90m^2              #WING AREA

MXIPTAOA  16.0deg              #MAX INPUT AOA
MXIPTSSA   5.0deg              #MAX INPUT SSA
MXIPTROL  80.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 42kt                  #MINIMUM MANEUVABLE SPEED
MANESPD2 50kt                  #FULLY MANEUVABLE SPEED
CPITMANE 6.5                   #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.5                   #PITCH STABILITY CONSTANT
CYAWMANE 6.0                   #YAW MANEUVABILITY CONSTANT
CYAWSTAB 4.5                   #YAW STABILITY CONSTANT
CROLLMAN 7.0                   #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                  #CONTROL LANDINGGEAR
CTLBRAKE FALSE                 #CONTROL BRAKE
CTLSPOIL 0.0                   #CONTROL SPOILER
CTLABRNR FALSE                 #CONTROL AFTERBURNER
CTLTHROT 0.0                   #CONTROL THRUST
CTLIFLAP 0.0                   #CONTROL FLAP
CTLINVGW 0.0                   #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m             #POSITION
ATTITUDE 45deg 0deg 0deg       #ATTITUDE
INITFUEL 100%                  #INITIAL FUEL
INITLOAD 0.0t                  #INITIAL PAYLOAD
INITSPED 0.0MACH               #INITIAL SPEED

INITIGUN 0                    #INITIAL GUN

RADARCRS 0.05

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 106kt                 #CRUISING SPEED
REFACRUS 7000ft                #CRUISING ALTITUDE
REFTCRUS 0.75                   #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 55kt                  #LANDING SPEED
REFAOALD 8.0deg               #LANDING AOA                             [TailStrikeAngle:15.31deg]
REFLNRWY 470m                  #RUNWAY REQUIRED TO STOP         [145m]
REFTHRLD 0.45                  #LANDING THROTTLE SETTING

AUTOCALC
