IDENTIFY "Mirage_III_CJ_253SQ_LATE"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  6000kg               #THRUST WITH AFTERBURNER
THRMILIT  4250kg                #THRUST AT MILITARY POWER
THRSTREV  0.45                 #EFFICIENCY OF THRUST REVERSER
WEIGHCLN  6255kg                #WEIGHT CLEAN
WEIGFUEL  3206kg                #WEIGHT OF FUEL
WEIGLOAD  2300kg                #WEIGHT OF PAYLOAD
FUELABRN 3.63kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 1.25kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.048

COCKPITP  0.0m  0.45m  4.30m  #COCKPIT POSITION
EXCAMERA "UP" 0.0m  0.45m  4.30m  0deg 30deg 0deg INSIDE
EXCAMERA "LEFT_SHOULDER" -0.2m  0.45m  4.25m  125deg 0deg 0deg INSIDE
EXCAMERA "RIGHT_SHOULDER" 0.2m  0.45m  4.25m  -125deg 0deg 0deg INSIDE
LEFTGEAR -1.74m -2.08m -0.70m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.74m -2.08m -0.70m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.13m  4.12m  #WHEEL POSITION
ARRESTER  0.0m -1.30m -4.20m
MACHNGUN -0.70m  -0.60m  3.30m  #MACHINE GUN POSITION
MACHNGN2 0.70m  -0.60m  3.30m  #MACHINE GUN POSITION
SMOKEGEN 0.0m -0.65m -6.00m  #SMOKE GENERATOR POSITION
VAPORPO0  4.15m  -0.70m -3.55m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.15m  -0.70m -3.55m  #VAPOR POSITION (Wings Spread)
HTRADIUS  9.8m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  20.00deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.1MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.0                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.0                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.6                  #EFFECT OF SPOILER FOR Cd

WINGAREA 34.8m^2                #WING AREA

MXIPTAOA 20.0deg              #MAX INPUT AOA
MXIPTSSA 6.0deg               #MAX INPUT SSA
MXIPTROL 280.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 200kt                 #FULLY MANEUVABLE SPEED
CPITMANE 6.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 6.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 6.0                  #YAW STABILITY CONSTANT
CROLLMAN 7.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

HRDPOINT 3.2m -1.00m -1.9m AIM9 B500
HRDPOINT -3.2m -1.00m -1.9m AIM9 B500

REM HRDPOINT 2.35m -0.95m -1.0m B500 AIM9
REM HRDPOINT -2.35m -0.95m -1.0m B500 AIM9
REM HRDPOINT 2.35m -1.00m 0.00m RKT
REM HRDPOINT -2.35m -1.00m 0.00m RKT

HRDPOINT 0.0m -1.25m -0.76m AIM120 B500

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

GUNPOWER 2
LOADWEPN AIM9 2
LOADWEPN AIM120 1
REM LOADWEPN B500 2
REM LOADWEPN RKT 36
INITIGUN 250                 #INITIAL GUN

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.90MACH              #CRUISING SPEED
REFACRUS 40000ft              #CRUISING ALTITUDE
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 160kt                 #LANDING SPEED
REFAOALD 13deg                 #LANDING AOA
REFLNRWY 1200m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.50                 #LANDING THROTTLE SETTING

AUTOCALC
