REM ex. A-10 THUNDERBOLT II SD
IDENTIFY "A-10_Warthog_SuperDeformed"
CATEGORY AEROBATIC
AFTBURNR FALSE                #HAVE AFTERBURNER?
THRAFTBN  0.0t                #THRUST WITH AFTERBURNER
THRMILIT  9.0t                #THRUST AT MILITARY POWER
WEIGHCLN  9.0t                #WEIGHT CLEAN
WEIGFUEL  3.6t                #WEIGHT OF FUEL
WEIGLOAD  1.0t                #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.45kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  1.32m  0.96m  #COCKPIT POSITION
LEFTGEAR -2.10m -1.9m -0.80m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.10m -1.9m -0.80m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.33m -1.9m  1.20m  #WHEEL POSITION
MACHNGUN  -0.1m  -0.43m  3.0m  #MACHINE GUN POSITION
MACHNGN2  -0.1m  -0.44m  3.0m  #MACHINE GUN POSITION
SMOKEGEN -0.0m -0.00m -3.8m  #SMOKE GENERATOR POSITION
VAPORPO0  4.96m  -0.51m -0.98m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.96m  -0.51m -0.98m  #VAPOR POSITION (Wings Spread)
HTRADIUS 6.0m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

SMOKEOIL 500.0kg
WEAPONCH GUN

STRENGTH 25

CRITAOAP  25deg               #CRITICAL AOA POSITIVE
CRITAOAM -20deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.5MACH              #CRITICAL SPEED
MAXSPEED 706km/h              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?
VGWSPED1 150kt                #VARIABLE GEOMETRY WING SPEED
VGWSPED2 300kt                #VARIABLE GEOMETRY WING SPEED

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 1.0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 35m^2                #WING AREA

MXIPTAOA 20.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 180.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 80kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 100kt                #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

INITIGUN 30000                 #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITIAGM 16                    #INITIAL AGM
INITBOMB 16

MAXNMAAM 16
MAXNMAGM 16
MAXNMRKT 38
MAXNBOMB 16

AAMSLOT_ 4.0m -0.65m -0.10m
AAMSLOT_ -4.0m -0.65m -0.10m
AAMVISIB FALSE

BOMBSLOT 0.6m -0.8m -0.1m
BOMBSLOT -0.6m -0.8m -0.1m
BOMVISIB FALSE

AGMSLOT_ 1.3m -0.8m -0.1m
AGMSLOT_ -1.3m -0.8m -0.1m
AGMVISIB FALSE

RKTSLOT_ 3.2m -0.7m -0.1m
RKTSLOT_ -3.2m -0.7m -0.1m
RKTVISIB FALSE

RADARCRS 0.5                  #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 700km/h              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 110kt                #LANDING SPEED
REFAOALD 15deg                #LANDING AOA
REFLNRWY 610m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
