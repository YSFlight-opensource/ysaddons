REM GACGLIMT 7.3
REM GACRADRG 46
REM GACRADCS 0.8
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 0
REM GACLNDNV 0
REM GACLIMIT 0

IDENTIFY "F-8E_Crusader_Shin_Kazama"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  8.16t                #THRUST WITH AFTERBURNER
THRMILIT  4.83t                #THRUST AT MILITARY POWER
WEIGHCLN  7.96t                #WEIGHT CLEAN
WEIGFUEL  5t                #WEIGHT OF FUEL
WEIGLOAD  2.2t                #WEIGHT OF PAYLOAD
FUELABRN 12.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.75kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.075

COCKPITP  0.0m  0.80m  6.15m  #COCKPIT POSITION
LEFTGEAR -1.70m -1.65m -1.40m   #LEFT LANDING GEAR POSITION
RIGHGEAR  1.70m -1.65m -1.40m   #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.95m  4.35m  #WHEEL POSITION

MACHNGUN  -0.60m -0.65m  6.10m  #MACHINE GUN POSITION
MACHNGN2 0.60m -0.35m  6.10m  #MACHINE GUN POSITION
MACHNGN3  -0.60m -0.35m  6.20m  #MACHINE GUN POSITION
MACHNGN4 0.60m -0.65m  6.20m  #MACHINE GUN POSITION

ARRESTER  0.0m -1.60m -5.50m
SMOKEGEN 0.0m  1.00m -7.30m  #SMOKE GENERATOR POSITION
VAPORPO0  5.40m 0.36m -4.30m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.40m 0.36m -4.30m  #VAPOR POSITION (Wings Spread)
HTRADIUS  12m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  22.35deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 1.86MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.7                  #EFFECT OF SPOILER FOR Cd

WINGAREA 32.5m^2                #WING AREA

MXIPTAOA 24.45deg              #MAX INPUT AOA
MXIPTSSA 7.0deg               #MAX INPUT SSA
MXIPTROL 220.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 270kt                #FULLY MANEUVABLE SPEED
CPITMANE 5.25                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.2                  #PITCH STABILITY CONSTANT
CYAWMANE 7.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 7.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT -1.2m 0.2m 3.0m AIM9
HRDPOINT 1.2m 0.2m 3.0m AIM9
HRDPOINT -1.2m -0.1m 2.5m AIM9
HRDPOINT 1.2m -0.1m 2.5m AIM9
HRDPOINT -2.1m -0.2m -0.6m RKT B500 AGM65
HRDPOINT 2.1m -0.2m -0.6m RKT B500 AGM65
HRDPOINT -2.5m 0.2m -0.6m B500 AGM65
HRDPOINT 2.5m 0.2m -0.6m B500 AGM65
HRDPOINT -1.7m 0.2m -0.6m B500 AGM65
HRDPOINT 1.7m 0.2m -0.6m B500 AGM65
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 500                 #INITIAL GUN
INITIAAM 4                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.8MACH              #CRUISING SPEED
REFACRUS 40000ft              #CRUISING ALTITUDE
REFTCRUS 2.5                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 140kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1200m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.37                  #LANDING THROTTLE SETTING

AUTOCALC
