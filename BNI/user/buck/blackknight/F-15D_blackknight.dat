REM F-15 EAGLE
IDENTIFY "F-15D_blackknight(buck)"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 26.9t                #THRUST WITH AFTERBURNER
THRMILIT 15.8t                #THRUST AT MILITARY POWER
WEIGHCLN 12.2t                #WEIGHT CLEAN
WEIGFUEL  8.7t                #WEIGHT OF FUEL
WEIGLOAD  9.0t                #WEIGHT OF PAYLOAD
FUELABRN 4.5kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.55kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  0.86m   6.8m  #COCKPIT POSITION

LEFTGEAR -1.35m -2.54m -0.14m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.35m -2.54m -0.14m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.54m  5.10m  #WHEEL POSITION
MACHNGUN  1.69m  0.08m  3.7m  #MACHINE GUN POSITION
GUNINTVL 0.02
SMOKEGEN -1.34m -0.99m -2.50m  #SMOKE GENERATOR POSITION
VAPORPO0  6.53m  -0.02m -2.65m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  6.53m  -0.02m -2.65m  #VAPOR POSITION (Wings Spread)
HTRADIUS  11.0m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.5MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd
VGWSPED1 100kt
VGWSPED2 1200kt

WINGAREA 56.5m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 290.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 200kt                #FULLY MANEUVABLE SPEED
CPITMANE 8.5                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.5                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 2.3                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED


HRDPOINT 3.2m -0.35m 1.3m AIM9 AIM120
HRDPOINT -3.2m -0.35m 1.3m AIM9 AIM120
HRDPOINT 2.4m -0.35m 1.3m AIM9 AIM120
HRDPOINT -2.4m -0.35m 1.3m AIM9 AIM120

HRDPOINT 2.8m -0.95m 1m AGM65 RKT
HRDPOINT -2.8m -0.95m 1m AGM65 RKT


HRDPOINT 1.6m -1.5m 2.25m B500 AGM65 
HRDPOINT -1.6m -1.5m 2.25m B500 AGM65
HRDPOINT 1.6m -1.5m -0.25m B500 AGM65
HRDPOINT -1.6m -1.5m -0.25m B500 AGM65

HRDPOINT 2.14m -1.31m -0.5m B500 AGM65
HRDPOINT -2.14m -1.31m -0.5m B500 AGM65
HRDPOINT 2.08m -1.31m 1.92m B500 AGM65
HRDPOINT -2.08m -1.31m 1.92m B500 AGM65

HRDPOINT 2.8m -0.95m 1m B500 AGM65
HRDPOINT 2.8m -0.95m -0.6m B500 AGM65
HRDPOINT -2.8m -0.95m 1m B500 AGM65
HRDPOINT -2.8m -0.95m -0.6m B500 AGM65
HRDPOINT 2.55m -0.7m 1m B500 AGM65
HRDPOINT 2.55m -0.7m -0.6m B500 AGM65
HRDPOINT -2.55m -0.7m 1m B500 AGM65
HRDPOINT -2.55m -0.7m -0.6m B500 AGM65
HRDPOINT 3.05m -0.7m 1m B500 AGM65
HRDPOINT 3.05m -0.7m -0.6m B500 AGM65
HRDPOINT -3.05m -0.7m 1m B500 AGM65
HRDPOINT -3.05m -0.7m -0.6m B500 AGM65


MAXNMAAM 4
MAXNMAGM 2
MAXNBOMB 26
MAXNMRKT 72
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
GUNPOWER 2
INITIGUN 3000                #INITIAL GUN
LOADWEPN AIM120 2
LOADWEPN AIM9 2



REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 135kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1200m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
