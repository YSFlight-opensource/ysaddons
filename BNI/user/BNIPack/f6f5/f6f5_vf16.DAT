REM Iceman in BNIPACK!! (c)
IDENTIFY "F6F-5 Hellcat VF-16"
CATEGORY FIGHTER
AFTBURNR FALSE                 #HAVE AFTERBURNER?
PROPELLR 2100HP              #PROPELLER ENGINE
WEIGHCLN  4190kg                #WEIGHT CLEAN
WEIGFUEL  1724kg                #WEIGHT OF FUEL
WEIGLOAD  1076kg                #WEIGHT OF PAYLOAD
FUELMILI 0.15kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  1.19m  -0.4m  #COCKPIT POSITION
INSTPANL user/bnipack/f6f5/f6f.ist
LEFTGEAR -1.79m -2.39m  1.23m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.79m -2.39m  1.23m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -0.91m -5.62m  #WHEEL POSITION
ARRESTER  0.0m -1.36m -7.28m #Arresting hook position
SMOKEGEN  0.0m  0.43m -3.89m  #SMOKE GENERATOR POSITION
VAPORPO0  -6.94m  0.23m  0.44m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  -6.94m  0.23m  0.44m  #VAPOR POSITION (Wings Spread)
HTRADIUS  10m                #OUTSIDE SPHERE RADIUS
GUNSIGHT FALSE

TRIGGER1 GUN
TRIGGER2 GUN
TRIGGER3 SMK
TRIGGER4 SMK

STRENGTH 15

CRITAOAP  23deg               #CRITICAL AOA POSITIVE
CRITAOAM -17deg               #CRITICAL AOA NEGATIVE

CRITSPED 590km/h              #CRITICAL SPEED
MAXSPEED 643km/h             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.2                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd
VGWSPED1 0kt
VGWSPED2 0kt

WINGAREA 31m^2                #WING AREA

MXIPTAOA 25.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 75.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 35kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 73kt                 #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED
WEAPONCH GUN
GUNPOWER 1

HRDPOINT -2.67m -0.61m 0.92m RKT
HRDPOINT 2.67m -0.61m 0.92m RKT
HRDPOINT -3.16m -0.53m 0.85m RKT
HRDPOINT 3.16m -0.53m 0.85m RKT
HRDPOINT -3.67m -0.45m 0.78m RKT
HRDPOINT 3.67m -0.45m 0.78m RKT
HRDPOINT -1.00m -0.80m 0.91m B250
HRDPOINT 1.00m -0.80m 0.91m B250

MAXNMRKT 6
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 2
MAXNBOMB 2

RKTVISIB FALSE
BOMVISIB FALSE


LMTBYHDP TRUE

INITIGUN 2400

RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 450km/h              #CRUISING SPEED(*1.2
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.5                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 100kt                 #LANDING SPEED
REFAOALD 8deg                #LANDING AOA
REFLNRWY 600m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.35                  #LANDING THROTTLE SETTING

MACHNGUN 2.41m -0.26m 1.76m  #MACHINE GUN POSITION
MACHNGN2 -2.41m -0.26m 1.76m  #MACHINE GUN POSITION
MACHNGN3 2.61m -0.24m 1.63m  #MACHINE GUN POSITION
MACHNGN4 -2.61m -0.24m 1.63m  #MACHINE GUN POSITION
MACHNGN5 2.80m -0.22m 1.44m  #MACHINE GUN POSITION
MACHNGN6 -2.80m -0.22m 1.44m  #MACHINE GUN POSITION
GUNINTVL 0.075

AUTOCALC
