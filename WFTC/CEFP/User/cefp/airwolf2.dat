IDENTIFY "AIRWOLF/V"
CATEGORY UTILITY

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 600.0t               #THRUST WITH AFTERBURNER
THRMILIT 60.0t                #THRUST AT MILITARY POWER
WEIGHCLN 20.0t                #WEIGHT CLEAN
WEIGFUEL 15.0t                #WEIGHT OF FUEL
WEIGLOAD 15.0t                #WEIGHT OF PAYLOAD
FUELABRN 10.0kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  2.0kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.36m  0.25m  2.00m #COCKPIT POSITION
INSTPANL User/cefp/airwolf2.ist
LEFTGEAR -1.44m -1.24m -0.87m #LEFT LANDING GEAR POSITION
RIGHGEAR  1.44m -1.24m -0.87m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.24m  2.90m #WHEEL POSITION
ARRESTER  0.15m -0.46m -8.74m
MACHNGUN  1.85m -0.61m  0.87m #MACHINE GUN POSITION
MACHNGN2 -1.85m -0.61m  0.87m #MACHINE GUN POSITION
SMOKEGEN  0.07m  0.76m -7.97m #SMOKE GENERATOR POSITION
VAPORPO0  6.38m  2.33m  0.00m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  6.38m  2.33m  0.00m
HTRADIUS  8.5m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 15

CRITAOAP  40deg               #CRITICAL AOA POSITIVE
CRITAOAM -35deg               #CRITICAL AOA NEGATIVE

CRITSPED 300kt                #CRITICAL SPEED
MAXSPEED 3MACH                #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m 1.0m 1.0m       #Thrust vector 0
TRSTDIR1 0.0m 1.0m 0.0m       #Thrust vector 1
PSTMPTCH 40deg                #Post-Stall Maneuver VPitch
PSTMYAW_ 30deg                #Post-Stall Maneuver VYaw
PSTMROLL 80deg                #Post-Stall Maneuver VRoll

CLVARGEO  0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO  0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP  0.0                 #EFFECT OF FLAP FOR Cl
CDBYFLAP  0.0                 #EFFECT OF FLAP FOR Cd
CDBYGEAR  0.2                 #EFFECT OF GEAR FOR Cd
CDSPOILR 10.0                 #EFFECT OF SPOILER FOR Cd

WINGAREA 1m^2                 #WING AREA  1m^2 for a helicopter

MXIPTAOA  50.0deg             #MAX INPUT AOA
MXIPTSSA  10.0deg             #MAX INPUT SSA
MXIPTROL 270.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1  80kt                #MINIMUM MANEUVABLE SPEED
MANESPD2 190kt                #FULLY MANEUVABLE SPEED
CPITMANE 6.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 6.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 5.0                  #YAW STABILITY CONSTANT
CROLLMAN 6.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE TRUE                 #CONTROL BRAKE
CTLSPOIL 1.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1 500kt
VGWSPED2 3.0MACH

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 270kt                #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.3                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 100kt                #LANDING SPEED
REFAOALD 30deg                #LANDING AOA
REFLNRWY 100m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.30                 #LANDING THROTTLE SETTING

GUNPOWER 2
INITIGUN 3000                 #INITIAL GUN
INITIAAM 12                   #INITIAL AAM
INITIAGM 12                   #INITIAL AGM

AAMSLOT_  0.00m -0.99m 1.49m
AGMSLOT_ -0.19m -0.99m 1.49m
AGMSLOT_  0.19m -0.99m 1.49m
AAMVISIB FALSE
AGMVISIB FALSE

RADARCRS 0.9

AUTOCALC
