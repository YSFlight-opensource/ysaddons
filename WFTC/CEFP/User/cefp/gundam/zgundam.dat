REM MSZ-006 Zeta GUNDAM
REM Manufactured by Makoto  2003/2005 

IDENTIFY "Z GUNDAM"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 50.0t                #THRUST WITH AFTERBURNER
THRMILIT 30.0t                #THRUST AT MILITARY POWER
THRSTREV  2.0                 #EFFICIENCY OF THRUST REVERSER
WEIGHCLN 28.7t                #WEIGHT CLEAN
WEIGFUEL  1.5t                #WEIGHT OF FUEL
WEIGLOAD 33.6t                #WEIGHT OF PAYLOAD
FUELABRN  0.5kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.1kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m   0.00m  4.00m #COCKPIT POSITION
LEFTGEAR -2.50m -11.50m  0.00m #LEFT LANDING GEAR POSITION
RIGHGEAR  2.50m -11.50m  0.00m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -11.50m  2.00m #WHEEL POSITION
ARRESTER  0.00m -11.50m  0.00m
MACHNGUN  0.00m   2.60m 12.82m #MACHINE GUN POSITION
SMOKEGEN  0.00m   0.00m -9.00m #SMOKE GENERATOR POSITION
VAPORPO0  8.80m  -1.28m -7.08m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  8.80m  -1.28m -7.08m #VAPOR POSITION (Wings Spread)
HTRADIUS  15.0m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 50

CRITAOAP  40deg               #CRITICAL AOA POSITIVE
CRITAOAM -35deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.5MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

CLVARGEO  0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO  0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP  1.0                 #EFFECT OF FLAP FOR Cl
CDBYFLAP  5.0                 #EFFECT OF FLAP FOR Cd
CDBYGEAR  1.0                 #EFFECT OF GEAR FOR Cd
CDSPOILR  5.0                 #EFFECT OF SPOILER FOR Cd

WINGAREA 50m^2                #WING AREA

MXIPTAOA  60.0deg             #MAX INPUT AOA
MXIPTSSA  10.0deg             #MAX INPUT SSA
MXIPTROL 270.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 80kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 10.0                 #PITCH STABILITY CONSTANT
CYAWMANE  5.0                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB  5.0                 #YAW STABILITY CONSTANT
CROLLMAN  5.0                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1  0kt
VGWSPED2 50kt

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 2
INITIGUN 2500                 #INITIAL GUN
INITIAAM 4                    #INITIAL AAM

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 100kt                #LANDING SPEED
REFAOALD 5deg                #LANDING AOA
REFLNRWY 100m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.50                 #LANDING THROTTLE SETTING

AAMSLOT_  -3.0m 0.0m -2.0m
AAMSLOT_   3.0m 0.0m -2.0m
AAMVISIB FALSE

AUTOCALC
