REM CORE FIGHTER - Transform Version

REM "core_kz(t).dat/core_kz(t).dnm/core_kz_cockpit.srf/core_kz_coll.srf/core_kz(t)_coarse.dnm"
REM     were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2003 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_CORE_FIGHTER/T"
CATEGORY ATTACKER

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 14.5t                #THRUST WITH AFTERBURNER
THRMILIT  7.5t                #THRUST AT MILITARY POWER
WEIGHCLN  5.4t                #WEIGHT CLEAN
WEIGFUEL  2.5t                #WEIGHT OF FUEL
WEIGLOAD  5.0t                #WEIGHT OF PAYLOAD
FUELABRN 2.50kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.50kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.90m  3.05m #COCKPIT POSITION
LEFTGEAR -1.25m -1.60m -2.75m #LEFT LANDING GEAR POSITION
RIGHGEAR  1.25m -1.60m -2.75m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.60m  1.59m #WHEEL POSITION
ARRESTER  0.00m -1.55m -4.65m #ARRESTING HOOK POSITION
MACHNGUN -0.44m -0.17m  5.32m #MACHINE GUN POSITION
MACHNGN2  0.44m -0.17m  5.32m #MACHINE GUN POSITION
SMOKEGEN  0.00m  0.30m -5.30m #SMOKE GENERATOR POSITION
VAPORPO0  3.74m -0.54m -2.35m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  3.74m -0.54m -2.35m #VAPOR POSITION (Wings Spread)
HTRADIUS  8.00m               #OUTSIDE SPHERE RADIUS

STRENGTH 10

CRITAOAP  22.5deg             #CRITICAL AOA POSITIVE
CRITAOAM -16.0deg             #CRITICAL AOA NEGATIVE

CRITSPED 0.85MACH             #CRITICAL SPEED
MAXSPEED 2.00MACH             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

VGWSPED1 0kt
VGWSPED2 1kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.1                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 22.50m^2             #WING AREA

MXIPTAOA  21.5deg             #MAX INPUT AOA [23.0deg]
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 80kt                 #FULLY MANEUVABLE SPEED
CPITMANE 9.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 1.5                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 0deg 0deg 0deg       #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 1

INITIGUN 1500                 #INITIAL GUN
INITIAGM 4                    #INITIAL AAM
INITRCKT 12                   #INITIAL AAM

MAXNMAAM 2
MAXNMAGM 6
MAXNBOMB 0
MAXNMRKT 24

AAMVISIB FALSE
AAMSLOT_ -0.90m 0.95m -3.75m
AAMSLOT_  0.90m 0.95m -3.75m

AGMVISIB FALSE
AGMSLOT_ -0.90m 0.95m -3.75m
AGMSLOT_  0.90m 0.95m -3.75m

BOMVISIB FALSE
BOMBSLOT -0.90m 0.95m -3.75m
BOMBSLOT  0.90m 0.95m -3.75m

RKTVISIB FALSE
RKTSLOT_ -0.90m 0.95m -3.75m
RKTSLOT_  0.90m 0.95m -3.75m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.95MACH             #CRUISING SPEED
REFACRUS 15000ft              #CRUISING ALTITUDE
REFTCRUS 0.6                  #CRUISING THROTTLE SETTING [0.8]

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 100kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA                  [TailStrikeAngle:22.19deg]
REFLNRWY 1000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
