IDENTIFY "R-352 Sepia"
CATEGORY FIGHTER
AFTBURNR TRUE                  #HAVE AFTERBURNER?
THRAFTBN  60.0t                #THRUST WITH AFTERBURNER
THRMILIT  40.0t                #THRUST AT MILITARY POWER
WEIGHCLN  20.0t                #WEIGHT CLEAN
WEIGFUEL  30.0t                #WEIGHT OF FUEL
WEIGLOAD  10.0t                #WEIGHT OF PAYLOAD
FUELABRN  22.6kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI   4.5kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  0.10m  7.60m  #COCKPIT POSITION
LEFTGEAR -2.4m -2.39m -4.68m  #LEFT LANDING GEAR POSITION
RIGHGEAR -2.3m -2.39m -4.68m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.30m 12.66m  #WHEEL POSITION
MACHNGUN  0.0m 0.17m 16.13m   #MACHINE GUN POSITION
SMOKEGEN  0.0m 0.40m -13.00m  #SMOKE GENERATOR POSITION
VAPORPO0  5.34m 2.41m -3.37m   #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.34m 2.41m -3.37m   #VAPOR POSITION (Wings Spread)
HTRADIUS  10.0m                 #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 20

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -10deg               #CRITICAL AOA NEGATIVE

CRITSPED 1.2MACH              #CRITICAL SPEED
MAXSPEED 3054km/h             #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.0                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.0                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 2.0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 160m^2               #WING AREA

MXIPTAOA 16.0deg              #MAX INPUT AOA
MXIPTSSA  5.0deg              #MAX INPUT SSA
MXIPTROL 300.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 150kt                #MINIMUM MANEUVABLE SPEED
MANESPD2 170kt                #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 2.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 0.4                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 70%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 5

INITIGUN 9999                 #INITIAL GUN
INITIAAM 16                   #INITIAL AAM
INITIAGM 16                   #INITIAL AGM

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.8MACH              #CRUISING SPEED
REFACRUS 50000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 180kt                #LANDING SPEED
REFAOALD 12deg                #LANDING AOA
REFLNRWY 3000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.2                  #LANDING THROTTLE SETTING

AAMSLOT_ 1.80m -1.90m 1.60m
AAMSLOT_ 1.40m -1.90m 1.60m
AAMSLOT_ -1.80m -1.90m 1.60m
AAMSLOT_ -1.40m -1.90m 1.60m
AAMVISIB FALSE

AGMSLOT_ 1.80m -1.90m 1.60m
AGMSLOT_ 1.40m -1.90m 1.60m
AGMSLOT_ -1.80m -1.90m 1.60m
AGMSLOT_ -1.40m -1.90m 1.60m
AGMVISIB FALSE

RADARCRS 1.0                 #RADAR CROSS SECTION

AUTOCALC
