IDENTIFY "IKARUGA/ATTRIBUTE CHANGE"
CATEGORY FIGHTER

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 500.0t               #THRUST WITH AFTERBURNER
THRMILIT 30.0t                #THRUST AT MILITARY POWER
WEIGHCLN 28.7t                #WEIGHT CLEAN
WEIGFUEL  1.5t                #WEIGHT OF FUEL
WEIGLOAD 33.6t                #WEIGHT OF PAYLOAD
FUELABRN  0.5kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.1kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP       0m 0.695m 2.83m                              #COCKPIT POSITION
EXCAMERA "TOP" 0m    90m   30m 0deg -90deg 0deg OUTSIDE

LEFTGEAR  -3.33m  -5.73m      0m #LEFT LANDING GEAR POSITION
RIGHGEAR   3.33m  -5.73m      0m #RIGHT LANDING GEAR POSITION
WHELGEAR      0m  -5.73m      3m #WHEEL POSITION
ARRESTER      0m  -5.73m      0m
MACHNGUN -2.237m -0.282m  3.614m #MACHINE GUN POSITION
MACHNGN2  2.237m -0.282m  3.614m
MACHNGN3 -2.299m -0.635m  3.614m
MACHNGN4  2.299m -0.635m  3.614m
SMOKEGEN      0m    0.7m   -0.7m #SMOKE GENERATOR POSITION
VAPORPO0  8.673m -2.711m  0.614m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  8.673m -2.711m  0.614m #VAPOR POSITION (Wings Spread)
HTRADIUS   15.0m                 #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  40deg               #CRITICAL AOA POSITIVE
CRITAOAM -35deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.5MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m 0.0m 1.0m       #Thrust vector 0
TRSTDIR1 0.0m 0.5m 1.0m       #Thrust vector 1
PSTMPTCH  20deg               #Post-Stall Maneuver VPitch
PSTMYAW_  20deg               #Post-Stall Maneuver VYaw
PSTMROLL  20deg               #Post-Stall Maneuver VRoll

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 1.0                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 5.0                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 1.0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 5.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 50m^2                #WING AREA

MXIPTAOA  60.0deg             #MAX INPUT AOA
MXIPTSSA  10.0deg             #MAX INPUT SSA
MXIPTROL 270.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 80kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 10.0                 #PITCH STABILITY CONSTANT
CYAWMANE  5.0                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB  5.0                 #YAW STABILITY CONSTANT
CROLLMAN  5.0                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1 1.5MACH
VGWSPED2 30MACH

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 100kt                #LANDING SPEED
REFAOALD 5deg                 #LANDING AOA
REFLNRWY 100m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.50                 #LANDING THROTTLE SETTING

GUNPOWER 1
INITIGUN 2500                 #INITIAL GUN

HRDPOINT -2.96m 2.51m 0.75m AIM9 AGM65
HRDPOINT  2.96m 2.51m 0.75m AIM9 AGM65

HRDPOINT -3.33m 2.25m 1.52m AIM9 AGM65
HRDPOINT  3.33m 2.25m 1.52m AIM9 AGM65
HRDPOINT -2.69m 2.13m 1.52m AIM9 AGM65
HRDPOINT  2.69m 2.13m 1.52m AIM9 AGM65

HRDPOINT -3.11m 1.61m 2.10m AIM9 AGM65
HRDPOINT  3.11m 1.61m 2.10m AIM9 AGM65

HRDPOINT -3.57m 0.92m 2.42m AIM9 AGM65
HRDPOINT  3.57m 0.92m 2.42m AIM9 AGM65
HRDPOINT -2.91m 0.80m 2.42m AIM9 AGM65
HRDPOINT  2.91m 0.80m 2.42m AIM9 AGM65

AAMVISIB FALSE
AGMVISIB FALSE

LMTBYHDP TRUE

LOADWEPN AIM9  6
LOADWEPN AGM65 6

AUTOCALC
