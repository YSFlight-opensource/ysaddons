REM hellhound_
IDENTIFY "HELLHOUND_GUNNER"
CATEGORY ATTACKER
AFTBURNR FALSE                 #HAVE AFTERBURNER?
PROPELLR 5650HP              #PROPELLER ENGINE
WEIGHCLN  9.86t                #WEIGHT CLEAN
WEIGFUEL  10.20t                #WEIGHT OF FUEL
WEIGLOAD  5.0t                #WEIGHT OF PAYLOAD
FUELMILI 0.25kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  0.94m  3.88m  #COCKPIT POSITION
LEFTGEAR -1.50m -0.50m  -6.87m  #LEFT LANDING GEAR POSITION
RIGHGEAR 1.50m -0.50m  -6.87m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.68m  -0.55m  #WHEEL POSITION
MACHNGUN  0.00m -0.66m  5.90m  #MACHINE GUN POSITION
VAPORPO0 3.8m  3.74m  -1.28m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1 3.8m  3.74m  -1.28m  #VAPOR POSITION (Wings Spread)
HTRADIUS  7.0m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  12deg               #CRITICAL AOA POSITIVE
CRITAOAM -5deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.15MACH              #CRITICAL SPEED
MAXSPEED 436km/h              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR FALSE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.8                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 25.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 15m^2                #WING AREA

MXIPTAOA 11.0deg              #MAX INPUT AOA
MXIPTSSA 25.0deg               #MAX INPUT SSA
MXIPTROL 30.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 10kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 25kt                 #FULLY MANEUVABLE SPEED
CPITMANE 9.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 4.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.5                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA FALSE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 8
INITIGUN 2200                  #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 8                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB
INITRCKT 50                   #INITIAL ROCKET (10kg/each)

AGMVISIB FALSE
AGMSLOT_  -2.27m 0.60m 2.09m
AGMSLOT_ 2.27m 0.60m 2.09m
AGMSLOT_ -2.80m 0.60m 2.08m
AGMSLOT_ 2.80m 0.60m 2.08m
AGMSLOT_ -2.28m 0.06m 2.10m
AGMSLOT_ 2.28m 0.06m 2.10m
AGMSLOT_ -2.78m 0.06m 2.08m
AGMSLOT_ 2.78m 0.06m 2.08m
RKTVISIB FALSE
RKTSLOT_ -3.89m -0.09m 1.69m
RKTSLOT_ 3.89m -0.09m 1.69m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.3MACH              #CRUISING SPEED
REFACRUS 6900ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 50kt                 #LANDING SPEED
REFAOALD 10deg                 #LANDING AOA
REFLNRWY 100m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.30                 #LANDING THROTTLE SETTING

AUTOCALC
