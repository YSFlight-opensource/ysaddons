WEIGFUEL   5.0t               #WEIGHT OF FUEL
WEIGLOAD 200.0t               #WEIGHT OF PAYLOAD
FUELABRN   5.0kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI   0.3kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  2.26m  0.90m #COCKPIT POSITION
LEFTGEAR -1.70m -4.00m  2.00m #LEFT LANDING GEAR POSITION
