REM WF-04J 長距離要撃型「天女」(JASDF)
IDENTIFY "WF-04J_HERA"
CATEGORY AEROBATIC
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  30.0t               #THRUST WITH AFTERBURNER
THRMILIT  22.0t                #THRUST AT MILITARY POWER
WEIGHCLN  10.5t                #WEIGHT CLEAN
WEIGFUEL  9.5t                #WEIGHT OF FUEL
WEIGLOAD  7.0t                #WEIGHT OF PAYLOAD
FUELABRN 4.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.20kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0m 1.0m 5.5m  #COCKPIT POSITION
LEFTGEAR -1.75m -2.05m -3.5m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.75m -2.05m -3.5m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0m -2.05m 6m  #WHEEL POSITION
ARRESTER  0m -1.83m -7.50m
MACHNGUN 2.02m  -0.18m  2.67m  #MACHINE GUN POSITION
MACHNGN2 -2.02m  -0.18m  2.67m  #MACHINE GUN POSITION
GUNINTVL 0.04
SMOKEGEN 0.45m -0.65m -5.75m  #SMOKE GENERATOR POSITION
VAPORPO0  7m -0.3m -4m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  7m -0.3m -4m  #VAPOR POSITION (Wings Spread)
HTRADIUS  10.0m

THRSTREV 1.0 

PSTMPTCH 80deg                #Post-Stall Maneuver VPitch
PSTMYAW_ 30deg                #Post-Stall Maneuver VYaw
PSTMROLL 30deg                #Post-Stall Maneuver VRoll

SMOKEOIL 100.0kg
WEAPONCH SMK

STRENGTH 12

CRITAOAP  30deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 1.3MACH              #CRITICAL SPEED
MAXSPEED 2.5MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 35m^2                #WING AREA

MXIPTAOA 30.5deg              #MAX INPUT AOA
MXIPTSSA 7.0deg               #MAX INPUT SSA
MXIPTROL 380.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 60kt                 #FULLY MANEUVABLE SPEED
CPITMANE 18.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 5.0                  #PITCH STABILITY CONSTANT
CYAWMANE 7.5                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 5.0                  #YAW STABILITY CONSTANT
CROLLMAN 5.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                #CONTROL AUTO VARIABLE GEOMETRY WING

VGWSPED1 0.6MACH
VGWSPED2 0.8MACH

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

REM Slots must come before Inits.
HRDPOINT 6.7m -0.675m -3.35m AIM9 AIM120
HRDPOINT -6.7m -0.675m -3.35m AIM9 AIM120

HRDPOINT 5.28m -0.675m -2m AIM9 AIM120
HRDPOINT -5.28m -0.675m -2m AIM9 AIM120
HRDPOINT 4.68m -0.675m -2m AIM9 AIM120
HRDPOINT -4.68m -0.675m -2m AIM9 AIM120
HRDPOINT 5m -0.975m -2m AIM9 AIM120
HRDPOINT -5m -0.975m -2m AIM9 AIM120

HRDPOINT 4m -0.77m -2.2m AGM65 RKT
HRDPOINT -4m -0.77m -2.2m AGM65 RKT

HRDPOINT 3.4m -0.65m -1.8m AGM65
HRDPOINT -3.4m -0.65m -1.8m AGM65
HRDPOINT 2.6m -0.65m -1.8m AGM65
HRDPOINT -2.6m -0.65m -1.8m AGM65
HRDPOINT 3m -1.1m -1.8m AGM65
HRDPOINT -3m -1.1m -1.8m AGM65

MAXNMAAM 8
MAXNMAGM 8
MAXNMRKT 38
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 3000                 #INITIAL GUN
LOADWEPN AIM9 2
LOADWEPN AIM120 6
LOADWEPN AGM65 6
LOADWEPN RKT 38

RADARCRS 0.45                  #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.3MACH              #CRUISING SPEED
REFACRUS 25000ft              #CRUISING ALTITUDE
REFTCRUS 0.7                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 130kt                 #LANDING SPEED
REFAOALD 10deg                 #LANDING AOA
REFLNRWY 2000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.30                 #LANDING THROTTLE SETTING

AUTOCALC
