IDENTIFY "RAH-00B_Crocodile(Gre)"
CATEGORY ATTACKER
AIRCLASS HELICOPTER
AFTBURNR FALSE                 #HAVE AFTERBURNER?
PROPELLR 2800HP              #PROPELLER ENGINE
PROPEFCY 0.65                 #Propeller efficiency
PROPVMIN 20m/s                #Minimum speed that T=P/V becomes valid
WEIGHCLN  3.0t                #WEIGHT CLEAN
WEIGFUEL  1.0t                #WEIGHT OF FUEL
WEIGLOAD  1.0t                #WEIGHT OF PAYLOAD
FUELMILI 0.40kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  1.16m 1.48m   #COCKPIT POSITION
LEFTGEAR -1.07m -1.00m 1.40m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.07m -1.00m 1.40m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.00m -7.00m  #WHEEL POSITION
MACHNGUN  0.00m -0.49m 4.40m  #MACHINE GUN POSITION
SMOKEGEN  0.0m  0.12m -9.00m  #SMOKE GENERATOR POSITION
VAPORPO0 -6.07m 2.28m 0.00m  #VAPOR POSITION (Wings Swept Back or no-VGW)
HTRADIUS  6.00m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 200kt                 #CRITICAL SPEED
MAXSPEED 200kt                 #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.0                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.0                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 2.0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 20.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 116m^2                #WING AREA

MXIPTAOA 0.0deg              #MAX INPUT AOA
MXIPTSSA 0.0deg              #MAX INPUT SSA
MXIPTROL 0.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 75kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 100kt                 #FULLY MANEUVABLE SPEED
CPITMANE 5.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 2.0                  #YAW STABILITY CONSTANT
CROLLMAN 5.0                  #ROLL MANEUVABILITY CONSTANT

TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m 1.0m 0.0m       #Thrust vector 0
TRSTDIR1 0.0m 1.0m 0.0m       #Thrust vector 1
PSTMPTCH 80deg                #Post-Stall Maneuver VPitch
PSTMYAW_ 80deg                #Post-Stall Maneuver VYaw
PSTMROLL 120deg                #Post-Stall Maneuver VRoll

CTLLDGEA TRUE                #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

GUNPOWER 5
INITIGUN 600                  #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITIAGM 4                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB
INITRCKT 14
MAXNMAAM 8
MAXNMAGM 6
MAXNBOMB 0
MAXNMRKT 42

AAMSLOT_ -1.81m 0.02m -0.35m
AAMSLOT_ 1.81m 0.02m -0.35m
AAMSLOT_ -1.73m 0.02m -0.35m
AAMSLOT_ 1.73m 0.02m -0.35m
AAMSLOT_ -1.38m 0.10m -0.35m
AAMSLOT_ 1.38m 0.10m -0.35m
AAMSLOT_ -1.30m 0.10m -0.35m
AAMSLOT_ 1.30m 0.10m -0.35m
AAMVISIB FALSE

AGMSLOT_ -1.53m 0.03m -0.35m
AGMSLOT_ 1.53m 0.03m -0.35m
AGMSLOT_ -1.23m 0.03m -0.35m
AGMSLOT_ 1.23m 0.03m -0.35m
AGMSLOT_ -1.77m -0.06m -0.35m
AGMSLOT_ 1.77m -0.06m -0.35m
AGMVISIB FALSE

RKTSLOT_ -0.93m -0.07m -0.35m
RKTSLOT_ 0.93m -0.07m -0.35m
RKTVISIB FALSE


RADARCRS 0.1                  #RADAR CROSS SECTION

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 310km/h              #CRUISING SPEED
REFACRUS 3000ft               #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 0kt                 #LANDING SPEED
REFAOALD 0deg                #LANDING AOA
REFLNRWY 100m                  #RUNWAY REQUIRED TO STOP
REFTHRLD 0.0                 #LANDING THROTTLE SETTING

AUTOCALC
