REM Modded from GAC repaint pack 090927's A340 SAC by YS Major
REM pilots and cockpit from Takaty's 737's
REM DAT modification by Ricm96
IDENTIFY "Airbus_A330_Korean_Airlines"
CATEGORY NORMAL
AFTBURNR FALSE #HAVE AFTERBURNER?
THRAFTBN 0.0t #THRUST WITH AFTERBURNER
THRMILIT 101.6t #THRUST AT MILITARY POWER
THRSTREV 0.1 #EFFICIENCY OF THRUST REVERSER
WEIGHCLN 177t #WEIGHT CLEAN
WEIGFUEL 194t #WEIGHT OF FUEL
WEIGLOAD 56t #WEIGHT OF PAYLOAD
FUELABRN 0.0kg #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.4kg #FUEL CONSUMPTION AT MILITARY POWER
 
COCKPITP -0.57m 0.83m 23.48m #COCKPIT POSITION 
INSTPANL  User/major/airliners/a330.ist
LEFTGEAR -5.0m -4.85m -1.6m #LEFT LANDING GEAR POSITION 
RIGHGEAR 5.0m -4.85m -1.6m #RIGHT LANDING GEAR POSITION
WHELGEAR 0.00m -5.0m 19.09m #WHEEL POSITION
ARRESTER 0.00m -4.55m -1.45m
MACHNGUN 0.0m 0.0m 0.0m #MACHINE GUN POSITION
SMOKEGEN -0.0m 3.60m -30.00m #SMOKE GENERATOR POSITION
VAPORPO0 30.15m 2.95m -13.1m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1 30.15m 2.95m -13.1m #VAPOR POSITION (Wings Spread)
HTRADIUS 50m #OUTSIDE SPHERE RADIUS
 
EXCAMERA "CO-PILOT" 0.625m 0.76m 23.7m 0deg 0deg 0deg INSIDE
 
TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK
 
STRENGTH 20
 
CRITAOAP 12.0deg #CRITICAL AOA POSITIVE
CRITAOAM -10deg #CRITICAL AOA NEGATIVE
 
CRITSPED 880km/h #CRITICAL SPEED
MAXSPEED 914km/h #MAXIMUM SPEED
 
HASSPOIL TRUE #HAS SPOILER?
RETRGEAR TRUE #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE #HAS VARIABLE GEOMETRY WING?
 
VGWSPED1 140kt
VGWSPED2 170kt
 
CLVARGEO 0.0 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5 #EFFECT OF GEAR FOR Cd
CDSPOILR 0.4 #EFFECT OF SPOILER FOR Cd
 
WINGAREA 439m^2 #WING AREA
 
MXIPTAOA 24.0deg #MAX INPUT AOA
MXIPTSSA 5.0deg #MAX INPUT SSA
MXIPTROL 15.0deg #MAX INPUT ROLL
 
REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??
 
MANESPD1 90kt #MINIMUM MANEUVABLE SPEED
MANESPD2 250kt #FULLY MANEUVABLE SPEED
CPITMANE 4.2 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 10.0 #PITCH STABILITY CONSTANT
CYAWMANE 2.0 #YAW MANEUVABILITY CONSTANT
CYAWSTAB 2.1 #YAW STABILITY CONSTANT
CROLLMAN 0.7 #ROLL MANEUVABILITY CONSTANT
 
CTLLDGEA TRUE #CONTROL LANDINGGEAR
CTLBRAKE FALSE #CONTROL BRAKE
CTLSPOIL 0.0 #CONTROL SPOILER
CTLABRNR FALSE #CONTROL AFTERBURNER
CTLTHROT 0.0 #CONTROL THRUST
CTLIFLAP 0.0 #CONTROL FLAP
CTLINVGW 0.0 #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE #CONTROL AUTO VARIABLE GEOMETRY WING
 
POSITION 0m 3ft 0m #POSITION
ATTITUDE 45deg 0deg 0deg #ATTITUDE
INITFUEL 75% #INITIAL FUEL
INITLOAD 0.0t #INITIAL PAYLOAD
INITSPED 0.0MACH #INITIAL SPEED
 
INITIGUN 0 #INITIAL GUN
INITIAAM 0 #INITIAL AAM
INITIAGM 0 #INITIAL AGM
INITBOMB 0 #INITIAL BOMB
 
REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 910km/h #CRUISING SPEED
REFACRUS 20000ft #CRUISING ALTITUDE
REFTCRUS 1.0 #CRUISING THROTTLE SETTING
 
REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 135kt #LANDING SPEED
REFAOALD 9deg #LANDING AOA
REFLNRWY 2500m #RUNWAY REQUIRED TO STOP
REFTHRLD 0.6 #LANDING THROTTLE SETTING
 
AUTOCALC