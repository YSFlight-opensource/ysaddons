REM Original model Type99 by Oranleed
REM modded by YS Major

IDENTIFY "Type99_PLAAF"
CATEGORY FIGHTER
AFTBURNR FALSE                 #HAVE AFTERBURNER?
PROPELLR 610HP              #PROPELLER ENGINE
WEIGHCLN  1.23t                #WEIGHT CLEAN
WEIGFUEL  0.32t                #WEIGHT OF FUEL
WEIGLOAD  0.3t                #WEIGHT OF PAYLOAD
FUELMILI 0.04kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  0.86m  0.0m  #COCKPIT POSITION
LEFTGEAR -1.58m -2.11m  0.81m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.58m -2.11m  0.81m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -0.47m -6.35m  #WHEEL POSITION
ARRESTER  0.0m -0.25m -6.35m #Arresting hook position
SMOKEGEN  0.00m  0.0m -6.35m  #SMOKE GENERATOR POSITION
VAPORPO0  7.19m  0.15m  -0.18m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  7.19m  0.15m  -0.18m  #VAPOR POSITION (Wings Spread)
HTRADIUS  5m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 GUN
TRIGGER3 SMK
TRIGGER4 SMK

STRENGTH 4

CRITAOAP 19deg #CRITICAL AOA POSITIVE
CRITAOAM -15deg #CRITICAL AOA NEGATIVE

CRITSPED 370km/h #CRITICAL SPEED
MAXSPEED 463km/h #MAXIMUM SPEED

HASSPOIL FALSE #HAS SPOILER?
RETRGEAR TRUE #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.6 #EFFECT OF FLAP FOR Cd          
CDBYGEAR 0.5 #EFFECT OF GEAR FOR Cd          
CDSPOILR 0.0 #EFFECT OF SPOILER FOR Cd

WINGAREA 17.8m^2 #WING AREA      

MXIPTAOA 15.0deg #MAX INPUT AOA     
MXIPTSSA 5.0deg #MAX INPUT SSA
MXIPTROL 62.0deg #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt #MINIMUM MANEUVABLE SPEED
MANESPD2 70kt #FULLY MANEUVABLE SPEED
CPITMANE 8.9 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0 #PITCH STABILITY CONSTANT
CYAWMANE 5.0 #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0 #YAW STABILITY CONSTANT
CROLLMAN 1.7 #ROLL MANEUVABILITY CONSTANT    

CTLLDGEA TRUE #CONTROL LANDINGGEAR
CTLBRAKE FALSE #CONTROL BRAKE
CTLSPOIL 0.0 #CONTROL SPOILER
CTLABRNR FALSE #CONTROL AFTERBURNER
CTLTHROT 0.0 #CONTROL THRUST
CTLIFLAP 0.0 #CONTROL FLAP
CTLINVGW 0.0 #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m #POSITION
ATTITUDE 45deg 0deg 0deg #ATTITUDE
INITFUEL 75% #INITIAL FUEL
INITLOAD 0.0t #INITIAL PAYLOAD
INITSPED 0.0MACH #INITIAL SPEED

INITIGUN 0                  #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM

NMTURRET 1
TURRETPO 0 0m 0.75m -2.09m 180deg 0deg 0deg       # Number x y z h p b
TURRETPT 0 0deg 50deg 0deg                     # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 -40deg 40deg 0deg                 # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 5000                                # Ammo
TURRETIV 0 0.14sec                              # Number ShootingInterval
TURRETNM 0 gun                                # DNM Node Name
TURRETAR 0                                     # Anti Air Capable
TURRETCT 0 GUNNER                              # Controlled by PILOT or GUNNER
TURRETRG 0 1500m

RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 370km/h #CRUISING SPEED     
REFACRUS 12000ft #CRUISING ALTITUDE
REFTCRUS 1.0 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 73kt #LANDING SPEED
REFAOALD 14deg #LANDING AOA                   
REFLNRWY 600m #RUNWAY REQUIRED TO STOP
REFTHRLD 0.9 #LANDING THROTTLE SETTING         

AUTOCALC
