REM Original model by YS Major converted from FMS model
REM DAT by Ricm96
REM guns and crew from Oranleed's B-29/B-17.

IDENTIFY "B-26_Marauder"
CATEGORY ATTACKER
AFTBURNR FALSE                 #HAVE AFTERBURNER?
PROPELLR 3800HP                #PROPELLER ENGINE
WEIGHCLN  24000lb                 #WEIGHT CLEAN
WEIGFUEL  7000lb                #WEIGHT OF FUEL
WEIGLOAD  6000lb                 #WEIGHT OF PAYLOAD
FUELMILI 0.50lb                #FUEL CONSUMPTION AT MILITARY POWER

INSTPANL user/major/B26/B26.ist
COCKPITP -0.36m  1.02m  3.99m  #COCKPIT POSITION
EXCAMERA "copilot" 0.36m  1.01m  3.99m 0deg 0deg 0deg INSIDE
EXCAMERA "Bomber1" 0m  0.44m  6.31m 0deg -25deg 0deg INSIDE
EXCAMERA "Gunner2" 0m  1.36m  -5.27m 0deg 10deg 0deg INSIDE
EXCAMERA "Gunner3" 0m  0.74m  -9.93m 180deg 0deg 0deg INSIDE
EXCAMERA "bombs" 0m  0.0m  -1.0m 0deg -90deg 0deg INSIDE
LEFTGEAR -3.1m -1.9m  -1.8m  #LEFT LANDING GEAR POSITION
RIGHGEAR  3.1m -1.9m  -1.8m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.8m  4.07m  #WHEEL POSITION
ARRESTER  0.00m -1.62m -2.0m  #Arresting hook position 

GUNINTVL 0.12
GUNPOWER 0

SMOKEGEN  3.2m 0.5m  -2.60m  #SMOKE GENERATOR POSITION
VAPORPO0  10.78m  1.0m  -1.28m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  10.78m  1.0m  -1.28m  #VAPOR POSITION (Wings Spread)
HTRADIUS  12.0m                 #OUTSIDE SPHERE RADIUS
GUNSIGHT FALSE

TRIGGER1 GUN
TRIGGER2 SMK

STRENGTH 20

CRITAOAP 22deg                 #CRITICAL AOA POSITIVE
CRITAOAM -12deg                #CRITICAL AOA NEGATIVE

CRITSPED 220kt               #CRITICAL SPEED
MAXSPEED 250kt               #MAXIMUM SPEED

HASSPOIL FALSE                  #HAS SPOILER?
RETRGEAR TRUE                  #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                 #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.2                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 1.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 1.6                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.2                  #EFFECT OF SPOILER FOR Cd

WINGAREA 38.3m^2                 #WING AREA

MXIPTAOA 24.0deg               #MAX INPUT AOA
MXIPTSSA 7.0deg                #MAX INPUT SSA
MXIPTROL 40.0deg               #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 80kt                  #MINIMUM MANEUVABLE SPEED
MANESPD2 100kt                  #FULLY MANEUVABLE SPEED
CPITMANE 7.8                   #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 9.0                   #PITCH STABILITY CONSTANT
CYAWMANE 6.0                   #YAW MANEUVABILITY CONSTANT
CYAWSTAB 4.0                   #YAW STABILITY CONSTANT
CROLLMAN 3.5                   #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                  #CONTROL LANDINGGEAR
CTLBRAKE FALSE                 #CONTROL BRAKE
CTLSPOIL 0.0                   #CONTROL SPOILER
CTLABRNR FALSE                 #CONTROL AFTERBURNER
CTLTHROT 0.0                   #CONTROL THRUST
CTLIFLAP 0.0                   #CONTROL FLAP
CTLINVGW 0.0                   #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m             #POSITION
ATTITUDE 45deg 15deg 0deg      #ATTITUDE
INITFUEL 75%                   #INITIAL FUEL
INITLOAD 0.0t                  #INITIAL PAYLOAD
INITSPED 0.0MACH               #INITIAL SPEED
WEAPONCH GUN

GUNPOWER 1

NMTURRET 8
TURRETPO 0 1.0m -0.2m 2.8m 0deg 0deg 0deg
TURRETPT 0 0deg 0deg 0deg
TURRETHD 0 0deg 0deg 0deg
TURRETAM 0 1500
TURRETIV 0 0.06sec
TURRETAR 0 
TURRETCT 0 PILOT
TURRETRG 0 1600m

TURRETPO 1 0.0m 0.27m  -10.7m 180deg 0.0deg 0.0deg
TURRETPT 1 -35deg 35deg 0deg
TURRETHD 1 -35deg 35deg 0deg
TURRETAM 1 3000
TURRETIV 1 0.04sec
TURRETNM 1 tur2                           # DNM Node Name
TURRETAR 1
TURRETCT 1 GUNNER
TURRETRG 1 1800m

TURRETPO 2 0.0m 1.3m  -5.6m 180deg 0.0deg 0.0deg
TURRETPT 2 0deg 80deg 0deg
TURRETHD 2 -180deg 180deg 0deg
TURRETAM 2 3000
TURRETIV 2 0.02sec
TURRETNM 2 tur3                           # DNM Node Name
TURRETAR 2
TURRETCT 2 GUNNER
TURRETRG 2 1800m

TURRETPO 3 0.0m 1.3m  -5.6m 180deg 0.0deg 0.0deg
TURRETPT 3 0deg 0deg 0deg
TURRETHD 3 -180deg 180deg 0deg
TURRETAM 3 0
TURRETIV 3 0.02sec
TURRETNM 3 toptur                           # DNM Node Name
TURRETAR 3 
TURRETCT 3 GUNNER
TURRETRG 3 1800m

TURRETPO 4 0.0m 0.14m 6.95m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 4 -25deg 25deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 4 -25deg 25deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 4 1500                              # Ammo
TURRETIV 4 0.04sec                            # Number ShootingInterval
TURRETNM 4 tur1                           # DNM Node Name
TURRETAR 4                                   # Anti Air Capable
TURRETCT 4 GUNNER                            # Controlled by PILOT or GUNNER
TURRETRG 4 1800m

TURRETPO 5 -1m -0.2m 2.8m -0.02deg 0.03deg 0deg
TURRETPT 5 0deg 0deg 0deg
TURRETHD 5 0deg 0deg 0deg
TURRETAM 5 1500
TURRETIV 5 0.08sec
TURRETAR 5 
TURRETCT 5 PILOT
TURRETRG 5 1600m

TURRETPO 6 -0.76m -0.6m 2.8m -0.01deg 0deg 0deg
TURRETPT 6 0deg 0deg 0deg
TURRETHD 6 0deg 0deg 0deg
TURRETAM 6 1500
TURRETIV 6 0.07sec
TURRETAR 6 
TURRETCT 6 PILOT
TURRETRG 6 1600m

TURRETPO 7 0.76m -0.6m 2.8m 0.02deg 0.01deg 0deg
TURRETPT 7 0deg 0deg 0deg
TURRETHD 7 0deg 0deg 0deg
TURRETAM 7 1500
TURRETIV 7 0.08sec
TURRETAR 7 
TURRETCT 7 PILOT
TURRETRG 7 1600m

BOMVISIB TRUE
HRDPOINT 0.4m -0.6m 0.3m B250
HRDPOINT -0.4m -0.6m 0.3m B250
HRDPOINT 0.4m -0.6m -1.3m B250
HRDPOINT -0.4m -0.6m -1.3m B250

HRDPOINT 0.4m -0.3m 0.3m B250
HRDPOINT -0.4m -0.3m 0.3m B250
HRDPOINT 0.4m -0.3m -1.3m B250
HRDPOINT -0.4m -0.3m -1.3m B250
HRDPOINT 0.4m 0m 0.3m B250
HRDPOINT -0.4m 0m 0.3m B250
HRDPOINT 0.4m 0m -1.3m B250
HRDPOINT -0.4m 0m -1.3m B250

BOMINBAY TRUE                 #Bombs in Bomb Bay?
BMBAYRCS 1.0                  #Effect on Bomb Bay Opening on Radar Cross Section
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 0                    #INITIAL GUN
LOADWEPN B250 12               #INITIAL BOMB

RADARCRS 1.0

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 250kt                 #CRUISING SPEED(*1.2
REFACRUS 5000ft               #CRUISING ALTITUDE
REFTCRUS 1.0                   #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 114kt                 #LANDING SPEED
REFAOALD 12deg                  #LANDING AOA
REFLNRWY 1200m                  #RUNWAY REQUIRED TO STOP
REFTHRLD 0.95                  #LANDING THROTTLE SETTING

AUTOCALC
