REM Original model by YS Major
REM DAT modification by ricm96

IDENTIFY "Airspeed_Horsa_DC-3tow"
CATEGORY NORMAL
AFTBURNR TRUE #HAVE AFTERBURNER?
PROPELLR 2400HP #PROPELLER ENGINE 
WEIGHCLN 10.8t #WEIGHT CLEAN
WEIGFUEL 0.5t #WEIGHT OF FUEL
WEIGLOAD 3.0t #WEIGHT OF PAYLOAD
FUELABRN 99999999kg #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.35kg #FUEL CONSUMPTION AT MILITARY POWER
 
COCKPITP 0.45m -0.2m 7.05m #COCKPIT POSITION
EXCAMERA "CO-PILOT" -0.45m -0.2m 7.05m 0deg 0deg 0deg INSIDE
EXCAMERA "inside" 0.00m -0.3m 5.0m 0deg 0deg 0deg INSIDE
INSTPANL User/major/101st/horsa.ist
LEFTGEAR -2.2m -2.5m -1.0m #LEFT LANDING GEAR POSITION
RIGHGEAR 2.2m -2.5m -1.0m #RIGHT LANDING GEAR POSITION
WHELGEAR 0.0m -2.5m 6.0m #WHEEL POSITION
ARRESTER 0.0m -2.5m -1.0m
MACHNGUN 0.00m 0.00m 0.00m #MACHINE GUN POSITION
SMOKEGEN 0.0m 0.0m 0.0m #SMOKE GENERATOR POSITION
VAPORPO0 13.38m 0.30m 1.5m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1 13.38m 0.30m 1.5m #VAPOR POSITION (Wings Spread)
HTRADIUS 19m #OUTSIDE SPHERE RADIUS
 
RADARCRS 1.0 #RADAR CROSS SECTION
 
TRIGGER1 SMK
TRIGGER2 NON
TRIGGER3 NON
TRIGGER4 NON
SMOKEOIL 100.0kg
WEAPONCH SMK
 
STRENGTH 12
 
CRITAOAP 22.5deg #CRITICAL AOA POSITIVE
CRITAOAM -15deg #CRITICAL AOA NEGATIVE
 
CRITSPED 105kt #CRITICAL SPEED
MAXSPEED 150kt #MAXIMUM SPEED
 
HASSPOIL TRUE #HAS SPOILER?
RETRGEAR FALSE #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE #HAS VARIABLE GEOMETRY WING?
VGWSPED1 0kt
VGWSPED2 20kt
 
CLVARGEO 0.0 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.8 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.55 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.6 #EFFECT OF GEAR FOR Cd
CDSPOILR 1.7 #EFFECT OF SPOILER FOR Cd
 
WINGAREA 30.7m^2 #WING AREA
 
MXIPTAOA 23.5deg #MAX INPUT AOA
MXIPTSSA 5.5deg #MAX INPUT SSA
MXIPTROL 25.0deg #MAX INPUT ROLL
 
REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??
 
MANESPD1 45kt #MINIMUM MANEUVABLE SPEED
MANESPD2 60kt #FULLY MANEUVABLE SPEED
CPITMANE 8.0 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 4.0 #PITCH STABILITY CONSTANT
CYAWMANE 2.0 #YAW MANEUVABILITY CONSTANT
CYAWSTAB 2.5 #YAW STABILITY CONSTANT
CROLLMAN 0.7 #ROLL MANEUVABILITY CONSTANT
 
CTLLDGEA TRUE #CONTROL LANDINGGEAR
CTLBRAKE FALSE #CONTROL BRAKE
CTLSPOIL 0.0 #CONTROL SPOILER
CTLABRNR FALSE #CONTROL AFTERBURNER
CTLTHROT 0.0 #CONTROL THRUST
CTLIFLAP 0.0 #CONTROL FLAP
CTLINVGW 0.0 #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE #CONTROL AUTO VARIABLE GEOMETRY WING
 
POSITION 0m 3ft 0m #POSITION
ATTITUDE 45deg 0deg 0deg #ATTITUDE
INITFUEL 75% #INITIAL FUEL
INITLOAD 0.0t #INITIAL PAYLOAD
INITSPED 0.0MACH #INITIAL SPEED
 
REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 150kt #CRUISING SPEED
REFACRUS 1000ft #CRUISING ALTITUDE
REFTCRUS 1.0 #CRUISING THROTTLE SETTING 
 
REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 67kt #LANDING SPEED
REFAOALD 12deg #LANDING AOA
REFLNRWY 140m #RUNWAY REQUIRED TO STOP
REFTHRLD 0.8 #LANDING THROTTLE SETTING 
 
AUTOCALC