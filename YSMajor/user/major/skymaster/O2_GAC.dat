REM Original model YS Major

IDENTIFY "O-2_GAC_Skymaster"
CATEGORY UTILITY               #[NORMAL]

AFTBURNR FALSE                 #HAVE AFTERBURNER?
PROPELLR 420HP                 #PROPELLER ENGINE [108HP]
WEIGHCLN 2655lb                 #WEIGHT CLEAN
WEIGFUEL 800lb                 #WEIGHT OF FUEL
WEIGLOAD 1000lb                 #WEIGHT OF PAYLOAD
FUELMILI 0.022lb                #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP -0.22m  0.2m  1.05m  #COCKPIT POSITION
EXCAMERA "FAC" 0.25m  0.25m  1.13m  0deg 0deg 0deg INSIDE
INSTPANL user/major/skymaster/c337.ist
LEFTGEAR -1.22m -1.4m -0.6m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.22m -1.4m -0.6m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.53m  2.23m  #WHEEL POSITION
ARRESTER  0.00m -1.3m -3.40m  #ARRESTING HOOK POSITION
MACHNGUN  0.0m 0.0m  0.0m  #MACHINE GUN POSITION
SMOKEGEN  0.0m  -0.20m -2.2m  #SMOKE GENERATOR POSITION
VAPORPO0  6.85m  0.8m  -0.23m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  6.85m  0.8m  -0.23m  #VAPOR POSITION (Wings Spread)
HTRADIUS  6.50m                #OUTSIDE SPHERE RADIUS

STRENGTH 4

CRITAOAP  20deg                #CRITICAL AOA POSITIVE
CRITAOAM -15deg                #CRITICAL AOA NEGATIVE

CRITSPED  94kt                 #CRITICAL SPEED
MAXSPEED 174kt                 #MAXIMUM SPEED  [106kt/109kt]

HASSPOIL FALSE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                 #HAS VARIABLE GEOMETRY WING?

REM VGWSPED1 0kt
REM VGWSPED2 90kt

CLVARGEO 0.0                   #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                   #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.4                   #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                   #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.0                   #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                   #EFFECT OF SPOILER FOR Cd

WINGAREA 14.90m^2              #WING AREA

MXIPTAOA  16.0deg              #MAX INPUT AOA
MXIPTSSA   5.0deg              #MAX INPUT SSA
MXIPTROL 110.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 42kt                  #MINIMUM MANEUVABLE SPEED
MANESPD2 50kt                  #FULLY MANEUVABLE SPEED
CPITMANE 5.0                   #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.5                   #PITCH STABILITY CONSTANT
CYAWMANE 4.0                   #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.5                   #YAW STABILITY CONSTANT
CROLLMAN 1.8                   #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                  #CONTROL LANDINGGEAR
CTLBRAKE FALSE                 #CONTROL BRAKE
CTLSPOIL 0.0                   #CONTROL SPOILER
CTLABRNR FALSE                 #CONTROL AFTERBURNER
CTLTHROT 0.0                   #CONTROL THRUST
CTLIFLAP 0.0                   #CONTROL FLAP
CTLINVGW 0.0                   #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m             #POSITION
ATTITUDE 45deg 0deg 0deg       #ATTITUDE
INITFUEL 100%                  #INITIAL FUEL
INITLOAD 0.0t                  #INITIAL PAYLOAD
INITSPED 0.0MACH               #INITIAL SPEED

MAXNBOMB 4
MAXNMRKT 28
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

HRDPOINT -2.55m 0.2m 0.0m RKT B250
HRDPOINT 2.55m 0.2m 0.0m RKT B250
HRDPOINT -3.15m 0.25m 0.00m RKT B250
HRDPOINT 3.15m 0.25m 0.00m RKT B250

GUNINTVL 0.12
GUNPOWER 1

INITIGUN 0                 #INITIAL GUN
INITRCKT 14
LOADWEPN B250 2               #INITIAL BOMB

RADARCRS 0.05

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 126kt                 #CRUISING SPEED
REFACRUS 5000ft                #CRUISING ALTITUDE
REFTCRUS 0.75                   #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 55kt                  #LANDING SPEED
REFAOALD 8.0deg               #LANDING AOA                             [TailStrikeAngle:15.31deg]
REFLNRWY 370m                  #RUNWAY REQUIRED TO STOP         [145m]
REFTHRLD 0.6                   #LANDING THROTTLE SETTING

AUTOCALC
