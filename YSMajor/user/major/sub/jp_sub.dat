REM Original from Maru's U-boat
REM modded by YS Major

IDENTIFY "JP_Submarine_WW2"
CATEGORY NORMAL
AFTBURNR FALSE                 #HAVE AFTERBURNER?
PROPELLR 9600.00HP              #PROPELLER ENGINE
THRSTREV 1.00                 #EFFICIENCY OF THRUST REVERSER
WEIGHCLN  6.00t                #WEIGHT CLEAN
WEIGFUEL  3.00t                #WEIGHT OF FUEL
WEIGLOAD  4.00t                #WEIGHT OF PAYLOAD
FUELMILI 0.10kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.25m  -5.54m  -0.7m  #COCKPIT POSITION
INSTPANL user/major/sub/sub.ist
EXCAMERA "bridge" 0m  1.0m  1.68m 0deg 0deg 0deg INSIDE
EXCAMERA "scope" 0m  7.15m  0m 0deg 10deg 0deg INSIDE
EXCAMERA "torp" 0m  -6.5m  16m 0deg 10deg 0deg INSIDE
LEFTGEAR -2.7m -11m -36m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.7m -11m -36m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -11m  35m  #WHEEL POSITION
MACHNGUN  0.0m 0.0m  0.0m  #MACHINE GUN POSITION
VAPORPO0 1.2m  -9.12m  -34.5m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1 1.2m  -9.12m  -34.5m  #VAPOR POSITION (Wings Spread)
HTRADIUS  20.0m                #OUTSIDE SPHERE RADIUS
GUNSIGHT TRUE
GUNINTVL 0.05

RADARCRS 0.5                  #RADAR CROSS SECTION

STRENGTH 20

CRITAOAP 20deg               #CRITICAL AOA POSITIVE
CRITAOAM -20deg               #CRITICAL AOA NEGATIVE

CRITSPED 35kt
MAXSPEED 38kt

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR FALSE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m 0.0m 1.0m       #Thrust vector 0
TRSTDIR1 0.0m 1.0m 0.0m       #Thrust vector 1
PSTMPTCH 90deg                #Post-Stall Maneuver VPitch
PSTMYAW_ 90deg                #Post-Stall Maneuver VYaw
PSTMROLL 90deg                #Post-Stall Maneuver VRoll

CLVARGEO 0.01                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.0                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.0                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 5.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 0.01m^2                #WING AREA

MXIPTAOA 10.0deg              #MAX INPUT AOA
MXIPTSSA 4.0deg              #MAX INPUT SSA
MXIPTROL 5.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 5kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 10kt                 #FULLY MANEUVABLE SPEED
CPITMANE 0.1                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 5.0                  #PITCH STABILITY CONSTANT
CYAWMANE 0.1                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 4.0                  #YAW STABILITY CONSTANT
CROLLMAN 0.1                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA FALSE                #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m -2ft 0m            #POSITION
ATTITUDE 45deg 40deg 0deg      #ATTITUDE
INITFUEL 100%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

RKTSLOT_  1.2m -5.9m 20m
RKTSLOT_ -1.2m -5.9m 20m
RKTSLOT_  1.2m -7.4m 20m
RKTSLOT_ -1.63m -7.4m 20m
RKTVISIB FALSE

NMTURRET 8
TURRETPO 0 0.00m -2.0m 10.26m 0deg 0deg 0deg
TURRETPT 0 0deg 20deg 0deg
TURRETHD 0 -160deg 160deg 0deg
TURRETAM 0 100
TURRETIV 0 12.0sec
TURRETNM 0 cannon
TURRETAR 0
TURRETCT 0 PILOT
TURRETRG 0 1000m

TURRETPO 1 0.00m -2.0m 10.26m 0deg 0deg 0deg
TURRETPT 1 0deg 20deg 0deg
TURRETHD 1 -160deg 160deg 0deg
TURRETAM 1 100
TURRETIV 1 12.0sec
TURRETNM 1 cannon
TURRETAR 1
TURRETCT 1 PILOT
TURRETRG 1 1000m

TURRETPO 2 0.00m -2.0m 10.26m 0deg 0deg 0deg
TURRETPT 2 0deg 20deg 0deg
TURRETHD 2 -160deg 160deg 0deg
TURRETAM 2 100
TURRETIV 2 12.0sec
TURRETNM 2 cannon
TURRETAR 2
TURRETCT 2 PILOT
TURRETRG 2 1000m

TURRETPO 3 0.00m -2.0m 10.26m 0deg 0deg 0deg
TURRETPT 3 0deg 20deg 0deg
TURRETHD 3 -160deg 160deg 0deg
TURRETAM 3 100
TURRETIV 3 12.0sec
TURRETNM 3 cannon
TURRETAR 3
TURRETCT 3 PILOT
TURRETRG 3 1000m

TURRETPO 4 0.00m -2.0m 10.26m 0deg 0deg 0deg
TURRETPT 4 0deg 20deg 0deg
TURRETHD 4 -160deg 160deg 0deg
TURRETAM 4 100
TURRETIV 4 12.0sec
TURRETNM 4 cannon
TURRETAR 4
TURRETCT 4 PILOT
TURRETRG 4 1000m

TURRETPO 5 0.00m -2.0m 10.26m 0deg 0deg 0deg
TURRETPT 5 0deg 20deg 0deg
TURRETHD 5 -160deg 160deg 0deg
TURRETAM 5 100
TURRETIV 5 12.0sec
TURRETNM 5 cannon
TURRETAR 5
TURRETCT 5 PILOT
TURRETRG 5 1000m

TURRETPO 6 0.00m -2.0m 10.26m 0deg 0deg 0deg
TURRETPT 6 0deg 20deg 0deg
TURRETHD 6 -160deg 160deg 0deg
TURRETAM 6 100
TURRETIV 6 12.0sec
TURRETNM 6 cannon
TURRETAR 6
TURRETCT 6 PILOT
TURRETRG 6 1000m

TURRETPO 7 0.00m 0.9m -5m -180deg 45deg 0deg
TURRETPT 7 -30deg 45deg 0deg
TURRETHD 7 -160deg 160deg 0deg
TURRETAM 7 10000
TURRETIV 7 0.3sec
TURRETNM 7 gun
TURRETAR 7
TURRETCT 7 GUNNER
TURRETRG 7 1000m

INITIGUN 0                 #INITIAL GUN
INITRCKT 30                  #INITIAL ROCKET (10kg/each)

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 28kt             #CRUISING SPEED
REFACRUS 300ft              #CRUISING ALTITUDE
REFTCRUS 0.8                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 10kt               #LANDING SPEED
REFAOALD 15deg               #LANDING AOA
REFLNRWY 70m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.5                #LANDING THROTTLE SETTING

AUTOCALC