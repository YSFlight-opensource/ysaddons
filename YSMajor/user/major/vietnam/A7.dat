REM Original by Oranleed, Repaint and mod by YS Major

IDENTIFY "A-7C_CORSAIR_VA147"
CATEGORY ATTACKER
AFTBURNR FALSE                #HAVE AFTERBURNER?
THRAFTBN  0.0t                #THRUST WITH AFTERBURNER
THRMILIT  7.8t                #THRUST AT MILITARY POWER
WEIGHCLN  8.6t                #WEIGHT CLEAN
WEIGFUEL  6.6t                #WEIGHT OF FUEL
WEIGLOAD  5.8t                #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.45kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.70m  3.8m  #COCKPIT POSITION
INSTPANL user/major/vietnam/f8.ist
LEFTGEAR -1.64m -2.01m -2.40m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.64m -2.01m -2.40m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -2.33m  2.99m  #WHEEL POSITION
MACHNGUN -0.52m -0.65m  4.97m  #MACHINE GUN POSITION
MACHNGN2  0.52m -0.65m  4.97m  #MACHINE GUN POSITION
ARRESTER  0.00m -1.85m -5.51m  #ARRESTING HOOK
SMOKEGEN  0.00m -1.13m -4.17m  #SMOKE GENERATOR POSITION
VAPORPO0 -5.90m  0.17m -3.50m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1 -5.90m  0.17m -3.50m  #VAPOR POSITION (Wings Spread)
HTRADIUS  8.00m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  22deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 350kt                #CRITICAL SPEED
MAXSPEED 0.9MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?
TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 35m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 50.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 30kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 80kt                #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 4.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 4.0                  #YAW STABILITY CONSTANT
CROLLMAN 6.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

INITIGUN 0                 #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITIAGM 2                    #INITIAL AGM
INITBOMB 12                    #INITIAL BOMB
MAXNBOMB 12

AAMSLOT_ -1.0m 0.1m 1.6m
AAMSLOT_  1.0m 0.1m 1.6m
BOMBSLOT  2.3m -0.13m -0.2m
BOMBSLOT -2.3m -0.13m -0.2m
BOMBSLOT  2.3m -0.13m -2.5m
BOMBSLOT -2.3m -0.13m -2.5m
BOMBSLOT  2.88m -0.13m -0.2m
BOMBSLOT -2.88m -0.13m -0.2m
BOMBSLOT  2.88m -0.13m -2.5m
BOMBSLOT -2.88m -0.13m -2.5m
BOMBSLOT  2.58m -0.36m -0.2m
BOMBSLOT -2.58m -0.36m -0.2m
BOMBSLOT  2.58m -0.36m -2.5m
BOMBSLOT -2.58m -0.36m -2.5m
AGMSLOT_  3.34m -0.13m -1.7m
AGMSLOT_ -3.34m -0.13m -1.7m

NMTURRET 2
TURRETPO 0 -0.52m -0.65m 5m 0.1deg 0deg 0deg       # Number x y z h p b
TURRETPT 0 0deg 0deg 0deg                     # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 0deg 0deg 0deg                 # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 800                               # Ammo
TURRETIV 0 0.05sec                              # Number ShootingInterval
TURRETNM 0 aaaa                                # DNM Node Name
TURRETAR 0                                     # Anti Air Capable
TURRETCT 0 PILOT                              # Controlled by PILOT or GUNNER
TURRETRG 0 2500m

TURRETPO 1 0.52m -0.65m 5m -0.1deg 0.04deg 0deg       # Number x y z h p b
TURRETPT 1 0deg 0deg 0deg                     # Number MinPitch MaxPitch NeutralPitch
TURRETHD 1 0deg 0deg 0deg                 # Number MinHdg MaxHdg NeutralHdg
TURRETAM 1 800                               # Ammo
TURRETIV 1 0.045sec                              # Number ShootingInterval
TURRETNM 1 aaaa                                # DNM Node Name
TURRETAR 1                                     # Anti Air Capable
TURRETCT 1 PILOT                              # Controlled by PILOT or GUNNER
TURRETRG 1 2500m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 400kt                #CRUISING SPEED
REFACRUS 15000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 130kt                #LANDING SPEED
REFAOALD 15deg                #LANDING AOA
REFLNRWY 2000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
