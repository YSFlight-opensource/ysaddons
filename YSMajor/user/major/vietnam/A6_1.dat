REM Original model by Oranleed. Modded by YS Major

IDENTIFY "A-6E_INTRUDER#1"
CATEGORY ATTACKER
AFTBURNR FALSE                #HAVE AFTERBURNER?
THRAFTBN  0.0t                #THRUST WITH AFTERBURNER
THRMILIT  8.4t                #THRUST AT MILITARY POWER
WEIGHCLN 12.1t                #WEIGHT CLEAN
WEIGFUEL  5.4t                #WEIGHT OF FUEL
WEIGLOAD  11.1t                #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.8kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP -0.35m  1.24m  2.68m  #COCKPIT POSITION
INSTPANL user/major/vietnam/a6.ist
LEFTGEAR -2.05m -2.21m -1.56m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.05m -2.21m -1.56m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.20m  4.13m  #WHEEL POSITION
ARRESTER  0.0m -2.0m -7.28m
MACHNGUN -0.0m -0.00m  0.00m  #MACHINE GUN POSITION
SMOKEGEN -0.0m  0.00m -6.00m  #SMOKE GENERATOR POSITION
VAPORPO0  8.1m -0.22m -4.10m  #VAPOR POSITION (Wings Swept Back or no-mVGW)
VAPORPO1  5.4m  0.05m -1.60m  #VAPOR POSITION (Wings Spread)
HTRADIUS  11m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -12deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.8MACH              #CRITICAL SPEED
MAXSPEED 1050km/h             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.2                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 49.1m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 160.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 95kt                #FULLY MANEUVABLE SPEED
CPITMANE 7.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

HRDPOINT -2.12m -0.55m 0.6m B500
HRDPOINT 2.12m -0.55m 0.6m B500
HRDPOINT -2.77m -0.55m 0.6m B500
HRDPOINT 2.77m -0.55m 0.6m B500
HRDPOINT -2.45m -0.82m 0.6m B500 
HRDPOINT 2.45m -0.82m 0.6m B500 

HRDPOINT -2.12m -0.55m -1.7m B500
HRDPOINT 2.12m -0.55m -1.7m B500
HRDPOINT 2.77m -0.55m -1.7m B500
HRDPOINT -2.77m -0.55m -1.7m B500
HRDPOINT 2.45m -0.82m -1.7m B500 
HRDPOINT -2.45m -0.82m -1.7m B500 

HRDPOINT -4.1m -0.43m 0.1m B500
HRDPOINT 4.1m -0.43m 0.1m B500
HRDPOINT -3.5m -0.43m 0.1m B500
HRDPOINT 3.5m -0.43m 0.1m B500
HRDPOINT -3.83m -0.7m 0.1m B500 
HRDPOINT 3.83m -0.7m 0.1m B500 

HRDPOINT -4.1m -0.43m -2.1m B500
HRDPOINT 4.1m -0.43m -2.1m B500
HRDPOINT 3.5m -0.43m -2.1m B500
HRDPOINT -3.5m -0.43m -2.1m B500
HRDPOINT 3.83m -0.7m -2.1m B500 
HRDPOINT -3.83m -0.7m -2.1m B500 

HRDPOINT -3.83m -0.5m -0.8m AGM65
HRDPOINT 3.83m -0.5m -0.8m AGM65

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

MAXNMRKT 38
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
GUNPOWER 1
INITIGUN 0                  #INITIAL GUN
LOADWEPN B500 12
LOADWEPN AGM65 2

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.8MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 125kt                #LANDING SPEED
REFAOALD 9deg                #LANDING AOA
REFLNRWY 1000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.4                  #LANDING THROTTLE SETTING

AUTOCALC
