REM Original from CEP
REM modded by YS Major

IDENTIFY "OH-6_Gunship"
CATEGORY ATTACKER
AIRCLASS HELICOPTER

AFTBURNR FALSE #HAVE AFTERBURNER?
PROPELLR 420HP #PROPELLER ENGINE
PROPEFCY 0.65 #Propeller efficiency
PROPVMIN 20m/s #Minimum speed that T=P/V becomes valid
WEIGHCLN 0.6t #WEIGHT CLEAN
WEIGFUEL 0.3t #WEIGHT OF FUEL
WEIGLOAD 0.6t #WEIGHT OF PAYLOAD
FUELMILI 0.02kg #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.35m  0.81m 0.53m  #COCKPIT POSITION
INSTPANL user/Major/vietnam/loach.ist
LEFTGEAR -1.00m  -1.07m -1.0m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.00m  -1.07m -1.0m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.07m  1.45m  #WHEEL POSITION
MACHNGUN -0.70m -0.30m 0.60m  #MACHINE GUN POSITION
VAPORPO0 -0.76m 1.22m -3.51m  #VAPOR POSITION (Wings Swept Back or no-VGW)
HTRADIUS  5.0m                #OUTSIDE SPHERE RADIUS
STRENGTH 3

CRITAOAP 22deg #CRITICAL AOA POSITIVE
CRITAOAM -15deg #CRITICAL AOA NEGATIVE

CRITSPED 153kt #CRITICAL SPEED
MAXSPEED 153kt #MAXIMUM SPEED

HASSPOIL TRUE #HAS SPOILER?
RETRGEAR TRUE #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE #HAS VARIABLE GEOMETRY WING?

TRSTDIR0 0.0m 1.0m 0.1m #Thrust vector 0
TRSTDIR1 0.0m 1.0m 0.1m #Thrust vector 1
PSTMPTCH 240deg #Post-Stall Maneuver VPitch ricm96 was 40deg
PSTMYAW_ 130deg #Post-Stall Maneuver VYaw ricm96 was 30deg
PSTMROLL 280deg #Post-Stall Maneuver VRoll ricm96 was 80deg

CLVARGEO 0.0 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.0 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.0 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.0 #EFFECT OF GEAR FOR Cd
CDSPOILR 50.0 #EFFECT OF SPOILER FOR Cd

WINGAREA 1m^2 #WING AREA

MXIPTAOA 0.00deg #MAX INPUT AOA ricm96 was 0.0
MXIPTSSA 0.00deg #MAX INPUT SSA ricm96 was 0.0
MXIPTROL 0.00deg #MAX INPUT ROLL ricm96 was 0.0

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 30kt #MINIMUM MANEUVABLE SPEED
MANESPD2 135kt #FULLY MANEUVABLE SPEED
CPITMANE 1.0 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.5 #PITCH STABILITY CONSTANT
CYAWMANE 2.5 #YAW MANEUVABILITY CONSTANT
CYAWSTAB 2.0 #YAW STABILITY CONSTANT
CROLLMAN 5.0 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE #CONTROL LANDINGGEAR
CTLBRAKE FALSE #CONTROL BRAKE
CTLSPOIL 0.0 #CONTROL SPOILER
CTLABRNR FALSE #CONTROL AFTERBURNER
CTLTHROT 0.0 #CONTROL THRUST
CTLIFLAP 0.0 #CONTROL FLAP
CTLINVGW 0.0 #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE #CONTROL AUTO VARIABLE GEOMETRY WING

INITIGUN 0                  #INITIAL GUN

NMTURRET 4
TURRETPO 0 -0.9m -0.19m 0.0m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 0 -75deg 15deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 0deg 0deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 10000                                 # Ammo
TURRETIV 0 0.05sec                            # Number ShootingInterval
TURRETNM 0 gun                           # DNM Node Name
TURRETAR 0 TRUE                                # Anti Air Capable
TURRETGD 0 TRUE                                  # Anti Ground Capable
TURRETCT 0 PILOT                           # Controlled by PILOT or GUNNER
TURRETRG 0 2000m

TURRETPO 1 -0.4m 0.78m 0.45m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 1 -25deg 25deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 1 -50deg 50deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 1 0                              # Ammo
TURRETIV 1 0.50sec                            # Number ShootingInterval
TURRETNM 1 0006a                           # DNM Node Name
TURRETAR 1 TRUE                                # Anti Air Capable
TURRETGD 1 TRUE                                  # Anti Ground Capable
TURRETCT 1 GUNNER                            # Controlled by PILOT or GUNNER
TURRETRG 1 800m

TURRETPO 2 0.25m 0.67m -0.3m -55deg -15deg 0deg      # Number x y z h p b
TURRETPT 2 -25deg 25deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 2 -50deg 50deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 2 0                              # Ammo
TURRETIV 2 0.50sec                            # Number ShootingInterval
TURRETNM 2 gunnerc                           # DNM Node Name
TURRETAR 2 TRUE                                # Anti Air Capable
TURRETGD 2 TRUE                                  # Anti Ground Capable
TURRETCT 2 GUNNER                            # Controlled by PILOT or GUNNER
TURRETRG 2 800m

TURRETPO 3 1.0m 0.15m 0.2m -55deg -20deg -55deg      # Number x y z h p b
TURRETPT 3 -30deg 30deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 3 -30deg 30deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 3 50000                              # Ammo
TURRETIV 3 0.15sec                            # Number ShootingInterval
TURRETNM 3 gunnerd                           # DNM Node Name
TURRETAR 3 TRUE                                # Anti Air Capable
TURRETGD 3 TRUE                                  # Anti Ground Capable
TURRETCT 3 GUNNER                            # Controlled by PILOT or GUNNER
TURRETRG 3 800m

RADARCRS 1.0                 #RADAR CROSS SECTION

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 80%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 135kt #CRUISING SPEED
REFACRUS 3000ft #CRUISING ALTITUDE
REFTCRUS 0.8 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 0kt #LANDING SPEED
REFAOALD 0deg #LANDING AOA
REFLNRWY 20m #RUNWAY REQUIRED TO STOP
REFTHRLD 0 #LANDING THROTTLE SETTING

AUTOCALC
