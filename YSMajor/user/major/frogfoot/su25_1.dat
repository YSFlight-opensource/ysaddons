THRSTREV 1t

COCKPITP  0.0m  0.7m  4.1m  #COCKPIT POSITION
LEFTGEAR -1.23m -1.72m -0.62m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.23m -1.72m -0.62m  #RIGHT LANDING GEAR POSITION
