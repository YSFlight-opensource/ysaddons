TURRETNM 5 aaaa                                # DNM Node Name
TURRETAR 5                                     # Anti Air Capable
TURRETCT 5 PILOT                              # Controlled by PILOT or GUNNER
TURRETRG 5 2500m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.74MACH              #CRUISING SPEED    RICM
REFACRUS 7000m              #CRUISING ALTITUDE
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING    RICM

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 110kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA    RICM
REFLNRWY 2000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.7                  #LANDING THROTTLE SETTING    RICM

