REM modded from the YS Birdfarm by Iceman, Vandal, ao, original model by GAC
REM modded by YS Major for the January 2010 Birdfarm competition

IDENTIFY "SU-35S_KnAAPO_ver1"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 24.6t                #THRUST WITH AFTERBURNER
THRMILIT 16.0t                #THRUST AT MILITARY POWER
THRSTREV 0.5                  #EFFICIENCY OF THRUST REVERSER
WEIGHCLN 12.0t                #WEIGHT CLEAN
WEIGFUEL  7.8t                #WEIGHT OF FUEL
WEIGLOAD 10.0t                #WEIGHT OF PAYLOAD
FUELABRN 6.5kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.85kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  1.56m   5.30m  #COCKPIT POSITION
LEFTGEAR -2.28m -1.9m -2.725m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.28m -1.9m -2.725m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.9m  2.84m  #WHEEL POSITION
ARRESTER  0m -1.73m -8.86m
MACHNGUN  1.1m  0.47m 3.00m  #MACHINE GUN POSITION
SMOKEGEN -0.0m 0.00m -6.70m  #SMOKE GENERATOR POSITION
VAPORPO0  7.13m  0.425m -6.095m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  7.13m  0.425m -6.095m  #VAPOR POSITION (Wings Spread)
HTRADIUS 14.6m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.4MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                #HAS VARIABLE GEOMETRY WING?
VGWSPED1 0kt
VGWSPED2 80kt
 
CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 1.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 3.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 58m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 300.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 50kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 110kt                #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 2.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 12
MAXNMAGM 10
MAXNBOMB 14
MAXNMRKT 80
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

HRDPOINT 7.05m 0.12m -4.15m AIM9
HRDPOINT -7.05m 0.12m -4.15m AIM9
HRDPOINT 6.00m -0.12m -4.20m AIM120 AIM9 AGM65
HRDPOINT -6.00m -0.12m -4.20m AIM120 AIM9 AGM65
HRDPOINT 4.50m -0.25m -3.40m AIM120 RKT AGM65
HRDPOINT -4.50m -0.25m -3.40m AIM120 RKT AGM65
HRDPOINT 3.25m -0.25m -2.60m AIM120 RKT AGM65
HRDPOINT -3.25m -0.25m -2.60m AIM120 RKT AGM65

HRDPOINT 1.22m -0.95m -2.0m AIM9 B500 AIM120 AGM65
HRDPOINT -1.22m -0.95m -2.0m AIM9 B500 AIM120 AGM65
HRDPOINT 0m -0.35m -0.80m AIM120 B500 AGM65
HRDPOINT 0m -0.35m -5.80m AIM120 B500 AGM65

HRDPOINT 0m -0.75m 0.3m B250
HRDPOINT 0.3m -0.4m 0.3m B250
HRDPOINT -0.3m -0.4m 0.3m B250
HRDPOINT 0m -0.75m -1.5m B250
HRDPOINT 0.3m -0.4m -1.5m B250
HRDPOINT -0.3m -0.4m -1.5m B250
HRDPOINT 0m -0.75m -5m B250
HRDPOINT 0.3m -0.4m -5m B250
HRDPOINT -0.3m -0.4m -5m B250
HRDPOINT 0m -0.75m -6.8m B250
HRDPOINT 0.3m -0.4m -6.8m B250
HRDPOINT -0.3m -0.4m -6.8m B250

INITIGUN 0                 #INITIAL GUN
LOADWEPN AIM9 2
LOADWEPN AIM120 2
LOADWEPN RKT 80
LOADWEPN B250 12
INITIAGM 2                    #INITIAL AGM
RKTVISIB FALSE

NMTURRET 3
TURRETPO 0 0.97m 0.53m 3.11m 0.0deg 0.0deg 0deg       # Number x y z h p b
TURRETPT 0 0deg 0deg 0deg                     # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 0deg 0deg 0deg                 # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 550                                # Ammo
TURRETIV 0 0.06sec                              # Number ShootingInterval
TURRETAR 0                                     # Anti Air Capable
TURRETCT 0 PILOT                              # Controlled by PILOT or GUNNER
TURRETRG 0 2000m

TURRETPO 1 0.97m 0.53m 3.11m -0.01deg 0.01deg 0deg       # Number x y z h p b
TURRETPT 1 0deg 0deg 0deg                     # Number MinPitch MaxPitch NeutralPitch
TURRETHD 1 0deg 0deg 0deg                 # Number MinHdg MaxHdg NeutralHdg
TURRETAM 1 550                                # Ammo
TURRETIV 1 0.04sec                              # Number ShootingInterval
TURRETAR 1                                     # Anti Air Capable
TURRETCT 1 PILOT                              # Controlled by PILOT or GUNNER
TURRETRG 1 2000m

TURRETPO 2 0.97m 0.53m 3.11m 0.1deg -0.01deg 0deg       # Number x y z h p b
TURRETPT 2 0deg 0deg 0deg                     # Number MinPitch MaxPitch NeutralPitch
TURRETHD 2 0deg 0deg 0deg                 # Number MinHdg MaxHdg NeutralHdg
TURRETAM 2 550                                # Ammo
TURRETIV 2 0.05sec                              # Number ShootingInterval
TURRETAR 2                                     # Anti Air Capable
TURRETCT 2 PILOT                              # Controlled by PILOT or GUNNER
TURRETRG 2 2000m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 110kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1500m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC