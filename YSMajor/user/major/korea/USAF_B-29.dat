REM Original mesh by Oranleed, modded by YSMajor
REM DAT Modification by Ricm96

IDENTIFY "B29-SUPERFORTRESS"
CATEGORY WW2BOMBER
AFTBURNR FALSE                   #HAVE AFTERBURNER?
THRAFTBN 0.0t                    #THRUST WITH AFTERBURNER
PROPELLR 8800HP                  #THRUST AT MILITARY POWER
THRSTREV 0.0                     #EFFICIENCY OF THRUST REVERSER    RICM
WEIGHCLN 37.0t                 #WEIGHT CLEAN    RICM
WEIGFUEL 15.0t                 #WEIGHT OF FUEL
WEIGLOAD 1.0t                 #WEIGHT OF PAYLOAD    RICM
FUELABRN 0.0kg                   #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.9kg                   #FUEL CONSUMPTION AT MILITARY POWER    RICM

COCKPITP -0.68m 0.83m 9.15m      #COCKPIT POSITION
EXCAMERA "gunner" 0.00m  1.71m  -17.54m 180deg 0deg 0deg INSIDE
INSTPANL  user/major/korea/b29.ist
LEFTGEAR -4.15m -2.69m -0.50m    #LEFT LANDING GEAR POSITION
RIGHGEAR  4.15m -2.69m -0.50m    #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.80m 9.91m      #WHEEL POSITION
ARRESTER  0.0m 0.0m -15.17m
MACHNGUN  0.0m 0.0m 0.0m         #MACHINE GUN POSITION
GUNINTVL  0.00
GUNDIREC  0.0m 0.0m -1.0m        #MACHINE GUN DIRECTION
SMOKEGEN  0.0m  0.0m -14.71m     #SMOKE GENERATOR POSITION
VAPORPO0 -21.17m  1.15m -2.31m   #VAPOR POSITION (Wings Swept back)
VAPORPO1 -21.17m  1.15m -2.31m   #VAPOR POSITION (Wings Spread)
HTRADIUS 17.0m                   #OUTSIDE SPHERE RADIUS


STRENGTH 20

CRITAOAP  14deg                  #CRITICAL AOA POSITIVE   RICM
CRITAOAM -10deg                  #CRITICAL AOA NEGATIVE

CRITSPED 330kt                   #CRITICAL SPEED
MAXSPEED 320kt                   #MAXIMUM SPEED

HASSPOIL TRUE                    #HAS SPOILER?
RETRGEAR TRUE                    #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE			 #HAS VARIABLE GEOMETRY WING?     RICM

CLVARGEO 0.0                     #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                     #EFFECT OF VARIABLE GEOMETRY WING FOR Cd    RICM
CLBYFLAP 0.15                     #EFFECT OF FLAP FOR Cl   RICM
CDBYFLAP 0.3                     #EFFECT OF FLAP FOR Cd   RICM
CDBYGEAR 0.3                     #EFFECT OF GEAR FOR Cd   RICM
CDSPOILR 0.0                     #EFFECT OF SPOILER FOR Cd

WINGAREA 161.27m^2               #WING AREA

MXIPTAOA 18.0deg                 #MAX INPUT AOA
MXIPTSSA 5.0deg                  #MAX INPUT SSA
MXIPTROL 150deg                   #MAX INPUT ROLL     RICM

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 55kt                    #MINIMUM MANEUVABLE SPEED
MANESPD2 100kt                   #FULLY MANEUVABLE SPEED

CPITMANE 5.0                     #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 10.0                    #PITCH STABILITY CONSTANT
CYAWMANE 5.0                     #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                     #YAW STABILITY CONSTANT
CROLLMAN 0.2                     #ROLL MANEUVABILITY CONSTANT    RICM

CTLLDGEA TRUE                    #CONTROL LANDINGGEAR
CTLBRAKE FALSE                   #CONTROL BRAKE
CTLSPOIL 0.0                     #CONTROL SPOILER
CTLABRNR FALSE                   #CONTROL AFTERBURNER
CTLTHROT 0.0                     #CONTROL THRUST
CTLIFLAP 0.0                     #CONTROL FLAP
CTLINVGW 1.0                     #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                    #CONTROL AUTO VARIABLE GEOMETRY WING

VGWSPED1 170kt			 # RICM
VGWSPED2 220kt			 # RICM

POSITION 0m 3ft 0m               #POSITION
ATTITUDE 45deg 0deg 0deg         #ATTITUDE
INITFUEL 75%                     #INITIAL FUEL
INITLOAD 0.0t                    #INITIAL PAYLOAD
INITSPED 0.0MACH                 #INITIAL SPEED

REM Slots must come before Inits.
BOMVISIB TRUE
BOMBSLOT -0.43m -0.45m  3.14m
BOMBSLOT  0.43m -0.45m  3.14m
BOMBSLOT -0.43m -0.45m -2.79m
BOMBSLOT  0.43m -0.45m -2.79m
BOMINBAY TRUE                    #Bombs in Bomb Bay?
BMBAYRCS 1.0                     #Effect on Bomb Bay Opening on Radar Cross Section


INITIGUN 0                       #INITIAL GUN
INITIAAM 0                       #INITIAL AAM
INITIAGM 0                       #INITIAL AGM
INITBOMB 40                      #INITIAL BOMB
WEAPONCH BOM

NMTURRET 5
TURRETPO 0 0m 0.77m -18.40m 180deg 0deg 0deg   # Number x y z h p b
TURRETPT 0 -80deg 80deg 0deg                   # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 -80deg 80deg 0deg                   # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 5000                                # Ammo
TURRETIV 0 0.15sec                              # Number ShootingInterval
TURRETNM 0 0040                                # DNM Node Name
TURRETAR 0                                     # Anti Air Capable
TURRETCT 0 PILOT                              # Controlled by PILOT or GUNNER
TURRETRG 0 2500m

TURRETPO 1 0m -1.59m 5.94m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 1 -90deg 0deg 0deg                    # Number MinPitch MaxPitch NeutralPitch
TURRETHD 1 -180deg 180deg 0deg                 # Number MinHdg MaxHdg NeutralHdg
TURRETAM 1 5000                                # Ammo
TURRETIV 1 0.2sec                              # Number ShootingInterval
TURRETNM 1 0037                                # DNM Node Name
TURRETAR 1                                     # Anti Air Capable
TURRETCT 1 GUNNER                              # Controlled by PILOT or GUNNER
TURRETRG 1 1500m

TURRETPO 2 0m 1.59m -6.50m 180deg 0deg 0deg    # Number x y z h p b
TURRETPT 2 0deg 90deg 0deg                     # Number MinPitch MaxPitch NeutralPitch
TURRETHD 2 -180deg 180deg 0deg                 # Number MinHdg MaxHdg NeutralHdg
TURRETAM 2 5000                                # Ammo
TURRETIV 2 0.2sec                              # Number ShootingInterval
TURRETNM 2 0038                                # DNM Node Name
TURRETAR 2                                     # Anti Air Capable
TURRETCT 2 GUNNER                              # Controlled by PILOT or GUNNER
TURRETRG 2 1500m

TURRETPO 3 0m -0.65m -12.17m 180deg -8deg 0deg # Number x y z h p b
TURRETPT 3 -90deg 0deg 0deg                    # Number MinPitch MaxPitch NeutralPitch
TURRETHD 3 -180deg 180deg 0deg                 # Number MinHdg MaxHdg NeutralHdg
TURRETAM 3 5000                                # Ammo
TURRETIV 3 0.2sec                              # Number ShootingInterval
TURRETNM 3 0039                                # DNM Node Name
TURRETAR 3                                     # Anti Air Capable
TURRETCT 3 GUNNER                              # Controlled by PILOT or GUNNER
TURRETRG 3 1500m

TURRETPO 4 0m 1.59m 6.66m 0deg 0deg 0deg       # Number x y z h p b
TURRETPT 4 0deg 90deg 0deg                     # Number MinPitch MaxPitch NeutralPitch
TURRETHD 4 -180deg 180deg 0deg                 # Number MinHdg MaxHdg NeutralHdg
TURRETAM 4 5000                                # Ammo
TURRETIV 4 0.2sec                              # Number ShootingInterval
TURRETNM 4 0036                                # DNM Node Name
TURRETAR 4                                     # Anti Air Capable
TURRETCT 4 GUNNER                              # Controlled by PILOT or GUNNER
TURRETRG 4 1500m


REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 310kt                    #CRUISING SPEED    RICM
REFACRUS 23600ft                  #CRUISING ALTITUDE    RICM
REFTCRUS 1.0                      #CRUISING THROTTLE SETTING   RICM

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 118kt                     #LANDING SPEED    RICM
REFAOALD 6deg                     #LANDING AOA        RICM
REFLNRWY 1800m                    #RUNWAY REQUIRED TO STOP
REFTHRLD 1.0                      #LANDING THROTTLE SETTING    RICM


AUTOCALC
