REM Original by YS Major
REM DAT modded by Ricm96

IDENTIFY "RNAF_PC-7"
CATEGORY TRAINER
AFTBURNR FALSE #HAVE AFTERBURNER?
PROPELLR 550hp #PROPELLER ENGINE    RICM
THRSTREV 0.1 #EFFICIENCY OF THRUST REVERSER
WEIGHCLN 1.45t #WEIGHT CLEAN    RICM
WEIGFUEL 0.37t #WEIGHT OF FUEL
WEIGLOAD 0.50t #WEIGHT OF PAYLOAD  RICM
FUELMILI 0.022kg #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP 0.00m 0.57m -0.1m #COCKPIT POSITION   RICM
INSTPANL user/major/klu/pc7.ist
EXCAMERA "copilot" 0.00m 0.63m -1.32m 0deg 0deg 0deg INSIDE   RICM
LEFTGEAR -1.15m -1.65m 0.0m #LEFT LANDING GEAR POSITION
RIGHGEAR 1.15m -1.65m 0.0m #RIGHT LANDING GEAR POSITION
WHELGEAR 0.00m -1.53m 2.20m #WHEEL POSITION
ARRESTER 0.00m -1.65m -1.50m #ARRESTING HOOK POSITION
MACHNGUN 0.00m 0.00m 0.00m #MACHINE GUN POSITION
SMOKEGEN 0.00m -0.68m -2.47m #SMOKE GENERATOR POSITION
VAPORPO0 5.97m -0.21m -0.49m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1 5.97m -0.21m -0.49m #VAPOR POSITION (Wings Spread)
HTRADIUS 6.5m #OUTSIDE SPHERE RADIUS

STRENGTH 5

CRITAOAP 16.8deg #CRITICAL AOA POSITIVE
CRITAOAM -15deg #CRITICAL AOA NEGATIVE

CRITSPED 500km/h #CRITICAL SPEED   RICM
MAXSPEED 544km/h #MAXIMUM SPEED    RICM

VGWSPED1 0kt 
VGWSPED2 1kt

HASSPOIL TRUE #HAS SPOILER?
RETRGEAR TRUE #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE #HAS VARIABLE GEOMETRY WING? 

CLVARGEO 0.0 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.4 #EFFECT OF FLAP FOR Cl    RICM
CDBYFLAP 0.8 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.7 #EFFECT OF GEAR FOR Cd 
CDSPOILR 0.0 #EFFECT OF SPOILER FOR Cd

WINGAREA 19.4m^2 #WING AREA

MXIPTAOA 21.0deg #MAX INPUT AOA
MXIPTSSA 15.0deg #MAX INPUT SSA
MXIPTROL 85.0deg #MAX INPUT ROLL    RICM

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 35kt #MINIMUM MANEUVABLE SPEED
MANESPD2 90kt #FULLY MANEUVABLE SPEED
CPITMANE 6.0 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.5 #PITCH STABILITY CONSTANT
CYAWMANE 4.0 #YAW MANEUVABILITY CONSTANT
CYAWSTAB 2.5 #YAW STABILITY CONSTANT
CROLLMAN 2.0 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE #CONTROL LANDINGGEAR
CTLBRAKE FALSE #CONTROL BRAKE
CTLSPOIL 0.0 #CONTROL SPOILER
CTLABRNR FALSE #CONTROL AFTERBURNER
CTLTHROT 0.0 #CONTROL THRUST
CTLIFLAP 0.0 #CONTROL FLAP
CTLINVGW 0.0 #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m #POSITION
ATTITUDE 45deg 0deg 0deg #ATTITUDE
INITFUEL 100% #INITIAL FUEL
INITLOAD 0.0t #INITIAL PAYLOAD
INITSPED 0.0MACH #INITIAL SPEED

INITIGUN 0 #INITIAL GUN
SMOKEOIL 30.0kg
WEAPONCH SMK

RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 412km/h #CRUISING SPEED
REFACRUS 10000ft #CRUISING ALTITUDE
REFTCRUS 1.0 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 83kt #LANDING SPEED    RICM
REFAOALD 8.0deg #LANDING AOA
REFLNRWY 650m #RUNWAY REQUIRED TO STOP 
REFTHRLD 0.85 #LANDING THROTTLE SETTING   RICM 

AUTOCALC