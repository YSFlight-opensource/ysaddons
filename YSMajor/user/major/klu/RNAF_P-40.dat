REM Original from Oranleed
REM repaint by YS Major
REM DAT corrected by RICM96

IDENTIFY "RNAF_P-40"
CATEGORY FIGHTER
AFTBURNR FALSE #HAVE AFTERBURNER?
PROPELLR 1200HP #PROPELLER ENGINE
WEIGHCLN 2.8t #WEIGHT CLEAN
WEIGFUEL 0.3t #WEIGHT OF FUEL
WEIGLOAD 0.4t #WEIGHT OF PAYLOAD
FUELMILI 0.04kg #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP 0.00m 0.65m -1.25m #COCKPIT POSITION
INSTPANL user/major/klu/p40.ist
LEFTGEAR -1.00m -2.28m 0.37m #LEFT LANDING GEAR POSITION
RIGHGEAR 1.00m -2.28m 0.37m #RIGHT LANDING GEAR POSITION
WHELGEAR 0.00m -0.79m -4.80m #WHEEL POSITION
SMOKEGEN 0.0m 0.00m -5.00m #SMOKE GENERATOR POSITION
VAPORPO0 5.69m 0.11m -0.19m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1 5.69m 0.11m -0.19m #VAPOR POSITION (Wings Spread)
HTRADIUS 7.0m #OUTSIDE SPHERE RADIUS
GUNSIGHT FALSE

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP 20deg #CRITICAL AOA POSITIVE
CRITAOAM -15deg #CRITICAL AOA NEGATIVE

CRITSPED 480km/h #CRITICAL SPEED
MAXSPEED 552km/h #MAXIMUM SPEED

HASSPOIL FALSE #HAS SPOILER?
RETRGEAR TRUE #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE #HAS VARIABLE GEOMETRY WING?

VGWSPED1 150kt
VGWSPED2 250kt

CLVARGEO 0.1 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.1 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.7 #EFFECT OF FLAP FOR Cd 
CDBYGEAR 0.7 #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0 #EFFECT OF SPOILER FOR Cd

WINGAREA 21.92m^2 #WING AREA

MXIPTAOA 22.0deg #MAX INPUT AOA
MXIPTSSA 5.0deg #MAX INPUT SSA
MXIPTROL 65.0deg #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt #MINIMUM MANEUVABLE SPEED
MANESPD2 60kt #FULLY MANEUVABLE SPEED
CPITMANE 8.0 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0 #PITCH STABILITY CONSTANT
CYAWMANE 5.0 #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0 #YAW STABILITY CONSTANT
CROLLMAN 3.0 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE #CONTROL LANDINGGEAR
CTLBRAKE FALSE #CONTROL BRAKE
CTLSPOIL 0.0 #CONTROL SPOILER
CTLABRNR FALSE #CONTROL AFTERBURNER
CTLTHROT 0.0 #CONTROL THRUST
CTLIFLAP 0.0 #CONTROL FLAP
CTLINVGW 1.0 #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m #POSITION
ATTITUDE 45deg 0deg 0deg #ATTITUDE
INITFUEL 75% #INITIAL FUEL
INITLOAD 0.0t #INITIAL PAYLOAD
INITSPED 0.0MACH #INITIAL SPEED
WEAPONCH GUN

GUNPOWER 1
INITIGUN 0 #INITIAL GUN
INITIAAM 0 #INITIAL AAM
INITIAGM 0 #INITIAL AGM
INITBOMB 0 #INITIAL BOM

MACHNGUN 1.70m -0.45m 0.63m #MACHINE GUN POSITION
MACHNGN2 -1.70m -0.45m 0.63m #MACHINE GUN POSITION

NMTURRET 2
TURRETPO 0 -1.7m -0.45m 0.63m -0.5deg 0deg 0deg # Number x y z h p b
TURRETPT 0 0deg 0deg 0deg # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 0deg 0deg 0deg # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 850 # Ammo
TURRETIV 0 0.15sec # Number ShootingInterval
TURRETNM 0 0 # DNM Node Name
TURRETAR 0 # Anti Air Capable
TURRETGD 0 # Anti Ground Capable
TURRETCT 0 PILOT # Controlled by the pilot
TURRETRG 0 2000m

TURRETPO 1 1.7m -0.45m 0.63m 0.5deg 0deg 0deg # Number x y z h p b
TURRETPT 1 0deg 0deg 0deg # Number MinPitch MaxPitch NeutralPitch
TURRETHD 1 0deg 0deg 0deg # Number MinHdg MaxHdg NeutralHdg
TURRETAM 1 850 # Ammo
TURRETIV 1 0.16sec # Number ShootingInterval
TURRETNM 1 0 # DNM Node Name
TURRETAR 1 # Anti Air Capable
TURRETGD 1 # Anti Ground Capable
TURRETCT 1 PILOT # Controlled by the pilot
TURRETRG 1 2000m

RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 500km/h #CRUISING SPEED(*1.2
REFACRUS 13000ft #CRUISING ALTITUDE
REFTCRUS 0.9 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 95kt #LANDING SPEED
REFAOALD 8deg #LANDING AOA
REFLNRWY 500m #RUNWAY REQUIRED TO STOP
REFTHRLD 0.95 #LANDING THROTTLE SETTING

AUTOCALC