REM Original from Kzs Talon and BNI
REM repaint and mod by YS Major

IDENTIFY "RNAF_F-16B"
CATEGORY FIGHTER
AFTBURNR TRUE                  #HAVE AFTERBURNER?
THRAFTBN 13.42t                #THRUST WITH AFTERBURNER
THRMILIT  7.33t                #THRUST AT MILITARY POWER
WEIGHCLN  8.66t                #WEIGHT CLEAN
WEIGFUEL  4.20t                #WEIGHT OF FUEL
WEIGLOAD  5.00t                #WEIGHT OF PAYLOAD
FUELABRN  3.20kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.25kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.82m  4.25m  #COCKPIT POSITION     RICM
EXCAMERA "pilot2" 0m  0.92m  2.9m 0deg 0deg 0deg INSIDE    RICM
LEFTGEAR -1.28m -2.01m -0.65m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.28m -2.01m -0.65m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.99m  3.54m  #WHEEL POSITION
MACHNGUN -0.73m  0.19m  3.43m  #MACHINE GUN POSITION
SMOKEGEN -0.30m -0.15m -6.50m  #SMOKE GENERATOR POSITION
VAPORPO0 -4.86m -0.02m -2.53m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1 -4.86m -0.02m -2.53m  #VAPOR POSITION (Wings Spread)
HTRADIUS 10.00m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.92MACH              #CRITICAL SPEED   RICM
MAXSPEED 1.2MACH              #MAXIMUM SPEED    RICM

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.4                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.6                  #EFFECT OF SPOILER FOR Cd    RICM

WINGAREA 27.9m^2                #WING AREA

MXIPTAOA 37.0deg              #MAX INPUT AOA
MXIPTSSA 10.0deg               #MAX INPUT SSA
MXIPTROL 240.0deg             #MAX INPUT ROLL    RICM

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 80kt                 #MINIMUM MANEUVABLE SPEED   RICM
MANESPD2 140kt                 #FULLY MANEUVABLE SPEED    RICM
CPITMANE 2.3                  #PITCH MANEUVERBILITY CONSTANT     RICM
CPITSTAB 5.0                  #PITCH STABILITY CONSTANT   RICM
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 9.0                  #ROLL MANEUVABILITY CONSTANT    RICM

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

REM Slots must come before Inits.
HRDPOINT -4.94m -0.04m -1.10m AIM9
HRDPOINT  4.94m -0.04m -1.10m AIM9

HRDPOINT -4.00m -0.45m -1.10m AIM9 AIM120
HRDPOINT  4.00m -0.45m -1.10m AIM9 AIM120

HRDPOINT -3.42m -0.52m -0.79m B250
HRDPOINT  3.42m -0.52m -0.79m B250
HRDPOINT -2.79m -0.52m -0.79m B250
HRDPOINT  2.79m -0.52m -0.79m B250

HRDPOINT -3.1m -0.52m -0.79m AGM65
HRDPOINT  3.1m -0.52m -0.79m AGM65

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 0
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 512                 #INITIAL GUN
INITIAAM 4                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED   RICM
REFACRUS 25000ft              #CRUISING ALTITUDE  RICM
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 125kt                 #LANDING SPEED
REFAOALD 10deg                 #LANDING AOA
REFLNRWY 800m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.7                 #LANDING THROTTLE SETTING    RICM

AUTOCALC
