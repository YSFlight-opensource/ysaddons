IDENTIFY "Fokker_D21_Royal_Dutch_Airforce"
CATEGORY WW2FIGHTER
AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 830HP               #PROPELLER ENGINE
WEIGHCLN  1.45t                #WEIGHT CLEAN
WEIGFUEL  0.5t                #WEIGHT OF FUEL
WEIGLOAD  0.2t                #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.08kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP 0.00m  0.80m  -1.24m  #COCKPIT POSITION
INSTPANL user/Major/FokkerD21_KLU/FokkerDXXI.ist
LEFTGEAR -1.35m -1.96m  0.67m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.35m -1.96m  0.67m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -0.72m -4.52m  #WHEEL POSITION
ARRESTER  0.0m -0.38m -4.31m

GUNINTVL 0.12
GUNPOWER 0
MACHNGUN 0.15m 0.40m  1.75m  #MACHINE GUN POSITION
MACHNGN2 -0.15m 0.40m  1.75m  #MACHINE GUN POSITION
MACHNGN3  2.00m -0.37m  0.75m  #MACHINE GUN POSITION
MACHNGN4 -2.00m -0.37m  0.75m  #MACHINE GUN POSITION

SMOKEGEN  0.51m -0.52m 1.31m  #SMOKE GENERATOR POSITION
VAPORPO0 5.45m -0.20m  -0.17m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1 5.45m -0.20m  -0.17m  #VAPOR POSITION (Wings Swept Back or no-VGW)
HTRADIUS  7m                #OUTSIDE SPHERE RADIUS

STRENGTH 8

CRITAOAP  19deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 390km/h              #CRITICAL SPEED
MAXSPEED 460km/h              #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR FALSE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.5                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 16.2m^2                #WING AREA

MXIPTAOA 21.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 72.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 70kt                 #FULLY MANEUVABLE SPEED
CPITMANE 8.9                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 2.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

REM Slots must come before Inits.

INITIGUN 400                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
GUNSIGHT FALSE

NMTURRET 5

REM Scoped Optical Sight
TURRETPO 0 0.00m 0.74m -1.80m 0.0deg 0.0deg 0.0deg
TURRETPT 0 0deg 0deg 0deg
TURRETHD 0 0deg 0deg 0deg
TURRETAM 0 0
TURRETIV 0 1sec
TURRETAR 0
TURRETCT 0 PILOT
TURRETRG 0 0m

REM 7.92mm Machinegun x4
TURRETPO 1 0.00m 0.00m  3.00m 0.0deg 0.1deg 0.0deg
TURRETPT 1 0deg 0deg 0deg
TURRETHD 1 0deg 0deg 0deg
TURRETAM 1 400
TURRETIV 1 0.8sec
TURRETAR 1
TURRETCT 1 PILOT
TURRETRG 1 1200m

TURRETPO 2 -2.125m -0.045m  1.33m 0.2deg 0.0deg 0.0deg
TURRETPT 2 0deg 0deg 0deg
TURRETHD 2 0deg 0deg 0deg
TURRETAM 2 400
TURRETIV 2 0.8sec
TURRETAR 2
TURRETCT 2 PILOT
TURRETRG 2 1200m

TURRETPO 3 0.00m 0.00m  3.00m 0.0deg 0.1deg 0.0deg
TURRETPT 3 0deg 0deg 0deg
TURRETHD 3 0deg 0deg 0deg
TURRETAM 3 400
TURRETIV 3 0.8sec
TURRETAR 3
TURRETCT 3 PILOT
TURRETRG 3 1200m

TURRETPO 4 -2.125m -0.045m  1.33m 0.2deg 0.0deg 0.0deg
TURRETPT 4 0deg 0deg 0deg
TURRETHD 4 0deg 0deg 0deg
TURRETAM 4 400
TURRETIV 4 0.8sec
TURRETAR 4
TURRETCT 4 PILOT
TURRETRG 4 1200m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 450km/h              #CRUISING SPEED
REFACRUS 5000ft              #CRUISING ALTITUDE
REFTCRUS 1.0                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 73kt                 #LANDING SPEED
REFAOALD 9deg                #LANDING AOA
REFLNRWY 600m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.45                 #LANDING THROTTLE SETTING

AUTOCALC
