
REM Modified by Galen Thurber
REM galen@flightsimhq.org
REM www.flightsimhq.org
REM
REM DONE: 
REM rough landing speed and AOA
REM minor modification by Major Major for Airstrike Challenge 2 Missions

REM
REM REQUIRES USER/CEP/CEP_2/ shapes
REM
REM done: ammo count and power, SPPU-22 gun pod
REM
REM release 5 mods: G+7 limited

IDENTIFY "MIG27DFLOGGER"
CATEGORY ATTACKER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  11.5t               #THRUST WITH AFTERBURNER
THRMILIT  8.0t                #THRUST AT MILITARY POWER
WEIGHCLN  8.0t                #WEIGHT CLEAN
WEIGFUEL  6.2t                #WEIGHT OF FUEL
WEIGLOAD  8.0t                #WEIGHT OF PAYLOAD
FUELABRN 5.7kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.39kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  0.60m  5.62m   #COCKPIT POSITION
LEFTGEAR -1.72m -1.88m -1.58m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.72m -1.88m -1.58m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.15m  5.73m   #WHEEL POSITION
MACHNGUN -1.98m -0.20m  0.83m  #MACHINE GUN POSITION
SMOKEGEN -0.0m -0.15m -8.20m   #SMOKE GENERATOR POSITION
VAPORPO0  4.93m  0.48m -6.36m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  8.51m  0.48m -2.51m  #VAPOR POSITION (Wings Spread)
HTRADIUS  8.5m                 #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 12


CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.3MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 1.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 34.2m^2              #WING AREA

CRITAOAP  28deg               #CRITICAL AOA POSITIVE
CRITAOAM -8.5deg               #CRITICAL AOA NEGATIVE

MXIPTAOA 27.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 270.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 130kt                #FULLY MANEUVABLE SPEED
CPITMANE 0.6                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 0.5                  #PITCH STABILITY CONSTANT
CYAWMANE 0.5                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 0.5                  #YAW STABILITY CONSTANT
CROLLMAN 0.6                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 85%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 13.8
INITIGUN 500                  #INITIAL GUN  pod
INITIAAM 4                    #INITIAL AAM
INITRCKT 22                   #INITIAL RCKT

AAMSLOT_ 0.93m -1.16m 2.00m
AAMSLOT_ 0.61m -1.16m 2.00m
AAMSLOT_ -0.93m -1.16m 2.00m
AAMSLOT_ -0.61m -1.16m 2.00m
RKTSLOT_ 1.98m -0.11m 0.54m
RKTSLOT_ 1.98m -0.47m 0.54m

AAMVISIB TRUE
RKTVISIB FALSE
RADARCRS 1

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.7                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 130kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1500m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.63                  #LANDING THROTTLE SETTING

AUTOCALC
