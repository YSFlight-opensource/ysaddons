REM Original from Oranleed
REM repaint by YS Major

IDENTIFY "P-39_Pacific"
CATEGORY WW2FIGHTER
AFTBURNR FALSE                  #HAVE AFTERBURNER?
PROPELLR 1200HP                 #PROPELLER ENGINE
WEIGHCLN  2.9t                  #WEIGHT CLEAN
WEIGFUEL  0.3t                  #WEIGHT OF FUEL
WEIGLOAD  0.4t                  #WEIGHT OF PAYLOAD
FUELMILI 0.08kg                 #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.63m  0.12m   #COCKPIT POSITION
INSTPANL user/major/p40/p39.ist
LEFTGEAR -1.73m -2.04m -0.60m   #LEFT LANDING GEAR POSITION
RIGHGEAR  1.73m -2.04m -0.60m   #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -2.04m  2.15m   #WHEEL POSITION
MACHNGUN  0.12m  0.33m  2.65m   #MACHINE GUN POSITION
MACHNGN2 -0.12m  0.33m  2.65m   #MACHINE GUN POSITION
MACHNGN3  0.00m  0.00m  3.42m
MACHNGN4 -3.20m -0.44m  0.62m
MACHNGN5  3.20m -0.44m  0.62m
GUNINTVL 0.08
SMOKEGEN  0.0m   0.00m -5.00m   #SMOKE GENERATOR POSITION
VAPORPO0  5.18m -0.26m -0.23m   #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.18m -0.26m -0.23m   #VAPOR POSITION (Wings Spread)
HTRADIUS  7.0m                  #OUTSIDE SPHERE RADIUS
GUNSIGHT FALSE

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  14deg                #CRITICAL AOA POSITIVE
CRITAOAM -10deg                #CRITICAL AOA NEGATIVE

CRITSPED 600km/h               #CRITICAL SPEED
MAXSPEED 531km/h               #MAXIMUM SPEED

HASSPOIL FALSE                 #HAS SPOILER?
RETRGEAR TRUE                  #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                 #HAS VARIABLE GEOMETRY WING?

VGWSPED1 150kt
VGWSPED2 250kt

CLVARGEO 0.0                   #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                   #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                   #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                   #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                   #EFFECT OF GEAR FOR Cd
CDSPOILR 1.8                   #EFFECT OF SPOILER FOR Cd

WINGAREA 19.79m^2              #WING AREA

MXIPTAOA 18.0deg               #MAX INPUT AOA
MXIPTSSA 3.0deg                #MAX INPUT SSA
MXIPTROL 65.0deg               #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                  #MINIMUM MANEUVABLE SPEED
MANESPD2 55kt                  #FULLY MANEUVABLE SPEED
CPITMANE 7.0                   #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                   #PITCH STABILITY CONSTANT
CYAWMANE 5.0                   #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                   #YAW STABILITY CONSTANT
CROLLMAN 2.7                   #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                  #CONTROL LANDINGGEAR
CTLBRAKE FALSE                 #CONTROL BRAKE
CTLSPOIL 0.0                   #CONTROL SPOILER
CTLABRNR FALSE                 #CONTROL AFTERBURNER
CTLTHROT 0.0                   #CONTROL THRUST
CTLIFLAP 0.0                   #CONTROL FLAP
CTLINVGW 1.0                   #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                  #CONTROL AUTO VARIABLE GEOMETRY WING



POSITION 0m 3ft 0m             #POSITION
ATTITUDE 45deg 0deg 0deg       #ATTITUDE
INITFUEL 75%                   #INITIAL FUEL
INITLOAD 0.0t                  #INITIAL PAYLOAD
INITSPED 0.0MACH               #INITIAL SPEED
WEAPONCH GUN

GUNPOWER 1
INITIGUN 1750                  #INITIAL GUN
INITIAAM 0                     #INITIAL AAM
INITIAGM 0                     #INITIAL AGM
INITBOMB 0                     #INITIAL BOM

RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 480km/h               #CRUISING SPEED(*1.2
REFACRUS 13000ft               #CRUISING ALTITUDE
REFTCRUS 0.7                   #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 85kt                  #LANDING SPEED
REFAOALD 8deg                  #LANDING AOA
REFLNRWY 500m                  #RUNWAY REQUIRED TO STOP
REFTHRLD 0.6                  #LANDING THROTTLE SETTING

AUTOCALC
