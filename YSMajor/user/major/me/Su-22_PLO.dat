REM GACGLIMT 7
REM GACRADRG 5
REM GACRADCS 1
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 0
REM GACRCVCH 0
REM GACRADSD 0.25
REM GACRADEL 0.25

IDENTIFY "PLO_Su-22_FITTERBOMB"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  31.25t                #THRUST WITH AFTERBURNER
THRMILIT  7.8t                #THRUST AT MILITARY POWER
WEIGHCLN 10.8t                #WEIGHT CLEAN
WEIGFUEL  6.0t                #WEIGHT OF FUEL
WEIGLOAD  4.3t                #WEIGHT OF PAYLOAD
FUELABRN 15kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.65kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.036

VGWSPED1 500kt
VGWSPED2 501kt

COCKPITP  0.0m  0.99m  4.75m  #COCKPIT POSITION
LEFTGEAR -1.70m -1.75m -0.80m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.70m -1.75m -0.80m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.97m  4.40m  #WHEEL POSITION
ARRESTER  0.0m -1.35m -5.2m
MACHNGUN -1.30m 0.00m  2.50m  #MACHINE GUN POSITION
MACHNGN2 1.30m 0.00m  2.50m  #MACHINE GUN POSITION
SMOKEGEN  0.00m 0.87m -8.00m  #SMOKE GENERATOR POSITION
VAPORPO0  4.96m  -0.17m -5.30m  #VAPOR POSITION (Wings Swept back)
VAPORPO1  6.82m  0.23m -3.35m  #VAPOR POSITION (Wings Spread)
HTRADIUS 13m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 20

CRITAOAP  21.25deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 1.5MACH              #CRITICAL SPEED
MAXSPEED 2.74MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.6                  #EFFECT OF SPOILER FOR Cd

WINGAREA 38.5m^2                #WING AREA

MXIPTAOA 22.50deg              #MAX INPUT AOA
MXIPTSSA 7.5deg               #MAX INPUT SSA
MXIPTROL 170.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 260kt                #FULLY MANEUVABLE SPEED
CPITMANE 3.91                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.10                  #PITCH STABILITY CONSTANT
CYAWMANE 6.5                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 6.5                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED


MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT -0.55m -0.95m 1.65m AGM65 B500
HRDPOINT 0.55m -0.95m 1.65m AGM65 B500
HRDPOINT -1.25m -0.45m 2.70m RKT B500
HRDPOINT 1.25m -0.45m 2.70m RKT B500
HRDPOINT -1.74m -0.60m 1.00m B500
HRDPOINT 1.74m -0.60m 1.00m B500
HRDPOINT -2.55m -0.70m -1.50m AGM65 B500 B250
HRDPOINT 2.55m -0.70m -1.50m AGM65 B500 B250

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

GUNPOWER 2
INITIGUN 160                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 6

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 2.0MACH              #CRUISING SPEED
REFACRUS 30000ft              #CRUISING ALTITUDE
REFTCRUS 2.5                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 140kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.345                  #LANDING THROTTLE SETTING

AUTOCALC
