REM GACGLIMT 8.5
REM GACRADRG 72
REM GACRADCS 0.7
REM GACECMRG 0
REM GACAEWTR 50
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 3 6
REM GACRCVCH 3 6
REM GACRADSD 1
REM GACRADEL 0.5

IDENTIFY "CN_J-9_Xiao-Long"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  8.3t               #THRUST WITH AFTERBURNER
THRMILIT  5.0t                #THRUST AT MILITARY POWER
WEIGHCLN  6.41t                #WEIGHT CLEAN
WEIGFUEL  2.2t                #WEIGHT OF FUEL
WEIGLOAD  3.6t                #WEIGHT OF PAYLOAD
FUELABRN 5.5kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.4kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.018

COCKPITP  0.0m  0.54m   3.05m  #COCKPIT POSITION
LEFTGEAR -1.28m -1.81m -2.30m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.28m -1.81m -2.30m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.84m  2.54m  #WHEEL POSITION
ARRESTER  0.0m -1.00m -3.85m
MACHNGUN  -0.5m  -0.85m  2.0m  #MACHINE GUN POSITION
SMOKEGEN 0.00m 0.80m -6.85m  #SMOKE GENERATOR POSITION
VAPORPO0  4.45m  -0.25m -3.89m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.45m  -0.25m -3.89m  #VAPOR POSITION (Wings Spread)
HTRADIUS  9.3m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 7

CRITAOAP  24.82deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 1.6MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.6                  #EFFECT OF SPOILER FOR Cd

WINGAREA 23m^2                #WING AREA

MXIPTAOA 28.15deg              #MAX INPUT AOA
MXIPTSSA 7.5deg               #MAX INPUT SSA
MXIPTROL 260.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 270kt                 #FULLY MANEUVABLE SPEED
CPITMANE 7.71                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.9                  #PITCH STABILITY CONSTANT
CYAWMANE 8.5                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 7.5                  #YAW STABILITY CONSTANT
CROLLMAN 4.2                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT 0.00m -1.05m -0.85m B250

HRDPOINT 4.55m -0.25m -2.75m AIM9 AIM120
HRDPOINT -4.55m -0.25m -2.75m AIM9 AIM120

HRDPOINT 2.00m -0.70m -1.70m AIM9 AGM65 B500 AIM120 B250
HRDPOINT -2.00m -0.70m -1.70m AIM9 AGM65 B500 AIM120 B250
HRDPOINT 2.8m -0.70m -2.15m AIM9 AGM65 B500 RKT AIM120
HRDPOINT -2.8m -0.70m -2.15m AIM9 AGM65 B500 RKT AIM120

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 120                 #INITIAL GUN
LOADWEPN AIM9 2
LOADWEPN AIM120 2
INITIAGM 2                    #INITIAL AGM

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.6MACH              #CRUISING SPEED
REFACRUS 30000ft              #CRUISING ALTITUDE
REFTCRUS 2.2                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 130kt                 #LANDING SPEED
REFAOALD 10deg                 #LANDING AOA
REFLNRWY 1200m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.32                 #LANDING THROTTLE SETTING

AUTOCALC
