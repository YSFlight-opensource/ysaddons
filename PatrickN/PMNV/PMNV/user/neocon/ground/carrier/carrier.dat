TURRETPT 0 -5deg 80deg 0deg                 # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 -130deg 130deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 5000                              # Ammo
TURRETIV 0 0.1sec                            # Number ShootingInterval
TURRETNP 0 CANNON                            # DNM Node Name
TURRETNH 0 TURRET                            # DNM Node Name (Heading Rotation)
