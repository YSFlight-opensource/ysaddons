REM GACGLIMT 8
REM GACRADRG 70
REM GACRADCS 0.7
REM GACECMRG 0
REM GACAEWTR 50
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 1
REM GACRCVCH 1
REM GACRADSD 1
REM GACRADEL 0.5

IDENTIFY "TW_F-CK-1A_Ching-Kuo"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  8.60t               #THRUST WITH AFTERBURNER
THRMILIT  5.46t                #THRUST AT MILITARY POWER
WEIGHCLN  5.5t                #WEIGHT CLEAN
WEIGFUEL  2.5t                #WEIGHT OF FUEL
WEIGLOAD  3.9t                #WEIGHT OF PAYLOAD
FUELABRN 7.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.4kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.01

COCKPITP  0.0m  0.73m   3.15m  #COCKPIT POSITION
LEFTGEAR -1.40m -1.60m -1.38m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.40m -1.60m -1.38m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.60m  4.00m  #WHEEL POSITION
ARRESTER  0.0m -1.20m -4.00m
MACHNGUN -0.75m  0.36m  2.20m  #MACHINE GUN POSITION
SMOKEGEN 0.00m -0.52m -5.8m  #SMOKE GENERATOR POSITION
VAPORPO0  4.60m  -0.025m -2.58m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.60m  -0.025m -2.58m  #VAPOR POSITION (Wings Spread)
HTRADIUS  9.6m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 6

CRITAOAP  24.66deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 1.7MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 26m^2                #WING AREA

MXIPTAOA 27.92deg              #MAX INPUT AOA
MXIPTSSA 7.5deg               #MAX INPUT SSA
MXIPTROL 270.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 260kt                 #FULLY MANEUVABLE SPEED
CPITMANE 7.43                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.4                  #PITCH STABILITY CONSTANT
CYAWMANE 7.5                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 7.0                  #YAW STABILITY CONSTANT
CROLLMAN 4.5                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED


MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT 4.68m 0.03m -1.10m AIM9
HRDPOINT -4.68m 0.03m -1.10m AIM9

HRDPOINT 3.20m -0.47m -1.00m AIM9 B500
HRDPOINT -3.20m -0.47m -1.00m AIM9 B500

HRDPOINT 2.20m -0.47m -0.95m B500 AGM65 RKT B250
HRDPOINT -2.20m -0.47m -0.95m B500 AGM65 RKT B250

HRDPOINT 0.00m -0.75m 0.90m B500 AIM120
HRDPOINT 0.00m -0.75m -1.70m B500 AIM120 AGM65

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 511                 #INITIAL GUN
LOADWEPN AIM120 2
LOADWEPN AIM9 4
INITIAGM 2                    #INITIAL AGM

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.7MACH              #CRUISING SPEED
REFACRUS 50000ft              #CRUISING ALTITUDE
REFTCRUS 2.3                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 130kt                 #LANDING SPEED
REFAOALD 10deg                 #LANDING AOA
REFLNRWY 1200m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.33                 #LANDING THROTTLE SETTING

AUTOCALC
