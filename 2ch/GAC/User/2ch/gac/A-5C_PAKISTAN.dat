REM GACGLIMT 6.9
REM GACRADRG 5
REM GACRADCS 0.75
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 0
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 0
REM GACRCVCH 0
REM GACRADSD 0.25
REM GACRADEL 0.25

IDENTIFY "PK_A-5C_Fantan"
CATEGORY ATTACKER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 8.1t                #THRUST WITH AFTERBURNER
THRMILIT  6t                #THRUST AT MILITARY POWER
WEIGHCLN 6.4t                #WEIGHT CLEAN
WEIGFUEL  1.73t                #WEIGHT OF FUEL
WEIGLOAD  1.5t                #WEIGHT OF PAYLOAD
FUELABRN 6kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.4kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNSIGHT FALSE
GUNINTVL 0.066

COCKPITP  0.0m  0.43m  4m  #COCKPIT POSITION
LEFTGEAR -1.86m -1.54m 0.0m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.86m -1.54m 0.0m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.61m  4.38m  #WHEEL POSITION
MACHNGUN -1.025m -0.425m  2.515m  #MACHINE GUN POSITION
MACHNGN2 1.025m  -0.425m  2.515m  #MACHINE GUN POSITION
SMOKEGEN -0.00m  0.575m -4.63m  #SMOKE GENERATOR POSITION
VAPORPO0 -4.335m  -0.49m -2.62m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1 -4.335m  -0.49m -2.62m  #VAPOR POSITION (Wings Spread)
HTRADIUS  11.2m                #OUTSIDE SPHERE RADIUS
ARRESTER  0.0m -0.885m -2.375m

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  22.05deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 983km/h              #CRITICAL SPEED
MAXSPEED 1.5MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 27.9m^2                #WING AREA

MXIPTAOA 23.61deg              #MAX INPUT AOA
MXIPTSSA 6.0deg               #MAX INPUT SSA
MXIPTROL 170.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 90kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 280kt                #FULLY MANEUVABLE SPEED
CPITMANE 4.94                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.2                  #PITCH STABILITY CONSTANT
CYAWMANE 6.50                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 5.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.9                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 200                 #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 4
INITRCKT 38                   #INITIAL ROCKET


HRDPOINT 3.25m -0.8m -0.30m AIM9
HRDPOINT -3.25m -0.8m -0.30m AIM9

HRDPOINT 2.45m -0.80m -0.45m B250
HRDPOINT -2.45m -0.80m -0.45m B250

HRDPOINT 1.80m -0.75m 1.8m RKT
HRDPOINT -1.80m -0.75m 1.8m RKT

HRDPOINT 0.55m -1.05m 1.8m B500
HRDPOINT -0.55m -1.05m 1.8m B500
HRDPOINT 0.55m -0.90m -0.35m B500
HRDPOINT -0.55m -0.90m -0.35m B500

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.1MACH              #CRUISING SPEED
REFACRUS 30000ft              #CRUISING ALTITUDE
REFTCRUS 1.3                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 130kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 800m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.336                  #LANDING THROTTLE SETTING

AUTOCALC
