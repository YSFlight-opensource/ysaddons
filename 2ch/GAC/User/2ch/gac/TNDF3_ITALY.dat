REM GACGLIMT 7.5
REM GACRADRG 185
REM GACRADCS 1
REM GACECMRG 0
REM GACAEWTR 50
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 2
REM GACRCVCH 2
REM GACRADSD 1
REM GACRADEL 0.5

IDENTIFY "IT_Tornado_F.3"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  14.8t                #THRUST WITH AFTERBURNER
THRMILIT  8.2t                #THRUST AT MILITARY POWER
WEIGHCLN 14.5t                #WEIGHT CLEAN
WEIGFUEL  6.0t                #WEIGHT OF FUEL
WEIGLOAD  5.0t                #WEIGHT OF PAYLOAD
FUELABRN 15kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.65kg               #FUEL CONSUMPTION AT MILITARY POWER
THRSTREV 0.3                 #EFFICIENCY OF THRUST REVERSER
GUNINTVL 0.035

COCKPITP  0.0m  0.36m  3.35m  #COCKPIT POSITION
LEFTGEAR -1.715m -2.355m -1.565m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.715m -2.355m -1.565m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.395m  4.60m  #WHEEL POSITION
ARRESTER  0.0m -1.80m -4.8m
MACHNGUN 0.375m -1.18m  5.44m  #MACHINE GUN POSITION
SMOKEGEN  0.00m -0.55m -6.375m  #SMOKE GENERATOR POSITION
VAPORPO0  4.329m  0.24m -4.968m  #VAPOR POSITION (Wings Swept back)
VAPORPO1  6.95m  0.24m -3.52m  #VAPOR POSITION (Wings Spread)
HTRADIUS 11.1m                #OUTSIDE SPHERE RADIUS

EXCAMERA "CO-PILOT" 0.0m 0.60m 2.20m 0deg 0deg 0deg INSIDE

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 16

CRITAOAP  23.25deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.2MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.2                  #EFFECT OF SPOILER FOR Cd

WINGAREA 27m^2                #WING AREA

MXIPTAOA 25.80deg              #MAX INPUT AOA
MXIPTSSA 7.5deg               #MAX INPUT SSA
MXIPTROL 200.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 300kt                #FULLY MANEUVABLE SPEED
CPITMANE 6.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.2                  #PITCH STABILITY CONSTANT
CYAWMANE 10.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 8.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.9                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT 2.50m -0.5m -0.55m B250
HRDPOINT -2.50m -0.5m -0.55m B250

HRDPOINT -0.46m -1.25m 1.2m AIM120
HRDPOINT 0.46m -1.3m 2.2m AIM120
HRDPOINT -0.46m -1m -1.3m AIM120
HRDPOINT 0.46m -1.1m -0.65m AIM120

HRDPOINT 2m -0.3m -0.55m AIM9
HRDPOINT -2m -0.3m -0.55m AIM9
HRDPOINT 3.0m -0.3m -0.55m AIM9
HRDPOINT -3.0m -0.3m -0.55m AIM9

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

GUNPOWER 3
INITIGUN 180                 #INITIAL GUN
LOADWEPN AIM120 4
LOADWEPN AIM9 4
INITIAGM 0                    #INITIAL AGM

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 2.0MACH              #CRUISING SPEED
REFACRUS 50000ft              #CRUISING ALTITUDE
REFTCRUS 3.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 130kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 600m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.35                  #LANDING THROTTLE SETTING

AUTOCALC
