REM GACGLIMT 9
REM GACRADRG 185
REM GACRADCS 0.5
REM GACECMRG 0
REM GACAEWTR 50
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 1
REM GACSNDCH 1 2
REM GACRCVCH 1 2
REM GACRADSD 1
REM GACRADEL 0.5

IDENTIFY "IT_EF-2000B_Typhoon"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  18.4t               #THRUST WITH AFTERBURNER
THRMILIT  12.2t                #THRUST AT MILITARY POWER
WEIGHCLN  10.9t                #WEIGHT CLEAN
WEIGFUEL  4.0t                #WEIGHT OF FUEL
WEIGLOAD  6.5t                #WEIGHT OF PAYLOAD
FUELABRN 10kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.45kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.035

COCKPITP  0.0m  1.15m   3.75m  #COCKPIT POSITION
LEFTGEAR -2.05m -1.70m -1.00m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.05m -1.70m -1.00m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.90m  2.75m  #WHEEL POSITION
ARRESTER  0.0m -1.05m -4.40m
MACHNGUN -0.65m  -0.20m  3.45m  #MACHINE GUN POSITION
SMOKEGEN 0.00m 0.65m -6.45m  #SMOKE GENERATOR POSITION
VAPORPO0  5.25m  -0.20m -4.50m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.25m  -0.20m -4.50m  #VAPOR POSITION (Wings Spread)
HTRADIUS  10.6m                #OUTSIDE SPHERE RADIUS

EXCAMERA "CO-PILOT" 0.0m  1.32m 2.90m 0deg 0deg 0deg INSIDE

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  26deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 1.2MACH              #CRITICAL SPEED
MAXSPEED 2.2MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.8                  #EFFECT OF SPOILER FOR Cd

WINGAREA 50m^2                #WING AREA

MXIPTAOA 28.77deg              #MAX INPUT AOA
MXIPTSSA 7.5deg               #MAX INPUT SSA
MXIPTROL 250.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 280kt                 #FULLY MANEUVABLE SPEED
CPITMANE 9.63                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.4                  #PITCH STABILITY CONSTANT
CYAWMANE 7.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 6.5                  #YAW STABILITY CONSTANT
CROLLMAN 4.8                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT 0.9m -0.3m 1.55m AIM120
HRDPOINT -0.9m -0.3m 1.55m AIM120
HRDPOINT 0.9m -0.3m -2.05m AIM120
HRDPOINT -0.9m -0.3m -2.05m AIM120
HRDPOINT 2.7m -0.55m -0.75m AGM65 B500 B250 AIM120
HRDPOINT -2.7m -0.55m -0.75m AGM65 B500 B250 AIM120
HRDPOINT 4.35m -0.55m -2.30m AIM9 AIM120
HRDPOINT -4.35m -0.55m -2.30m AIM9 AIM120


LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

GUNPOWER 3
INITIGUN 240                 #INITIAL GUN
LOADWEPN AIM120 4
LOADWEPN AIM9 2
INITBOMB 2                    #INITIAL BOMB

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 2.0MACH              #CRUISING SPEED
REFACRUS 50000ft              #CRUISING ALTITUDE
REFTCRUS 2.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 130kt                 #LANDING SPEED
REFAOALD 10deg                 #LANDING AOA
REFLNRWY 1300m                #RUNWAY REQUIRED TO STOP
REM REFTHRLD 0.267                 #LANDING THROTTLE SETTING
REFTHRLD 0.26                 #LANDING THROTTLE SETTING

AUTOCALC
