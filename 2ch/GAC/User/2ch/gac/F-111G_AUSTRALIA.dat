REM GACGLIMT 7.3
REM GACRADRG 75
REM GACRADCS 1.3
REM GACECMRG 0
REM GACAEWTR 50
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 1
REM GACRCVCH 1
REM GACRADSD 1
REM GACRADEL 0.5

IDENTIFY "AU_F-111G_Ardvark"
CATEGORY ATTACKER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 22.7t                #THRUST WITH AFTERBURNER
THRMILIT 17.8t                #THRUST AT MILITARY POWER
WEIGHCLN 23.5t                #WEIGHT CLEAN
WEIGFUEL 19.1t                #WEIGHT OF FUEL
WEIGLOAD  11.0t                #WEIGHT OF PAYLOAD
FUELABRN 43.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 2.0kg               #FUEL CONSUMPTION AT MILITARY POWER

VGWSPED1 500kt
VGWSPED2 501kt

COCKPITP -0.31m  0.68m 6.40m  #COCKPIT POSITION
LEFTGEAR -1.90m -2.00m -1.30m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.90m -2.00m -1.30m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.20m  6.10m  #WHEEL POSITION
ARRESTER  0.0m -1.70m -6.30m
MACHNGUN 0.00m  -1.35m  6.00m  #MACHINE GUN POSITION
GUNINTVL 0.01
SMOKEGEN  0.0m -0.90m -10.4m  #SMOKE GENERATOR POSITION
VAPORPO0 -5.57m  0.55m -8.12m  #VAPOR POSITION (Wings Swept back)
VAPORPO1 -11.19m  0.55m -2.13m  #VAPOR POSITION (Wings Spread)
HTRADIUS  15m                #OUTSIDE SPHERE RADIUS

EXCAMERA "CO-PILOT" 0.31m  0.68m 6.40m 0deg 0deg 0deg INSIDE

STRENGTH 16

CRITAOAP  20.60deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.5MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 57m^2                #WING AREA

MXIPTAOA 22.20deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 120.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                #MINIMUM MANEUVABLE SPEED
MANESPD2 300kt                #FULLY MANEUVABLE SPEED
CPITMANE 3.42                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 1.6                  #PITCH STABILITY CONSTANT
CYAWMANE 5.5                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 5.5                  #YAW STABILITY CONSTANT
CROLLMAN 2.1                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT -3.20m -0.75m -1.50m B500
HRDPOINT 3.20m -0.75m -1.50m B500
HRDPOINT -2.65m -0.20m -1.50m B500
HRDPOINT 2.65m -0.20m -1.50m B500
HRDPOINT -3.75m -0.20m -1.50m B500
HRDPOINT 3.75m -0.20m -1.50m B500

HRDPOINT -4.90m -0.75m -2.85m B500
HRDPOINT 4.90m -0.75m -2.85m B500
HRDPOINT -4.35m -0.20m -2.85m B500
HRDPOINT 4.35m -0.20m -2.85m B500
HRDPOINT -5.45m -0.20m -2.85m B500
HRDPOINT 5.45m -0.20m -2.85m B500

HRDPOINT -6.65m -0.75m -4.15m B500
HRDPOINT 6.65m -0.75m -4.15m B500
HRDPOINT -6.10m -0.20m -4.15m B500
HRDPOINT 6.10m -0.20m -4.15m B500
HRDPOINT -7.20m -0.20m -4.15m B500
HRDPOINT 7.20m -0.20m -4.15m B500

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 2084                 #INITIAL GUN
LOADWEPN B500 18
LOADWEPN AIM120 0
LOADWEPN AIM9 0

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 2.0MACH              #CRUISING SPEED
REFACRUS 35000ft              #CRUISING ALTITUDE
REFTCRUS 2.6                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 140kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1700m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.4                  #LANDING THROTTLE SETTING

AUTOCALC
