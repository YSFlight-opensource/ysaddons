REM GACGLIMT 9
REM GACRADRG 150
REM GACRADCS 0.7
REM GACECMRG 0
REM GACAEWTR 50
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 2
REM GACRCVCH 2
REM GACRADSD 1
REM GACRADEL 0.5

IDENTIFY "FR_Mirage_2000-5_Mk.2"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  9.7t                #THRUST WITH AFTERBURNER
THRMILIT  6.6t                #THRUST AT MILITARY POWER
WEIGHCLN  7.5t                #WEIGHT CLEAN
WEIGFUEL  3.0t                #WEIGHT OF FUEL
WEIGLOAD  6.0t                #WEIGHT OF PAYLOAD
FUELABRN 6.5kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.4kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.046

COCKPITP  0.0m  0.83m  2.40m  #COCKPIT POSITION
LEFTGEAR -1.7m -1.80m -3.05m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.7m -1.80m -3.05m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.80m  2.75m  #WHEEL POSITION
ARRESTER  0.0m -1.60m -6.29m
MACHNGUN  0.7m -0.20m  1.30m  #MACHINE GUN POSITION
MACHNGN2 -0.7m -0.20m  1.30m  #MACHINE GUN POSITION
SMOKEGEN -0.0m  0.65m -7.30m  #SMOKE GENERATOR POSITION
VAPORPO0  4.2m  -0.20m -5.25m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.2m  -0.20m -5.25m  #VAPOR POSITION (Wings Spread)
HTRADIUS  9.6m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  24.60deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.3MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.4                  #EFFECT OF SPOILER FOR Cd

WINGAREA 41m^2                #WING AREA

MXIPTAOA 27.70deg              #MAX INPUT AOA
MXIPTSSA 7.0deg               #MAX INPUT SSA
MXIPTROL 270.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 280kt                #FULLY MANEUVABLE SPEED
CPITMANE 8.4                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.1                  #PITCH STABILITY CONSTANT
CYAWMANE 8.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 8.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.6                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT 0.00m -0.63m -2.20m B250

HRDPOINT -3.1m -0.65m -3.25m AIM9
HRDPOINT 3.1m -0.65m -3.25m AIM9

HRDPOINT -2.20m -0.65m -3.15m AIM9 AIM120 AGM65 B500 B250
HRDPOINT 2.20m -0.65m -3.15m AIM9 AIM120 AGM65 B500 B250

HRDPOINT -1.0m -0.60m -0.65m B500
HRDPOINT 1.0m -0.60m -0.65m B500
HRDPOINT -1.1m -0.60m -3.7m B500
HRDPOINT 1.1m -0.60m -3.7m B500
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

GUNPOWER 2
INITIGUN 250                 #INITIAL GUN
LOADWEPN AIM120 4
LOADWEPN AIM9 4
INITIAGM 0                    #INITIAL AGM

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 2.0MACH              #CRUISING SPEED
REFACRUS 50000ft              #CRUISING ALTITUDE
REFTCRUS 2.6                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 150kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1400m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
