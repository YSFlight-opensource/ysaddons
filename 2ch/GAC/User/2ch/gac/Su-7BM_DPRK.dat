REM GACGLIMT 6.5
REM GACRADRG 5
REM GACRADCS 1.1
REM GACRADCS 0.7
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 0
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 0
REM GACRCVCH 0
REM GACRADSD 0.25
REM GACRADEL 0.25

IDENTIFY "KP_Su-7BM_Fitter"
CATEGORY ATTACKER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  9.58t                #THRUST WITH AFTERBURNER
THRMILIT  6.77t                #THRUST AT MILITARY POWER
WEIGHCLN  8.9t                #WEIGHT CLEAN
WEIGFUEL  2.3t                #WEIGHT OF FUEL
WEIGLOAD  1t                #WEIGHT OF PAYLOAD
FUELABRN 8kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.39kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.066

COCKPITP  0.0m  0.87m  4.55m  #COCKPIT POSITION
LEFTGEAR -2.65m -1.92m -0.95m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.65m -1.92m -0.95m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.895m  4.03m  #WHEEL POSITION
ARRESTER  0.0m -0.79m -4.54m
MACHNGUN  0.35m  -0.5m  3.35m  #MACHINE GUN POSITION
MACHNGN2  -0.35m  -0.5m  3.35m  #MACHINE GUN POSITION
SMOKEGEN -0.0m  0.735m -7.605m  #SMOKE GENERATOR POSITION
VAPORPO0  4.605m  -0.47m -5.63m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.605m  -0.47m -5.63m  #VAPOR POSITION (Wings Spread)
HTRADIUS  11.6m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 15

CRITAOAP  21.95deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 1.6MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.6                  #EFFECT OF SPOILER FOR Cd

WINGAREA 34m^2                #WING AREA

MXIPTAOA 23.45deg              #MAX INPUT AOA
MXIPTSSA 6.0deg               #MAX INPUT SSA
MXIPTROL 180.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 300kt                #FULLY MANEUVABLE SPEED
CPITMANE 5.02                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.8                  #PITCH STABILITY CONSTANT
CYAWMANE 6.5                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 5.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

HRDPOINT 0.35m -1.15m 0.90m B500 B250
HRDPOINT -0.35m -1.15m 0.90m B500 B250
HRDPOINT 2.5m -0.75m -1.5m AIM9 B500
HRDPOINT -2.5m -0.75m -1.5m AIM9 B500
HRDPOINT 3.2m -0.8m -2.7m RKT B500
HRDPOINT -3.2m -0.8m -2.7m RKT B500
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

GUNPOWER 2
INITIGUN 140                 #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITBOMB 4                    #INITIAL AGM
INITRCKT 0

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.5MACH              #CRUISING SPEED
REFACRUS 30000ft              #CRUISING ALTITUDE
REFTCRUS 2.2                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 130kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 800m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.37                  #LANDING THROTTLE SETTING

AUTOCALC