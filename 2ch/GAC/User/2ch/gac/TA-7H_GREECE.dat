REM GACGLIMT 7.3
REM GACRADRG 35
REM GACRADCS 0.8
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 0
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 0
REM GACRCVCH 0
REM GACRADSD 1
REM GACRADEL 1

IDENTIFY "GR_TA-7H_Corsair_II"
CATEGORY ATTACKER
AFTBURNR FALSE                 #HAVE AFTERBURNER?
THRMILIT  6.8t                #THRUST AT MILITARY POWER
WEIGHCLN  8.95t                #WEIGHT CLEAN
WEIGFUEL  5t                #WEIGHT OF FUEL
WEIGLOAD  6.8t                #WEIGHT OF PAYLOAD
FUELABRN 12.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.75kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.01

COCKPITP  0.0m  0.77m  4.30m  #COCKPIT POSITION
LEFTGEAR -1.70m -1.65m -1.15m   #LEFT LANDING GEAR POSITION
RIGHGEAR  1.70m -1.65m -1.15m   #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.20m  3.60m  #WHEEL POSITION
MACHNGUN  -0.60m -1.00m  4.75m  #MACHINE GUN POSITION
ARRESTER  0.0m -1.40m -5.10m
SMOKEGEN 0.0m  1.00m -7.30m  #SMOKE GENERATOR POSITION
VAPORPO0  5.90m 0.30m -3.65m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.90m 0.30m -3.65m  #VAPOR POSITION (Wings Spread)
HTRADIUS  10.5m                #OUTSIDE SPHERE RADIUS

EXCAMERA "CO-PILOT" 0.0m  1.05m  3.00m 0deg 0deg 0deg INSIDE

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 12

CRITAOAP  22.20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 450kt              #CRITICAL SPEED
MAXSPEED 606kt              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.7                  #EFFECT OF SPOILER FOR Cd

WINGAREA 32.5m^2                #WING AREA

MXIPTAOA 24.10deg              #MAX INPUT AOA
MXIPTSSA 7.0deg               #MAX INPUT SSA
MXIPTROL 200.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 90kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 250kt                #FULLY MANEUVABLE SPEED
CPITMANE 5.10                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 4.0                  #PITCH STABILITY CONSTANT
CYAWMANE 7.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 7.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT -1.10m 0.00m 1.30m AIM9
HRDPOINT 1.10m 0.00m 1.30m AIM9

HRDPOINT -1.60m 0.00m 0.20m B500 B250 AGM65
HRDPOINT 1.60m 0.00m 0.20m B500 B250 AGM65
HRDPOINT -2.50m -0.15m -0.05m B500 AGM65 RKT
HRDPOINT 2.50m -0.15m -0.05m B500 AGM65 RKT
HRDPOINT -3.40m -0.15m -1.00m B500 AGM65 RKT AIM9
HRDPOINT 3.40m -0.15m -1.00m B500 AGM65 RKT AIM9

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 500                 #INITIAL GUN
INITIAAM 4                    #INITIAL AAM
INITIAGM 4                    #INITIAL AGM
INITBOMB 0

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 605kt              #CRUISING SPEED
REFACRUS 15000ft              #CRUISING ALTITUDE
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 120kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1200m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.50                  #LANDING THROTTLE SETTING

AUTOCALC
