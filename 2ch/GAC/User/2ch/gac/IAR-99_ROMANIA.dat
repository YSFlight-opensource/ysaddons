REM GACGLIMT 7
REM GACRADRG 0
REM GACRADCS 0.6
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 0
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 0
REM GACRCVCH 0
REM GACRADSD 0
REM GACRADEL 0

IDENTIFY "RO_IAR-99_Soim"
CATEGORY TRAINER
AFTBURNR FALSE                 #HAVE AFTERBURNER?
THRMILIT  1.81t                #THRUST AT MILITARY POWER
WEIGHCLN  3.20t                #WEIGHT CLEAN
WEIGFUEL  1.1t                #WEIGHT OF FUEL
WEIGLOAD  1.5t                #WEIGHT OF PAYLOAD
FUELMILI 0.13kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.018

COCKPITP  0.0m  0.385m   2.55m  #COCKPIT POSITION
LEFTGEAR -1.35m -1.70m -0.50m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.35m -1.70m -0.50m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.65m  3.75m  #WHEEL POSITION
ARRESTER  0.0m -1.00m -4.10m
MACHNGUN 0.00m  -0.83m  2.30m  #MACHINE GUN POSITION
SMOKEGEN 0.00m -0.70m -3.00m  #SMOKE GENERATOR POSITION
VAPORPO0  4.80m  -0.31m -1.40m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.80m  -0.31m -1.40m  #VAPOR POSITION (Wings Spread)
HTRADIUS  8.0m                #OUTSIDE SPHERE RADIUS

EXCAMERA "CO-PILOT" 0.0m 0.75m 1.40m 0deg 0deg 0deg INSIDE

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 5

CRITAOAP  24.40deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 650km/h             #CRITICAL SPEED
MAXSPEED 865km/h              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.4                  #EFFECT OF SPOILER FOR Cd

WINGAREA 18.7m^2                #WING AREA

MXIPTAOA 25.80deg              #MAX INPUT AOA
MXIPTSSA 7.00deg               #MAX INPUT SSA
MXIPTROL 170.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 70kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 200kt                 #FULLY MANEUVABLE SPEED
CPITMANE 5.8                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.5                  #PITCH STABILITY CONSTANT
CYAWMANE 7.4                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 7.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT -2.50m -0.75m -0.05m AIM9 B500 B250 RKT
HRDPOINT 2.50m -0.75m -0.05m AIM9 B500 B250 RKT
HRDPOINT -3.50m -0.70m -0.20m AIM9 B500
HRDPOINT 3.50m -0.70m -0.20m AIM9 B500

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 200                 #INITIAL GUN
LOADWEPN AIM9 2
LOADWEPN B500 2

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 860km/h              #CRUISING SPEED
REFACRUS 15000ft              #CRUISING ALTITUDE
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 90kt                 #LANDING SPEED
REFAOALD 10deg                 #LANDING AOA
REFLNRWY 600m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.36                 #LANDING THROTTLE SETTING

AUTOCALC
