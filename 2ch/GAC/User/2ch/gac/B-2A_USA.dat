REM GACGLIMT 3
REM GACRADRG 120
REM GACRADCS 0
REM GACECMRG 0
REM GACAEWTR 50
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 2
REM GACSNDCH 2
REM GACRCVCH 2
REM GACRADSD 1
REM GACRADEL 0.5

IDENTIFY "US_B-2A_Spirit"
CATEGORY HEAVYBOMBER
AFTBURNR FALSE               #HAVE AFTERBURNER?
THRAFTBN 0.0t                #THRUST WITH AFTERBURNER
THRMILIT 31.4t               #THRUST AT MILITARY POWER
WEIGHCLN 49t                 #WEIGHT CLEAN
WEIGFUEL 80t                #WEIGHT OF FUEL
WEIGLOAD 18t                 #WEIGHT OF PAYLOAD
FUELABRN 0.0kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 1.75kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP -0.6m  1.1m   5.1m  #COCKPIT POSITION
LEFTGEAR -6.7m -2.90m -3.10m  #LEFT LANDING GEAR POSITION
RIGHGEAR  6.7m -2.90m -3.10m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.90m  6.99m  #WHEEL POSITION
ARRESTER  0.0m -1.7m -7.30m
MACHNGUN 0.0m -0.00m  0.00m  #MACHINE GUN POSITION
SMOKEGEN  0.0m  0.00m -10.3m  #SMOKE GENERATOR POSITION
VAPORPO0 26.78m  0.00m  -10.2m  #VAPOR POSITION (Wings Swept back)
VAPORPO1 26.78m  0.00m  -10.2m  #VAPOR POSITION (Wings Spread)
HTRADIUS 34.8m                #OUTSIDE SPHERE RADIUS

EXCAMERA "CO-PILOT" 0.6m  1.1m   5.1m 0deg 0deg 0deg INSIDE

TRIGGER1 NON
TRIGGER2 NON
TRIGGER3 AGM
TRIGGER4 NON

STRENGTH 33

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 700km/h              #CRITICAL SPEED
MAXSPEED 980km/h              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.3                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.45                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 464m^2              #WING AREA

MXIPTAOA 15.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 38deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 220kt                #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 6.0                  #PITCH STABILITY CONSTANT
CYAWMANE 3.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 0.6                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                   #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

INITIGUN 0                    #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 16                   #INITIAL AGM
INITBOMB 80
WEAPONCH AGM

AGMSLOT_ -3.00m -1.70m 2.50m
AGMSLOT_ 3.00m -1.70m 2.50m
AGMSLOT_ -3.00m -1.70m -1.00m
AGMSLOT_ 3.00m -1.70m -1.00m
AGMSLOT_ -4.00m -1.70m 2.50m
AGMSLOT_ 4.00m -1.70m 2.50m
AGMSLOT_ -4.00m -1.70m -1.00m
AGMSLOT_ 4.00m -1.70m -1.00m

AGMVISIB FALSE

BOMBSLOT -1.00m -1.70m 2.50m
BOMBSLOT 1.00m -1.70m 2.50m
BOMBSLOT -1.00m -1.70m -1.00m
BOMBSLOT 1.00m -1.70m -1.00m
BOMBSLOT -2.00m -1.70m 2.50m
BOMBSLOT 2.00m -1.70m 2.50m
BOMBSLOT -2.00m -1.70m -1.00m
BOMBSLOT 2.00m -1.70m -1.00m

BOMVISIB FALSE

RADARCRS 0.1                  #RADAR CROSS SECTION
BMBAYRCS 1.0
BOMINBAY TRUE

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 970km/h              #CRUISING SPEED
REFACRUS 15000ft              #CRUISING ALTITUDE
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 150kt                 #LANDING SPEED
REFAOALD 10deg                 #LANDING AOA
REFLNRWY 2600m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.7                  #LANDING THROTTLE SETTING

AUTOCALC
