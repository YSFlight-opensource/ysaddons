REM GACGLIMT 6
REM GACRADRG 30
REM GACRADCS 1
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 0
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 0
REM GACRCVCH 0
REM GACRADSD 1
REM GACRADEL 0.5

IDENTIFY "SE_AJS-37_Viggen"
CATEGORY ATTACKER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  11.7t                #THRUST WITH AFTERBURNER
THRMILIT  6.69t                #THRUST AT MILITARY POWER
WEIGHCLN 11.8t                #WEIGHT CLEAN
WEIGFUEL  7.4t                #WEIGHT OF FUEL
WEIGLOAD  4t                #WEIGHT OF PAYLOAD
FUELABRN 18kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.65kg               #FUEL CONSUMPTION AT MILITARY POWER
THRSTREV 0.4                 #EFFICIENCY OF THRUST REVERSER
GUNINTVL 0.044

COCKPITP  0.0m  0.67m  3.80m  #COCKPIT POSITION
LEFTGEAR -2.85m -2.24m -2.34m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.85m -2.24m -2.34m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.27m  3.75m  #WHEEL POSITION
ARRESTER  0.0m -1.80m -4.8m
MACHNGUN -0.27m -1.05m  1.65m  #MACHINE GUN POSITION
SMOKEGEN  0.00m 1.00m -7.60m  #SMOKE GENERATOR POSITION
VAPORPO0  5.4m  -0.50m -6.38m  #VAPOR POSITION (Wings Swept back)
VAPORPO1  5.4m  -0.50m -6.38m  #VAPOR POSITION (Wings Spread)
HTRADIUS 10.8m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 14

CRITAOAP  21deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.4MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                 #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.4                  #EFFECT OF SPOILER FOR Cd

WINGAREA 46m^2                #WING AREA

MXIPTAOA 22.80deg              #MAX INPUT AOA
MXIPTSSA 6.0deg               #MAX INPUT SSA
MXIPTROL 160.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 320kt                #FULLY MANEUVABLE SPEED
CPITMANE 4.11                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.8                  #PITCH STABILITY CONSTANT
CYAWMANE 6.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 6.0                  #YAW STABILITY CONSTANT
CROLLMAN 2.7                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAGM 2

HRDPOINT 0.00m -1.20m 0.00m B250

HRDPOINT -0.90m -1.25m 0.60m AIM9 B500
HRDPOINT 0.90m -1.25m 0.60m AIM9 B500

HRDPOINT -2.55m -1.05m -3.60m AGM65 B500 RKT
HRDPOINT 2.55m -1.05m -3.60m AGM65 B500 RKT
HRDPOINT -3.35m -1.05m -3.60m AGM65 B500 RKT
HRDPOINT 3.35m -1.05m -3.60m AGM65 B500 RKT

HRDPOINT -4.20m -1.05m -4.55m AIM9 B500 RKT
HRDPOINT 4.20m -1.05m -4.55m AIM9 B500 RKT

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

GUNPOWER 4
INITIGUN 0                 #INITIAL GUN
LOADWEPN AIM9 2
LOADWEPN B500 6
INITIAGM 0                    #INITIAL AGM

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 2.0MACH              #CRUISING SPEED
REFACRUS 50000ft              #CRUISING ALTITUDE
REFTCRUS 3.1                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 120kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 600m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.47                  #LANDING THROTTLE SETTING

AUTOCALC
