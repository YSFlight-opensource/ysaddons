REM GACGLIMT 7
REM GACRADRG 80
REM GACRADCS 0.8
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 2
REM GACSNDCH 8
REM GACRCVCH 8
REM GACRADSD 1
REM GACRADEL 0.5

IDENTIFY "RU_Yak-141_Freestyle"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 19.5t                #THRUST WITH AFTERBURNER
THRMILIT  10t                #THRUST AT MILITARY POWER
WEIGHCLN  11.65t                #WEIGHT CLEAN
WEIGFUEL  4.4t                #WEIGHT OF FUEL
WEIGLOAD  4.0t                #WEIGHT OF PAYLOAD
FUELABRN 11kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.5kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.04

COCKPITP  0.0m  0.595m  5.2m  #COCKPIT POSITION
LEFTGEAR -1.81m -2.48m -1.00m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.81m -2.48m -1.00m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.4m  5.53m  #WHEEL POSITION
MACHNGUN  -0.99m  -0.99m  3.33m  #MACHINE GUN POSITION
SMOKEGEN -1.1m -0.45m -7.895m  #SMOKE GENERATOR POSITION
VAPORPO0  5.18m  -0.47m -3.93m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.18m  -0.47m -3.93m  #VAPOR POSITION (Wings Spread)
HTRADIUS  12.2m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  23deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 1.8MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m 0.0m 1.0m       #Thrust vector 0
TRSTDIR1 0.0m 1.0m 0.0m       #Thrust vector 1
PSTMPTCH 10deg                #Post-Stall Maneuver VPitch
PSTMYAW_ 20deg                #Post-Stall Maneuver VYaw
PSTMROLL 40deg                #Post-Stall Maneuver VRoll

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 31.7m^2                #WING AREA

MXIPTAOA 25.35deg              #MAX INPUT AOA
MXIPTSSA 6.5deg               #MAX INPUT SSA
MXIPTROL 250.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 280kt                #FULLY MANEUVABLE SPEED
CPITMANE 5.55                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.4                  #PITCH STABILITY CONSTANT
CYAWMANE 7.5                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 8.5                  #YAW STABILITY CONSTANT
CROLLMAN 3.3                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT 5.35m -0.4m -3m AIM9
HRDPOINT -5.35m -0.4m -3m AIM9

HRDPOINT 2.9m -0.65m -1.75m AIM9 AIM120 AGM65 B500 RKT
HRDPOINT -2.9m -0.65m -1.75m AIM9 AIM120 AGM65 B500 RKT
HRDPOINT 2.0m -0.65m -1.35m AIM9 AIM120 AGM65 B500 RKT
HRDPOINT -2.0m -0.65m -1.35m AIM9 AIM120 AGM65 B500 RKT

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

GUNPOWER 3
INITIGUN 118                 #INITIAL GUN
LOADWEPN AIM120 4
LOADWEPN AIM9 2
INITIAGM 0                    #INITIAL AGM
INITBOMB 0

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.8MACH              #CRUISING SPEED
REFACRUS 40000ft              #CRUISING ALTITUDE
REFTCRUS 2.6                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 150kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1500m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.38                  #LANDING THROTTLE SETTING

AUTOCALC
