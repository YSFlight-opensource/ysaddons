REM GACGLIMT 5.2
REM GACRADRG 5
REM GACRADCS 0.9
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 0
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 0
REM GACRCVCH 0
REM GACRADSD 0.25
REM GACRADEL 0.25

IDENTIFY "BY_Su-25UB_Frogfoot"
CATEGORY ATTACKER
AFTBURNR FALSE                #HAVE AFTERBURNER?
THRAFTBN  0.0t                #THRUST WITH AFTERBURNER
THRMILIT  9.0t                #THRUST AT MILITARY POWER
WEIGHCLN  9.3t                #WEIGHT CLEAN
WEIGFUEL  5.0t                #WEIGHT OF FUEL
WEIGLOAD  4.4t                #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.45kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.085

COCKPITP  0.0m  0.40m  3.70m  #COCKPIT POSITION
LEFTGEAR -1.435m -1.99m -0.15m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.435m -1.99m -0.15m  #RIGHT LANDING GEAR POSITION
WHELGEAR  -0.22m -2.01m  3.34m  #WHEEL POSITION
ARRESTER  0.0m -1.00m -4.00m
MACHNGUN  -0.36m -0.845m  4.935m  #MACHINE GUN POSITION
SMOKEGEN  0.0m  1.10m -6.00m  #SMOKE GENERATOR POSITION
VAPORPO0 -7.33m  0.08m -2.27m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1 -7.33m  0.08m -2.27m  #VAPOR POSITION (Wings Spread)
HTRADIUS 10.3m                #OUTSIDE SPHERE RADIUS

EXCAMERA "CO-PILOT" 0.0m 0.90m 2.20m 0deg 0deg 0deg INSIDE

STRENGTH 22

CRITAOAP  20.77deg               #CRITICAL AOA POSITIVE
CRITAOAM -17deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 1000km/h             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 30m^2                #WING AREA

MXIPTAOA 22.50deg              #MAX INPUT AOA
MXIPTSSA 7.0deg               #MAX INPUT SSA
MXIPTROL 180.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 80kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 240kt                #FULLY MANEUVABLE SPEED
CPITMANE 3.4                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 1.6                  #PITCH STABILITY CONSTANT
CYAWMANE 6.5                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 8.5                  #YAW STABILITY CONSTANT
CROLLMAN 2.4                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 3
INITIGUN 250                 #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 8                    #INITIAL BOMB
INITRCKT 0                   #INITIAL ROCKET (10kg/each)
MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 999

HRDPOINT 5.2m -0.30m -0.25m AIM9
HRDPOINT -5.2m -0.30m -0.25m AIM9

HRDPOINT 2.10m -0.35m 0.25m B500 AGM65 RKT B250
HRDPOINT -2.10m -0.35m 0.25m B500 AGM65 RKT B250
HRDPOINT 2.90m -0.40m 0.00m B500 AGM65 RKT B250
HRDPOINT -2.90m -0.40m 0.00m B500 AGM65 RKT B250
HRDPOINT 3.70m -0.45m -0.25m B500 AGM65 RKT
HRDPOINT -3.70m -0.45m -0.25m B500 AGM65 RKT
HRDPOINT 4.50m -0.50m -0.50m B500 RKT
HRDPOINT -4.50m -0.50m -0.50m B500 RKT

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 800km/h              #CRUISING SPEED
REFACRUS 3000ft              #CRUISING ALTITUDE
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 130kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 800m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.46                  #LANDING THROTTLE SETTING

AUTOCALC
