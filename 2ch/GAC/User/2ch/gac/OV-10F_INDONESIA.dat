REM GACGLIMT 6
REM GACRADRG 0
REM GACRADCS 0.6
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 0
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 0
REM GACRCVCH 0
REM GACRADSD 0
REM GACRADEL 0

IDENTIFY "ID_OV-10F_Bronco"
CATEGORY NORMAL
AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 1430HP               #PROPELLER ENGINE
WEIGHCLN 3.16t                #WEIGHT CLEAN
WEIGFUEL  1.0t                #WEIGHT OF FUEL
WEIGLOAD  1.5t                #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.03kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.26m 2.25m  #COCKPIT POSITION
LEFTGEAR -2.45m -1.85m -1.10m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.45m -1.85m -1.10m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.96m  2.73m  #WHEEL POSITION
ARRESTER  0.0m -1.30m -2.00m
MACHNGUN  -0.75m -0.75m 0.55m  #MACHINE GUN POSITION
MACHNGN2  0.75m -0.75m 0.55m  #MACHINE GUN POSITION
MACHNGN3  -0.95m -0.80m 0.55m  #MACHINE GUN POSITION
MACHNGN4  0.95m -0.80m 0.55m  #MACHINE GUN POSITION
SMOKEGEN  0.00m -0.90m -1.30m  #SMOKE GENERATOR POSITION
VAPORPO0  6.41m  0.61m -1.66m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  6.41m  0.61m -1.66m  #VAPOR POSITION (Wings Spread)
HTRADIUS  10m                #OUTSIDE SPHERE RADIUS

EXCAMERA "CO-PILOT" 0.00m 0.45m 1.00m 0deg 0deg 0deg INSIDE

RADARCRS 0.6                  #RADAR CROSS SECTION

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 7

CRITAOAP  17.0deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 190kt              #CRITICAL SPEED
MAXSPEED 240kt              #MAXIMUM SPEED

HASSPOIL FALSE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.3                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.45                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 27m^2                #WING AREA

MXIPTAOA 18.5deg              #MAX INPUT AOA
MXIPTSSA 6.0deg               #MAX INPUT SSA
MXIPTROL 90.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 65kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 100kt                #FULLY MANEUVABLE SPEED
CPITMANE 5.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.5                  #PITCH STABILITY CONSTANT
CYAWMANE 3.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 2.0                  #YAW STABILITY CONSTANT
CROLLMAN 2.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

HRDPOINT 0.00m -1.30m 0.00m B250
HRDPOINT -0.60m -1.15m -0.15m RKT B500
HRDPOINT 0.60m -1.15m -0.15m RKT B500

HRDPOINT 4.20m 0.0m 0.30m AIM9
HRDPOINT -4.20m 0.0m 0.30m AIM9

GUNINTVL 0.12
GUNPOWER 0

INITIGUN 2000                 #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITRCKT 38

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 239kt              #CRUISING SPEED
REFACRUS 4000ft              #CRUISING ALTITUDE
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 75kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 400m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.55                  #LANDING THROTTLE SETTING

AUTOCALC
