REM GACGLIMT 8
REM GACRADRG 40
REM GACRADCS 0.8
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 0
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 0
REM GACRCVCH 0
REM GACRADSD 1
REM GACRADEL 0.5

IDENTIFY "MAT_MiG-23_Flogger"
CATEGORY TRAINER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  50.0t                #THRUST WITH AFTERBURNER
THRMILIT  20.00t                #THRUST AT MILITARY POWER
WEIGHCLN  10.2t                #WEIGHT CLEAN
WEIGFUEL  4.5t                #WEIGHT OF FUEL
WEIGLOAD  3.0t                #WEIGHT OF PAYLOAD
FUELABRN 10kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.45kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.018

VGWSPED1 400kt
VGWSPED2 401kt

COCKPITP  0.0m  0.55m 3.45m  #COCKPIT POSITION
LEFTGEAR -1.44m -1.65m -1.37m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.44m -1.65m -1.37m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.83m  3.93m  #WHEEL POSITION
ARRESTER  0.0m -1.45m -4.575m
MACHNGUN  0.0m -0.70m  1.90m  #MACHINE GUN POSITION
SMOKEGEN 0.0m -0.90m -7.20m  #SMOKE GENERATOR POSITION
VAPORPO0  3.70m 0.55m -5.25m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.45m  0.55m -3.70m  #VAPOR POSITION (Wings Spread)
HTRADIUS  8.0m                #OUTSIDE SPHERE RADIUS

EXCAMERA "CO-PILOT" 0.0m  0.82m 2.00m 0deg 0deg 0deg INSIDE

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 9

CRITAOAP  60.0deg               #CRITICAL AOA POSITIVE
CRITAOAM -30deg               #CRITICAL AOA NEGATIVE

CRITSPED 2.9MACH              #CRITICAL SPEED
MAXSPEED 4.3MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

PSTMPTCH 60deg                #Post-Stall Maneuver VPitch
PSTMYAW_ 60deg                #Post-Stall Maneuver VYaw
PSTMROLL 50deg                #Post-Stall Maneuver VRoll

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.6                  #EFFECT OF SPOILER FOR Cd

WINGAREA 40m^2                #WING AREA

MXIPTAOA 60.10deg              #MAX INPUT AOA
MXIPTSSA 60.5deg               #MAX INPUT SSA
MXIPTROL 180.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 90kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 200kt                #FULLY MANEUVABLE SPEED
CPITMANE 5.96                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.4                  #PITCH STABILITY CONSTANT
CYAWMANE 7.5                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 8.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.3                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 32

HRDPOINT -1.30m 0.10m -0.15m AIM9 B500 RKT
HRDPOINT 1.30m 0.10m -0.15m AIM120 AIM9 B500 RKT

HRDPOINT 0.00m -0.85m -0.45m AIM9 B250

HRDPOINT -0.40m -0.85m 0.55m AIM9 B250 B500 AGM65
HRDPOINT 0.40m -0.85m 0.55m AIM9 B250 B500 AGM65

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 200                 #INITIAL GUN
LOADWEPN AIM120 0
LOADWEPN AIM9 2
INITIAGM 0                    #INITIAL AGM

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.9MACH              #CRUISING SPEED
REFACRUS 50000ft              #CRUISING ALTITUDE
REFTCRUS 0.9                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 120kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.31                  #LANDING THROTTLE SETTING

AUTOCALC
