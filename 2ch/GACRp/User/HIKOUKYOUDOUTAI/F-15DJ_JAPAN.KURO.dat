REM GACGLIMT 9
REM GACRADRG 160
REM GACRADCS 1.0
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 0

IDENTIFY "JP_F-15DJ_KURO"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 21.2t                #THRUST WITH AFTERBURNER
THRMILIT 13t                #THRUST AT MILITARY POWER
WEIGHCLN 12.9t                #WEIGHT CLEAN
WEIGFUEL  5.9t                #WEIGHT OF FUEL
WEIGLOAD 10.0t                #WEIGHT OF PAYLOAD
FUELABRN 15kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.55kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.01

COCKPITP  0.0m  1.10m   4.60m  #COCKPIT POSITION
LEFTGEAR -1.6m -2.175m -2.0m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.6m -2.175m -2.0m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.135m  3.175m  #WHEEL POSITION
ARRESTER  0.0m -0.65m -7.40m
MACHNGUN  1.725m  0.50m  1.50m  #MACHINE GUN POSITION
SMOKEGEN -1.74m -0.30m -8.25m  #SMOKE GENERATOR POSITION
VAPORPO0  6.52m  0.38m -4.45m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  6.52m  0.38m -4.45m  #VAPOR POSITION (Wings Spread)
HTRADIUS  13.0m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 12

CRITAOAP  24deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.8MACH              #CRITICAL SPEED
MAXSPEED 2.5MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 58m^2                #WING AREA

MXIPTAOA 27.0deg              #MAX INPUT AOA
MXIPTSSA 7.0deg               #MAX INPUT SSA
MXIPTROL 200.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 280kt                #FULLY MANEUVABLE SPEED
CPITMANE 6.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.4                  #PITCH STABILITY CONSTANT
CYAWMANE 9.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 9.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.6                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

HRDPOINT -3.3m -0.05m -1.3m AIM9 AIM120 B500
HRDPOINT 3.3m -0.05m -1.3m AIM9 AIM120 B500
HRDPOINT -2.7m -0.05m -1.3m AIM9 AIM120 B500
HRDPOINT 2.7m -0.05m -1.3m AIM9 AIM120 B500

HRDPOINT -1.6m -0.8m 0m AIM120
HRDPOINT 1.6m -0.8m 0m AIM120
HRDPOINT -1.6m -0.8m -3.8m AIM120
HRDPOINT 1.6m -0.8m -3.8m AIM120

HRDPOINT 0.0m -1.0m -2m B250

HRDPOINT 3m -0.4m -1.3m B500 B250
HRDPOINT -3m -0.4m -1.3m B500 B250
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 940                 #INITIAL GUN
LOADWEPN AIM9 4
LOADWEPN AIM120 4
LOADWEPN B250 1

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 2.3MACH              #CRUISING SPEED
REFACRUS 50000ft              #CRUISING ALTITUDE
REFTCRUS 3.5                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 150kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1500m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.315                  #LANDING THROTTLE SETTING

AUTOCALC
