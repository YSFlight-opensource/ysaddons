REM GACGLIMT 9
REM GACRADRG 130
REM GACRADCS 0.6
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 1
REM GACSNDCH 2
REM GACRCVCH 2
REM GACRADSD 1
REM GACRADEL 0.5

IDENTIFY "JP_Rafale_J"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  15.2t                #THRUST WITH AFTERBURNER
THRMILIT  10.0t                #THRUST AT MILITARY POWER
WEIGHCLN  9.0t                #WEIGHT CLEAN
WEIGFUEL  4.2t                #WEIGHT OF FUEL
WEIGLOAD  3.5t                #WEIGHT OF PAYLOAD
FUELABRN 12.5kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.55kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.024

COCKPITP  0.0m  0.94m  3.645m  #COCKPIT POSITION
LEFTGEAR -1.725m -1.94m -1.72m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.725m -1.94m -1.72m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.94m  3.065m  #WHEEL POSITION
ARRESTER  0.0m -1.40m -5.00m
MACHNGUN 1.23m  -0.525m  2.0m  #MACHINE GUN POSITION
SMOKEGEN -0.0m  0.40m -7.00m  #SMOKE GENERATOR POSITION
VAPORPO0  5.415m  -0.165m -5.125m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.415m  -0.165m -5.125m  #VAPOR POSITION (Wings Spread)
HTRADIUS  10.2m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  26.5deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 1.2MACH              #CRITICAL SPEED
MAXSPEED 2.0MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 46m^2                #WING AREA

MXIPTAOA 28.92deg              #MAX INPUT AOA
MXIPTSSA 7.5deg               #MAX INPUT SSA
MXIPTROL 270.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 270kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.44                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.6                  #PITCH STABILITY CONSTANT
CYAWMANE 9.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 7.0                  #YAW STABILITY CONSTANT
CROLLMAN 4.8                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT 5.5m -0.165m -3.9m AIM9
HRDPOINT -5.5m -0.165m -3.9m AIM9

HRDPOINT 1.35m -0.9m -3.6m AIM120
HRDPOINT -1.35m -0.9m -3.6m AIM120

HRDPOINT 0m -1.0m -0.5m AIM120 B250

HRDPOINT 2.3m -0.70m -2.1m AIM120 AGM65 B500
HRDPOINT -2.3m -0.70m -2.1m AIM120 AGM65 B500
HRDPOINT 3.5m -0.70m -2.8m AIM120 AIM9 AGM65 B500
HRDPOINT -3.5m -0.70m -2.8m AIM120 AIM9 AGM65 B500

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

GUNPOWER 3
INITIGUN 125                 #INITIAL GUN
LOADWEPN AIM9 4
LOADWEPN AIM120 3
LOADWEPN AGM65 2

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 2.0MACH              #CRUISING SPEED
REFACRUS 50000ft              #CRUISING ALTITUDE
REFTCRUS 2.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 125kt                 #LANDING SPEED
REFAOALD 10deg                 #LANDING AOA
REFLNRWY 1000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.26                 #LANDING THROTTLE SETTING

AUTOCALC
