REM GACGLIMT 7.9
REM GACRADRG 230
REM GACRADCS 0.1
REM GACECMRG 0
REM GACAEWTR 50
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 2
REM GACSNDCH 2
REM GACRCVCH 2
REM GACRADSD 2
REM GACRADEL 1

IDENTIFY "EASA_X-02_WyvernProtoType"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 36.0t                #THRUST WITH AFTERBURNER
THRMILIT 24.0t                #THRUST AT MILITARY POWER
WEIGHCLN 14.4t                #WEIGHT CLEAN
WEIGFUEL  10.0t                #WEIGHT OF FUEL
WEIGLOAD 8.0t                #WEIGHT OF PAYLOAD
FUELABRN 25kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 1.25kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.01

COCKPITP  0.0m  1.50m   4.90m  #COCKPIT POSITION
LEFTGEAR -2.09m -2.01m -2.55m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.09m -2.01m -2.55m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.01m  3.155m  #WHEEL POSITION
ARRESTER  0.0m -1.50m -7.00m
MACHNGUN  1.00m  0.60m  2.015m  #MACHINE GUN POSITION
SMOKEGEN 0.00m -0.00m -8.50m  #SMOKE GENERATOR POSITION
VAPORPO0  5.58m  0.40m -5.25m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  8.80m  0.40m -2.90m  #VAPOR POSITION (Wings Spread)
HTRADIUS  12.6m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 7

CRITAOAP  31.75deg               #CRITICAL AOA POSITIVE
CRITAOAM -12deg               #CRITICAL AOA NEGATIVE

CRITSPED 1.6MACH              #CRITICAL SPEED
MAXSPEED 2.5MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE               #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.4                  #EFFECT OF SPOILER FOR Cd

WINGAREA 78m^2                #WING AREA

PSTMPTCH 30.22deg                #Post-Stall Maneuver VPitch
PSTMYAW_ 10.12deg                #Post-Stall Maneuver VYaw
PSTMROLL 20deg                #Post-Stall Maneuver VRoll

MXIPTAOA 32.0deg              #MAX INPUT AOA
MXIPTSSA 10.0deg               #MAX INPUT SSA
MXIPTROL 300.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

VGWSPED1 400kt
VGWSPED2 399kt

MANESPD1 75kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 170kt                 #FULLY MANEUVABLE SPEED
CPITMANE 12.45                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 4.5                  #PITCH STABILITY CONSTANT
CYAWMANE 7.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 6.0                  #YAW STABILITY CONSTANT
CROLLMAN 8.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT -0.7m -1.5m -1.0m B500 AGM65 AIM120
HRDPOINT 0.7m -1.5m -1.0m B500 AGM65 AIM120

BOMVISIB FALSE
AGMVISIB FALSE

HRDPOINT -0.7m -2.5m -3.0m AIM9
HRDPOINT 0.7m -2.5m -3.0m AIM9
HRDPOINT -0.5m -2.5m -3.0m AIM9
HRDPOINT 0.5m -2.5m -3.0m AIM9

HRDPOINT 0.00m -0.60m 1.20m B500 AIM120
HRDPOINT 0.46m -0.60m 1.20m B500 AIM120
HRDPOINT -0.46m -0.60m 1.20m B500 AIM120
HRDPOINT 0.46m -0.60m -3.00m B500 AIM120
HRDPOINT -0.46m -0.60m -3.00m B500 AIM120
HRDPOINT 0.00m -0.60m -3.00m B500 AIM120

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
AAMVISIB FALSE

BMBAYRCS 1.0
BOMINBAY TRUE

INITIGUN 620                 #INITIAL GUN
LOADWEPN AIM120 6
LOADWEPN AIM9 2
INITIAGM 0                    #INITIAL AGM

RADARCRS 0.3                  #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 2.3MACH              #CRUISING SPEED
REFACRUS 50000ft              #CRUISING ALTITUDE
REFTCRUS 2.5                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 150kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1800m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.265                  #LANDING THROTTLE SETTING

AUTOCALC
