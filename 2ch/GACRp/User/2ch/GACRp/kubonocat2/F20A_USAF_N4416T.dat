REM GACGLIMT 9
REM GACRADRG 80
REM GACRADCS 0.75
REM GACECMRG 0
REM GACAEWTR 50
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 1
REM GACSNDCH 1
REM GACRCVCH 1
REM GACRADSD 1
REM GACRADEL 0.5

IDENTIFY "US_F-20A_Tigershark_N4416T"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  9.32t               #THRUST WITH AFTERBURNER
THRMILIT  4.9t                #THRUST AT MILITARY POWER
WEIGHCLN  5.08t                #WEIGHT CLEAN
WEIGFUEL  4.0t                #WEIGHT OF FUEL
WEIGLOAD  5.2t                #WEIGHT OF PAYLOAD
FUELABRN 6.5kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.17kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.048

COCKPITP  0.0m  0.80m   2.30m  #COCKPIT POSITION
LEFTGEAR -1.715m -1.52m -1.455m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.715m -1.52m -1.455m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.535m  3.74m  #WHEEL POSITION
ARRESTER  0.0m -1.0m -5.00m
MACHNGUN -0.24m  0.14m  5.275m  #MACHINE GUN POSITION
MACHNGN2 0.24m  0.14m  5.275m  #MACHINE GUN POSITION
SMOKEGEN 0.0m -0.45m -5.35m  #SMOKE GENERATOR POSITION
VAPORPO0  4.00m  -0.245m -1.0m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.00m  -0.245m -1.0m  #VAPOR POSITION (Wings Spread)
HTRADIUS  9.8m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 6

CRITAOAP  22.9deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.0MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 17.28m^2                #WING AREA

MXIPTAOA 26.0deg              #MAX INPUT AOA
MXIPTSSA 6.0deg               #MAX INPUT SSA
MXIPTROL 280.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 290kt                 #FULLY MANEUVABLE SPEED
CPITMANE 6.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 6.5                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 7.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.6                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 2
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT 4.05m -0.245m 0.3m AIM9
HRDPOINT -4.05m -0.245m 0.3m AIM9

HRDPOINT 3m -0.76m 0.3m AIM9 AGM65
HRDPOINT -3m -0.76m 0.3m AIM9 AGM65
HRDPOINT 2.1m -0.77m 0.8m B500 RKT B250 AGM65
HRDPOINT -2.1m -0.77m 0.8m B500 RKT B250 AGM65


LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 560                 #INITIAL GUN
INITIAAM 4                    #INITIAL AAM
INITIAGM 2                    #INITIAL AGM
INITBOMB 0

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 2.0MACH              #CRUISING SPEED
REFACRUS 50000ft              #CRUISING ALTITUDE
REFTCRUS 2.7                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 130kt                 #LANDING SPEED
REFAOALD 10deg                 #LANDING AOA
REFLNRWY 1000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.325                 #LANDING THROTTLE SETTING

AUTOCALC
