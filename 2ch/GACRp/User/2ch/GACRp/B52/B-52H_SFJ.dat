REM GACGLIMT 3
REM GACRADRG 40
REM GACRADCS 2
REM GACECMRG 0
REM GACAEWTR 50
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 1
REM GACSNDCH 1 2
REM GACRCVCH 1 2
REM GACRADSD 1
REM GACRADEL 0.5

IDENTIFY "US_B-52H_SFJ"
CATEGORY HEAVYBOMBER
AFTBURNR FALSE               #HAVE AFTERBURNER?
THRAFTBN 0.0t                #THRUST WITH AFTERBURNER
THRMILIT 61.76t             #THRUST AT MILITARY POWER
WEIGHCLN 85t                #WEIGHT CLEAN
WEIGFUEL 100t                #WEIGHT OF FUEL
WEIGLOAD 31t                 #WEIGHT OF PAYLOAD
FUELABRN 0.0kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 8.6kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNSIGHT FALSE

COCKPITP -0.55m  1.46m  18.74m  #COCKPIT POSITION
LEFTGEAR -1.545m -3.28m -5.735m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.545m -3.28m -5.735m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -3.28m 10m  #WHEEL POSITION
ARRESTER  0.0m -1.80m -5.5m
MACHNGUN 0.0m -0.475m -27.05m  #MACHINE GUN POSITION
GUNDIREC  0.0m 0.0m -1.0m     #MACHINE GUN DIRECTION
SMOKEGEN  0.0m  2.00m -25.5m  #SMOKE GENERATOR POSITION
VAPORPO0 -27.65m  -0.20m -12.9m  #VAPOR POSITION (Wings Swept back)
VAPORPO1 -27.65m  -0.20m -12.9m  #VAPOR POSITION (Wings Spread)
HTRADIUS 37.6m                #OUTSIDE SPHERE RADIUS

EXCAMERA "CO-PILOT" 0.55m  1.46m  18.74m 0deg 0deg 0deg INSIDE

TRIGGER1 SMK
TRIGGER2 NON
TRIGGER3 NON
TRIGGER4 NON

STRENGTH 304001001001001001001001001304

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 442kt              #CRITICAL SPEED
MAXSPEED 516kt              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.5                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.45                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.3                  #EFFECT OF SPOILER FOR Cd

WINGAREA 500m^2                #WING AREA

MXIPTAOA 18.0deg              #MAX INPUT AOA
MXIPTSSA 4.0deg               #MAX INPUT SSA
MXIPTROL 30deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 260kt                #FULLY MANEUVABLE SPEED
CPITMANE 4.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 12.0                  #PITCH STABILITY CONSTANT
CYAWMANE 2.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 2.5                  #YAW STABILITY CONSTANT
CROLLMAN 0.7                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1 40kt
VGWSPED2 150kt

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                   #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED


INITIGUN 0                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                   #INITIAL AGM


REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 490kt              #CRUISING SPEED
REFACRUS 30000ft              #CRUISING ALTITUDE
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 150kt                 #LANDING SPEED
REFAOALD 5deg                 #LANDING AOA
REFLNRWY 3000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.7                  #LANDING THROTTLE SETTING

AGMVISIB FALSE
AGMSLOT_ -0.46m -2.5m 10.9m
AGMSLOT_  0.46m -2.5m 10.9m
BOMVISIB FALSE
BOMBSLOT -0.53m -2.5m 2.78m
BOMBSLOT  0.53m -2.5m 2.78m
BOMBSLOT -0.53m -2.5m 0m
BOMBSLOT  0.53m -2.5m 0m
BOMBSLOT -0.53m -2.5m -2.78m
BOMBSLOT  0.53m -2.5m -2.78m
BOMBSLOT -0.53m -2.5m -5.56m
BOMBSLOT  0.53m -2.5m -5.56m

AUTOCALC
