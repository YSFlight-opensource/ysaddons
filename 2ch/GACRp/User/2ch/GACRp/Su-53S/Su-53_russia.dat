REM GACGLIMT 7.9
REM GACRADRG 230
REM GACRADCS 0.1
REM GACECMRG 0
REM GACAEWTR 50
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 2
REM GACSNDCH 2
REM GACRCVCH 2
REM GACRADSD 2
REM GACRADEL 1

IDENTIFY "RU_Su-53S_Zephyros_A"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 38.0t                #THRUST WITH AFTERBURNER
THRMILIT 31.2t                #THRUST AT MILITARY POWER
WEIGHCLN 14.4t                #WEIGHT CLEAN
WEIGFUEL 11.0t                #WEIGHT OF FUEL
WEIGLOAD 8.0t                #WEIGHT OF PAYLOAD
FUELABRN 17kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.95kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.035

COCKPITP  0.0m  1.30m   6.10m  #COCKPIT POSITION
LEFTGEAR -2.09m -1.81m -2.55m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.09m -1.81m -2.55m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.81m  3.155m  #WHEEL POSITION
ARRESTER  0.0m -1.50m -7.00m
MACHNGUN  1.00m  0.60m  2.015m  #MACHINE GUN POSITION
SMOKEGEN 0.00m -0.00m -8.50m  #SMOKE GENERATOR POSITION
VAPORPO0  7.13m  0.425m -6.095m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  7.13m  0.425m -6.095m  #VAPOR POSITION (Wings Spread)
HTRADIUS  12.6m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 27

CRITAOAP  29.20deg               #CRITICAL AOA POSITIVE
CRITAOAM -6deg               #CRITICAL AOA NEGATIVE

CRITSPED 2.0MACH              #CRITICAL SPEED
MAXSPEED 2.5MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE               #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.4                  #EFFECT OF SPOILER FOR Cd

WINGAREA 78m^2                #WING AREA

MXIPTAOA 30.90deg              #MAX INPUT AOA
MXIPTSSA 8.0deg               #MAX INPUT SSA
MXIPTROL 260.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

VGWSPED1 400kt
VGWSPED2 399kt

MANESPD1 75kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 170kt                 #FULLY MANEUVABLE SPEED
CPITMANE 6.5                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.5                  #PITCH STABILITY CONSTANT
CYAWMANE 7.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 6.0                  #YAW STABILITY CONSTANT
CROLLMAN 5.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 64

HRDPOINT -2.7m -0.15m -3.0m AIM9
HRDPOINT 2.7m -0.15m -3.0m AIM9
HRDPOINT -3.30m -0.31m -3.0m AIM9 AIM120
HRDPOINT 3.30m -0.31m -3.0m AIM9 AIM120

HRDPOINT -4.25m -0.15m -3.0m AIM9 AIM120 B500 RKT
HRDPOINT 4.25m -0.15m -3.0m AIM9 AIM120 B500 RKT

HRDPOINT -1.05m -1.1m -0.6m AIM9 AIM120 AGM65 B500
HRDPOINT 1.05m -1.1m -0.6m AIM9 AIM120 AGM65 B500
HRDPOINT 0.00m -0.19m -1.9m AIM120 AGM65
HRDPOINT 0.00m -0.29m -6.3m AIM120 AGM65

HRDPOINT 3m -0.65m -3.0m B250 B500 AIM120 AGM65 RKT
HRDPOINT -3m -0.65m -3.0m B250 B500 AIM120 AGM65 RKT

BOMVISIB TRUE
AGMVISIB TRUE
BOMVISIB TRUE
AAMVISIB TRUE

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

BMBAYRCS 0.46

GUNPOWER 3
INITIGUN 230                #INITIAL GUN
LOADWEPN AIM120 9
LOADWEPN AIM9 9
INITIAGM 0                    #INITIAL AGM

RADARCRS 0.46                  #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 2.3MACH              #CRUISING SPEED
REFACRUS 50000ft              #CRUISING ALTITUDE
REFTCRUS 2.5                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 150kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1800m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.265                  #LANDING THROTTLE SETTING

AUTOCALC
