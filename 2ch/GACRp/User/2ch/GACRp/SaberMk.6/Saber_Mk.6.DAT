REM GACGLIMT 6.5
REM GACRADRG 2
REM GACRADCS 0.5
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 0
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 0
REM GACRCVCH 0
REM GACRADSD 0.2
REM GACRADEL 0.2

IDENTIFY "DE_Saber_Mk.6_Black_Tulip"
SUBSTNAM US_F-86F-30_Saber
CATEGORY FIGHTER
AFTBURNR FALSE                 #HAVE AFTERBURNER?
THRAFTBN 2.68t                #THRUST WITH AFTERBURNER
THRMILIT  2.68t                #THRUST AT MILITARY POWER
WEIGHCLN 4.94t                #WEIGHT CLEAN
WEIGFUEL  0.8t                #WEIGHT OF FUEL
WEIGLOAD  0.9t                #WEIGHT OF PAYLOAD
FUELABRN 0.3kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.3kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNSIGHT FALSE

COCKPITP  0.00m  0.86m   2.10m  #COCKPIT POSITION
LEFTGEAR -1.465m -1.785m -1.00m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.465m -1.785m -1.00m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.81m  3.84m  #WHEEL POSITION
ARRESTER  0.0m -0.5m -5.00m

GUNPOWER 0
MACHNGUN  -0.55m 0.1m  3.4m  #MACHINE GUN POSITION
MACHNGN2  0.55m 0.1m  3.4m  #MACHINE GUN POSITION
MACHNGN3  -0.55m -0.35m  3.6m  #MACHINE GUN POSITION
MACHNGN4  0.55m -0.35m  3.6m  #MACHINE GUN POSITION


REM GUNINTVL 0.075
GUNINTVL 0.15
SMOKEGEN -0.0m 0.445m -5.735m  #SMOKE GENERATOR POSITION
VAPORPO0  5.95m  -0.385m -2.385m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.95m  -0.385m -2.385m  #VAPOR POSITION (Wings Spread)
HTRADIUS  7.6m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 7

CRITAOAP  24.5deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 785km/h              #CRITICAL SPEED
MAXSPEED 1120km/h              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 26.7m^2                #WING AREA

MXIPTAOA 22.37deg              #MAX INPUT AOA
MXIPTSSA 7.0deg               #MAX INPUT SSA
MXIPTROL 175.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 80kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 200kt                #FULLY MANEUVABLE SPEED
CPITMANE 5.49                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.4                  #PITCH STABILITY CONSTANT
CYAWMANE 7.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 7.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.6                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

INITIGUN 1068                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM

NMTURRET 2
TURRETPO 0 -0.55m -0.1m  3.6m 0.0deg 0.0deg 0.0deg
TURRETPT 0 0deg 0deg 0deg
TURRETHD 0 0deg 0deg 0deg
TURRETAM 0 267
TURRETIV 0 0.075sec
TURRETAR 0
TURRETCT 0 PILOT
TURRETRG 0 1000m
TURRETPO 1 0.55m -0.1m  3.6m 0.0deg 0.0deg 0.0deg
TURRETPT 1 0deg 0deg 0deg
TURRETHD 1 0deg 0deg 0deg
TURRETAM 1 267
TURRETIV 1 0.075sec
TURRETAR 1
TURRETCT 1 PILOT
TURRETRG 1 1000m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1120km/h              #CRUISING SPEED
REFACRUS 25000ft              #CRUISING ALTITUDE
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 100kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 700m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.295                  #LANDING THROTTLE SETTING

AUTOCALC
