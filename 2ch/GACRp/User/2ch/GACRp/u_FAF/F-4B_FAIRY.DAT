REM GACGLIMT 6.5
REM GACRADRG 56
REM GACRADCS 1.5
REM GACECMRG 0
REM GACAEWTR 50
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 1
REM GACRCVCH 1
REM GACRADSD 1
REM GACRADEL 0.25

IDENTIFY "FAF_F-4B_Phantom_II"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 16.1t                #THRUST WITH AFTERBURNER
THRMILIT  10.4t                #THRUST AT MILITARY POWER
WEIGHCLN 14.2t                #WEIGHT CLEAN
WEIGFUEL  7.5t                #WEIGHT OF FUEL
WEIGLOAD  7.2t                #WEIGHT OF PAYLOAD
FUELABRN 8kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 1.00kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.01

COCKPITP  0.0m  0.505m   4.25m  #COCKPIT POSITION
LEFTGEAR -2.51m -2.275m -1.655m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.51m -2.275m -1.655m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.34m  5.315m  #WHEEL POSITION
ARRESTER  0.0m -1.71m -7.275m
MACHNGUN  0.0m -1.3m  2m  #MACHINE GUN POSITION
SMOKEGEN -0.0m -0.365m -9.05m  #SMOKE GENERATOR POSITION
VAPORPO0  5.825m  -0.35m -4.59m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.825m  -0.35m -4.59m  #VAPOR POSITION (Wings Spread)
HTRADIUS  12.8m                #OUTSIDE SPHERE RADIUS

EXCAMERA "CO-PILOT" 0.0m 0.85m 2.85m 0deg 0deg 0deg INSIDE

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 16

CRITAOAP  21.3deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.3MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.3                  #EFFECT OF SPOILER FOR Cd

WINGAREA 49m^2                #WING AREA

MXIPTAOA 22.70deg              #MAX INPUT AOA
MXIPTSSA 6.0deg               #MAX INPUT SSA
MXIPTROL 190.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 360kt                #FULLY MANEUVABLE SPEED
CPITMANE 4.35                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 4.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 2.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 114

HRDPOINT 0.00m -1.15m -0.95m B250

HRDPOINT -0.75m -1m 2.4m AIM120
HRDPOINT 0.75m -1m 2.4m AIM120
HRDPOINT -1.2m -1m -2.4m AIM120
HRDPOINT 1.2m -1m -2.4m AIM120

HRDPOINT -1.55m -1.10m 0.70m AIM9
HRDPOINT 1.55m -1.10m 0.70m AIM9
HRDPOINT -2.45m -1.10m 0.70m AIM9
HRDPOINT 2.45m -1.10m 0.70m AIM9

HRDPOINT -1.55m -1.3m 0.50m B500 RKT
HRDPOINT 1.55m -1.3m 0.50m B500 RKT
HRDPOINT -2.45m -1.3m 0.50m B500 RKT
HRDPOINT 2.45m -1.3m 0.50m B500 RKT
HRDPOINT -2.00m -1.75m 0.50m B500 RKT
HRDPOINT 2.00m -1.75m 0.50m B500 RKT

HRDPOINT -2.9m -1.1m -1.80m B500
HRDPOINT 2.9m -1.1m -1.80m B500
HRDPOINT -3.8m -1.1m -1.80m B500
HRDPOINT 3.8m -1.1m -1.80m B500
HRDPOINT -3.35m -1.3m -1.80m B500
HRDPOINT 3.35m -1.3m -1.80m B500

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
INITIGUN 1200                 #INITIAL GUN
LOADWEPN AIM120 0
LOADWEPN AIM9 4
INITIAGM 0                    #INITIAL AGM

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 2.0MACH              #CRUISING SPEED
REFACRUS 50000ft              #CRUISING ALTITUDE
REFTCRUS 2.5                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 150kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1500m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.38                  #LANDING THROTTLE SETTING

AUTOCALC
