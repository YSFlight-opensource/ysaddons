REM GACGLIMT 9
REM GACRADRG 100
REM GACRADCS 0.35
REM GACECMRG 60
REM GACAEWTR 75
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 2
REM GACSNDCH 1 2
REM GACRCVCH 1 2
REM GACRADSD 1
REM GACRADEL 1

IDENTIFY "FAF_FFR-31_Sylphide_Block15"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  27.3t               #THRUST WITH AFTERBURNER
THRMILIT  20.3t                #THRUST AT MILITARY POWER
WEIGHCLN  11.87t                #WEIGHT CLEAN
WEIGFUEL  9.2t                #WEIGHT OF FUEL
WEIGLOAD  15.78t                #WEIGHT OF PAYLOAD
FUELABRN 6.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.378kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.01

COCKPITP  0.0m  0.70m   5.80m  #COCKPIT POSITION
LEFTGEAR -2.35m -3.35m -4.00m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.35m -3.35m -4.00m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.60m  5.50m  #WHEEL POSITION
ARRESTER  0.0m -3.35m -6.00m
MACHNGUN -1m  0.70m  5m  #MACHINE GUN POSITION
SMOKEGEN 0.00m -0.50m -10.00m  #SMOKE GENERATOR POSITION
VAPORPO0  -7.57m 0.30m -4.10m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  -7.57m 0.30m -4.10m  #VAPOR POSITION (Wings Spread)
HTRADIUS  12.3m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 17

CRITAOAP 27.30deg               #CRITICAL AOA POSITIVE
CRITAOAM -6deg               #CRITICAL AOA NEGATIVE

CRITSPED 1.4MACH              #CRITICAL SPEED
MAXSPEED 2.7MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.6                  #EFFECT OF SPOILER FOR Cd

WINGAREA 50m^2                #WING AREA

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MXIPTAOA 28.20deg              #MAX INPUT AOA
MXIPTSSA 15.0deg               #MAX INPUT SSA
MXIPTROL 220.0deg             #MAX INPUT ROLL

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 245kt                 #FULLY MANEUVABLE SPEED
CPITMANE 9.21                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 4.0                  #PITCH STABILITY CONSTANT
CYAWMANE 3.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 5.0                  #YAW STABILITY CONSTANT
CROLLMAN 4.5                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

RADARCRS 0.3                  #RADAR CROSS SECTION

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT -3.90m -0.30m -4.50m B250
HRDPOINT 3.90m -0.30m -4.50m B250

HRDPOINT -0.00m -1.00m 2.00m AIM9 AIM120 AGM65
HRDPOINT -0.50m -1.00m 1.50m AIM9 AIM120 AGM65
HRDPOINT 0.50m -1.00m 1.50m AIM9 AIM120 AGM65
HRDPOINT 0.00m -1.00m -2.30m AIM9 AIM120 AGM65
HRDPOINT -0.50m -1.00m -2.80m AIM9 AIM120 AGM65
HRDPOINT 0.50m -1.00m -2.80m AIM9 AIM120 AGM65

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
AAMVISIB FALSE
AGMVISIB FALSE

INITIGUN 940                 #INITIAL GUN
LOADWEPN AIM9 2
LOADWEPN AIM120 4
LOADWEPN AGM65 0

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 2.7MACH              #CRUISING SPEED
REFACRUS 50000ft              #CRUISING ALTITUDE
REFTCRUS 1.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 150kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1600m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.265                  #LANDING THROTTLE SETTING

AUTOCALC
