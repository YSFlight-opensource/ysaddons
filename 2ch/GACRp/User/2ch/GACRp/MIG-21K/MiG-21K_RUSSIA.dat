REM GACGLIMT 8.5
REM GACRADRG 57
REM GACRADCS 0.7
REM GACECMRG 0
REM GACAEWTR 50
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 3
REM GACRCVCH 3
REM GACRADSD 1
REM GACRADEL 0.5

IDENTIFY "RU_MiG-21K"
CATEGORY  FIGHTER
AFTBURNR  TRUE               #HAVE AFTERBURNER?
THRAFTBN  9.9t               #THRUST WITH AFTERBURNER
THRMILIT  7.0t               #THRUST AT MILITARY POWER
WEIGHCLN  5.2t               #WEIGHT CLEAN
WEIGFUEL  2.5t               #WEIGHT OF FUEL
WEIGLOAD  2.5t               #WEIGHT OF PAYLOAD
FUELABRN  7.0kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.35kg             #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL  0.018

COCKPITP  0.000m  0.80m  3.000m  #COCKPIT POSITION
LEFTGEAR -1.750m -1.60m -1.450m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.750m -1.60m -1.450m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.000m -1.60m  3.270m  #WHEEL POSITION
ARRESTER  0.000m -1.00m -5.100m
MACHNGUN  0.000m -0.65m  2.500m  #MACHINE GUN POSITION
SMOKEGEN -0.000m  0.65m -6.600m  #SMOKE GENERATOR POSITION
VAPORPO0  3.505m -0.22m -2.795m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  3.505m -0.22m -2.795m  #VAPOR POSITION (Wings Spread)
HTRADIUS  9.8m                   #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  22.50deg           #CRITICAL AOA POSITIVE
CRITAOAM -15deg              #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH             #CRITICAL SPEED
MAXSPEED 2.0MACH             #MAXIMUM SPEED

HASSPOIL TRUE                #HAS SPOILER?
RETRGEAR TRUE                #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE               #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                 #EFFECT OF GEAR FOR Cd
CDSPOILR 0.7                 #EFFECT OF SPOILER FOR Cd

WINGAREA 23m^2               #WING AREA

MXIPTAOA 23.90deg            #MAX INPUT AOA
MXIPTSSA 6.5deg              #MAX INPUT SSA
MXIPTROL 90.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt               #MINIMUM MANEUVABLE SPEED
MANESPD2 300kt               #FULLY MANEUVABLE SPEED
CPITMANE 4.95                #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.2                 #PITCH STABILITY CONSTANT
CYAWMANE 7.0                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB 6.5                 #YAW STABILITY CONSTANT
CROLLMAN 1.5                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                #CONTROL LANDINGGEAR
CTLBRAKE FALSE               #CONTROL BRAKE
CTLSPOIL 0.0                 #CONTROL SPOILER
CTLABRNR FALSE               #CONTROL AFTERBURNER
CTLTHROT 0.0                 #CONTROL THRUST
CTLIFLAP 0.0                 #CONTROL FLAP
CTLINVGW 0.0                 #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE               #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg     #ATTITUDE
INITFUEL 75%                 #INITIAL FUEL
INITLOAD 0.0t                #INITIAL PAYLOAD
INITSPED 0.0MACH             #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT -1.80m -0.50m -0.10m AIM9 AIM120 B500 B250
HRDPOINT 1.80m -0.50m -0.10m AIM9 AIM120 B500 B250

HRDPOINT -2.60m -0.55m -1.50m AIM9 B250 RKT B250
HRDPOINT 2.60m -0.55m -1.50m AIM9 B250 RKT B250

HRDPOINT 0.00m -0.80m -0.25m B250
LMTBYHDP TRUE                #Limit Max Num Weapon by Hardpoint

INITIGUN 200                 #INITIAL GUN
LOADWEPN AIM120 2
LOADWEPN AIM9 2
INITBOMB 0                   #INITIAL AGM

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 2.0MACH             #CRUISING SPEED
REFACRUS 50000ft             #CRUISING ALTITUDE
REFTCRUS 2.6                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 150kt               #LANDING SPEED
REFAOALD 10deg               #LANDING AOA
REFLNRWY 880m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.32                #LANDING THROTTLE SETTING

AUTOCALC