REM GACGLIMT 8
REM GACRADRG 0
REM GACRADCS 0.6
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 0
REM GACLNDNV 0
REM GACLIMIT 0
REM GACSNDCH 0
REM GACRCVCH 0
REM GACRADSD 0
REM GACRADEL 0

IDENTIFY "JP_T-4_BLUEIMPULSE"
CATEGORY TRAINER
AFTBURNR FALSE                 #HAVE AFTERBURNER?
THRMILIT  3.34t                #THRUST AT MILITARY POWER
WEIGHCLN  3.65t                #WEIGHT CLEAN
WEIGFUEL  1.5t                #WEIGHT OF FUEL
WEIGLOAD  1.5t                #WEIGHT OF PAYLOAD
FUELMILI 0.25kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.35m  3.35m  #COCKPIT POSITION
LEFTGEAR -1.65m -1.75m -0.45m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.65m -1.75m -0.45m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.75m  4.75m  #WHEEL POSITION
ARRESTER  0.00m -1.25m -4.50m
MACHNGUN  0.00m  0.00m  0.00m  #MACHINE GUN POSITION
SMOKEGEN  0.45m -0.50m -4.35m  #SMOKE GENERATOR POSITION
VAPORPO0  5.05m -0.40m -2.20m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.05m -0.40m -2.20m  #VAPOR POSITION (Wings Spread)
HTRADIUS  9.0m                 #OUTSIDE SPHERE RADIUS

EXCAMERA "CO-PILOT" 0.0m 0.75m 2.00m 0deg 0deg 0deg INSIDE

SMOKEOIL 100.0kg
WEAPONCH SMK

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 5

CRITAOAP  24.40deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 805km/h             #CRITICAL SPEED
MAXSPEED 1037km/h              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.6                  #EFFECT OF SPOILER FOR Cd

WINGAREA 31m^2                #WING AREA

MXIPTAOA 26.50deg              #MAX INPUT AOA
MXIPTSSA 7.50deg               #MAX INPUT SSA
MXIPTROL 200.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 85kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 220kt                 #FULLY MANEUVABLE SPEED
CPITMANE 6.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.4                  #PITCH STABILITY CONSTANT
CYAWMANE 7.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 6.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.2                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT -2.5m -0.55m -0.2m B250
HRDPOINT 2.5m -0.55m -0.2m B250

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 0                 #INITIAL GUN

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1037km/h              #CRUISING SPEED
REFACRUS 16000ft              #CRUISING ALTITUDE
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 115kt                 #LANDING SPEED
REFAOALD 10deg                 #LANDING AOA
REFLNRWY 850m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.34                 #LANDING THROTTLE SETTING

AUTOCALC
