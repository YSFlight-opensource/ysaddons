REM GACGLIMT 6
REM GACRADRG 0
REM GACRADCS 1.6
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 0
REM GACLNDNV 0
REM GACLIMIT 0

IDENTIFY "US_A-10T_TripleThunderbolt_II"
CATEGORY ATTACKER
AFTBURNR TRUE                #HAVE AFTERBURNER?
THRAFTBN  32.4t                #THRUST WITH AFTERBURNER
THRMILIT  22t                #THRUST AT MILITARY POWER
WEIGHCLN  28t                #WEIGHT CLEAN
WEIGFUEL 13.0t                #WEIGHT OF FUEL
WEIGLOAD 10.2t                #WEIGHT OF PAYLOAD
FUELABRN 30.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 1.1kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP 0m  0.985m  8.035m  #COCKPIT POSITION
LEFTGEAR -5.54m -2.45m -1.85m  #LEFT LANDING GEAR POSITION
RIGHGEAR  5.54m -2.45m -1.85m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.37m -2.45m  8.65m  #WHEEL POSITION
ARRESTER  0.0m -1.50m -6.90m
MACHNGUN  2.715m -0.645m  8.20m  #MACHINE GUN POSITION
MACHNGN2  -2.965m -0.645m  8.20m  #MACHINE GUN POSITION
MACHNGN3  0.125m -0.65m  11.70m  #MACHINE GUN POSITION
SMOKEGEN -0.0m  0.425m -7.545m  #SMOKE GENERATOR POSITION
VAPORPO0  11.585m  0.00m -1.425m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  11.585m  0.00m -1.425m  #VAPOR POSITION (Wings Spread)
HTRADIUS 11.7m                #OUTSIDE SPHERE RADIUS
GUNINTVL 0.028

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 60

CRITAOAP  23.50deg               #CRITICAL AOA POSITIVE
CRITAOAM -12deg               #CRITICAL AOA NEGATIVE

CRITSPED 500km/h              #CRITICAL SPEED
MAXSPEED 682km/h              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 57m^2                #WING AREA

MXIPTAOA 26.0deg              #MAX INPUT AOA
MXIPTSSA 7.5deg               #MAX INPUT SSA
MXIPTROL 180.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 200kt                #FULLY MANEUVABLE SPEED
CPITMANE 4.39                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 1.8                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.8                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR TRUE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 4

HRDPOINT -9.64m -0.4m 0m AIM9
HRDPOINT -10.54m -0.4m 0m AIM9
HRDPOINT 9.64m -0.4m 0m AIM9
HRDPOINT 10.54m -0.4m 0m AIM9

HRDPOINT 6.64m -1.3m 0.0m AGM65 B500
HRDPOINT -6.64m -1.3m 0.0m AGM65 B500
HRDPOINT 6.14m -0.9m 0.0m AGM65 B500
HRDPOINT -6.14m -0.9m 0.0m AGM65 B500
HRDPOINT 7.14m -0.9m 0.0m AGM65 B500
HRDPOINT -7.14m -0.9m 0.0m AGM65 B500

HRDPOINT 3.54m -1.2m 0.0m B500
HRDPOINT 2.14m -1.2m 0.0m B500
HRDPOINT 4.64m -1.0m 0.0m AGM65 B500
HRDPOINT 1.04m -1.0m 0.0m AGM65 B500

HRDPOINT -3.54m -1.2m 0.0m B500
HRDPOINT -2.14m -1.2m 0.0m B500
HRDPOINT -4.64m -1.0m 0.0m AGM65 B500
HRDPOINT -1.04m -1.0m 0.0m AGM65 B500

HRDPOINT 8.14m -0.7m 0.0m AGM65 B500 RKT
HRDPOINT -8.14m -0.7m 0.0m AGM65 B500 RKT

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 64

INITIGUN 3525                 #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITIAGM 6                    #INITIAL AGM
INITBOMB 8                    #INITIAL BOMB
INITRCKT 38                   #INITIAL ROCKET (10kg/each)

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 680km/h              #CRUISING SPEED
REFACRUS 3000ft              #CRUISING ALTITUDE
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 130kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.55                  #LANDING THROTTLE SETTING

AUTOCALC
