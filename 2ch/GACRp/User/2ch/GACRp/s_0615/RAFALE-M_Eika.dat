REM GACGLIMT 9
REM GACRADRG 130
REM GACRADCS 0.6
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 1
REM GACLNDNV 0
REM GACLIMIT 1
REM GACSNDCH 2
REM GACRCVCH 2
REM GACRADSD 1
REM GACRADEL 0.5

IDENTIFY "KO_Rafale_M_Eika-Ichijo"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  14.4t                #THRUST WITH AFTERBURNER
THRMILIT  11.5t                #THRUST AT MILITARY POWER
WEIGHCLN  12.0t                #WEIGHT CLEAN
WEIGFUEL  4.2t                #WEIGHT OF FUEL
WEIGLOAD  3.5t                #WEIGHT OF PAYLOAD
FUELABRN 12.5kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.55kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 2

COCKPITP  0.0m  0.94m  3.645m  #COCKPIT POSITION
LEFTGEAR -1.725m -1.94m -1.72m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.725m -1.94m -1.72m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.94m  3.065m  #WHEEL POSITION
ARRESTER  0.0m -1.56m -5.54m
MACHNGUN 2.30m  -0.78m  -1.87m  #MACHINE GUN POSITION
MACHNGN2 -2.30m  -0.78m  -1.87m  #MACHINE GUN POSITION
SMOKEGEN -0.0m  0.40m -7.00m  #SMOKE GENERATOR POSITION
VAPORPO0  5.415m  -0.165m -5.125m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.415m  -0.165m -5.125m  #VAPOR POSITION (Wings Spread)
HTRADIUS  10.2m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 6

CRITAOAP  26.5deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 1.2MACH              #CRITICAL SPEED
MAXSPEED 2.0MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 46m^2                #WING AREA

MXIPTAOA 25.92deg              #MAX INPUT AOA
MXIPTSSA 7.0deg               #MAX INPUT SSA
MXIPTROL 270.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 270kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.44                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.7                  #PITCH STABILITY CONSTANT
CYAWMANE 9.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 6.3                  #YAW STABILITY CONSTANT
CROLLMAN 4.8                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT 5.5m -0.165m -3.9m AIM9
HRDPOINT -5.5m -0.165m -3.9m AIM9

HRDPOINT 1.35m -0.9m -3.6m AIM120
HRDPOINT -1.35m -0.9m -3.6m AIM120

HRDPOINT 0m -1.0m -0.5m AIM120 B250

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

GUNPOWER 20
INITIGUN 40                 #INITIAL GUN
LOADWEPN AIM9 2
LOADWEPN AIM120 2

NMTURRET 2

TURRETPO 0 -3.5m -0.73m 0.7m 0deg 0deg 0deg
TURRETPT 0 -4deg 3deg 0deg               
TURRETHD 0 -3deg 3deg 0deg              
TURRETAM 0 1000                       
TURRETIV 0 0.02sec         
TURRETAR 0                                           
TURRETCT 0 GUNNER                  
TURRETRG 0 1000m

TURRETPO 1 3.5m -0.73m 0.7m 0deg 0deg 0deg
TURRETPT 1 -4deg 3deg 0deg               
TURRETHD 1 -3deg 3deg 0deg              
TURRETAM 1 1000                       
TURRETIV 1 0.02sec         
TURRETAR 1                                           
TURRETCT 1 GUNNER                  
TURRETRG 1 1000m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 2.0MACH              #CRUISING SPEED
REFACRUS 50000ft              #CRUISING ALTITUDE
REFTCRUS 2.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 150kt                 #LANDING SPEED
REFAOALD 8deg                 #LANDING AOA
REFLNRWY 1400m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.4                 #LANDING THROTTLE SETTING

AUTOCALC
