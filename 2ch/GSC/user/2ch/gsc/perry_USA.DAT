TURRETPO 0 0m 8.36m -14.7m 180deg 0deg 0deg      # Number x y z h p b
TURRETPT 0 -15deg 85deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 -155deg 155deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 1000                              # Ammo
TURRETIV 0 0.6sec                           # Number ShootingInterval
TURRETNP 0 0008
TURRETNH 0 0007 
TURRETAR 0                                   # Anti Air Capable
TURRETCT 0 PILOT                             # Controlled by the pilot
TURRETRG 0 0m

TURRETPO 1 0m 9.4m -39.31m 180deg 0deg 0deg      # Number x y z h p b
TURRETPT 1 -2deg 85deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 1 -155deg 155deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 1 100000                              # Ammo
TURRETIV 1 0.01sec                           # Number ShootingInterval
TURRETNP 1 0006
TURRETNH 1 0005 
TURRETAR 1                                   # Anti Air Capable
TURRETCT 1 GUNNER                             # Controlled by the pilot
TURRETRG 1 2500m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 25kt              #CRUISING SPEED
REFACRUS 50ft              #CRUISING ALTITUDE
REFTCRUS 0.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 20kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 100m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.0                  #LANDING THROTTLE SETTING

AUTOCALC
