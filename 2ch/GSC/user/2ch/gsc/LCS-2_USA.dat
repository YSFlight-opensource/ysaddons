REM GACGLIMT 10
REM GACRADRG 200
REM GACRADCS 0.3
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 1
REM GACLNDNV 2
REM GACLIMIT 0
REM GACSNDCH 0
REM GACRCVCH 1 2
REM GACRADSD 3.14
REM GACRADEL 1

IDENTIFY "US_Independence_class[FLIGHT0]"

CATEGORY UTILITY
AFTBURNR FALSE              #HAVE AFTERBURNER?
THRAFTBN  0.0t                #THRUST WITH AFTERBURNER
THRMILIT  90t                #THRUST AT MILITARY POWER
THRSTREV 0.17                 #EFFICIENCY OF THRUST REVERSER
WEIGHCLN 1900t             #WEIGHT CLEAN
WEIGFUEL  0.1t                #WEIGHT OF FUEL
WEIGLOAD  10000000000.0t               #WEIGHT OF PAYLOAD
FUELABRN 0.00001kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.00001kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNSIGHT FALSE
THRSTREV 0.6                 #EFFICIENCY OF THRUST REVERSER

COCKPITP  -0m  13.29m 14.88m        #COCKPIT POSITION 
LEFTGEAR -14.73m -5.62m -60m  #LEFT LANDING GEAR POSITION
RIGHGEAR  14.73m -5.62m -60m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -5.62m  60m  #WHEEL POSITION
ARRESTER  0.0m 0m 0m
MACHNGUN  0.0m 10.20m  45.50m  #MACHINE GUN POSITION
SMOKEGEN 0.00m 0m 0m  #SMOKE GENERATOR POSITION
VAPORPO0  0m  0m 0m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  0m  0m 0m  #VAPOR POSITION (Wings Spread)
HTRADIUS  120.0m                #OUTSIDE SPHERE RADIUS
RADARCRS 0.3

TRIGGER1 RKT
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 160

CRITAOAP 0deg               #CRITICAL AOA POSITIVE
CRITAOAM 0deg               #CRITICAL AOA NEGATIVE

CRITSPED 20kt                #CRITICAL SPEED
MAXSPEED 47kt                #MAXIMUM SPEED

HASSPOIL FALSE                 #HAS SPOILER?
RETRGEAR FALSE                #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.0                #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.0                #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.0                #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                #EFFECT OF SPOILER FOR Cd

WINGAREA 1m^2               #WING AREA

MXIPTAOA 0.01deg  
MXIPTSSA 5deg
MXIPTROL 0.01deg            #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 10kt               #MINIMUM MANEUVABLE SPEED
MANESPD2 25kt               #FULLY MANEUVABLE SPEED
CPITMANE 0
CPITSTAB 50
CYAWMANE 1.6 
CYAWSTAB 6.2 
CROLLMAN 40                #ROLL MANEUVABILITY CONSTANT

CTLLDGEA FALSE              
CTLBRAKE FALSE
CTLSPOIL 0.0
CTLABRNR FALSE    
CTLTHROT 0.0      
CTLIFLAP 0.0       
CTLINVGW 0.0        
CTLATVGW FALSE

POSITION 0m 1ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

LMTBYHDP TRUE

GUNPOWER 0

INITIGUN 0                 #INITIAL GUN

NMTURRET 2
REM Bofors57mm
REM CANNONPW 0 7
REM CANNONRG 0 22000m
REM GSCRAMNO 1
REM GSCRAMAM 1 21

TURRETPO 0 0m 8.98m 42.06m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 0 -10deg 77deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 -140deg 140deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 1500                              # Ammo
TURRETIV 0 0.27sec                           # Number ShootingInterval
TURRETNP 0 0004
TURRETNH 0 0002 
TURRETAR 0                                   # Anti Air Capable
TURRETCT 0 PILOT                             # Controlled by the pilot
TURRETRG 0 0m

TURRETPO 1 0m 17.7m -13.75m 180deg 0deg 0deg      # Number x y z h p b
TURRETPT 1 -10deg 85deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 1 -135deg 135deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 1 0                              # Ammo
TURRETIV 1 0.27sec                           # Number ShootingInterval
TURRETNP 1 0006
TURRETNH 1 0005 
TURRETAR 1                                   # Anti Air Capable
TURRETCT 1 GUNNER                             # Controlled by the pilot
TURRETRG 1 0m

AAMVISIB FALSE

AGMVISIB FALSE

BOMVISIB FALSE

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 40kt                 #CRUISING SPEED
REFACRUS 0ft               #CRUISING ALTITUDE
REFTCRUS 0.0                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 20kt                 #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 700m                   #RUNWAY REQUIRED TO STOP
REFTHRLD 0.0                  #LANDING THROTTLE SETTING

AUTOCALC
