REM GACGLIMT 10
REM GACRADRG 50
REM GACRADCS 0.1
REM GACECMRG 0
REM GACAEWTR 0
REM GACAEWRC 1
REM GACLNDNV 3
REM GACLIMIT 2
REM GACSNDCH 0
REM GACRCVCH 3 4 7 8
REM GACRADSD 3.14
REM GACRADEL 1

IDENTIFY "RU_Akula_class"
CATEGORY UTILITY
AFTBURNR FALSE              #HAVE AFTERBURNER?
PROPELLR 100000HP             #PROPELLER ENGINE
PROPEFCY 0.12                #Propeller efficiency
WEIGHCLN 6000t             #WEIGHT CLEAN
WEIGFUEL  0.1t                #WEIGHT OF FUEL
WEIGLOAD  10000000000.0t               #WEIGHT OF PAYLOAD
FUELABRN 0.00001kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.00001kg              #FUEL CONSUMPTION AT MILITARY POWER
GUNSIGHT FALSE
THRSTREV 0.1                 #EFFICIENCY OF THRUST REVERSER

COCKPITP 0.29m 6.13m -2.11m     #COCKPIT POSITION
EXCAMERA "SLBM" 0m  400m  50000m 0deg -90deg 0deg OUTSIDE
LEFTGEAR -6.3m -0.01m -78.2m     #LEFT LANDING GEAR POSITION
RIGHGEAR  6.3m -0.01m -78.2m     #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -0.01m 61.5m    #WHEEL POSITION

SMOKEGEN 0m 0m 0m             #SMOKE GENERATOR POSITION
VAPORPO0  0m  0m 0m           #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  0m  0m 0m           #VAPOR POSITION (Wings Spread)
HTRADIUS  100.0m               #OUTSIDE SPHERE RADIUS
RADARCRS 0.2                  #RADAR CROSS SECTION

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK
STRENGTH 70

CRITAOAP  0deg               #CRITICAL AOA POSITIVE
CRITAOAM 0deg               #CRITICAL AOA NEGATIVE

CRITSPED 15kt              #CRITICAL SPEED
MAXSPEED 27kt              #MAXIMUM SPEED

HASSPOIL FALSE                 #HAS SPOILER?
RETRGEAR FALSE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.8                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 1m^2                #WING AREA

MXIPTAOA 1.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 1.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 2kt               #MINIMUM MANEUVABLE SPEED
MANESPD2 27kt               #FULLY MANEUVABLE SPEED
CPITMANE 0.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 50                  #PITCH STABILITY CONSTANT
CYAWMANE 1.2                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 5.0                  #YAW STABILITY CONSTANT
CROLLMAN 40                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 1.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

HRDPOINT 0m -15e9m -1e9m AIM9
HRDPOINT 0m -15e9m -1e9m AIM9
HRDPOINT 0m -15e9m -1e9m AIM9
HRDPOINT 0m -15e9m -1e9m AIM9
HRDPOINT 0m -15e9m -1e9m AIM9
HRDPOINT 0m -15e9m -1e9m AIM9
HRDPOINT 0m -15e9m -1e9m AIM9
HRDPOINT 0m -15e9m -1e9m AIM9

HRDPOINT 0m  1000m  50000m B500
HRDPOINT 0m  1000m  50000m B500
HRDPOINT 0m  1000m  50000m B500
HRDPOINT 0m  1000m  50000m B500
HRDPOINT 0m  1000m  50000m B500

HRDPOINT 0m  1000m  50000m B500
HRDPOINT 0m  1000m  50000m B500
HRDPOINT 0m  1000m  50000m B500
HRDPOINT 0m  1000m  50000m B500
HRDPOINT 0m  1000m  50000m B500

HRDPOINT 0m  1000m  50000m B500
HRDPOINT 0m  1000m  50000m B500
HRDPOINT 0m  1000m  50000m B500
HRDPOINT 0m  1000m  50000m B500
HRDPOINT 0m  1000m  50000m B500

HRDPOINT 0m  1000m  50000m B500
HRDPOINT 0m  1000m  50000m B500
HRDPOINT 0m  1000m  50000m B500
HRDPOINT 0m  1000m  50000m B500
HRDPOINT 0m  1000m  50000m B500

HRDPOINT 0m 1m 41.40m RKT
HRDPOINT 0m 1m 41.40m RKT

MAXNMRKT 22
LMTBYHDP TRUE
INITIGUN 0
LOADWEPN AIM9 8
LOADWEPN RKT 22
LOADWEPN B500 20

AAMVISIB FALSE
AGMVISIB FALSE
RKTVISIB FALSE
BOMVISIB FALSE

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 27kt              #CRUISING SPEED
REFACRUS 100ft              #CRUISING ALTITUDE
REFTCRUS 0.0                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 13kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 100m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.0                  #LANDING THROTTLE SETTING

AUTOCALC