HRDPOINT 20.41m 12.09m 110.36m AIM120
HRDPOINT -23.1m 12.09m -132.86m AIM120

MAXNMRKT 0
LMTBYHDP TRUE
INITIGUN 0
LOADWEPN AIM120 16
LOADWEPN B500 0
LOADWEPN RKT 0

AAMVISIB FALSE
AGMVISIB FALSE
RKTVISIB FALSE
BOMVISIB FALSE

NMTURRET 5
REM GSCRAMNO 3 4
REM GSCRAMAM 3 21
REM GSCRAMAM 4 21

TURRETPO 0 20.77m 11.52m 101.49m -90deg 0deg 0deg      # Number x y z h p b
TURRETPT 0 -5deg 85deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 -48deg 61deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 100000                              # Ammo
TURRETIV 0 0.01sec                           # Number ShootingInterval
TURRETNP 0 00071
TURRETNH 0 00061 
TURRETAR 0                                   # Anti Air Capable
TURRETCT 0 GUNNER                             # Controlled by the pilot
TURRETRG 0 2500m

TURRETPO 1 -20.5m 2.81m -145.19m 90deg 0deg 0deg      # Number x y z h p b
TURRETPT 1 -5deg 85deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 1 -27deg 99deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 1 100000                              # Ammo
TURRETIV 1 0.01sec                           # Number ShootingInterval
TURRETNP 1 00072
TURRETNH 1 00062 
TURRETAR 1                                   # Anti Air Capable
TURRETCT 1 GUNNER                              # Controlled by the pilot
TURRETRG 1 2500m

TURRETPO 2 11.68m 2.2m -151.19m 180deg 0deg 0deg      # Number x y z h p b
TURRETPT 2 -5deg 85deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 2 -90deg 90deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 2 100000                              # Ammo
TURRETIV 2 0.01sec                           # Number ShootingInterval
TURRETNP 2 00073
TURRETNH 2 00063 
TURRETAR 2                                   # Anti Air Capable
TURRETCT 2 GUNNER                              # Controlled by the pilot
TURRETRG 2 2500m

TURRETPO 3 -21.65m 11.02m 111.55m 90deg 0deg 0deg      # Number x y z h p b
TURRETPT 3 -5deg 85deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 3 -105deg 54deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 3 0                              # Ammo
TURRETIV 3 0.01sec                           # Number ShootingInterval
TURRETNP 3 00051
TURRETNH 3 00041 
TURRETAR 3                                   # Anti Air Capable
TURRETCT 3 GUNNER                              # Controlled by the pilot
TURRETRG 3 0m

TURRETPO 4 20.17m 13.88m -143.69m -90deg 0deg 0deg      # Number x y z h p b
TURRETPT 4 -5deg 85deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 4 -90deg 41deg 0deg               # Number MinHdg MaxHdg NeutralHdg
