IDENTIFY "US_Nimiz_class"
CATEGORY UTILITY
AFTBURNR FALSE              #HAVE AFTERBURNER?
PROPELLR 100000HP             #PROPELLER ENGINE
PROPEFCY 0.15                #Propeller efficiency
WEIGHCLN 10000t             #WEIGHT CLEAN
WEIGFUEL  0.1t                #WEIGHT OF FUEL
WEIGLOAD  10000000000.0t               #WEIGHT OF PAYLOAD
FUELABRN 0.00001kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.00001kg                #FUEL CONSUMPTION AT MILITARY POWER
GUNSIGHT FALSE
THRSTREV 0.6                 #EFFICIENCY OF THRUST REVERSER

COCKPITP 27.03m 26.05m -31.25m  
EXCAMERA "SSM" 0m  400m  10000m 0deg -90deg 0deg OUTSIDE
LEFTGEAR -15.0m -6.79m -15.0m    
RIGHGEAR  15.0m -6.79m -15.0m     
WHELGEAR  0.0m -6.79m 15.0m
ARRESTER  0.0m 0m 0m
GUNDIREC  0.0m 0.0m 0.0m     #MACHINE GUN DIRECTION
SMOKEGEN 0.00m 0m -0m  #SMOKE GENERATOR POSITION
VAPORPO0  0m  0m 0m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  0m  0m 0m  #VAPOR POSITION (Wings Spread)
HTRADIUS  175m                #OUTSIDE SPHERE RADIUS
RADARCRS 2

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK
STRENGTH 360

CRITAOAP 0deg 
CRITAOAM 0deg
  
CRITSPED 16kt
MAXSPEED 29kt

HASSPOIL FALSE                 #HAS SPOILER?
RETRGEAR FALSE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 1m^2                #WING AREA

MXIPTAOA 0.01deg  
MXIPTSSA 5deg
MXIPTROL 0.01deg

BMBAYRCS 1.0
BOMINBAY FALSE

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 15kt
MANESPD2 31kt  
CPITMANE 0
CPITSTAB 60  
CYAWMANE 0.9 
CYAWSTAB 6.2 
CROLLMAN 50

CTLLDGEA FALSE              
CTLBRAKE FALSE
CTLSPOIL 0.0
CTLABRNR FALSE    
CTLTHROT 0.0      
CTLIFLAP 0.0       
CTLINVGW 0.0        
CTLATVGW FALSE

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

HRDPOINT 20.41m 12.09m 110.36m AIM120
HRDPOINT -23.1m 12.09m -132.86m AIM120
HRDPOINT 20.41m 12.09m 110.36m AIM120
HRDPOINT -23.1m 12.09m -132.86m AIM120
HRDPOINT 20.41m 12.09m 110.36m AIM120
HRDPOINT -23.1m 12.09m -132.86m AIM120
HRDPOINT 20.41m 12.09m 110.36m AIM120
HRDPOINT -23.1m 12.09m -132.86m AIM120
HRDPOINT 20.41m 12.09m 110.36m AIM120
HRDPOINT -23.1m 12.09m -132.86m AIM120
HRDPOINT 20.41m 12.09m 110.36m AIM120
