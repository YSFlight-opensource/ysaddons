HRDPOINT 1.59m 2.49m 21.32m AIM9
HRDPOINT -1.59m 2.49m 21.32m AIM9
HRDPOINT 1.59m 2.49m 21.32m AIM9
HRDPOINT -1.59m 2.49m 21.32m AIM9
HRDPOINT 1.59m 2.49m 21.32m AIM9
HRDPOINT -1.59m 2.49m 21.32m AIM9

HRDPOINT 0.00m 100m  10000m B500
HRDPOINT 0.00m 100m  10000m B500
HRDPOINT 0.00m 100m  10000m B500
HRDPOINT 0.00m 100m  10000m B500
HRDPOINT 0.00m 100m  10000m B500
HRDPOINT 0.00m 100m  10000m B500
HRDPOINT 0.00m 100m  10000m B500
HRDPOINT 0.00m 100m  10000m B500


MAXNMRKT 24
LMTBYHDP TRUE
INITIGUN 0
LOADWEPN AIM9 24
LOADWEPN B500 8

AAMVISIB FALSE
AGMVISIB FALSE
RKTVISIB FALSE
BOMVISIB FALSE

NMTURRET 4
REM A-190 100mm
REM CANNONPW 0 11
REM CANNONRG 0 20000m
REM GSCRAMAM 1 48

TURRETPO 0 0m 0.46m 29.04m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 0 -2deg 80deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 -98deg 98deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 1000                              # Ammo
TURRETIV 0 1.6sec                           # Number ShootingInterval
TURRETNP 0 0011
TURRETNH 0 0010 
TURRETAR 0                                   # Anti Air Capable
TURRETCT 0 PILOT                             # Controlled by the pilot
TURRETRG 0 0m

TURRETPO 1 0m 1.86m 20.67m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 1 -5deg 89deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 1 -108deg 108deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 1 100000                              # Ammo
TURRETIV 1 0.01sec                           # Number ShootingInterval
TURRETNP 1 0007
TURRETNH 1 0005 
TURRETAR 1                                   # Anti Air Capable
TURRETCT 1 GUNNER                             # Controlled by the pilot
TURRETRG 1 3500m

TURRETPO 2 -4.09m 2.34m -13.57m 90deg 0deg 0deg      # Number x y z h p b
TURRETPT 2 -5deg 83deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 2 -75deg 90deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 2 100000                              # Ammo
TURRETIV 2 0.02sec                           # Number ShootingInterval
TURRETNP 2 0018
TURRETNH 2 0017 
TURRETAR 2                                   # Anti Air Capable
TURRETCT 2 GUNNER                             # Controlled by the pilot
TURRETRG 2 3000m

TURRETPO 3 4.09m 2.34m -13.57m -90deg 0deg 0deg      # Number x y z h p b
TURRETPT 3 -5deg 83deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 3 -90deg 75deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 3 100000                              # Ammo
TURRETIV 3 0.02sec                           # Number ShootingInterval
TURRETNP 3 00182
TURRETNH 3 00172 
TURRETAR 3                                   # Anti Air Capable
TURRETCT 3 GUNNER                             # Controlled by the pilot
TURRETRG 3 3000m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 27kt              #CRUISING SPEED
REFACRUS 50ft              #CRUISING ALTITUDE
REFTCRUS 0.0                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 20kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 100m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.0                  #LANDING THROTTLE SETTING

AUTOCALC
