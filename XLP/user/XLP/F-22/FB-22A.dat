VAPORPO1 -7.30m -0.355m -8.45m  #VAPOR POSITION (Wings Spread)
HTRADIUS  14.0m                 #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
