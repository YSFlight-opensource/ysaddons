IDENTIFY "F-5EM_TIGERII(FAB)"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  4.5t                #THRUST WITH AFTERBURNER
THRMILIT  3.2t                #THRUST AT MILITARY POWER
WEIGHCLN  4.4t                #WEIGHT CLEAN
WEIGFUEL  2.0t                #WEIGHT OF FUEL
WEIGLOAD  3.2t                #WEIGHT OF PAYLOAD
FUELABRN 4.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.4kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.87m  2.83m  #COCKPIT POSITION
LEFTGEAR -1.9m -1.495m -1.0m #LEFT LANDING GEAR POSITION
RIGHGEAR  1.9m -1.495m -1.0m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.498m  4.15m  #WHEEL POSITION
ARRESTER  0.00m -1.0m -4.0m  #ARRESTING FOOK
MACHNGUN  0.24m  0.33m  5.9m  #MACHINE GUN POSITION
MACHNGN2 -0.24m  0.33m  5.9m  #MACHINE GUN POSITION
SMOKEGEN  0.00m -0.19m -5.00m  #SMOKE GENERATOR POSITION
VAPORPO0  3.84m -0.19m -2.15m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  3.84m -0.19m -2.15m  #VAPOR POSITION (Wings Spread)
HTRADIUS  8.0m                 #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 9

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.8MACH              #CRITICAL SPEED
MAXSPEED 1.5MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?
VGWSPED1 0kt
VGWSPED2 0kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 17m^2                #WING AREA

MXIPTAOA 25.0deg              #MAX INPUT AOA
MXIPTSSA 4.0deg               #MAX INPUT SSA
MXIPTROL 180.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                #MINIMUM MANEUVABLE SPEED
MANESPD2 160kt                #FULLY MANEUVABLE SPEED
CPITMANE 8.5                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 4.5                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED


HRDPOINT  3.92m -0.19m -0.7m AIM9 
HRDPOINT -3.92m -0.19m -0.7m AIM9 

HRDPOINT  3.06m -0.70m -0.2m AIM9 AGM65 B500 RKT
HRDPOINT -3.06m -0.70m -0.2m AIM9 AGM65 B500 RKT 

HRDPOINT  2.29m -0.75m  -0.4m AIM9 B500
HRDPOINT -2.29m -0.75m  -0.4m AIM9 B500

HRDPOINT  0.3m -0.8m  1.1m  B500
HRDPOINT -0.3m -0.8m  1.1m  B500
HRDPOINT  0.0m  -1m   1.1m  B500

LMTBYHDP TRUE

INITIGUN 560                  #INITIAL GUN
GUNINTVL  0.04
LOADWEPN AIM9 2              #INITIAL AAM
LOADWEPN AGM65 2          
MAXNMRKT 38

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.75MACH              #CRUISING SPEED
REFACRUS 10980ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 110kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 700m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
