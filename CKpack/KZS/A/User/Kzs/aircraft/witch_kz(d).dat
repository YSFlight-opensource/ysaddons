REM Witch ride on the Magic-Broom(little-girl version [Cyan])

REM "witch_kz(d).dat/witch_kz(d).dnm/witch_kz_cockpit.srf/witch_kz(d)_coll.srf/witch_kz(d)_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2002-2003 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_WITCH/D"
CATEGORY NORMAL

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  4.50t               #THRUST WITH AFTERBURNER
THRMILIT  2.00t               #THRUST AT MILITARY POWER
WEIGHCLN  0.10t               #WEIGHT CLEAN
WEIGFUEL  0.50t               #WEIGHT OF FUEL
WEIGLOAD  5.00t               #WEIGHT OF PAYLOAD
FUELABRN  0.35kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.05kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.000m  0.470m  0.300m #COCKPIT POSITION
LEFTGEAR -0.235m -1.001m -0.166m #LEFT LANDING GEAR POSITION
RIGHGEAR  0.235m -1.001m -0.166m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.000m -1.001m  0.180m #WHEEL POSITION
ARRESTER  0.000m -0.950m  0.000m #ARRESTING HOOK POSITION
MACHNGUN  0.000m  0.000m  1.110m #MACHINE GUN POSITION
SMOKEGEN  0.000m -0.075m -0.395m #SMOKE GENERATOR POSITION
VAPORPO0  0.362m -0.055m -0.318m #VAPOR POSITION (Wings Swept back)
VAPORPO1  0.362m -0.055m -0.318m #VAPOR POSITION (Wings Spread)
HTRADIUS  1.632m                 #OUTSIDE SPHERE RADIUS

STRENGTH 2

CRITAOAP  32.5deg             #CRITICAL AOA POSITIVE
CRITAOAM -18.0deg             #CRITICAL AOA NEGATIVE

CRITSPED 280kt                #CRITICAL SPEED
MAXSPEED 420kt                #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

VGWSPED1 0kt
VGWSPED2 1kt

CLVARGEO 0.00                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.00                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.60                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.01                 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.10                 #EFFECT OF GEAR FOR Cd
CDSPOILR 1.25                 #EFFECT OF SPOILER FOR Cd

WINGAREA 1.00m^2              #WING AREA

MXIPTAOA  23.0deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 20kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 30kt                 #FULLY MANEUVABLE SPEED
CPITMANE 12.5                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  2.0                 #PITCH STABILITY CONSTANT
CYAWMANE  5.0                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB  3.0                 #YAW STABILITY CONSTANT
CROLLMAN  3.0                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 6

INITIGUN 4000                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

MAXNMAAM 4
MAXNMAGM 2
MAXNBOMB 0
MAXNMRKT 25

AAMVISIB FALSE
AAMSLOT_  0.00m -0.30m 0.80m

AGMVISIB FALSE
AGMSLOT_  0.00m -0.30m 0.80m

BOMVISIB FALSE
BOMBSLOT  0.00m -0.30m 0.80m

RKTVISIB FALSE
RKTSLOT_  0.00m -0.30m 0.80m

RADARCRS 0.05                 #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 320kt                #CRUISING SPEED
REFACRUS 2500ft               #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 105kt                #LANDING SPEED [70kt]
REFAOALD 25deg                #LANDING AOA                 [max:67.27deg,visual-max:30deg]
REFLNRWY 100m                 #RUNWAY REQUIRED TO STOP [15m]
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
