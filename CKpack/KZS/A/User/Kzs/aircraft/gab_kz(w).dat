REM Angel - White Version

REM "gab_kz(w).dat/gab_kz(w).dnm/gab_kz_cockpit.srf/gab_kz_coll.srf/gab_kz(w)_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2002-2003 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_ANGEL"
CATEGORY NORMAL

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  4.00t               #THRUST WITH AFTERBURNER
THRMILIT  2.00t               #THRUST AT MILITARY POWER
WEIGHCLN  0.05t               #WEIGHT CLEAN
WEIGFUEL  0.50t               #WEIGHT OF FUEL
WEIGLOAD  3.00t               #WEIGHT OF PAYLOAD
FUELABRN  0.20kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.02kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.000m  0.285m  0.535m #COCKPIT POSITION
LEFTGEAR -0.088m -1.091m -0.055m #LEFT LANDING GEAR POSITION
RIGHGEAR  0.088m -1.091m -0.055m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.000m -1.091m  0.165m #WHEEL POSITION
ARRESTER  0.000m -1.085m -0.060m #ARRESTING HOOK POSITION
MACHNGUN  0.302m  0.119m  1.353m #MACHINE GUN POSITION
SMOKEGEN -0.496m -0.659m -0.301m #SMOKE GENERATOR POSITION
VAPORPO0  1.426m  0.393m  0.062m #VAPOR POSITION (Wings Swept back)
VAPORPO1  1.426m  0.393m  0.062m #VAPOR POSITION (Wings Spread)
HTRADIUS  1.750m                 #OUTSIDE SPHERE RADIUS

STRENGTH 30

CRITAOAP  33deg               #CRITICAL AOA POSITIVE
CRITAOAM -17deg               #CRITICAL AOA NEGATIVE

CRITSPED 300kt                #CRITICAL SPEED
MAXSPEED 500kt                #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING? [TRUE]

VGWSPED1 0kt
VGWSPED2 1kt

CLVARGEO 0.00                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.00                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.475                #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.01                 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.30                 #EFFECT OF GEAR FOR Cd
CDSPOILR 1.25                 #EFFECT OF SPOILER FOR Cd

WINGAREA 2m^2                 #WING AREA

MXIPTAOA  25.0deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 25kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 40kt                 #FULLY MANEUVABLE SPEED
CPITMANE  9.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  1.5                 #PITCH STABILITY CONSTANT
CYAWMANE  5.0                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB  3.0                 #YAW STABILITY CONSTANT
CROLLMAN  3.0                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 1.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 10

INITIGUN 10000                #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

MAXNMAAM 8
MAXNMAGM 6
MAXNBOMB 2
MAXNMRKT 38

REM Maximum missile/bomb(10) + rocket(38) + SMOKE

AAMVISIB FALSE
AAMSLOT_ 0.302m  0.119m  3.353m

AGMVISIB FALSE
AGMSLOT_ 0.302m  0.119m  3.353m

BOMVISIB FALSE
BOMBSLOT  0.00m -0.50m  0.050m

RKTVISIB FALSE
RKTSLOT_ 0.302m  0.119m  3.353m



REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 380kt                #CRUISING SPEED
REFACRUS 5000ft               #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 120kt                #LANDING SPEED [80kt]
REFAOALD 15.0deg              #LANDING AOA [22.5deg]
REFLNRWY 60m                  #RUNWAY REQUIRED TO STOP [20m]
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
