REM Drone-Target - Raytheon (Beech) MQM-107D Streaker

REM "mqm107d_kz.dat/mqm107d_kz.dnm/mqm107d_kz_coll.srf/mqm107d_kz_cockpit.srf"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2003 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_MQM-107D_Streaker"
CATEGORY FIGHTER

AFTBURNR FALSE                #HAVE AFTERBURNER?
THRAFTBN 0.00lb               #THRUST WITH AFTERBURNER
THRMILIT  960lb               #THRUST AT MILITARY POWER [960lb : Teledyne CAE J402-CA-702]
WEIGHCLN 0.261t               #WEIGHT CLEAN
WEIGFUEL 0.025t               #WEIGHT OF FUEL
WEIGLOAD 0.251t               #WEIGHT OF PAYLOAD
FUELABRN 0.006kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.003kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.00m  2.25m #COCKPIT POSITION
LEFTGEAR -0.75m -1.00m -1.50m #LEFT LANDING GEAR POSITION
RIGHGEAR  0.75m -1.00m -1.50m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.50m  1.50m #WHEEL POSITION
ARRESTER  0.00m -0.88m -2.24m #ARRESTING HOOK POSITION
MACHNGUN  0.00m -0.29m  1.50m #MACHINE GUN POSITION
SMOKEGEN  0.00m -0.35m -1.50m #SMOKE GENERATOR POSITION
VAPORPO0  1.50m -0.33m -0.75m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  1.50m -0.33m -0.75m #VAPOR POSITION (Wings Spread)
HTRADIUS  3.50m               #OUTSIDE SPHERE RADIUS

STRENGTH 8

CRITAOAP  20.0deg             #CRITICAL AOA POSITIVE
CRITAOAM -15.0deg             #CRITICAL AOA NEGATIVE

CRITSPED 400kt                #CRITICAL SPEED
MAXSPEED 548kt                #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR FALSE                #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

VGWSPED1 150kt
VGWSPED2 300kt

CLVARGEO 0.2                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.0                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 1.75m^2              #WING AREA

MXIPTAOA  15.0deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 50kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  1.5                 #PITCH STABILITY CONSTANT
CYAWMANE  5.0                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB  3.0                 #YAW STABILITY CONSTANT
CROLLMAN  3.0                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 1

INITIGUN 0                    #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOM
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

SMOKEOIL 100.0kg
WEAPONCH SMK

MAXNMAAM 2
MAXNMAGM 2
MAXNBOMB 2
MAXNMRKT 12

AAMVISIB FALSE
AAMSLOT_ -0.75m -0.75m -0.75m
AAMSLOT_  0.75m -0.75m -0.75m

AGMVISIB FALSE
AGMSLOT_ -0.75m -0.75m -0.75m
AGMSLOT_  0.75m -0.75m -0.75m

BOMVISIB FALSE
BOMBSLOT -0.75m -0.75m -0.75m
BOMBSLOT  0.75m -0.75m -0.75m

RKTVISIB FALSE
RKTSLOT_ -0.75m -0.75m -0.75m
RKTSLOT_  0.75m -0.75m -0.75m

REM RADARCRS 0.75             #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 525kt                #CRUISING SPEED
REFACRUS 1250ft               #CRUISING ALTITUDE
REFTCRUS 0.5                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 90kt                 #LANDING SPEED
REFAOALD 15deg                #LANDING AOA
REFLNRWY 2000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
