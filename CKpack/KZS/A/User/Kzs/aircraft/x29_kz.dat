REM Grumman X-29 aircraft no.1

REM "x29_kz.dat/x29_kz.dnm/x29_kz_coll.srf/x29_kz_cockpit.srf/x29_kz_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2004 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_X-29_#1"
CATEGORY UTILITY

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 7.248t               #THRUST WITH AFTERBURNER (F404-GE-400)
THRMILIT 4.810t               #THRUST AT MILITARY POWER
WEIGHCLN 6.1608t              #WEIGHT CLEAN
WEIGFUEL 1.812t               #WEIGHT OF FUEL
WEIGLOAD 2.000t               #WEIGHT OF PAYLOAD
FUELABRN 3.25kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.50kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.62m  2.77m  #COCKPIT POSITION
LEFTGEAR -1.16m -1.70m -1.40m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.16m -1.70m -1.40m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.68m  4.25m  #WHEEL POSITION
ARRESTER  0.00m -1.65m -5.55m  #ARRESTING HOOK POSITION
MACHNGUN -0.50m -0.85m  1.00m  #MACHINE GUN POSITION
MACHNGN2  0.50m -0.85m  1.00m  #MACHINE GUN POSITION2
SMOKEGEN  0.00m  0.55m -7.20m  #SMOKE GENERATOR POSITION
VAPORPO0  4.12m -0.527m -0.06m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.12m -0.527m -0.06m #VAPOR POSITION (Wings Spread)
HTRADIUS  8.00m                #OUTSIDE SPHERE RADIUS

STRENGTH 8

CRITAOAP  45deg               #CRITICAL AOA POSITIVE
CRITAOAM -20deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.85MACH             #CRITICAL SPEED
MAXSPEED 1.60MACH             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

VGWSPED1  75kt
VGWSPED2 225kt

CLVARGEO 0.20                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.05                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.08                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.20                 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.50                 #EFFECT OF GEAR FOR Cd
CDSPOILR 2.00                 #EFFECT OF SPOILER FOR Cd

WINGAREA 18.00m^2             #WING AREA[???]

MXIPTAOA  23.0deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA [8.0deg]
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 80kt                 #FULLY MANEUVABLE SPEED
CPITMANE 12.5                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  1.75                #PITCH STABILITY CONSTANT
CYAWMANE  6.5                 #YAW MANEUVABILITY CONSTANT [4.5]
CYAWSTAB  2.5                 #YAW STABILITY CONSTANT     [1.5]
CROLLMAN  3.5                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 0deg 0deg 0deg       #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 0

INITIGUN 0                    #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOM
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

SMOKEOIL 0.0kg
WEAPONCH SMK

MAXNMAAM 0
MAXNMAGM 0
MAXNBOMB 0
MAXNMRKT 0

AAMVISIB TRUE
AAMSLOT_ -2.40m -0.75m -1.25m
AAMSLOT_  2.40m -0.75m -1.25m

AGMVISIB TRUE
AGMSLOT_ -2.40m -0.75m -1.25m
AGMSLOT_  2.40m -0.75m -1.25m

BOMVISIB TRUE
BOMBSLOT -2.40m -0.75m -1.25m
BOMBSLOT  2.40m -0.75m -1.25m

RKTVISIB TRUE
RKTSLOT_ -2.40m -0.75m -1.25m
RKTSLOT_  2.40m -0.75m -1.25m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.00MACH             #CRUISING SPEED
REFACRUS 30000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 90kt                 #LANDING SPEED
REFAOALD 11.25deg             #LANDING AOA                              [TailStrikeAngle:12.9364deg]
REFLNRWY 1000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.30                 #LANDING THROTTLE SETTING

AUTOCALC
