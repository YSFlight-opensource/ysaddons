REM Thunder Bird TB-3 - VTOL(?) Version

REM "tb3_kz(v).dat/tb3_kz.dnm/tb3_kz_cockpit.srf/tb3_kz_coll.srf/tb3_kz_coarse.dnm"
REM     were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2003 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_ThunderBird_3/V"
CATEGORY UTILITY

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 885.00t              #THRUST WITH AFTERBURNER
THRMILIT 200.00t              #THRUST AT MILITARY POWER
WEIGHCLN 135.00t              #WEIGHT CLEAN
WEIGFUEL  20.00t              #WEIGHT OF FUEL
WEIGLOAD 465.00t              #WEIGHT OF PAYLOAD
FUELABRN 20.00kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  5.25kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP   0.00m   0.30m  37.90m #COCKPIT POSITION
LEFTGEAR -12.25m -10.50m -43.75m #LEFT LANDING GEAR POSITION
RIGHGEAR  12.25m -10.50m -43.75m #RIGHT LANDING GEAR POSITION
WHELGEAR   0.00m  14.00m -43.76m #WHEEL POSITION
ARRESTER   0.00m   0.00m -43.70m #ARRESTING HOOK POSITION
MACHNGUN   0.00m  -4.50m  38.00m #MACHINE GUN POSITION
SMOKEGEN   0.00m   0.00m -35.50m #SMOKE GENERATOR POSITION
VAPORPO0   6.65m   0.00m -38.25m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1   6.65m   0.00m -38.25m #VAPOR POSITION (Wings Spread) [14.02m  -6.76m -29.00m]
HTRADIUS  28.00m                 #OUTSIDE SPHERE RADIUS

STRENGTH 50

CRITAOAP  20.0deg             #CRITICAL AOA POSITIVE
CRITAOAM -20.0deg             #CRITICAL AOA NEGATIVE

CRITSPED  1.75MACH            #CRITICAL SPEED
MAXSPEED 32.47MACH            #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

REM TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?
REM TRSTDIR0 0.0m 0.00m 1.0m      #Thrust vector 0
REM TRSTDIR1 0.0m 1.00m 0.0m      #Thrust vector 1
REM PSTMPTCH  6deg                #Post-Stall Maneuver VPitch
REM PSTMYAW_ 10deg                #Post-Stall Maneuver VYaw
REM PSTMROLL  5deg                #Post-Stall Maneuver VRoll

VGWSPED1 -1kt
VGWSPED2 0kt

CLVARGEO 0.00                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.00                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.40                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.10                 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.00                 #EFFECT OF GEAR FOR Cd
CDSPOILR 40.0                 #EFFECT OF SPOILER FOR Cd

WINGAREA 420.00m^2            #WING AREA

MXIPTAOA  18.0deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 300.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 80kt                 #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 6.0                  #PITCH STABILITY CONSTANT
CYAWMANE 3.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 5.0                  #YAW STABILITY CONSTANT
CROLLMAN 2.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 41.01ft 0m        #POSITION
ATTITUDE 0deg 0deg 0deg       #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 2
SMOKEOIL 100.0kg
WEAPONCH SMK

INITIGUN 0                    #INITIAL GUN
INITIAAM 0                    #INITIAL AAM

REM MAXNMAAM 40
REM MAXNMAGM 50
REM MAXNBOMB 80
REM MAXNMRKT 100

AAMVISIB FALSE
AAMSLOT_ 0.00m -5.50m 22.63m

AGMVISIB FALSE
AGMSLOT_ 0.00m -5.50m 22.63m

BOMVISIB FALSE
BOMBSLOT 0.00m -5.50m 22.63m

RKTVISIB FALSE
RKTSLOT_ 0.00m -5.50m 22.63m

REM RADARCRS 0.1              #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 16.25MACH            #CRUISING SPEED
REFACRUS 50000ft              #CRUISING ALTITUDE
REFTCRUS 0.10                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 120kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA                  [TailStrikeAngle:N/A]
REFLNRWY 2000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.15                 #LANDING THROTTLE SETTING

AUTOCALC
