REM Junkers Ju EF009/Hubjager

REM "ef009_kz.dat/ef009_kz.dnm/ef009_kz_coll.srf/ef009_kz_cockpit.srf/ef009_kz_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2005 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_Ju-EF009"
CATEGORY FIGHTER

AFTBURNR FALSE                #HAVE AFTERBURNER?
THRAFTBN 0.00t                #THRUST WITH AFTERBURNER
THRMILIT 1.50t                #THRUST AT MILITARY POWER (Jumo ???? turbojet ? * 10)
WEIGHCLN 1.20t                #WEIGHT CLEAN
WEIGFUEL 0.10t                #WEIGHT OF FUEL
WEIGLOAD 0.50t                #WEIGHT OF PAYLOAD
FUELABRN 0.00kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.10kg               #FUEL CONSUMPTION AT MILITARY POWER (around 16.67min)

COCKPITP  0.000m  0.100m  1.650m #COCKPIT POSITION
LEFTGEAR -0.080m -0.960m  1.200m #LEFT LANDING GEAR POSITION
RIGHGEAR  0.080m -0.960m  1.200m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.000m -0.357m -2.240m #WHEEL POSITION
ARRESTER  0.000m -0.391m -1.932m #ARRESTING HOOK POSITION
MACHNGUN -0.320m -0.110m  2.500m #MACHINE GUN POSITION
MACHNGN2  0.320m  0.110m  2.500m #MACHINE GUN POSITION
SMOKEGEN  0.000m -0.210m -2.475m #SMOKE GENERATOR POSITION
VAPORPO0  2.000m -0.105m  0.000m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  2.000m -0.105m  0.000m #VAPOR POSITION (Wings Spread)
HTRADIUS  4.500m                 #OUTSIDE SPHERE RADIUS

STRENGTH 6

CRITAOAP  20.0deg             #CRITICAL AOA POSITIVE
CRITAOAM -15.0deg             #CRITICAL AOA NEGATIVE

CRITSPED 415kt                #CRITICAL SPEED
MAXSPEED 489kt                #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

REM VGWSPED1 150kt
REM VGWSPED2 300kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.35                 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.75                 #EFFECT OF SPOILER FOR Cd

WINGAREA 10.00m^2             #WING AREA

MXIPTAOA  20.0deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 300.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 80kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  2.0                 #PITCH STABILITY CONSTANT
CYAWMANE  5.0                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB  3.0                 #YAW STABILITY CONSTANT
CROLLMAN  3.0                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 1

INITIGUN 750                  #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOM
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

REM SMOKEOIL 100.0kg
REM WEAPONCH SMK

MAXNMAAM 0
MAXNMAGM 0
MAXNBOMB 0
MAXNMRKT 24

AAMVISIB FALSE
AAMSLOT_ 0.00m -0.80m 0.00m

AGMVISIB FALSE
AGMSLOT_ 0.00m -0.80m 0.00m

BOMVISIB FALSE
BOMBSLOT 0.00m -0.80m 0.00m

RKTVISIB FALSE
RKTSLOT_ -0.320m -0.11m  3.50m
RKTSLOT_  0.320m  0.11m  3.50m

REM RADARCRS 0.75             #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 415kt                #CRUISING SPEED
REFACRUS 25000ft              #CRUISING ALTITUDE
REFTCRUS 0.7                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 90kt                 #LANDING SPEED
REFAOALD 10deg                #LANDING AOA                               [TailStrikeAngle:10.700deg]
REFLNRWY 750m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
