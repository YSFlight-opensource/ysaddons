REM     were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2003 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_G-1_JetFighter"
CATEGORY FIGHTER

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 17.5t                #THRUST WITH AFTERBURNER
THRMILIT  9.5t                #THRUST AT MILITARY POWER
WEIGHCLN  4.8t                #WEIGHT CLEAN
WEIGFUEL  2.5t                #WEIGHT OF FUEL
WEIGLOAD  5.0t                #WEIGHT OF PAYLOAD
FUELABRN 2.50kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.50kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.50m  2.50m #COCKPIT POSITION
LEFTGEAR -1.60m -1.50m -1.78m #LEFT LANDING GEAR POSITION
RIGHGEAR  1.60m -1.50m -1.78m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.50m  2.02m #WHEEL POSITION
ARRESTER  0.00m -1.45m -1.85m #ARRESTING HOOK POSITION
MACHNGUN -0.41m  0.40m  0.75m #MACHINE GUN POSITION
MACHNGN2  0.41m  0.40m  0.75m #MACHINE GUN POSITION
SMOKEGEN  0.00m  0.25m -4.65m #SMOKE GENERATOR POSITION
VAPORPO0  4.50m  0.23m -5.00m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  2.76m  0.23m -5.73m #VAPOR POSITION (Wings Spread)
HTRADIUS  7.75m               #OUTSIDE SPHERE RADIUS

STRENGTH 6

CRITAOAP  22.5deg             #CRITICAL AOA POSITIVE
CRITAOAM -16.0deg             #CRITICAL AOA NEGATIVE

CRITSPED 1.15MACH             #CRITICAL SPEED
MAXSPEED 4.00MACH             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

VGWSPED1 0kt
VGWSPED2 1kt

CLVARGEO 0.00                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.00                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.25                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.05                 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.50                 #EFFECT OF GEAR FOR Cd
CDSPOILR 2.00                 #EFFECT OF SPOILER FOR Cd

WINGAREA 36.00m^2             #WING AREA

MXIPTAOA  22.5deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 80kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.00                #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  1.75                #PITCH STABILITY CONSTANT
CYAWMANE  5.00                #YAW MANEUVABILITY CONSTANT
CYAWSTAB  3.00                #YAW STABILITY CONSTANT
CROLLMAN  3.00                #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 0deg 0deg 0deg       #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 2

INITIGUN 2000                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB
INITRCKT 0                    #INITIAL ROCKET (10kg/each)
SMOKEOIL 100.0kg

WEAPONCH SMK

MAXNMAAM 0
MAXNMAGM 0
MAXNBOMB 0
MAXNMRKT 0

AAMVISIB FALSE
AAMSLOT_ 0.00m -1.00m 0.00m

AGMVISIB FALSE
AGMSLOT_ 0.00m -1.00m 0.00m

BOMVISIB FALSE
BOMBSLOT 0.00m -1.00m 0.00m

RKTVISIB FALSE
RKTSLOT_ 0.00m -1.00m 0.00m

RADARCRS 0.5                  #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.25MACH             #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.75                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 100kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA                  [TailStrikeAngle:12.87deg]
REFLNRWY 2000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.2                  #LANDING THROTTLE SETTING

AUTOCALC
