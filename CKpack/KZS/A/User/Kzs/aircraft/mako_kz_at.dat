IDENTIFY "[Kzs] Mako-AT"
CATEGORY TRAINER

REM
REM If You want real setting, please replace these values with value included in each comment.
REM     values : AFTBURNR,THRAFTBN,THRMILIT,FUELABRN,REFVCRUS
REM

AFTBURNR TRUE                 #HAVE AFTERBURNER?                       [FALSE]
THRAFTBN  7.70t               #THRUST WITH AFTERBURNER                 [0.00t]
THRMILIT  6.16t               #THRUST AT MILITARY POWER                [7.70t]
WEIGHCLN  5.80t               #WEIGHT CLEAN
WEIGFUEL  3.30t               #WEIGHT OF FUEL
WEIGLOAD  4.50t               #WEIGHT OF PAYLOAD
FUELABRN  0.30kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER [0.00kg]
FUELMILI  0.25kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.40m  3.00m #COCKPIT POSITION
REM COCKPITP  0.00m  0.70m  1.60m #COCKPIT POSITION(rear)

LEFTGEAR -1.20m -2.00m -2.00m #LEFT LANDING GEAR POSITION
RIGHGEAR  1.20m -2.00m -2.00m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -2.00m  2.87m #WHEEL POSITION
ARRESTER  0.00m -1.95m -5.50m #ARRESTING HOOK POSITION
MACHNGUN  0.90m  0.10m  1.00m #MACHINE GUN POSITION
SMOKEGEN  0.00m -0.80m -6.28m #SMOKE GENERATOR POSITION
VAPORPO0  4.12m  0.00m -3.63m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.12m  0.00m -3.63m #VAPOR POSITION (Wings Spread)
HTRADIUS  8.00m               #OUTSIDE SPHERE RADIUS

STRENGTH 6

CRITAOAP  35deg               #CRITICAL AOA POSITIVE
CRITAOAM -18deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 1.5MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

VGWSPED1  90kt
VGWSPED2 200kt

CLVARGEO 0.1                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.05                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 25m^2                #WING AREA

MXIPTAOA  23.0deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1  90kt                #MINIMUM MANEUVABLE SPEED
MANESPD2 100kt                #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 1.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 1

INITIGUN 0                    #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOM
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

MAXNMAAM 0
MAXNMAGM 0
MAXNBOMB 0
MAXNMRKT 0

REM AAMSLOT_  4.29m  0.00m -2.88m
REM AAMSLOT_ -4.29m  0.00m -2.88m
REM AAMSLOT_  3.45m -0.40m -1.85m
REM AAMSLOT_ -3.45m -0.40m -1.85m

REM AGMSLOT_  2.65m -0.80m -2.25m
REM AGMSLOT_ -2.65m -0.80m -2.25m
REM AGMSLOT_  2.90m -0.50m -2.25m
REM AGMSLOT_ -2.90m -0.50m -2.25m
REM AGMSLOT_  2.40m -0.50m -2.25m
REM AGMSLOT_ -2.40m -0.50m -2.25m

REM BOMBSLOT  1.60m -0.70m -1.70m
REM BOMBSLOT -1.60m -0.70m -1.70m
REM BOMBSLOT  1.75m -0.40m -1.70m
REM BOMBSLOT -1.75m -0.40m -1.70m
REM BOMBSLOT  1.45m -0.40m -1.70m
REM BOMBSLOT -1.45m -0.40m -1.70m

REM RKTSLOT_  1.05m -0.95m  0.45m
REM RKTSLOT_ -1.05m -0.95m  0.45m

RADARCRS 0.5                  #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.90MACH             #CRUISING SPEED                          [1.00MACH]
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 90kt                 #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 750m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.30                 #LANDING THROTTLE SETTING

AUTOCALC
