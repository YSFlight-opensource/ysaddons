IDENTIFY "[Kzs]_ARCHANGEL"
CATEGORY FIGHTER

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 40.00t               #THRUST WITH AFTERBURNER
THRMILIT 28.00t               #THRUST AT MILITARY POWER [22.50t]
WEIGHCLN  9.00t               #WEIGHT CLEAN
WEIGFUEL  0.50t               #WEIGHT OF FUEL
WEIGLOAD 30.00t               #WEIGHT OF PAYLOAD
FUELABRN  0.20kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.02kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m   2.85m  5.35m #COCKPIT POSITION
LEFTGEAR -0.88m -10.91m -0.55m #LEFT LANDING GEAR POSITION
RIGHGEAR  0.88m -10.91m -0.55m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -10.91m  1.65m #WHEEL POSITION
ARRESTER  0.00m -10.85m -0.60m #ARRESTING HOOK POSITION
MACHNGUN -3.02m   1.19m 13.53m #MACHINE GUN POSITION
SMOKEGEN  4.96m  -6.59m -3.01m #SMOKE GENERATOR POSITION
VAPORPO0 14.26m   3.93m  0.62m #VAPOR POSITION (Wings Swept back)
VAPORPO1 14.26m   3.93m  0.62m #VAPOR POSITION (Wings Spread)
HTRADIUS 15.00m                #OUTSIDE SPHERE RADIUS

STRENGTH 100

CRITAOAP  36deg               #CRITICAL AOA POSITIVE [33deg]
CRITAOAM -20deg               #CRITICAL AOA NEGATIVE [-17deg]

CRITSPED 0.8MACH              #CRITICAL SPEED
MAXSPEED 2.5MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING? [TRUE]

VGWSPED1 0kt
VGWSPED2 1kt

CLVARGEO 0.00                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.00                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.50                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.01                 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.30                 #EFFECT OF GEAR FOR Cd
CDSPOILR 1.75                 #EFFECT OF SPOILER FOR Cd

WINGAREA 100m^2               #WING AREA

MXIPTAOA  25.5deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 30kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 60kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT [9.0]
CPITSTAB  2.50                #PITCH STABILITY CONSTANT [1.25]
CYAWMANE  6.0                 #YAW MANEUVABILITY CONSTANT [5.0]
CYAWSTAB  3.0                 #YAW STABILITY CONSTANT
CROLLMAN  4.5                 #ROLL MANEUVABILITY CONSTANT [3.0]

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 1.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 25

INITIGUN 10000                #INITIAL GUN
INITIAAM 12                   #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

MAXNMAAM 24
MAXNMAGM 16
MAXNBOMB 8
MAXNMRKT 38

AAMVISIB FALSE
AAMSLOT_ -3.02m  1.19m 12.13m

AGMVISIB FALSE
AGMSLOT_ -3.02m  1.19m 12.13m

BOMVISIB FALSE
BOMBSLOT  0.00m -1.00m  0.50m

RKTVISIB FALSE
RKTSLOT_ -3.02m  1.19m 12.13m



REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.90MACH             #CRUISING SPEED
REFACRUS 10000ft              #CRUISING ALTITUDE [15000ft]
REFTCRUS 0.6                  #CRUISING THROTTLE SETTING [0.8]

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 120kt                #LANDING SPEED [90kt]
REFAOALD 15.0deg              #LANDING AOA [22.5deg]
REFLNRWY 500m                 #RUNWAY REQUIRED TO STOP [50m]
REFTHRLD 0.4                  #LANDING THROTTLE SETTING [0.3]

AUTOCALC
