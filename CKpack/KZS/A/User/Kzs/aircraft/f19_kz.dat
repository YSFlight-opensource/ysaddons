REM Lockheed F-19 Stealth Fighter

REM "f19_kz.dat/f19_kz.dnm/f19_kz_coll.srf/f19_kz_cockpit.srf/f19_kz_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2005 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_F-19_STEALTH"
CATEGORY ATTACKER

AFTBURNR FALSE                #HAVE AFTERBURNER?
THRAFTBN  0.00t               #THRUST WITH AFTERBURNER
THRMILIT 10.88t               #THRUST AT MILITARY POWER
WEIGHCLN  9.50t               #WEIGHT CLEAN
WEIGFUEL  6.25t               #WEIGHT OF FUEL
WEIGLOAD  2.72t               #WEIGHT OF PAYLOAD
FUELABRN  0.00kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.65kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.000m  0.900m  5.100m #COCKPIT POSITION
LEFTGEAR -2.523m -1.478m -1.860m #LEFT LANDING GEAR POSITION
RIGHGEAR  2.523m -1.478m -1.860m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.000m -1.443m  4.022m #WHEEL POSITION
ARRESTER  0.000m -1.420m -5.250m #ARRESTING HOOK POSITION
MACHNGUN  0.000m -0.020m  8.700m #MACHINE GUN POSITION
SMOKEGEN  0.150m  0.050m -6.550m #SMOKE GENERATOR POSITION
VAPORPO0  4.840m -0.483m -4.120m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.840m -0.483m -4.120m #VAPOR POSITION (Wings Spread)
HTRADIUS  8.000m                 #OUTSIDE SPHERE RADIUS

STRENGTH 8

CRITAOAP  18.5deg             #CRITICAL AOA POSITIVE
CRITAOAM -10.0deg             #CRITICAL AOA NEGATIVE

CRITSPED 0.85MACH             #CRITICAL SPEED
MAXSPEED 1.12MACH             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 62m^2                #WING AREA

MXIPTAOA 20.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 70kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 115kt                #FULLY MANEUVABLE SPEED
CPITMANE 8.00                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 1.50                 #PITCH STABILITY CONSTANT
CYAWMANE 5.00                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.25                 #YAW STABILITY CONSTANT        [3.00]
CROLLMAN 2.85                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

INITIGUN 0                    #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITIAGM 6                    #INITIAL AGM
INITBOMB 4                    #INITIAL BOMB

WEAPONCH AGM

MAXNMAAM 4
MAXNMAGM 8
MAXNBOMB 8
MAXNMRKT 0

AAMVISIB FALSE
AAMSLOT_ -0.325m -0.800m 0.7025m
AAMSLOT_  0.325m -0.800m 0.7025m
AAMSLOT_ -0.325m -0.800m -1.7525m
AAMSLOT_  0.325m -0.800m -1.7525m

AGMVISIB FALSE
AAMSLOT_ -0.000m -0.800m -1.7525m
AGMSLOT_  0.000m -0.800m -1.7525m

BOMVISIB FALSE
BOMBSLOT -0.000m -0.800m -1.7525m
BOMBSLOT  0.000m -0.800m -1.7525m

RKTVISIB FALSE
RKTSLOT_  0.00m -0.800m -0.525m


RADARCRS 0.1                  #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 35000ft              #CRUISING ALTITUDE         [40000ft]
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 130kt                #LANDING SPEED
REFAOALD 12.00deg             #LANDING AOA                              [TailStrikeAngle:13.06deg]
REFLNRWY 2000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.45                 #LANDING THROTTLE SETTING

AUTOCALC
