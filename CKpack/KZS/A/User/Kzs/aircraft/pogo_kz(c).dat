REM Convair XFY-1 Pogo - Compromise(Horizontal Takeoff/Landing) Version

REM "pogo_kz(c).dat/pogo_kz.dnm/pogo_kz_cockpit.srf/pogo_kz_coll.srf/pogo_kz_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2003 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_XFY-1_POGO/C"
CATEGORY ATTACKER              # [*]

AFTBURNR FALSE                 #HAVE AFTERBURNER?
PROPELLR 5850HP                #PROPELLER ENGINE  [Allison YT40-A-6:5850HP or 5500HP ?] or [Allison XT40-A-16 6955HP ?]
WEIGHCLN 11760lb               #WEIGHT CLEAN      [max take-off weight:16250lb]
WEIGFUEL 1.08t                 #WEIGHT OF FUEL    [...]
WEIGLOAD 1.25t                 #WEIGHT OF PAYLOAD [...]
FUELMILI 0.30kg                #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  1.13m  0.60m  #COCKPIT POSITION
LEFTGEAR -3.75m -4.80m  2.85m  #LEFT LANDING GEAR POSITION  [*]
RIGHGEAR  3.75m -4.80m  2.85m  #RIGHT LANDING GEAR POSITION [*]
WHELGEAR  0.00m -4.80m -2.85m  #WHEEL POSITION              [*]
ARRESTER  0.00m -4.75m  0.00m  #ARRESTING HOOK POSITION
MACHNGUN  3.96m -0.12m -1.47m  #MACHINE GUN POSITION
MACHNGN2 -3.96m -0.12m -1.47m  #MACHINE GUN POSITION2
SMOKEGEN -3.96m -0.12m -4.35m  #SMOKE GENERATOR POSITION
VAPORPO0  3.96m -0.12m -4.32m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.14m -0.12m -3.09m  #VAPOR POSITION (Wings Spread)
HTRADIUS  8.25m                #OUTSIDE SPHERE RADIUS

STRENGTH 6

CRITAOAP  30deg                #CRITICAL AOA POSITIVE
CRITAOAM -20deg                #CRITICAL AOA NEGATIVE

CRITSPED 320kt                 #CRITICAL SPEED
MAXSPEED 530kt                 #MAXIMUM SPEED

HASSPOIL FALSE                 #HAS SPOILER?
RETRGEAR FALSE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                 #HAS VARIABLE GEOMETRY WING?

VGWSPED1 65kt                  # [*]
VGWSPED2 70kt                  # [*]

CLVARGEO 0.00                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.00                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.20                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.01                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.00                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.00                  #EFFECT OF SPOILER FOR Cd

WINGAREA 33.0m^2               #WING AREA [355ft^2]

MXIPTAOA  22.5deg              #MAX INPUT AOA
MXIPTSSA   5.0deg              #MAX INPUT SSA
MXIPTROL 320.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 50kt                  #MINIMUM MANEUVABLE SPEED [*]
MANESPD2 55kt                  #FULLY MANEUVABLE SPEED   [*]
CPITMANE  9.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  3.5                  #PITCH STABILITY CONSTANT
CYAWMANE  4.5                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB  3.5                  #YAW STABILITY CONSTANT
CROLLMAN  3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                  #CONTROL LANDINGGEAR
CTLBRAKE FALSE                 #CONTROL BRAKE
CTLSPOIL 0.0                   #CONTROL SPOILER
CTLABRNR FALSE                 #CONTROL AFTERBURNER
CTLTHROT 0.0                   #CONTROL THRUST
CTLIFLAP 0.0                   #CONTROL FLAP
CTLINVGW 1.0                   #CONTROL VARIABLE GEOMETRY WING [*]
CTLATVGW TRUE                  #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 16ft 0m            #POSITION
ATTITUDE 0deg 0deg 0deg        #ATTITUDE
INITFUEL 100%                  #INITIAL FUEL
INITLOAD 0.0t                  #INITIAL PAYLOAD
INITSPED 0.0MACH               #INITIAL SPEED

INITIGUN 3000                  #INITIAL GUN
INITIAAM 0                     #INITIAL AAM
INITIAGM 0                     #INITIAL AGM
INITBOMB 0                     #INITIAL BOMB
INITRCKT 24                    #INITIAL ROCKET (10kg/each)

GUNPOWER 2                     #[4 x 20-mm cannon]

MAXNMAAM 0
MAXNMAGM 0
MAXNBOMB 0
MAXNMRKT 48                    #[2.75-inches FFAR]

AAMVISIB FALSE
AAMSLOT_ -3.96m -0.70m -2.90m
AAMSLOT_  3.96m -0.70m -2.90m

AGMVISIB FALSE
AGMSLOT_ -3.96m -0.70m -2.90m
AGMSLOT_  3.96m -0.70m -2.90m

BOMVISIB FALSE
BOMBSLOT -3.96m -0.70m -2.90m
BOMBSLOT  3.96m -0.70m -2.90m

RKTVISIB FALSE
RKTSLOT_ -3.96m -0.70m -2.90m
RKTSLOT_  3.96m -0.70m -2.90m



RADARCRS 0.25

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 360kt                 #CRUISING SPEED [*]
REFACRUS 15000ft               #CRUISING ALTITUDE
REFTCRUS 0.8                   #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 100kt                 #LANDING SPEED [*]
REFAOALD 10.0deg               #LANDING AOA                 #[tail-hit-angle:63.59deg]
REFLNRWY 85.0m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                   #LANDING THROTTLE SETTING

AUTOCALC
