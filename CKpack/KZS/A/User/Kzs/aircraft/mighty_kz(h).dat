REM Mighty Atom - Hyoutan-Tsugi

REM "mighty_kz(h).dat/mighty_kz(h).dnm/mighty_kz(h)_coll.srf/mighty_kz_cockpit.srf"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2002-2003 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs] HYOUTANTSUGI"
CATEGORY FIGHTER

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 10.00t               #THRUST WITH AFTERBURNER
THRMILIT  3.50t               #THRUST AT MILITARY POWER
WEIGHCLN  4.50t               #WEIGHT CLEAN
WEIGFUEL  0.50t               #WEIGHT OF FUEL
WEIGLOAD  8.00t               #WEIGHT OF PAYLOAD
FUELABRN  0.35kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.05kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  1.85m  3.20m #COCKPIT POSITION
LEFTGEAR -2.50m -5.35m -1.50m #LEFT LANDING GEAR POSITION
RIGHGEAR  2.50m -5.35m -1.50m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -5.35m  1.50m #WHEEL POSITION
ARRESTER  0.00m -5.00m  0.00m #ARRESTING HOOK POSITION
MACHNGUN  0.70m -0.90m  3.50m #MACHINE GUN POSITION
MACHNGN2 -0.70m -0.90m  3.50m #MACHINE GUN POSITION
SMOKEGEN  0.00m -2.72m -4.84m #SMOKE GENERATOR POSITION
VAPORPO0  4.03m -0.95m -0.95m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.03m -0.95m -0.95m #VAPOR POSITION (Wings Spread)
HTRADIUS 16.00m               #OUTSIDE SPHERE RADIUS

STRENGTH 25

CRITAOAP  45.0deg             #CRITICAL AOA POSITIVE
CRITAOAM -30.0deg             #CRITICAL AOA NEGATIVE

CRITSPED 0.6MACH              #CRITICAL SPEED
MAXSPEED 1.0MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

REM VGWSPED1 0kt
REM VGWSPED2 1kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.5                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 10.00m^2             #WING AREA

MXIPTAOA  30.0deg             #MAX INPUT AOA
MXIPTSSA   8.0deg             #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 20kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 25kt                 #FULLY MANEUVABLE SPEED
CPITMANE 16.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  2.0                 #PITCH STABILITY CONSTANT
CYAWMANE  6.0                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB  3.0                 #YAW STABILITY CONSTANT
CROLLMAN  8.0                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 1

INITIGUN 10000                #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOM
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

MAXNMAAM 8
MAXNMAGM 6
MAXNBOMB 2
MAXNMRKT 38

AAMVISIB FALSE
AAMSLOT_  0.00m -3.50m 3.50m

AGMVISIB FALSE
AGMSLOT_  0.00m -3.50m 3.50m

BOMVISIB FALSE
BOMBSLOT  0.00m -3.50m 3.50m

RKTVISIB FALSE
RKTSLOT_  0.00m -3.50m 3.50m

REM RADARCRS 1.00                 #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.5MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.6                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 30kt                 #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 10m                  #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
