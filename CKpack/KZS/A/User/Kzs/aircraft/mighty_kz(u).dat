REM Mighty Atom - Uraine(Uran)

REM "mighty_kz(u).dat/mighty_kz(u).dnm/mighty_kz(u)_coll.srf/mighty_kz_cockpit.srf/mighty_kz(u)_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2002-2003 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs] URAN"
CATEGORY NORMAL

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 5.50t                #THRUST WITH AFTERBURNER
THRMILIT 1.65t                #THRUST AT MILITARY POWER
WEIGHCLN 1.00t                #WEIGHT CLEAN
WEIGFUEL 0.50t                #WEIGHT OF FUEL
WEIGLOAD 6.00t                #WEIGHT OF PAYLOAD
FUELABRN 0.35kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.05kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.000m  0.185m  0.440m #COCKPIT POSITION
LEFTGEAR -0.100m -1.000m -0.081m #LEFT LANDING GEAR POSITION
RIGHGEAR  0.100m -1.000m -0.081m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.000m -1.000m  0.034m #WHEEL POSITION
ARRESTER  0.000m -0.950m  0.000m #ARRESTING HOOK POSITION
MACHNGUN  0.155m  0.000m  0.465m #MACHINE GUN POSITION
REM MACHNGN2 -0.155m  0.000m  0.465m #MACHINE GUN POSITION
SMOKEGEN  0.000m  0.090m -0.180m #SMOKE GENERATOR POSITION
VAPORPO0  0.345m  0.000m  0.550m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  0.345m  0.000m  0.550m #VAPOR POSITION (Wings Spread)
HTRADIUS  1.450m                 #OUTSIDE SPHERE RADIUS

STRENGTH 4

CRITAOAP  30.0deg             #CRITICAL AOA POSITIVE
CRITAOAM -15.0deg             #CRITICAL AOA NEGATIVE

CRITSPED 0.6MACH              #CRITICAL SPEED
MAXSPEED 1.0MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

VGWSPED1 0kt
VGWSPED2 1kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.5                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.1                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.8                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 1.00m^2              #WING AREA

MXIPTAOA  22.0deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 340.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 50kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  2.0                 #PITCH STABILITY CONSTANT
CYAWMANE  5.0                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB  3.0                 #YAW STABILITY CONSTANT
CROLLMAN  3.0                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 2

INITIGUN 4500                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOM
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

MAXNMAAM 2
MAXNMAGM 6
MAXNBOMB 2
MAXNMRKT 50

AAMVISIB FALSE
AAMSLOT_  0.00m -0.300m 0.00m

AGMVISIB FALSE
AGMSLOT_  0.00m -0.300m 0.00m

BOMVISIB FALSE
BOMBSLOT  0.00m -0.300m 0.00m

RKTVISIB FALSE
RKTSLOT_  0.00m -0.300m 0.00m

RADARCRS 0.75                 #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.5MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.85                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 70kt                 #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 15m                  #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
