REM Focke-Wulf - Volksjager

REM "vlksjgr_kz.dat/vlksjgr_kz.dnm/vlksjgr_kz_coll.srf/vlksjgr_kz_cockpit.srf/vlksjgr_kz_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2005 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_Fw-VOLKSJAGER"
CATEGORY FIGHTER

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 1.95t                #THRUST WITH AFTERBURNER (with rocket booster)
THRMILIT 1.70t                #THRUST AT MILITARY POWER (Walter HWK 109-509 A-2 rocket engine)
WEIGHCLN 1.45t                #WEIGHT CLEAN
WEIGFUEL 0.20t                #WEIGHT OF FUEL
WEIGLOAD 1.15t                #WEIGHT OF PAYLOAD
FUELABRN 0.415kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER (around  8.0min)
FUELMILI 0.210kg              #FUEL CONSUMPTION AT MILITARY POWER      (around 15.8min)

COCKPITP  0.000m  0.400m  0.600m #COCKPIT POSITION
LEFTGEAR -0.090m -0.900m  0.480m #LEFT LANDING GEAR POSITION
RIGHGEAR  0.090m -0.900m  0.480m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.000m -0.700m  1.600m #WHEEL POSITION
ARRESTER  0.000m -0.850m -0.400m #ARRESTING HOOK POSITION
MACHNGUN -0.313m -0.363m  1.650m #MACHINE GUN POSITION
MACHNGN2  0.313m -0.363m  1.650m #MACHINE GUN POSITION
SMOKEGEN  0.250m -0.150m -1.400m #SMOKE GENERATOR POSITION
VAPORPO0  2.400m -0.180m -0.670m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  2.400m -0.180m -0.670m #VAPOR POSITION (Wings Spread)
HTRADIUS  4.500m                 #OUTSIDE SPHERE RADIUS

STRENGTH 8

CRITAOAP  20.0deg             #CRITICAL AOA POSITIVE
CRITAOAM -15.0deg             #CRITICAL AOA NEGATIVE

CRITSPED 420kt                #CRITICAL SPEED
MAXSPEED 540kt                #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

REM VGWSPED1 150kt
REM VGWSPED2 300kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.4                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.75                 #EFFECT OF SPOILER FOR Cd

WINGAREA 8.50m^2              #WING AREA

MXIPTAOA  20.0deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 300.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 65kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 80kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  2.5                 #PITCH STABILITY CONSTANT
CYAWMANE  5.0                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB  1.5                 #YAW STABILITY CONSTANT
CROLLMAN  3.0                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 1

INITIGUN 1250                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOM
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

REM SMOKEOIL 100.0kg
REM WEAPONCH SMK

MAXNMAAM 0
MAXNMAGM 0
MAXNBOMB 0
MAXNMRKT 24

AAMVISIB FALSE
AAMSLOT_ 0.00m -0.85m -0.70m

AGMVISIB FALSE
AGMSLOT_ 0.00m -0.85m -0.70m

BOMVISIB FALSE
BOMBSLOT 0.00m -0.85m -0.70m

RKTVISIB FALSE
RKTSLOT_ -0.313m -0.363m  2.15m
RKTSLOT_  0.313m -0.363m  2.15m

REM RADARCRS 0.75             #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 475kt                #CRUISING SPEED
REFACRUS 30000ft              #CRUISING ALTITUDE
REFTCRUS 0.6                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 90kt                 #LANDING SPEED
REFAOALD 10deg                #LANDING AOA                               [TailStrikeAngle:15.677deg]
REFLNRWY 750m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
