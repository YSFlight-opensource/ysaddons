REM NASA HiMAT(Highly Maneuverable Aircraft Technology)

REM "himat_kz.dat/himat_kz.dnm/himat_kz_cockpit.srf/himat_kz_coll.srf/himat_kz_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2001-2003 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs] HiMAT"
CATEGORY NORMAL

AFTBURNR FALSE                #HAVE AFTERBURNER?
THRAFTBN  0.00t               #THRUST WITH AFTERBURNER
THRMILIT  2.50t               #THRUST AT MILITARY POWER
WEIGHCLN  1.50t               #WEIGHT CLEAN
WEIGFUEL  0.30t               #WEIGHT OF FUEL
WEIGLOAD  0.00t               #WEIGHT OF PAYLOAD
FUELABRN  0.00kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.10kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.60m  1.50m #COCKPIT POSITION
LEFTGEAR -1.40m -0.76m -2.80m #LEFT LANDING GEAR POSITION
RIGHGEAR  1.40m -0.76m -2.80m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.00m  1.05m #WHEEL POSITION
ARRESTER  0.00m -0.70m -2.50m #ARRESTING HOOK POSITION
MACHNGUN  0.00m  0.33m  2.35m #MACHINE GUN POSITION
SMOKEGEN  0.00m -0.42m -2.58m #SMOKE GENERATOR POSITION
VAPORPO0  2.65m  0.00m -2.75m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  2.65m  0.00m -2.75m #VAPOR POSITION (Wings Spread)
HTRADIUS  6.00m               #OUTSIDE SPHERE RADIUS

STRENGTH 4

CRITAOAP  26deg               #CRITICAL AOA POSITIVE
CRITAOAM -18deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 1.4MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

VGWSPED1  90kt
VGWSPED2 200kt

CLVARGEO 0.1                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.05                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 20m^2                #WING AREA

MXIPTAOA  24.5deg             #MAX INPUT AOA
MXIPTSSA   6.5deg             #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 20kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 60kt                 #FULLY MANEUVABLE SPEED
CPITMANE 12.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  1.0                 #PITCH STABILITY CONSTANT
CYAWMANE  6.0                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB  3.0                 #YAW STABILITY CONSTANT
CROLLMAN  4.5                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

REM GUNPOWER 1

INITIGUN 0                    #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOM
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

MAXNMAAM 0
MAXNMAGM 0
MAXNBOMB 0
MAXNMRKT 0

REM AAMSLOT_  0.65m -0.25m -0.15m
REM AAMSLOT_ -0.65m -0.25m -0.15m

REM AGMSLOT_  0.90m -0.25m -0.75m
REM AGMSLOT_ -0.90m -0.25m -0.75m

REM BOMBSLOT  1.15m -0.25m -1.00m
REM BOMBSLOT -1.15m -0.25m -1.00m

REM RKTSLOT_  1.75m -0.25m -1.60m
REM RKTSLOT_ -1.75m -0.25m -1.60m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.99MACH             #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 90kt                 #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.30                 #LANDING THROTTLE SETTING

AUTOCALC
