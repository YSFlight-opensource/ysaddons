REM Thunder Bird TB-1 - VTOL Version

REM "tb1_kz(v).dat/tb1_kz(v).dnm/tb1_kz_cockpit.srf/tb1_kz_coll.srf/tb1_kz(v)_coarse.dnm"
REM     were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2003 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_ThunderBird_1/V"
CATEGORY UTILITY

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 200.00t              #THRUST WITH AFTERBURNER
THRMILIT  45.50t              #THRUST AT MILITARY POWER
WEIGHCLN  35.41t              #WEIGHT CLEAN
WEIGFUEL   5.00t              #WEIGHT OF FUEL
WEIGLOAD  30.00t              #WEIGHT OF PAYLOAD [20.00t]
FUELABRN  5.00kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER [2.50kg]
FUELMILI  1.25kg              #FUEL CONSUMPTION AT MILITARY POWER      [0.50kg]

COCKPITP  0.00m  0.30m  11.90m #COCKPIT POSITION
LEFTGEAR -8.72m -8.00m  -6.17m #LEFT LANDING GEAR POSITION
RIGHGEAR  8.72m -8.00m  -6.17m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -7.00m -15.70m #WHEEL POSITION
ARRESTER  0.00m -6.95m -15.70m #ARRESTING HOOK POSITION
MACHNGUN -0.75m -3.25m   8.00m #MACHINE GUN POSITION
MACHNGN2  0.75m -3.25m   8.00m #MACHINE GUN POSITION
SMOKEGEN -1.89m -1.89m -17.40m #SMOKE GENERATOR POSITION
VAPORPO0  4.00m  0.00m -11.00m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1 12.20m  0.00m  -9.00m #VAPOR POSITION (Wings Spread)
HTRADIUS 18.00m                #OUTSIDE SPHERE RADIUS

STRENGTH 45

CRITAOAP  20.0deg             #CRITICAL AOA POSITIVE
CRITAOAM -20.0deg             #CRITICAL AOA NEGATIVE

CRITSPED  1.25MACH            #CRITICAL SPEED
MAXSPEED 19.48MACH            #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m 0.00m 1.0m      #Thrust vector 0
TRSTDIR1 0.0m 1.00m 0.0m      #Thrust vector 1
PSTMPTCH  6deg                #Post-Stall Maneuver VPitch [10deg]
PSTMYAW_ 10deg                #Post-Stall Maneuver VYaw [3deg]
PSTMROLL  5deg                #Post-Stall Maneuver VRoll

VGWSPED1 1.75MACH             #[2.75MACH]
VGWSPED2 2.00MACH             #[3.00MACH]

CLVARGEO 0.10                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.05                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.40                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.15                 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.50                 #EFFECT OF GEAR FOR Cd
CDSPOILR 30.0                 #EFFECT OF SPOILER FOR Cd [9.99]

WINGAREA 100.00m^2            #WING AREA

MXIPTAOA  20.0deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 320.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 80kt                 #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 4.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 4.0                  #YAW STABILITY CONSTANT
CROLLMAN 2.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 26.25ft 0m        #POSITION
ATTITUDE 0deg 0deg 0deg       #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 3
SMOKEOIL 100.0kg
WEAPONCH SMK

INITIGUN 5000                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM

REM MAXNMAAM 10
REM MAXNMAGM 10
REM MAXNBOMB 25
REM MAXNMRKT 50

AAMVISIB FALSE
AAMSLOT_ 0.00m -3.50m 8.00m

AGMVISIB FALSE
AGMSLOT_ 0.00m -3.50m 8.00m

BOMVISIB FALSE
BOMBSLOT 0.00m -3.50m 8.00m

RKTVISIB FALSE
RKTSLOT_ 0.00m -3.50m 8.00m

REM RADARCRS 0.1              #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 7.75MACH             #CRUISING SPEED
REFACRUS 50000ft              #CRUISING ALTITUDE
REFTCRUS 0.25                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 100kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA                     [TailStrikeAngle:32.28deg]
REFLNRWY 1750m                #RUNWAY REQUIRED TO STOP [1000m] [ standstill AOA: 5.99deg]
REFTHRLD 0.15                 #LANDING THROTTLE SETTING

AUTOCALC
