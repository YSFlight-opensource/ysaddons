REM Nausicaa ride on MOWE - Figter Version

REM "mowe_kz(f).dat/mowe_kz(f).dnm/mowe_kz_cockpit.srf/mowe_kz_coll.srf/mowe_kz(f)_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2002-2003 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_MOWE/F"
CATEGORY FIGHTER

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  2.5t                #THRUST WITH AFTERBURNER
THRMILIT  1.25t               #THRUST AT MILITARY POWER
WEIGHCLN  0.1t                #WEIGHT CLEAN
WEIGFUEL  0.5t                #WEIGHT OF FUEL
WEIGLOAD  2.0t                #WEIGHT OF PAYLOAD
FUELABRN  0.20kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.02kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.58m  0.46m #COCKPIT POSITION
LEFTGEAR -0.23m -1.00m -0.08m #LEFT LANDING GEAR POSITION
RIGHGEAR  0.23m -1.00m -0.08m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -0.90m -1.00m #WHEEL POSITION
ARRESTER  0.00m -0.85m -0.50m #ARRESTING HOOK POSITION
MACHNGUN  0.17m  0.35m  1.43m #MACHINE GUN POSITION
SMOKEGEN  0.00m -0.13m -0.60m #SMOKE GENERATOR POSITION
VAPORPO0  2.53m  0.05m -0.65m #VAPOR POSITION (Wings Swept back)
VAPORPO1  2.53m  0.05m -0.55m #VAPOR POSITION (Wings Spread)
HTRADIUS  2.00m               #OUTSIDE SPHERE RADIUS

STRENGTH 6

CRITAOAP  32deg               #CRITICAL AOA POSITIVE
CRITAOAM -16deg               #CRITICAL AOA NEGATIVE

CRITSPED 280kt                #CRITICAL SPEED
MAXSPEED 480kt                #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

REM VGWSPED1 64kt
REM VGWSPED2 70kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.4                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.1                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.3                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.25                 #EFFECT OF SPOILER FOR Cd

WINGAREA 6m^2                 #WING AREA

MXIPTAOA  21.0deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 20kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 40kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  1.0                 #PITCH STABILITY CONSTANT
CYAWMANE  5.0                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB  3.0                 #YAW STABILITY CONSTANT
CROLLMAN  3.0                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 2

INITIGUN 2000                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

MAXNMAAM 2
MAXNMAGM 2
MAXNBOMB 2
MAXNMRKT 12

AAMVISIB FALSE
AAMSLOT_  0.00m -0.30m 0.00m

AGMVISIB FALSE
AGMSLOT_  0.00m -0.30m 0.00m

BOMVISIB FALSE
BOMBSLOT  0.00m -0.30m 0.00m

RKTVISIB FALSE
RKTSLOT_  0.00m -0.30m 0.00m



REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 360kt                #CRUISING SPEED
REFACRUS 2500ft               #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 95kt                 #LANDING SPEED                *kz[90kt]
REFAOALD 10deg                #LANDING AOA
REFLNRWY 150m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
