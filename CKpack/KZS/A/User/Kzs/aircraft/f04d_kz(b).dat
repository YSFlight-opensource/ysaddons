REM F-04D MI-C.A.D.O.3  - Black Version

REM "f04d_kz(b).dat/f04d_kz(b).dnm/f04d_kz_cockpit.srf/f04d_kz_coll.srf/f04d_kz(b)_coarse.dnm"
REM     were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2003 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_MI-C.A.D.O.3/B"
CATEGORY ATTACKER

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 39.33t               #THRUST WITH AFTERBURNER
THRMILIT 24.00t               #THRUST AT MILITARY POWER
WEIGHCLN  7.54t               #WEIGHT CLEAN
WEIGFUEL  3.00t               #WEIGHT OF FUEL
WEIGLOAD 12.00t               #WEIGHT OF PAYLOAD
FUELABRN  3.25kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.25kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  1.05m  3.90m #COCKPIT POSITION
LEFTGEAR -1.10m -2.23m -3.00m #LEFT LANDING GEAR POSITION
RIGHGEAR  1.10m -2.23m -3.00m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -2.23m  2.50m #WHEEL POSITION
ARRESTER  0.00m -2.18m -5.90m #ARRESTING HOOK POSITION
MACHNGUN -0.97m  0.14m  3.75m #MACHINE GUN POSITION
MACHNGN2  0.97m  0.14m  3.75m #MACHINE GUN POSITION
SMOKEGEN  0.00m -0.40m -7.60m #SMOKE GENERATOR POSITION
VAPORPO0  5.04m -0.43m -0.50m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.04m -0.43m -0.50m #VAPOR POSITION (Wings Spread)
HTRADIUS  9.00m               #OUTSIDE SPHERE RADIUS

STRENGTH 16

CRITAOAP  65.0deg             #CRITICAL AOA POSITIVE
CRITAOAM -65.0deg             #CRITICAL AOA NEGATIVE

CRITSPED 0.85MACH             #CRITICAL SPEED
MAXSPEED 3.75MACH             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

VGWSPED1 240kt
VGWSPED2 360kt

CLVARGEO 0.10                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.10                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.10                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.20                 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.75                 #EFFECT OF GEAR FOR Cd
CDSPOILR 2.50                 #EFFECT OF SPOILER FOR Cd

WINGAREA 37.60m^2             #WING AREA

MXIPTAOA  28.0deg             #MAX INPUT AOA
MXIPTSSA   8.0deg             #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 35kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 40kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.00                #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  1.50                #PITCH STABILITY CONSTANT
CYAWMANE  5.00                #YAW MANEUVABILITY CONSTANT
CYAWSTAB  3.50                #YAW STABILITY CONSTANT
CROLLMAN  3.00                #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING


POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 2

INITIGUN 10000                #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITIAGM 6                    #INITIAL AGM
INITBOMB 2                    #INITIAL BOM
INITRCKT 24                   #INITIAL ROCKET (10kg/each)

MAXNMAAM 10
MAXNMAGM 10
MAXNBOMB 10
MAXNMRKT 50

AAMVISIB FALSE
AAMSLOT_ -2.36m -1.03m -1.80m
AAMSLOT_  2.36m -1.03m -1.80m

AGMVISIB FALSE
AGMSLOT_ -2.36m -1.03m -1.80m
AGMSLOT_  2.36m -1.03m -1.80m

BOMVISIB FALSE
BOMBSLOT -2.36m -1.03m -1.80m
BOMBSLOT  2.36m -1.03m -1.80m

RKTVISIB FALSE
RKTSLOT_ -2.36m -1.03m -1.80m
RKTSLOT_  2.36m -1.03m -1.80m



RADARCRS 0.50                 #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.75MACH             #CRUISING SPEED   [2.25MACH]
REFACRUS 20000ft              #CRUISING ALTITUDE[30000ft]
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 110kt                #LANDING SPEED
REFAOALD 10.0deg              #LANDING AOA                  [TailStrikeAngle:21.69deg]
REFLNRWY 125m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.15                 #LANDING THROTTLE SETTING

AUTOCALC
