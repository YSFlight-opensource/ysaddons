REM Republic F-105D Thunderchief

REM "f105d_kz.dat/f105d_kz.dnm/f105d_kz_cockpit.srf/f105d_kz_coll.srf/f105d_kz_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2001-2003 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs] F-105D Thunderchief"
CATEGORY ATTACKER

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 12.00t               #THRUST WITH AFTERBURNER
THRMILIT  7.80t               #THRUST AT MILITARY POWER
WEIGHCLN 12.47t               #WEIGHT CLEAN
WEIGFUEL  4.50t               #WEIGHT OF FUEL
WEIGLOAD  5.50t               #WEIGHT OF PAYLOAD
FUELABRN  3.50kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.30kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  1.60m  3.62m #COCKPIT POSITION

LEFTGEAR -2.74m -2.10m -2.00m #LEFT LANDING GEAR POSITION
RIGHGEAR  2.74m -2.10m -2.00m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -2.55m  4.00m #WHEEL POSITION
ARRESTER  0.00m -1.82m -7.70m  #ARRESTING HOOK POSITION
MACHNGUN -0.82m  0.00m  6.00m #MACHINE GUN POSITION
SMOKEGEN  0.00m -0.75m -8.20m #SMOKE GENERATOR POSITION
VAPORPO0  5.15m  0.30m -5.55m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.15m  0.30m -5.55m #VAPOR POSITION (Wings Spread)
HTRADIUS 10.00m               #OUTSIDE SPHERE RADIUS

STRENGTH 16

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.90MACH             #CRITICAL SPEED
MAXSPEED 2.15MACH             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

VGWSPED1  90kt
VGWSPED2 200kt

CLVARGEO 0.10                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.05                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 35.58m^2             #WING AREA

MXIPTAOA  23.0deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1  80kt                #MINIMUM MANEUVABLE SPEED
MANESPD2 110kt                #FULLY MANEUVABLE SPEED
CPITMANE 6.5                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 1

INITIGUN 3000                 #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITIAGM 2                    #INITIAL AGM
INITBOMB 8                    #INITIAL BOM
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

MAXNMAAM 2
MAXNMAGM 4
MAXNBOMB 16
MAXNMRKT 38

AAMSLOT_  4.55m -0.10m -3.95m
AAMSLOT_ -4.55m -0.10m -3.95m

AGMSLOT_  3.80m -0.15m -3.25m
AGMSLOT_ -3.80m -0.15m -3.25m
AGMSLOT_  3.30m -0.15m -3.25m
AGMSLOT_ -3.30m -0.15m -3.25m

BOMBSLOT  0.00m -1.60m  1.60m
BOMBSLOT  0.30m -1.30m  1.60m
BOMBSLOT -0.30m -1.30m  1.60m
BOMBSLOT  0.00m -1.60m -1.40m
BOMBSLOT  0.30m -1.30m -1.40m
BOMBSLOT -0.30m -1.30m -1.40m
BOMBSLOT  3.55m -0.55m -3.00m
BOMBSLOT -3.55m -0.55m -3.00m

RKTSLOT_  1.60m -0.25m  0.80m
RKTSLOT_ -1.60m -0.25m  0.80m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.95MACH             #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 110kt                #LANDING SPEED
REFAOALD 7deg                 #LANDING AOA
REFLNRWY 2000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.30                 #LANDING THROTTLE SETTING

AUTOCALC
