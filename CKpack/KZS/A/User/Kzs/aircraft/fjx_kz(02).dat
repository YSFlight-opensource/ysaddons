REM Fighter-Japan-X #02

REM "fjx_kz(02).dat/fjx_kz(02).dnm/fjx_kz_cockpit.srf/fjx_kz_coll.srf/fjx_kz(02)_coarse.dnm"
REM     were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2003 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_FJX_#02"
CATEGORY FIGHTER

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 27.50t               #THRUST WITH AFTERBURNER
THRMILIT 16.25t               #THRUST AT MILITARY POWER
WEIGHCLN  7.25t               #WEIGHT CLEAN
WEIGFUEL  3.00t               #WEIGHT OF FUEL
WEIGLOAD  9.25t               #WEIGHT OF PAYLOAD
FUELABRN  3.25kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.25kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  1.20m  4.60m #COCKPIT POSITION
LEFTGEAR -1.50m -1.90m -3.50m #LEFT LANDING GEAR POSITION
RIGHGEAR  1.50m -1.90m -3.50m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.90m  5.50m #WHEEL POSITION
ARRESTER  0.00m -1.85m -7.00m #ARRESTING HOOK POSITION
MACHNGUN -2.60m  0.00m  0.60m #MACHINE GUN POSITION
SMOKEGEN  1.65m -0.30m -6.80m #SMOKE GENERATOR POSITION
VAPORPO0  6.00m  0.00m -7.75m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  6.00m  0.00m -6.00m #VAPOR POSITION (Wings Spread)
HTRADIUS  9.00m               #OUTSIDE SPHERE RADIUS

STRENGTH 12

CRITAOAP  62.5deg             #CRITICAL AOA POSITIVE
CRITAOAM -30.0deg             #CRITICAL AOA NEGATIVE

CRITSPED 0.85MACH             #CRITICAL SPEED
MAXSPEED 2.50MACH             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

VGWSPED1  90kt
VGWSPED2 200kt

CLVARGEO 0.00                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.00                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.10                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.20                 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.50                 #EFFECT OF GEAR FOR Cd
CDSPOILR 2.25                 #EFFECT OF SPOILER FOR Cd

WINGAREA 52.00m^2             #WING AREA

MXIPTAOA  28.0deg             #MAX INPUT AOA
MXIPTSSA   8.0deg             #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 30kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 40kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.00                #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  1.25                #PITCH STABILITY CONSTANT
CYAWMANE  5.00                #YAW MANEUVABILITY CONSTANT
CYAWSTAB  3.25                #YAW STABILITY CONSTANT
CROLLMAN  3.00                #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING


POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 1

INITIGUN 3000                 #INITIAL GUN
INITIAAM 4                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOM
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

MAXNMAAM 8
MAXNMAGM 6
MAXNBOMB 4
MAXNMRKT 32

REM AAMVISIB FALSE
AAMSLOT_ -0.90m -0.90m  0.25m
AAMSLOT_  0.90m -0.90m  0.25m
AAMSLOT_ -1.45m -0.80m  0.00m
AAMSLOT_  1.45m -0.80m  0.00m
AAMSLOT_ -2.00m -0.70m -0.25m
AAMSLOT_  2.00m -0.70m -0.25m
AAMSLOT_ -3.725m -0.90m -5.75m
AAMSLOT_  3.725m -0.90m -5.75m

REM AGMVISIB FALSE
AGMSLOT_ -3.450m -0.50m -5.75m
AGMSLOT_  3.450m -0.50m -5.75m
AGMSLOT_ -4.000m -0.50m -5.75m
AGMSLOT_  4.000m -0.50m -5.75m
AGMSLOT_ -1.50m -0.70m -5.25m
AGMSLOT_  1.50m -0.70m -5.25m

REM BOMVISIB FALSE
BOMBSLOT  0.00m -1.10m -1.00m
BOMBSLOT  0.00m -1.10m -3.50m
BOMBSLOT -1.00m -0.80m -4.70m
BOMBSLOT  1.00m -0.80m -4.70m

REM RKTVISIB FALSE
RKTSLOT_  0.00m -1.35m -1.00m



RADARCRS 0.75                 #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.25MACH             #CRUISING SPEED
REFACRUS 15000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 90kt                 #LANDING SPEED
REFAOALD 10deg                #LANDING AOA                  [TailStrikeAngle:18.02deg]
REFLNRWY 350m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.30                 #LANDING THROTTLE SETTING

AUTOCALC
