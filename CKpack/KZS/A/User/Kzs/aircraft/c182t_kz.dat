REM Cessna Model-182T Skylane

REM "c182t_kz.dat/c182t_kz.dnm/c182t_kz_cockpit.srf/c182t_kz_coll.srf/c182t_kz_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2004 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_Cessna_C182T_Skylane"
CATEGORY NORMAL

AFTBURNR FALSE                 #HAVE AFTERBURNER?
PROPELLR 275HP                 #PROPELLER ENGINE [230HP]
WEIGHCLN 0.860t                #WEIGHT CLEAN [0.860t]
WEIGFUEL 0.20t                 #WEIGHT OF FUEL
WEIGLOAD 0.45t                 #WEIGHT OF PAYLOAD
FUELMILI 0.02kg                #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP -0.23m  0.49m  0.24m  #COCKPIT POSITION
LEFTGEAR -1.30m -1.10m -0.20m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.30m -1.10m -0.20m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.21m  1.46m  #WHEEL POSITION
ARRESTER  0.00m -0.75m -3.80m  #ARRESTING HOOK POSITION
MACHNGUN -0.27m -0.34m  1.99m  #MACHINE GUN POSITION
MACHNGN2  0.27m -0.34m  1.99m  #MACHINE GUN POSITION
SMOKEGEN -5.20m  0.77m -0.37m  #SMOKE GENERATOR POSITION
VAPORPO0  5.50m  0.72m  0.16m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.50m  0.72m  0.16m  #VAPOR POSITION (Wings Spread)
HTRADIUS  6.50m                #OUTSIDE SPHERE RADIUS

STRENGTH 4

CRITAOAP  20deg                #CRITICAL AOA POSITIVE
CRITAOAM -15deg                #CRITICAL AOA NEGATIVE

CRITSPED 150kt                 #CRITICAL SPEED                  [145kt]
MAXSPEED 178kt                 #MAXIMUM SPEED                   [149kt]

HASSPOIL FALSE                 #HAS SPOILER?
RETRGEAR FALSE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                 #HAS VARIABLE GEOMETRY WING?

REM VGWSPED1 0kt
REM VGWSPED2 90kt

CLVARGEO 0.0                   #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                   #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.25                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.1                   #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.0                   #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                   #EFFECT OF SPOILER FOR Cd

WINGAREA 16.2m^2               #WING AREA

MXIPTAOA  16.0deg              #MAX INPUT AOA
MXIPTSSA   5.0deg              #MAX INPUT SSA
MXIPTROL 110.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 54kt                  #MINIMUM MANEUVABLE SPEED
MANESPD2 62kt                  #FULLY MANEUVABLE SPEED
CPITMANE 6.5                   #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.5                   #PITCH STABILITY CONSTANT
CYAWMANE 5.0                   #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.5                   #YAW STABILITY CONSTANT
CROLLMAN 2.0                   #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                  #CONTROL LANDINGGEAR
CTLBRAKE FALSE                 #CONTROL BRAKE
CTLSPOIL 0.0                   #CONTROL SPOILER
CTLABRNR FALSE                 #CONTROL AFTERBURNER
CTLTHROT 0.0                   #CONTROL THRUST
CTLIFLAP 0.0                   #CONTROL FLAP
CTLINVGW 0.0                   #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m             #POSITION
ATTITUDE 45deg 0deg 0deg       #ATTITUDE
INITFUEL 100%                  #INITIAL FUEL
INITLOAD 0.0t                  #INITIAL PAYLOAD
INITSPED 0.0MACH               #INITIAL SPEED

INITIGUN 0                     #INITIAL GUN
INITIAAM 0                     #INITIAL AAM
INITIAGM 0                     #INITIAL AGM
INITBOMB 0                     #INITIAL BOMB
INITRCKT 0                     #INITIAL ROCKET (10kg/each)

SMOKEOIL 25.0kg
WEAPONCH SMK

MAXNMAAM 0
MAXNMAGM 0
MAXNBOMB 0
MAXNMRKT 0

AAMSLOT_  0.00m -0.85m 0.00m
AGMSLOT_  0.00m -0.85m 0.00m
BOMBSLOT  0.00m -0.85m 0.00m
RKTSLOT_  3.90m  0.45m 0.00m
RKTSLOT_ -3.90m  0.45m 0.00m



RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 165kt                 #CRUISING SPEED
REFACRUS 2500ft                #CRUISING ALTITUDE
REFTCRUS 0.8                   #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 80kt                  #LANDING SPEED
REFAOALD 10.0deg               #LANDING AOA                             [TailStrikeAngle:14.98deg]
REFLNRWY 250m                  #RUNWAY REQUIRED TO STOP         [180m]
REFTHRLD 0.3                   #LANDING THROTTLE SETTING

AUTOCALC
