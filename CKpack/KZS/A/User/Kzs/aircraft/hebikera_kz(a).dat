REM Hebikera in Nausicaa of Valley of Wind(Long-Version)

REM "hebikera_kz(a).dat/hebikera_kz(l).dnm/hebikera_kz_cockpit.srf/hebikera_kz_coll.srf/hebikera_kz(l)_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2002-2003 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_HEBIKERA/A"
CATEGORY ATTACKER

AFTBURNR FALSE                #HAVE AFTERBURNER?
THRAFTBN  0.0t                #THRUST WITH AFTERBURNER
THRMILIT 20.0t                #THRUST AT MILITARY POWER
WEIGHCLN 12.0t                #WEIGHT CLEAN
WEIGFUEL  1.0t                #WEIGHT OF FUEL
WEIGLOAD  4.0t                #WEIGHT OF PAYLOAD
FUELABRN  0.00kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.15kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.14m  0.25m #COCKPIT POSITION
LEFTGEAR -1.00m -1.00m -1.25m #LEFT LANDING GEAR POSITION
RIGHGEAR  1.00m -1.00m -1.25m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.00m  0.50m #WHEEL POSITION
ARRESTER  0.00m -0.95m -1.50m #ARRESTING HOOK POSITION
MACHNGUN  0.00m  0.00m  0.65m #MACHINE GUN POSITION
SMOKEGEN  0.00m -0.32m -0.50m #SMOKE GENERATOR POSITION
VAPORPO0  1.00m  0.00m -0.50m #VAPOR POSITION (Wings Swept back)
VAPORPO1  1.00m  0.00m -0.50m #VAPOR POSITION (Wings Spread)
HTRADIUS 16.00m               #OUTSIDE SPHERE RADIUS

STRENGTH 8

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 200kt                #CRITICAL SPEED
MAXSPEED 500kt                #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR FALSE                #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

VGWSPED1  10kt
VGWSPED2 360kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.2                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.1                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 72m^2                #WING AREA

MXIPTAOA 26deg                #MAX INPUT AOA
MXIPTSSA  5deg                #MAX INPUT SSA
MXIPTROL 45deg                #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 80kt                 #FULLY MANEUVABLE SPEED
CPITMANE  8.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  4.0                 #PITCH STABILITY CONSTANT
CYAWMANE  6.0                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB  1.5                 #YAW STABILITY CONSTANT
CROLLMAN  4.0                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 1

INITIGUN 500                  #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITIAGM 2                    #INITIAL AGM
INITBOMB 2                    #INITIAL BOMB
INITRCKT 18                   #INITIAL ROCKET (10kg/each)

MAXNMAAM 2
MAXNMAGM 4
MAXNBOMB 4
MAXNMRKT 38

AAMVISIB FALSE
AAMSLOT_  0.00m -0.50m 0.00m

AGMVISIB FALSE
AGMSLOT_  0.00m -0.50m 0.00m

BOMVISIB FALSE
BOMBSLOT  0.00m -0.50m 0.00m

RKTVISIB FALSE
RKTSLOT_  0.00m -0.50m 0.00m



REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 300kt                #CRUISING SPEED
REFACRUS 8000ft               #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 90kt                 #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 750m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
