REM General Dynamics/Lockeed Martin F-16D FightingFalcon  Block 30 or later
REM Tsvah Haganah le Israel/Heyl Ha'Avir (Israel Defense Force/Air Force)
REM "Barakeet"

REM "f16d_kz_brk.dat/f16d_kz_brk.dnm/f16c_kz_coll.srf/f16d_kz_cockpit.srf/f16d_kz_brk_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2004 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_F-16D_BARAKEET"
CATEGORY ATTACKER

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 13.40t               #THRUST WITH AFTERBURNER(F110-GE-129 turbofan),[13.18t:F100-PW-229 turbofan]
THRMILIT  8.05t               #THRUST AT MILITARY POWER
WEIGHCLN  8.809t              #WEIGHT CLEAN
WEIGFUEL  2.567t              #WEIGHT OF FUEL
WEIGLOAD  7.225t              #WEIGHT OF PAYLOAD
FUELABRN  6.50kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.25kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.90m  3.35m #COCKPIT POSITION
LEFTGEAR -1.26m -1.84m -1.98m #LEFT LANDING GEAR POSITION
RIGHGEAR  1.26m -1.84m -1.98m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.81m  2.22m #WHEEL POSITION
ARRESTER  0.00m -1.78m -5.15m #ARRESTING HOOK POSITION
MACHNGUN -0.71m  0.24m  2.18m #MACHINE GUN POSITION
SMOKEGEN -0.70m  0.10m -6.50m #SMOKE GENERATOR POSITION
VAPORPO0  4.58m -0.03m -3.55m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.58m -0.03m -3.55m #VAPOR POSITION (Wings Spread)
HTRADIUS  8.00m               #OUTSIDE SPHERE RADIUS

STRENGTH 10

CRITAOAP  22deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.90MACH             #CRITICAL SPEED
MAXSPEED 2.05MACH             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

VGWSPED1  90kt
VGWSPED2 200kt

CLVARGEO 0.10                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.15                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.10                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.30                 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.50                 #EFFECT OF GEAR FOR Cd
CDSPOILR 2.00                 #EFFECT OF SPOILER FOR Cd

WINGAREA 27.87m^2             #WING AREA

MXIPTAOA  23.0deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 80kt                 #FULLY MANEUVABLE SPEED
CPITMANE 10.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  1.5                 #PITCH STABILITY CONSTANT
CYAWMANE  5.0                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB  3.0                 #YAW STABILITY CONSTANT
CROLLMAN  3.5                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 0deg 0deg 0deg       #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 1

INITIGUN 3000                 #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITIAGM 4                    #INITIAL AGM
INITBOMB 6                    #INITIAL BOM
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

MAXNMAAM 4
MAXNMAGM 6
MAXNBOMB 6
MAXNMRKT 38

AAMVISIB TRUE
AAMSLOT_ -4.790m -0.025m -2.250m
AAMSLOT_  4.790m -0.025m -2.250m
AAMSLOT_ -4.000m -0.440m -2.000m
AAMSLOT_  4.000m -0.440m -2.000m

AGMVISIB TRUE
AGMSLOT_ -3.050m -0.915m -1.930m
AGMSLOT_  3.050m -0.915m -1.930m
AGMSLOT_ -3.450m -0.475m -1.930m
AGMSLOT_  3.450m -0.475m -1.930m
AGMSLOT_ -2.650m -0.475m -1.930m
AGMSLOT_  2.650m -0.475m -1.930m

BOMVISIB TRUE
BOMBSLOT -1.800m -0.959m -0.750m
BOMBSLOT  1.800m -0.959m -0.750m
BOMBSLOT -2.070m -0.659m -0.750m
BOMBSLOT  2.070m -0.659m -0.750m
BOMBSLOT -1.530m -0.659m -0.750m
BOMBSLOT  1.530m -0.659m -0.750m

RKTVISIB FALSE
RKTSLOT_ -1.800m -0.639m -1.250m
RKTSLOT_  1.800m -0.639m -1.250m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.05MACH             #CRUISING SPEED
REFACRUS 30000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 90kt                 #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 900m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.30                 #LANDING THROTTLE SETTING

AUTOCALC
