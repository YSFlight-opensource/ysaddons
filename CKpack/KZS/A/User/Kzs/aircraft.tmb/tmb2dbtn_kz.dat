REM TIMEBOKAN - TIME DotaBattan

REM "tmb2dbtn_kz.dat/tmb2dbtn_kz.dnm/tmb2dbtn_kz_cockpit.srf"
REM and "tmb2dbtn_kz_coll.srf/tmb2dbtn_kz_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2002-2003,2004 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_TIME-DOTABATTAN"
CATEGORY UTILITY

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 62.5t                #THRUST WITH AFTERBURNER
THRMILIT 40.0t                #THRUST AT MILITARY POWER
WEIGHCLN 16.5t                #WEIGHT CLEAN
WEIGFUEL  2.5t                #WEIGHT OF FUEL
WEIGLOAD 10.0t                #WEIGHT OF PAYLOAD
FUELABRN  2.00kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.50kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.87m  3.70m #COCKPIT POSITION [view pos of "TANPEI"]
REM COCKPITP  0.40m  0.87m  2.80m # [view pos of "JUNKO"]
LEFTGEAR -0.65m -1.76m -0.60m #LEFT LANDING GEAR POSITION
RIGHGEAR  0.65m -1.76m -0.60m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.76m  1.38m #WHEEL POSITION
ARRESTER  0.00m -1.67m -0.70m #ARRESTING HOOK POSITION
MACHNGUN -0.25m  1.23m  5.22m #MACHINE GUN POSITION
MACHNGN2  0.25m  1.23m  5.22m #MACHINE GUN POSITION2
SMOKEGEN  0.00m -0.75m -2.63m #SMOKE GENERATOR POSITION
VAPORPO0  1.81m  0.01m -4.37m #VAPOR POSITION (Wings Swept back)
VAPORPO1  1.81m  0.01m -4.37m #VAPOR POSITION (Wings Spread)
HTRADIUS 10.00m               #OUTSIDE SPHERE RADIUS

STRENGTH 24

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 200kt                #CRITICAL SPEED [250km/h]
MAXSPEED 320kt                #MAXIMUM SPEED [400km/h]

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

VGWSPED1 64kt
VGWSPED2 70kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.4                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.1                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.3                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 25m^2                #WING AREA

MXIPTAOA  23.5deg             #MAX INPUT AOA
MXIPTSSA   7.5deg             #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 45kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 70kt                 #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.5                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 2

INITIGUN 3000                 #INITIAL GUN
INITIAAM 4                    #INITIAL AAM
INITIAGM 2                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

SMOKEOIL 100.0kg
WEAPONCH SMK

MAXNMAAM 8
MAXNMAGM 4
MAXNBOMB 4
MAXNMRKT 18

AAMVISIB FALSE
AAMSLOT_  0.25m 0.00m 5.62m
AAMSLOT_ -0.25m 0.00m 5.62m

AGMVISIB FALSE
AGMSLOT_  0.25m 0.00m 5.62m
AGMSLOT_ -0.25m 0.00m 5.62m

BOMVISIB FALSE
BOMBSLOT  0.30m -0.50m 4.00m
BOMBSLOT -0.30m -0.50m 4.00m

RKTVISIB FALSE
RKTSLOT_  0.25m 1.23m 6.62m
RKTSLOT_ -0.25m 1.23m 6.62m



REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 256kt                #CRUISING SPEED            [320km/h]
REFACRUS 5000ft               #CRUISING ALTITUDE         [1500ft]
REFTCRUS 0.65                 #CRUISING THROTTLE SETTING [0.6]

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 90kt                 #LANDING SPEED           [100km/h]
REFAOALD 10deg                #LANDING AOA                         [TailStrikeAngle:13.67deg]
REFLNRWY 50m                  #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
