REM TIMEBOKAN - TIME KuwaGattan_

REM "tmb3kgtn_kz.dat/tmb3kgtn_kz.dnm/tmb3kgtn_kz_cockpit.srf"
REM and "tmb3kgtn_kz_coll.srf/tmb3kgtn_kz_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2002-2003,2004 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_TIME-KUWAGATTAN"
CATEGORY UTILITY

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 45.5t                #THRUST WITH AFTERBURNER
THRMILIT 30.0t                #THRUST AT MILITARY POWER
WEIGHCLN 20.0t                #WEIGHT CLEAN
WEIGFUEL  2.5t                #WEIGHT OF FUEL
WEIGLOAD 15.0t                #WEIGHT OF PAYLOAD
FUELABRN  1.50kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.45kg              #FUEL CONSUMPTION AT MILITARY POWER

REM COCKPITP -0.35m  0.29m  0.37m #COCKPIT POSITION
COCKPITP  0.00m  0.29m  0.35m #COCKPIT POSITION
LEFTGEAR -3.46m -2.38m  1.63m #LEFT LANDING GEAR POSITION
RIGHGEAR  3.46m -2.38m  1.63m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -0.73m -4.75m #WHEEL POSITION
ARRESTER  0.00m -0.70m -3.37m #ARRESTING HOOK POSITION
MACHNGUN  0.00m -0.62m  0.80m #MACHINE GUN POSITION
SMOKEGEN  0.00m -0.31m -4.74m #SMOKE GENERATOR POSITION
VAPORPO0  5.25m  1.63m -0.69m #VAPOR POSITION (Wings Swept back)
VAPORPO1  5.25m  1.63m -0.69m #VAPOR POSITION (Wings Spread)
HTRADIUS 10.00m               #OUTSIDE SPHERE RADIUS

STRENGTH 20

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 100kt                #CRITICAL SPEED
MAXSPEED 135kt                #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

VGWSPED1 30kt
VGWSPED2 31kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.4                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.1                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.4                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 30m^2                #WING AREA

MXIPTAOA  22.0deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 200.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 30kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 40kt                 #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 4.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 2

INITIGUN 3000                 #INITIAL GUN
INITIAAM 4                    #INITIAL AAM
INITIAGM 6                    #INITIAL AGM
INITBOMB 8                    #INITIAL BOMB
INITRCKT 18                   #INITIAL ROCKET (10kg/each)

SMOKEOIL 100.0kg
WEAPONCH SMK

MAXNMAAM 6
MAXNMAGM 8
MAXNBOMB 12
MAXNMRKT 38

AAMVISIB FALSE
AAMSLOT_  0.35m -0.90m -0.50m
AAMSLOT_ -0.35m -0.90m -0.50m

AGMVISIB FALSE
AGMSLOT_  0.35m -0.90m -0.50m
AGMSLOT_ -0.35m -0.90m -0.50m

BOMVISIB FALSE
BOMBSLOT  0.35m -0.90m -0.50m
BOMBSLOT -0.35m -0.90m -0.50m

RKTVISIB FALSE
RKTSLOT_  0.35m -0.90m -0.50m
RKTSLOT_ -0.35m -0.90m -0.50m



REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 130kt                #CRUISING SPEED
REFACRUS 1500ft               #CRUISING ALTITUDE
REFTCRUS 0.6                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 54kt                 #LANDING SPEED
REFAOALD 10deg                #LANDING AOA                         [TailStrikeAngle:71.10deg]
REFLNRWY 180m                 #RUNWAY REQUIRED TO STOP             [standing AOA   :14.50deg]
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
