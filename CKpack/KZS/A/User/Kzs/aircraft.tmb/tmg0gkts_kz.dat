REM TIMEBOKAN - TIME Gaikotts

REM "tmg0gkts_kz.dat/tmg0gkts_kz.dnm/tmg0gkts_kz_cockpit.srf"
REM and "tmg0gkts_kz_coll.srf/tmg0gkts_kz_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2002-2003,2004 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs]_TIME-GAIKOTTS"
CATEGORY UTILITY

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 75.5t                #THRUST WITH AFTERBURNER
THRMILIT 18.0t                #THRUST AT MILITARY POWER
WEIGHCLN  6.5t                #WEIGHT CLEAN
WEIGFUEL  1.8t                #WEIGHT OF FUEL
WEIGLOAD  1.0t                #WEIGHT OF PAYLOAD
FUELABRN  7.50kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.25kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP -0.25m  0.10m  0.20m #COCKPIT POSITION
LEFTGEAR -1.00m -1.00m -0.55m #LEFT LANDING GEAR POSITION
RIGHGEAR  1.00m -1.00m -0.55m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.00m  1.24m #WHEEL POSITION
ARRESTER  0.00m -0.95m -1.00m  #ARRESTING HOOK POSITION
MACHNGUN  0.00m -0.45m  1.14m #MACHINE GUN POSITION
SMOKEGEN  0.00m -0.73m -1.20m #SMOKE GENERATOR POSITION
VAPORPO0  1.55m -0.26m -0.55m #VAPOR POSITION (Wings Swept back)
VAPORPO1  1.55m -0.26m -0.55m #VAPOR POSITION (Wings Spread)
HTRADIUS  5.00m               #OUTSIDE SPHERE RADIUS

STRENGTH 24

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 100km/h              #CRITICAL SPEED
MAXSPEED 250km/h              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

REM VGWSPED1 10kt
REM VGWSPED2 15kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.4                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.1                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.2                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 20m^2                #WING AREA

MXIPTAOA  23.0deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 20kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 40kt                 #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 1

INITIGUN 3000                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

MAXNMAAM 2
MAXNMAGM 6
MAXNBOMB 8
MAXNMRKT 38

AAMVISIB FALSE
AAMSLOT_  0.45m -0.85m 0.40m
AAMSLOT_ -0.45m -0.85m 0.40m

AGMVISIB FALSE
AGMSLOT_  0.45m -0.85m 0.40m
AGMSLOT_ -0.45m -0.85m 0.40m

BOMVISIB FALSE
BOMBSLOT  0.45m -0.85m 0.40m
BOMBSLOT -0.45m -0.85m 0.40m

RKTVISIB FALSE
RKTSLOT_  0.45m -0.85m 0.40m
RKTSLOT_ -0.45m -0.85m 0.40m



REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 225km/h              #CRUISING SPEED
REFACRUS 1000ft               #CRUISING ALTITUDE
REFTCRUS 0.4                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 100km/h              #LANDING SPEED
REFAOALD 10deg                #LANDING AOA                         [TailStrikeAngle:24.04deg]
REFLNRWY 350m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
