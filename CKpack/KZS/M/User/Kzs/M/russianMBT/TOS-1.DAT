IDENTIFY "TOS-1[Buratino]"
CATEGORY UTILITY
AFTBURNR FALSE              #HAVE AFTERBURNER?
PROPELLR 780HP             #PROPELLER ENGINE
PROPEFCY 2            #Propeller efficiency
WEIGHCLN 41.0t             #WEIGHT CLEAN
WEIGFUEL 600kg             #WEIGHT OF FUEL
WEIGLOAD 1t                #WEIGHT OF PAYLOAD
FUELMILI 0.06kg             #FUEL CONSUMPTION AT MILITARY POWER
GUNSIGHT FALSE
THRSTREV 0.4                 #EFFICIENCY OF THRUST REVERSER

COCKPITP  0m  1.35m 1.39m
EXCAMERA  "comander" 0.36m  1.95m -0.21m 0deg 0deg 0deg INSIDE
LEFTGEAR  -1.38m -0.005m -2.65m
RIGHGEAR  1.38m -0.005m -2.65m 
WHELGEAR  0.0m -0.001m  2.11m
ARRESTER  0.0m 0m 0m
GUNDIREC  0.0m 0.0m 0.0m     #MACHINE GUN DIRECTION
SMOKEGEN 0.00m 0m -0m  #SMOKE GENERATOR POSITION
VAPORPO0  0m  0m 0m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  0m  0m 0m  #VAPOR POSITION (Wings Spread)
HTRADIUS  6m                #OUTSIDE SPHERE RADIUS
RADARCRS 0.7

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK
STRENGTH 45

CRITAOAP 0deg 
CRITAOAM 0deg
  
CRITSPED 33km/h
MAXSPEED 60km/h

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR FALSE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 1m^2                #WING AREA

MXIPTAOA 1deg  
MXIPTSSA 5.0deg
MXIPTROL 1deg

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40km/h
MANESPD2 55km/h  
CPITMANE 0
CPITSTAB 50
CYAWMANE 8 
CYAWSTAB 6.2 
CROLLMAN 40

CTLLDGEA TRUE              
CTLBRAKE FALSE
CTLSPOIL 0.5
CTLABRNR FALSE    
CTLTHROT 0.0      
CTLIFLAP 0.0       
CTLINVGW 1.0        
CTLATVGW FALSE

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED


HRDPOINT 0.68m 3.13m 0.18m RKT
HRDPOINT 0.42m 3.13m 0.18m RKT
HRDPOINT 0.15m 3.13m 0.18m RKT
HRDPOINT -0.13m 3.13m 0.18m RKT
HRDPOINT -0.40m 3.13m 0.18m RKT
HRDPOINT -0.67m 3.13m 0.18m RKT
HRDPOINT 0.94m 2.85m 0.18m RKT
HRDPOINT 0.68m 2.85m 0.18m RKT
HRDPOINT 0.42m 2.85m 0.18m RKT
HRDPOINT 0.15m 2.85m 0.18m RKT
HRDPOINT -0.13m 2.85m 0.18m RKT
HRDPOINT -0.40m 2.85m 0.18m RKT
HRDPOINT -0.67m 2.85m 0.18m RKT
HRDPOINT -0.93m 2.85m 0.18m RKT
HRDPOINT 0.94m 2.57m 0.18m RKT
HRDPOINT 0.68m 2.57m 0.18m RKT
HRDPOINT 0.42m 2.57m 0.18m RKT
HRDPOINT 0.15m 2.57m 0.18m RKT
HRDPOINT -0.13m 2.57m 0.18m RKT
HRDPOINT -0.40m 2.57m 0.18m RKT
HRDPOINT -0.67m 2.57m 0.18m RKT
HRDPOINT -0.93m 2.57m 0.18m RKT
HRDPOINT 0.94m 2.30m 0.18m RKT
HRDPOINT 0.68m 2.30m 0.18m RKT
HRDPOINT 0.42m 2.30m 0.18m RKT
HRDPOINT 0.15m 2.30m 0.18m RKT
HRDPOINT -0.13m 2.30m 0.18m RKT
HRDPOINT -0.40m 2.30m 0.18m RKT
HRDPOINT -0.67m 2.30m 0.18m RKT
HRDPOINT -0.93m 2.30m 0.18m RKT

GUNPOWER 1
INITIGUN 0                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITRCKT 30
LOADWEPN RKT 30

RKTVISIB FALSE

NMTURRET 1

TURRETPO 0 0m 1.71m 0m 0deg 0deg 0deg
TURRETPT 0 -3deg 45deg 0deg               
TURRETHD 0 -180deg 180deg 0deg              
TURRETAM 0 0                       
TURRETIV 0 0.3sec
TURRETNP 0 0004
TURRETNH 0 0006          
TURRETAR 0                                           
TURRETCT 0 PILOT                  
TURRETRG 0 2250m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 60km/h              #CRUISING SPEED
REFACRUS 100ft              #CRUISING ALTITUDE
REFTCRUS 1                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 50km/h                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 25m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.75            #LANDING THROTTLE SETTING

AUTOCALC
