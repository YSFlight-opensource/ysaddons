IDENTIFY "Panjandrum"
CATEGORY UTILITY

AFTBURNR FALSE                #HAVE AFTERBURNER?
THRAFTBN 0.0t                 #THRUST WITH AFTERBURNER
THRMILIT 7.0t                 #THRUST AT MILITARY POWER
WEIGHCLN 2.0t                 #WEIGHT CLEAN
WEIGFUEL 0.1t                 #WEIGHT OF FUEL
WEIGLOAD 1.0t                 #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.5kg                #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  1.00m  1.00m #COCKPIT POSITION
LEFTGEAR -1.50m -1.50m -1.50m #LEFT LANDING GEAR POSITION
RIGHGEAR  1.50m -1.50m -1.50m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m  1.50m -1.51m #WHEEL POSITION
ARRESTER  0.00m -1.09m  0.00m
MACHNGUN  0.00m  0.00m  0.00m #MACHINE GUN POSITION
SMOKEGEN  0.00m  0.00m  0.00m #SMOKE GENERATOR POSITION
VAPORPO0  1.50m -1.50m -1.50m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  1.50m -1.50m -1.50m #VAPOR POSITION (Wings Spread)
HTRADIUS  4.0m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP 0deg                 #CRITICAL AOA POSITIVE
CRITAOAM 0deg                 #CRITICAL AOA NEGATIVE

CRITSPED 200kt                #CRITICAL SPEED
MAXSPEED 500kt                #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR FALSE                #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m -1.0m 0.0m      #Thrust vector 0
TRSTDIR1 0.0m -1.0m 0.0m      #Thrust vector 1
PSTMPTCH   1deg               #Post-Stall Maneuver VPitch
PSTMYAW_   0deg               #Post-Stall Maneuver VYaw
PSTMROLL 100deg               #Post-Stall Maneuver VRoll

CLVARGEO  0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO  0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP  0.0                 #EFFECT OF FLAP FOR Cl
CDBYFLAP  0.0                 #EFFECT OF FLAP FOR Cd
CDBYGEAR  0.0                 #EFFECT OF GEAR FOR Cd
CDSPOILR  0.0                 #EFFECT OF SPOILER FOR Cd

WINGAREA 50m^2                #WING AREA

MXIPTAOA 0.0deg               #MAX INPUT AOA
MXIPTSSA 0.0deg               #MAX INPUT SSA
MXIPTROL 0.0deg               #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 1000kt               #MINIMUM MANEUVABLE SPEED
MANESPD2 1000kt               #FULLY MANEUVABLE SPEED
CPITMANE  10                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB   0.01               #PITCH STABILITY CONSTANT
CYAWMANE   0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB   0                  #YAW STABILITY CONSTANT
CROLLMAN -10.0                #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1 0.0kt
VGWSPED2 0.1kt

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

INITIGUN 0                    #INITIAL GUN

HRDPOINT 0m 0m -1.4m B250
LMTBYHDP TRUE
LOADWEPN B250 1
BOMVISIB FALSE

RADARCRS 0.001                #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 200kt                #CRUISING SPEED
REFACRUS 0ft                  #CRUISING ALTITUDE
REFTCRUS 0.1                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 120kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 100m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.30                 #LANDING THROTTLE SETTING

AUTOCALC
