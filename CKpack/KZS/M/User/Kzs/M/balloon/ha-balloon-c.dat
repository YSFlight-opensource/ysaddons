IDENTIFY "Hot-Air Balloon(C)"
CATEGORY UTILITY

AFTBURNR TRUE                  #HAVE AFTERBURNER?
THRAFTBN 430kg                 #THRUST WITH AFTERBURNER

THRMILIT  0.0t                 #THRUST AT MILITARY POWER

WEIGHCLN  400kg                #WEIGHT CLEAN
WEIGFUEL   20kg                #WEIGHT OF FUEL
WEIGLOAD  100kg                #WEIGHT OF PAYLOAD
FUELABRN  0.01kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.0kg                #FUEL CONSUMPTION AT MILITARY POWER

INSTPANL User/Kzs/M/balloon/ha-balloon.ist
COCKPITP -0.3066m -1.308m -0.186m #COCKPIT POSITION
EXCAMERA "F"  0m -2m  1.5m   0deg -45deg 0deg INSIDE
EXCAMERA "L" -1m -2m    0m  90deg -45deg 0deg INSIDE
EXCAMERA "R"  1m -2m    0m -90deg -45deg 0deg INSIDE
EXCAMERA "B"  0m -2m -1.5m 180deg -45deg 0deg INSIDE

LEFTGEAR -1.18m -3.068m -1.18m    #LEFT LANDING GEAR POSITION
RIGHGEAR  1.18m -3.068m -1.18m    #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -3.068m  1.18m    #WHEEL POSITION
ARRESTER  0.00m  0.00m   0.00m
MACHNGUN  0.00m  0.00m   0.00m    #MACHINE GUN POSITION
SMOKEGEN  0.00m  0.00m   0.00m    #SMOKE GENERATOR POSITION
VAPORPO0  0.00m  0.00m   0.00m    #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  0.00m  0.00m   0.00m    #VAPOR POSITION (Wings Spread)
HTRADIUS  20.0m                   #OUTSIDE SPHERE RADIUS

TRIGGER1 NON
TRIGGER2 NON
TRIGGER3 NON
TRIGGER4 NON

STRENGTH 5

CRITAOAP 90deg               #CRITICAL AOA POSITIVE
CRITAOAM -1deg               #CRITICAL AOA NEGATIVE

CRITSPED 18kt                #CRITICAL SPEED
MAXSPEED 45kt                #MAXIMUM SPEED

HASSPOIL FALSE               #HAS SPOILER?
RETRGEAR FALSE               #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE               #HAS VARIABLE GEOMETRY WING?

TRSTVCTR TRUE                #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m 1.0m 0.1m      #Thrust vector 0
TRSTDIR1 0.0m 1.0m 0.1m      #Thrust vector 1
PSTMPTCH  0deg               #Post-Stall Maneuver VPitch
PSTMYAW_  5deg               #Post-Stall Maneuver VYaw
PSTMROLL  0deg               #Post-Stall Maneuver VRoll

CLVARGEO -0.7                #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO  0.0                #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP  3.0                #EFFECT OF FLAP FOR Cl
CDBYFLAP  0.0                #EFFECT OF FLAP FOR Cd
CDBYGEAR  1.0                #EFFECT OF GEAR FOR Cd
CDSPOILR  5.0                #EFFECT OF SPOILER FOR Cd

WINGAREA 16m^2               #WING AREA

MXIPTAOA  0.0deg             #MAX INPUT AOA
MXIPTSSA  0.0deg             #MAX INPUT SSA
MXIPTROL  0.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 5000kt               #MINIMUM MANEUVABLE SPEED
MANESPD2 5000kt               #FULLY MANEUVABLE SPEED
CPITMANE 0.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 1.0                  #PITCH STABILITY CONSTANT
CYAWMANE 0.1                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 0.01                 #YAW STABILITY CONSTANT
CROLLMAN 1.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE TRUE                 #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 1.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1 0kt
VGWSPED2 0kt

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

INITIGUN 0                    #INITIAL GUN
GUNSIGHT FALSE

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 45kt                 #CRUISING SPEED
REFACRUS 5000ft               #CRUISING ALTITUDE
REFTCRUS 0.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 20kt                 #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 5m                   #RUNWAY REQUIRED TO STOP
REFTHRLD 0.0                  #LANDING THROTTLE SETTING

NMTURRET 1
TURRETPO 0 -0.930m -1.926m 0.468m 0deg 0deg 0deg # Number x y z h p b
TURRETPT 0  -90deg  90deg 0deg                   # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 -180deg 180deg 0deg                   # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 1                                     # Ammo
TURRETIV 0 1sec                                  # Number ShootingInterval
TURRETNM 0 CAMERA                                # DNM Node Name
TURRETCT 0 PILOT                                 # Controlled by PILOT or GUNNER

AUTOCALC
