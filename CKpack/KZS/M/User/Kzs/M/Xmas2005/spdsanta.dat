REM  Imperial Speeder Bike & SANTA CLAUS
REM  Manufactured by Makoto 2005/12

IDENTIFY "SPEEDER_SANTA"
CATEGORY UTILITY
AIRCLASS HELICOPTER

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 1050.0t              #THRUST WITH AFTERBURNER
THRMILIT 1005.0t              #THRUST AT MILITARY POWER
WEIGHCLN  100.0t              #WEIGHT CLEAN
WEIGFUEL    0.01kg            #WEIGHT OF FUEL
WEIGLOAD   10.0t              #WEIGHT OF PAYLOAD
FUELABRN    0.00001kg         #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI    0.0000005kg       #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.90m -0.37m #COCKPIT POSITION
LEFTGEAR -0.50m -0.49m -0.50m #LEFT LANDING GEAR POSITION
RIGHGEAR  0.50m -0.49m -0.50m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -0.49m  0.50m #WHEEL POSITION
ARRESTER  0.00m -0.49m  0.00m
MACHNGUN  0.00m -0.14m  0.24m #MACHINE GUN POSITION
SMOKEGEN  0.00m -0.18m -1.59m #SMOKE GENERATOR POSITION
VAPORPO0  0.00m  0.00m  0.00m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  0.00m  0.00m  0.00m #VAPOR POSITION (Wings Spread)
HTRADIUS  3.0m                #OUTSIDE SPHERE RADIUS

RADARCRS 0.10                 #RADAR CROSS SECTION

TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m 0.1m 1.0m       #Thrust vector 0
TRSTDIR1 0.0m 0.1m 1.0m       #Thrust vector 1
PSTMPTCH 0deg                 #Post-Stall Maneuver VPitch
PSTMYAW_ 90deg                #Post-Stall Maneuver VYaw
PSTMROLL 0deg                 #Post-Stall Maneuver VRoll

TRIGGER1 RKT
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 5

CRITAOAP  0.01deg             #CRITICAL AOA POSITIVE
CRITAOAM -0.01deg             #CRITICAL AOA NEGATIVE

CRITSPED  50.0kt              #CRITICAL SPEED
MAXSPEED 112.5kt              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR FALSE                #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

CLVARGEO   0.0                #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO   5.0                #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP   0.0                #EFFECT OF FLAP FOR Cl
CDBYFLAP 100.0                #EFFECT OF FLAP FOR Cd
CDBYGEAR   0.0                #EFFECT OF GEAR FOR Cd
CDSPOILR   5.0                #EFFECT OF SPOILER FOR Cd

WINGAREA 1.0m^2               #WING AREA

MXIPTAOA  0.0deg              #MAX INPUT AOA
MXIPTSSA 90.0deg              #MAX INPUT SSA
MXIPTROL  0.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 1000kt               #MINIMUM MANEUVABLE SPEED
MANESPD2 1000kt               #FULLY MANEUVABLE SPEED
CPITMANE  0.01                #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  9.99                #PITCH STABILITY CONSTANT
CYAWMANE 10.00                #YAW MANEUVABILITY CONSTANT
CYAWSTAB  2.00                #YAW STABILITY CONSTANT
CROLLMAN  0.01                #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1 1000.0kt
VGWSPED2 1000.0kt

POSITION 0m 0ft 0m            #POSITION
ATTITUDE 0deg 0deg 0deg       #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

INITIGUN 0                    #INITIAL GUN

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.0kt                #CRUISING SPEED
REFACRUS 1.0ft                #CRUISING ALTITUDE
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND   0kt                #LANDING SPEED
REFAOALD   0deg               #LANDING AOA
REFLNRWY 100m                 #RUNWAY REQUIRED TO STOP
REFTHRLD   0                  #LANDING THROTTLE SETTING

NMTURRET 1
TURRETPO 0 0m -0.14m 0.24m 0deg 0deg 0deg    # Number x y z h p b
TURRETPT 0  -5deg  5deg 0deg                 # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 -40deg 40deg 0deg                 # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 1000                              # Ammo
TURRETIV 0 0.33sec                           # Number ShootingInterval
TURRETNM 0 CANNON                            # DNM Node Name
TURRETAR 0                                   # Anti Air Capable
TURRETCT 0 PILOT                             # Controlled by PILOT or GUNNER

AUTOCALC
