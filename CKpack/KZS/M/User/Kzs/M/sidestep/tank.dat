IDENTIFY "RC_TANK" 
CATEGORY NORMAL
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 2000.0kg            #THRUST WITH AFTERBURNER
THRMILIT  500.0kg             #THRUST AT MILITARY POWER
THRSTREV  1.0                 #EFFICIENCY OF THRUST REVERSER
WEIGHCLN  52.0kg              #WEIGHT CLEAN
WEIGFUEL  0.5kg               #WEIGHT OF FUEL
WEIGLOAD  0kg                 #WEIGHT OF PAYLOAD
FUELABRN  0.00kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.00kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.40m -2.95m #COCKPIT POSITION
LEFTGEAR -0.16m -1.00m -0.19m #LEFT LANDING GEAR POSITION
RIGHGEAR  0.16m -1.00m -0.19m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.00m  0.18m #WHEEL POSITION
ARRESTER  0.00m -1.00m  0.00m
MACHNGUN  0.00m -0.80m  0.36m #MACHINE GUN POSITION
SMOKEGEN  0.00m  0.00m  0.00m #SMOKE GENERATOR POSITION
VAPORPO0  0.00m  0.00m  0.00m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  0.00m  0.00m  0.00m #VAPOR POSITION (Wings Spread)
HTRADIUS  3.5m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 50

CRITAOAP  0.0deg             #CRITICAL AOA POSITIVE
CRITAOAM  0.0deg             #CRITICAL AOA NEGATIVE

CRITSPED 10kt                #CRITICAL SPEED
MAXSPEED 20kt                #MAXIMUM SPEED

HASSPOIL TRUE                #HAS SPOILER?
RETRGEAR FALSE               #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE               #HAS VARIABLE GEOMETRY WING?

TRSTVCTR TRUE                #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m 0.0m 1.0m      #Thrust vector 0
TRSTDIR1 1.0m 0.0m 0.0m      #Thrust vector 1
PSTMPTCH  0deg               #Post-Stall Maneuver VPitch
PSTMYAW_ 90deg               #Post-Stall Maneuver VYaw
PSTMROLL  0deg               #Post-Stall Maneuver VRoll

CLVARGEO 0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.0                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 8.0                 #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.2                 #EFFECT OF GEAR FOR Cd
CDSPOILR 5.0                 #EFFECT OF SPOILER FOR Cd

WINGAREA 1m^2                #WING AREA

MXIPTAOA   0.0deg            #MAX INPUT AOA
MXIPTSSA  20.0deg            #MAX INPUT SSA
MXIPTROL   0.0deg            #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt               #MINIMUM MANEUVABLE SPEED
MANESPD2 100kt               #FULLY MANEUVABLE SPEED
CPITMANE  0.00               #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  0.00               #PITCH STABILITY CONSTANT
CYAWMANE 10.00               #YAW MANEUVABILITY CONSTANT
CYAWSTAB 10.00               #YAW STABILITY CONSTANT
CROLLMAN  0.00               #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                #CONTROL LANDINGGEAR
CTLBRAKE FALSE               #CONTROL BRAKE
CTLSPOIL 0.0                 #CONTROL SPOILER
CTLABRNR FALSE               #CONTROL AFTERBURNER
CTLTHROT 0.0                 #CONTROL THRUST
CTLIFLAP 0.0                 #CONTROL FLAP
CTLINVGW 1.0                 #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1 0kt
VGWSPED2 1kt

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg     #ATTITUDE
INITFUEL 100%                #INITIAL FUEL
INITLOAD 0.0t                #INITIAL PAYLOAD
INITSPED 0.0MACH             #INITIAL SPEED

INITIGUN 1000                #INITIAL GUN
GUNPOWER 0

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 10kt                #CRUISING SPEED
REFACRUS 10ft                #CRUISING ALTITUDE
REFTCRUS 0.8                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 2kt                 #LANDING SPEED
REFAOALD 10deg               #LANDING AOA
REFLNRWY 1m                  #RUNWAY REQUIRED TO STOP
REFTHRLD 0.50                #LANDING THROTTLE SETTING

AUTOCALC
