IDENTIFY "RocketBelt" 
CATEGORY NORMAL
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 5000.0kg             #THRUST WITH AFTERBURNER
THRMILIT 2050.0kg             #THRUST AT MILITARY POWER
WEIGHCLN 1200.8kg             #WEIGHT CLEAN
WEIGFUEL  220.7kg             #WEIGHT OF FUEL
WEIGLOAD    0.0kg             #WEIGHT OF PAYLOAD
FUELABRN    4.0kg             #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI    0.4kg             #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.41m  0.20m #COCKPIT POSITION
LEFTGEAR -0.14m -1.25m -0.01m #LEFT LANDING GEAR POSITION
RIGHGEAR  0.14m -1.25m -0.01m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.25m  0.50m #WHEEL POSITION
ARRESTER  0.00m -1.25m  0.00m
MACHNGUN  0.00m  0.00m  0.00m #MACHINE GUN POSITION
SMOKEGEN  0.00m  0.50m -0.27m #SMOKE GENERATOR POSITION
VAPORPO0  0.43m -0.13m  0.05m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  0.43m -0.13m  0.05m #VAPOR POSITION (Wings Spread)
HTRADIUS  2.5m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 5

CRITAOAP  180deg              #CRITICAL AOA POSITIVE
CRITAOAM -180deg              #CRITICAL AOA NEGATIVE

CRITSPED 100kt                #CRITICAL SPEED
MAXSPEED 200kt                #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR FALSE                #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m 0.9m  0.3m      #Thrust vector 0
TRSTDIR1 0.0m 0.9m -0.3m      #Thrust vector 1
PSTMPTCH  30deg               #Post-Stall Maneuver VPitch
PSTMYAW_ 180deg               #Post-Stall Maneuver VYaw
PSTMROLL  30deg               #Post-Stall Maneuver VRoll

CLVARGEO  0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO  0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP  0.0                 #EFFECT OF FLAP FOR Cl
CDBYFLAP  0.0                 #EFFECT OF FLAP FOR Cd
CDBYGEAR  0.0                 #EFFECT OF GEAR FOR Cd
CDSPOILR  0.0                 #EFFECT OF SPOILER FOR Cd

WINGAREA 1m^2                 #WING AREA

MXIPTAOA 0.0deg               #MAX INPUT AOA
MXIPTSSA 0.0deg               #MAX INPUT SSA
MXIPTROL 0.0deg               #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 10.0MACH             #MINIMUM MANEUVABLE SPEED
MANESPD2 10.0MACH             #FULLY MANEUVABLE SPEED
CPITMANE  5.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  2.0                 #PITCH STABILITY CONSTANT
CYAWMANE  1.0                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB  2.0                 #YAW STABILITY CONSTANT
CROLLMAN  5.0                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE TRUE                 #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1  0kt
VGWSPED2 10kt

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

INITIGUN 0                    #INITIAL GUN

RADARCRS 0.05                 #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 120kt                #CRUISING SPEED
REFACRUS 7000ft               #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 50kt                 #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 10m                  #RUNWAY REQUIRED TO STOP
REFTHRLD 0.50                 #LANDING THROTTLE SETTING

AUTOCALC

