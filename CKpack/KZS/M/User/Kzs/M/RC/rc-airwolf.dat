IDENTIFY "RC-AIRWOLF"
CATEGORY UTILITY

AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 10000HP
PROPEFCY 0.50                 #Propeller efficiency
PROPVMIN 20m/s                #Minimum speed that T=P/V becomes valid
WEIGHCLN 20640kg              #WEIGHT CLEAN
WEIGFUEL 14970kg              #WEIGHT OF FUEL
WEIGLOAD 14060kg              #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 2.0kg                #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  0.0m  1.0m    #COCKPIT POSITION
EXCAMERA "NOHUD" 0.0m 0.047m 1.0m 0deg 0deg 0deg INSIDE
EXCAMERA "BACK"  0.0m 0.209m -2.0m 0deg 0deg 0deg OUTSIDE

LEFTGEAR -0.144m -0.124m -0.087m #LEFT LANDING GEAR POSITION
RIGHGEAR  0.144m -0.124m -0.087m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.000m -0.124m  0.290m #WHEEL POSITION
ARRESTER  0.000m -0.124m -0.087m
MACHNGUN  0.000m  0.000m  0.000m #MACHINE GUN POSITION
SMOKEGEN  0.000m -0.030m -0.250m #SMOKE GENERATOR POSITION
VAPORPO0  0.638m  0.233m  0.00m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  0.638m  0.233m  0.00m
HTRADIUS  1.0m                   #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 1

CRITAOAP  10deg               #CRITICAL AOA POSITIVE
CRITAOAM -10deg               #CRITICAL AOA NEGATIVE

CRITSPED  50kt                #CRITICAL SPEED
MAXSPEED 100kt                #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m 1.0m  0.5m      #Thrust vector 0
TRSTDIR1 0.0m 1.0m -0.5m      #Thrust vector 1
PSTMPTCH   0deg               #Post-Stall Maneuver VPitch
PSTMYAW_ 360deg               #Post-Stall Maneuver VYaw
PSTMROLL   0deg               #Post-Stall Maneuver VRoll

CLVARGEO  0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO  0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP  0.0                 #EFFECT OF FLAP FOR Cl
CDBYFLAP  0.0                 #EFFECT OF FLAP FOR Cd
CDBYGEAR  0.0                 #EFFECT OF GEAR FOR Cd
CDSPOILR  0.0                 #EFFECT OF SPOILER FOR Cd

WINGAREA 1m^2                 #WING AREA

MXIPTAOA 0.0deg               #MAX INPUT AOA
MXIPTSSA 0.0deg               #MAX INPUT SSA
MXIPTROL 0.0deg               #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 1000.0kt             #MINIMUM MANEUVABLE SPEED
MANESPD2 1000.0kt             #FULLY MANEUVABLE SPEED
CPITMANE  0.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  1.0                 #PITCH STABILITY CONSTANT
CYAWMANE  1.0                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB  2.0                 #YAW STABILITY CONSTANT
CROLLMAN  1.0                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE TRUE                 #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1  0kt
VGWSPED2 10kt

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

INITIGUN 0                    #INITIAL GUN
GUNSIGHT FALSE

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 60kt                 #CRUISING SPEED
REFACRUS 7000ft               #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 25kt                 #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 10m                  #RUNWAY REQUIRED TO STOP
REFTHRLD 0.50                 #LANDING THROTTLE SETTING

AUTOCALC
