TURRETPT 3 0deg 0deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 3 -180deg 180deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 3 0                             # Ammo
TURRETIV 3 1.5sec                            # Number ShootingInterval
TURRETNM 3 GUNTURRET                           # DNM Node Name
TURRETAR 3                                   # Anti Air Capable
TURRETCT 3 PILOT                            # Controlled by PILOT or GUNNER
TURRETRG 3 4000m

TURRETPO 4 0m 0.75m 0.3m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 4 -10deg 12deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 4 0deg 0deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 4 0                             # Ammo
TURRETIV 4 4sec                            # Number ShootingInterval
TURRETNM 4 GUN                           # DNM Node Name
TURRETAR 4                                   # Anti Air Capable
TURRETCT 4 PILOT                           # Controlled by PILOT or GUNNER
TURRETRG 4 4000m

TURRETPO 5 0m 0.7m 0.3m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 5 -10deg 12deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 5 -180deg 180deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 5 500                             # Ammo
TURRETIV 5 4sec                            # Number ShootingInterval
TURRETAR 5                                   # Anti Air Capable
TURRETCT 5 PILOT                           # Controlled by PILOT or GUNNER
TURRETRG 5 4000m

TURRETPO 6 0m 0.8m 0.3m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 6 -10deg 12deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 6 -180deg 180deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 6 500                             # Ammo
TURRETIV 6 4sec                            # Number ShootingInterval
TURRETAR 6                                   # Anti Air Capable
TURRETCT 6 PILOT                           # Controlled by PILOT or GUNNER
TURRETRG 6 4000m

TURRETPO 7 0m 0.75m 0.2m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 7 -10deg 12deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 7 -180deg 180deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 7 500                             # Ammo
TURRETIV 7 4sec                            # Number ShootingInterval
TURRETAR 7                                   # Anti Air Capable
TURRETCT 7 PILOT                           # Controlled by PILOT or GUNNER
TURRETRG 7 4000m

TURRETPO 8 0m 0.75m 0.4m 0deg 0deg 0deg      # Number x y z h p b
TURRETPT 8 -10deg 12deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 8 -180deg 180deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 8 500                             # Ammo
TURRETIV 8 4sec                            # Number ShootingInterval
TURRETAR 8                                   # Anti Air Capable
TURRETCT 8 PILOT                           # Controlled by PILOT or GUNNER
TURRETRG 8 4000m



REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 70km/h              #CRUISING SPEED
REFACRUS 100ft              #CRUISING ALTITUDE
REFTCRUS 0.9                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 25km/h                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 15m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.8                  #LANDING THROTTLE SETTING

AUTOCALC
