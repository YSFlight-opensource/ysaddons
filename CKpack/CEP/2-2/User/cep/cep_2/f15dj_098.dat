REM F-15 EAGLE
IDENTIFY "F-15DJ_203SQ"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 21.6t                #THRUST WITH AFTERBURNER
THRMILIT 15.0t                #THRUST AT MILITARY POWER
WEIGHCLN 12.9t                #WEIGHT CLEAN
WEIGFUEL  7.7t                #WEIGHT OF FUEL
WEIGLOAD  9.0t                #WEIGHT OF PAYLOAD
FUELABRN 6.5kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.55kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  0.89m   5.6m  #COCKPIT POSITION
EXCAMERA "CO_P" 0.0m 0.91m 4.03m 0deg 0deg 0deg INSIDE
LEFTGEAR -1.3m -2.5m -1.45m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.3m -2.5m -1.45m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.5m  4.04m  #WHEEL POSITION
MACHNGUN  1.68m  0.07m  2.55m  #MACHINE GUN POSITION
GUNINTVL 0.02
SMOKEGEN -1.34m -0.99m -2.50m  #SMOKE GENERATOR POSITION
VAPORPO0  6.56m  -0.04m -3.8m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  6.56m  -0.04m -3.8m  #VAPOR POSITION (Wings Spread)
HTRADIUS  13.0m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.5MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd
VGWSPED1 100kt
VGWSPED2 1200kt

WINGAREA 56.5m^2                #WING AREA

MXIPTAOA 24.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 250.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 350kt                #FULLY MANEUVABLE SPEED
CPITMANE 12.5                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.5                  #PITCH STABILITY CONSTANT
CYAWMANE 9.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 4.0                  #YAW STABILITY CONSTANT
CROLLMAN 4.3                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED


HRDPOINT -3.2m -0.36m 0.2m AIM9
HRDPOINT 3.2m -0.36m 0.2m AIM9
HRDPOINT -2.4m -0.36m 0.2m AIM9
HRDPOINT 2.4m -0.36m 0.2m AIM9
HRDPOINT -1.48m -1.11m 1.1m AIM120
HRDPOINT 1.48m -1.11m 1.1m AIM120
HRDPOINT -1.43m -1.08m -2.78m AIM120
HRDPOINT 1.43m -1.08m -2.78m AIM120

HRDPOINT -2.8m -1.05m 0.3m B500
HRDPOINT 2.8m -1.05m 0.3m B500
HRDPOINT -2.55m -0.8m 0.3m B500
HRDPOINT 2.55m -0.8m 0.3m B500
HRDPOINT -3.05m -0.8m 0.3m B500
HRDPOINT 3.05m -0.8m 0.3m B500
HRDPOINT -2.8m -1.05m -2m B500
HRDPOINT 2.8m -1.05m -2m B500
HRDPOINT -2.55m -0.8m -2m B500
HRDPOINT 2.55m -0.8m -2m B500
HRDPOINT -3.05m -0.8m -2m B500
HRDPOINT 3.05m -0.8m -2m B500

MAXNMAAM 4
MAXNMAGM 2
MAXNBOMB 12
MAXNMRKT 38
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint
GUNPOWER 2
INITIGUN 940                #INITIAL GUN
LOADWEPN AIM120 0
LOADWEPN AIM9 2

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.0MACH              #CRUISING SPEED
REFACRUS 30000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 135kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1200m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
