REM F-86D-45 Sabre Dog
REM JASDF "103SQ #04-8199"

IDENTIFY "F-86D_JASDF"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  3.4t                #THRUST WITH AFTERBURNER
THRMILIT  2.46t               #THRUST AT MILITARY POWER
WEIGHCLN  5.65t               #WEIGHT CLEAN
WEIGFUEL  2.57t               #WEIGHT OF FUEL (with 2 droptanks)
WEIGLOAD  0.0t                #WEIGHT OF PAYLOAD
FUELABRN 5.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.50kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP 0.0m 0.88m 3.15m  #COCKPIT POSITION
INSTPANL user/cep/cep_2/f86d.ist

LEFTGEAR -1.18m -1.8m 2.38m   #LEFT LANDING GEAR POSITION
RIGHGEAR  1.18m -1.8m 2.38m   #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.8m  6.32m   #WHEEL POSITION

MACHNGUN 0.44m -1.11m 3.95m
MACHNGN2 0.20m -1.14m 3.95m
MACHNGN3 -0.20m -1.14m 3.95m
MACHNGN4 -0.44m -1.11m 3.95m
MACHNGN5 0.44m -1.195m 3.95m 
MACHNGN6 0.20m -1.205m 3.95m
MACHNGN7 -0.20m -1.215m 3.95m
MACHNGN8 -0.44m -1.225m 3.95m

GUNINTVL 0.1
GUNPOWER 3

SMOKEGEN  0.0m  -0.10m -6.30m #SMOKE GENERATOR POSITION
VAPORPO0  5.65m -0.40m -1.56m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.65m -0.40m -1.56m #VAPOR POSITION (Wings Spread)
HTRADIUS  10.0m               #OUTSIDE SPHERE RADIUS


STRENGTH 8

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 500kt                #CRITICAL SPEED
MAXSPEED 615kt                #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?
VGWSPED1 0kt
VGWSPED2 150kt

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 26.75m^2             #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 180.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 84kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 100kt                #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 5.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

REM Slots must come before Inits.
INITIGUN 300                    #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITRCKT 0                   #INITIAL ROCKET
RKTVISIB FALSE

HRDPOINT 0.44m -1.11m 5.15m RKT
HRDPOINT 0.36m -1.12m 5.15m RKT
HRDPOINT 0.28m -1.13m 5.15m RKT
HRDPOINT 0.20m -1.14m 5.15m RKT
HRDPOINT 0.12m -1.15m 5.15m RKT
HRDPOINT 0.04m -1.16m 5.15m RKT
HRDPOINT -0.04m -1.16m 5.15m RKT
HRDPOINT -0.12m -1.15m 5.15m RKT
HRDPOINT -0.20m -1.14m 5.15m RKT
HRDPOINT -0.28m -1.13m 5.15m RKT
HRDPOINT -0.36m -1.12m 5.15m RKT
HRDPOINT -0.44m -1.11m 5.15m RKT
HRDPOINT 0.44m -1.195m 5.15m RKT
HRDPOINT 0.36m -1.205m 5.15m RKT
HRDPOINT 0.28m -1.215m 5.15m RKT
HRDPOINT 0.20m -1.225m 5.15m RKT
HRDPOINT 0.12m -1.235m 5.15m RKT
HRDPOINT 0.04m -1.245m 5.15m RKT
HRDPOINT -0.04m -1.245m 5.15m RKT
HRDPOINT -0.12m -1.235m 5.15m RKT
HRDPOINT -0.20m -1.225m 5.15m RKT
HRDPOINT -0.28m -1.215m 5.15m RKT
HRDPOINT -0.36m -1.205m 5.15m RKT
HRDPOINT -0.44m -1.195m 5.15m RKT

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 456kt                #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 120kt                #LANDING SPEED
REFAOALD 5deg                 #LANDING AOA
REFLNRWY 1200m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
