REM SU30MKI
IDENTIFY "SU30MKI-Zhuravlik(demo01)"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 25.0t                #THRUST WITH AFTERBURNER
THRMILIT 18.0t                #THRUST AT MILITARY POWER
WEIGHCLN 15.0t                #WEIGHT CLEAN
WEIGFUEL  8.0t                #WEIGHT OF FUEL
WEIGLOAD 10.0t                #WEIGHT OF PAYLOAD
FUELABRN 6.5kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.45kg               #FUEL CONSUMPTION AT MILITARY POWER


COCKPITP  0.0m  1.58m   5.34m  #COCKPIT POSITION
EXCAMERA "CO-PILOT" 0.0m  1.63m   5.44m 0deg -30deg 0deg INSIDE
EXCAMERA "CO-PILOT" 0.25m  1.58m   5.34m -90deg -30deg 0deg INSIDE
EXCAMERA "CO-PILOT" -0.25m  1.58m   5.34m 90deg -30deg 0deg INSIDE
LEFTGEAR -2.35m -1.70m -3.80m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.35m -1.70m -3.80m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.50m  2.66m   #WHEEL POSITION
ARRESTER  0.00m -0.78m -10.30m #Arresting hook position
MACHNGUN  1.08m  0.46m  3.75m  #MACHINE GUN POSITION
GUNINTVL 0.04
SMOKEGEN -0.0m -0.00m -10.6m   #SMOKE GENERATOR POSITION
VAPORPO0  7.39m  0.40m -6.50m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  7.39m  0.40m -6.50m  #VAPOR POSITION (Wings Spread)
HTRADIUS 15.0m                 #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  30deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.4MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd
VGWSPED1 0kt
VGWSPED2 200kt

WINGAREA 62m^2                #WING AREA

MXIPTAOA 70.0deg              #MAX INPUT AOA
MXIPTSSA 8.0deg               #MAX INPUT SSA
MXIPTROL 380.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 20kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 80kt                 #FULLY MANEUVABLE SPEED
CPITMANE 9.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
REM 2*sqrt(8)=5.65685424949238
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.5                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

REM Slots must come before Inits.
HRDPOINT 7.33m 0.16m -4.50m AIM9
HRDPOINT -7.33m 0.16m -4.50m AIM9
HRDPOINT 6.50m -0.08m -4.89m AIM9
HRDPOINT -6.50m -0.08m -4.89m AIM9
HRDPOINT 5.50m -0.08m -4.12m AIM9 AIM120 AGM65
HRDPOINT -5.50m -0.08m -4.12m AIM9 AIM120 AGM65
HRDPOINT 3.95m -0.08m -3.20m AIM9 AIM120 AGM65
HRDPOINT -3.95m -0.08m -3.20m AIM9 AIM120 AGM65

HRDPOINT 1.29m -1.05m -1.90m AIM120 AGM65
HRDPOINT -1.29m -1.05m -1.90m AIM120 AGM65
HRDPOINT 0.00m -0.27m 0.60m B250
HRDPOINT 0.22m -0.01m 0.60m B250
HRDPOINT -0.22m -0.01m 0.60m B250
HRDPOINT 0.00m -0.27m -0.73m B250
HRDPOINT 0.22m -0.01m -0.73m B250
HRDPOINT -0.22m -0.01m -0.73m B250
HRDPOINT 0.00m -0.27m -3.00m B250
HRDPOINT 0.22m -0.01m -3.00m B250
HRDPOINT -0.22m -0.01m -3.00m B250
HRDPOINT 0.00m -0.27m -4.33m B250
HRDPOINT 0.22m -0.01m -4.33m B250
HRDPOINT -0.22m -0.01m -4.33m B250

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 3000                 #INITIAL GUN
LOADWEPN AIM9 8
LOADWEPN AIM120 0
LOADWEPN AGM65 0
LOADWEPN B250 0

WEAPONCH GUN

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 110kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 800m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC