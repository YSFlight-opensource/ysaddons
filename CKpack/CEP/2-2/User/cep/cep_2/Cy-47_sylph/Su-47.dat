IDENTIFY "Su-47(S-37)_Firkin[MAKS]"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 29.0t                #THRUST WITH AFTERBURNER
THRMILIT 19.0t                #THRUST AT MILITARY POWER
WEIGHCLN 24.0t                #WEIGHT CLEAN
WEIGFUEL  3.67t                #WEIGHT OF FUEL
WEIGLOAD 8.0t                #WEIGHT OF PAYLOAD
FUELABRN 5kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.27kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.035

COCKPITP  0.0m  0.99m   6.11m  #COCKPIT POSITION
LEFTGEAR -2.16m -2.50m -2.80m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.16m -2.50m -2.80m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.86m  5.37m  #WHEEL POSITION
ARRESTER  0m -1.73m -8.86m
MACHNGUN  -1.1m  0.57m 3.51m  #MACHINE GUN POSITION
SMOKEGEN -0.0m 0.00m -6.70m  #SMOKE GENERATOR POSITION
VAPORPO0  8.38m  -0.09m -1.02m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  8.38m  -0.09m -1.02m  #VAPOR POSITION (Wings Spread)
HTRADIUS 12.5m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 14

CRITAOAP  35.0deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.3MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.2                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.3                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.4                  #EFFECT OF SPOILER FOR Cd

WINGAREA 56m^2                #WING AREA

MXIPTAOA 29.0deg              #MAX INPUT AOA
MXIPTSSA 9.5deg               #MAX INPUT SSA
MXIPTROL 173.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 76kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 320kt                 #FULLY MANEUVABLE SPEED
CPITMANE 11.1                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 4.0                  #PITCH STABILITY CONSTANT
CYAWMANE 11.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 11.0                  #YAW STABILITY CONSTANT
CROLLMAN 5.5                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE               #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1 350kt
VGWSPED2 351kt

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

MAXNMAAM 999
MAXNMAGM 999
MAXNBOMB 999
MAXNMRKT 38
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

GUNPOWER 3
INITIGUN 150                 #INITIAL GUN
LOADWEPN AIM9 0
LOADWEPN AIM120 0
INITIAGM 0                    #INITIAL AGM

NMTURRET 1

TURRETPO 0 0m 1.03m 5.78m 0deg 0deg 0deg
TURRETPT 0 0deg 24deg 0deg               
TURRETHD 0 0deg 0deg 0deg              
TURRETAM 0 0                       
TURRETIV 0 0.14sec
TURRETNM 0 0084         
TURRETAR 0                                           
TURRETCT 0 PILOT                  
TURRETRG 0 800m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 1.3MACH              #CRUISING SPEED
REFACRUS 30000ft              #CRUISING ALTITUDE
REFTCRUS 0.85                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 135kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.22                 #LANDING THROTTLE SETTING

AUTOCALC
