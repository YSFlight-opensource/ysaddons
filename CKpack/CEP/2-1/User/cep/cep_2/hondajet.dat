IDENTIFY "HONDAJET"
CATEGORY UTILITY
AFTBURNR FALSE                #HAVE AFTERBURNER?
THRAFTBN  0.0t                #THRUST WITH AFTERBURNER
THRMILIT  3.0t                #THRUST AT MILITARY POWER
WEIGHCLN  4.0t                #WEIGHT CLEAN
WEIGFUEL  3.0t                #WEIGHT OF FUEL
WEIGLOAD  3.0t                #WEIGHT OF PAYLOAD
FUELABRN  0.0kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.3kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP      -0.3668m 0.96m  3.39m #COCKPIT POSITION
EXCAMERA "PL" -0.3668m 0.96m  3.39m   0deg 0deg 0deg INSIDE
EXCAMERA "PR"  0.3668m 0.96m  3.39m   0deg 0deg 0deg INSIDE
EXCAMERA "CN"  0.00m   1.01m  2.30m   0deg 0deg 0deg INSIDE
EXCAMERA "S0"  0.39m   0.84m  2.30m  90deg 0deg 0deg INSIDE
EXCAMERA "S1"  0.39m   0.84m  1.63m 180deg 0deg 0deg INSIDE
EXCAMERA "S2" -0.39m   0.84m  1.63m 180deg 0deg 0deg INSIDE
EXCAMERA "S3" -0.39m   0.84m  0.18m   0deg 0deg 0deg INSIDE
EXCAMERA "S4"  0.39m   0.84m  0.18m   0deg 0deg 0deg INSIDE
EXCAMERA "S5"  0.39m   0.84m -0.51m  90deg 0deg 0deg INSIDE

INSTPANL User/cep/cep_2/hondajet.ist

LEFTGEAR -1.58m -1.08m -0.37m #LEFT LANDING GEAR POSITION
RIGHGEAR  1.58m -1.08m -0.37m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.08m  4.91m #WHEEL POSITION
ARRESTER  0.00m  0.00m  0.00m
MACHNGUN  0.00m  0.00m  0.00m #MACHINE GUN POSITION
SMOKEGEN  0.00m -0.06m -2.70m #SMOKE GENERATOR POSITION
VAPORPO0  6.08m  0.78m -0.87m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  6.08m  0.78m -0.87m #VAPOR POSITION (Wings Spread)
HTRADIUS  8.0m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 5

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 200kt                #CRITICAL SPEED
MAXSPEED 420kt                #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO  0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO  0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP  0.3                 #EFFECT OF FLAP FOR Cl
CDBYFLAP  0.4                 #EFFECT OF FLAP FOR Cd
CDBYGEAR  0.5                 #EFFECT OF GEAR FOR Cd
CDSPOILR  0.0                 #EFFECT OF SPOILER FOR Cd

WINGAREA 25m^2                #WING AREA

MXIPTAOA  10.0deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL  50.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1  70kt                #MINIMUM MANEUVABLE SPEED
MANESPD2 140kt                #FULLY MANEUVABLE SPEED
CPITMANE  4.0                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB  2.0                 #PITCH STABILITY CONSTANT
CYAWMANE  5.0                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB  3.0                 #YAW STABILITY CONSTANT
CROLLMAN  2.0                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1 120kt
VGWSPED2 120kt

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

INITIGUN 0                    #INITIAL GUN

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 350Kt                #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 100kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 1000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.7                  #LANDING THROTTLE SETTING

AUTOCALC
