REM
REM
REM      \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
REM      \                 F-11F_Tiger                    \
REM      \                                                     \
REM      \  Carrier_Based_Light_Weight_Fighter    \
REM      \                                                     \
REM      \           Created_By_Masa_AT              \
REM      \        Fixed Version 2002 8 5              \
REM      \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
REM
REM
IDENTIFY "F-11F_TIGER"
CATEGORY FIGHTER
AFTBURNR TRUE                #HAVE AFTERBURNER?
THRAFTBN  4.72t                #THRUST WITH AFTERBURNER
THRMILIT  3.35t                #THRUST AT MILITARY POWER
WEIGHCLN  6.5t                #WEIGHT CLEAN
WEIGFUEL  3.1t                #WEIGHT OF FUEL
WEIGLOAD  4.4t                #WEIGHT OF PAYLOAD
FUELABRN 1.25kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.25kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  1.0m   3.10m  #COCKPIT POSITION
LEFTGEAR -1.4m -0.95m -2.55m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.4m -0.95m -2.55m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.35m  3.95m  #WHEEL POSITION
MACHNGUN -0.57m 0.01m  2.20m  #MACHINE GUN POSITION
MACHNGN2 0.57m  0.1m  2.20m  #MACHINE GUN POSITION
SMOKEGEN -0.00m  1.20m -6.40m  #SMOKE GENERATOR POSITION
VAPORPO0 -5.25m  0.50m -4.3m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1 -5.25m  0.50m -4.3m  #VAPOR POSITION (Wings Spread)
HTRADIUS  7.5m                #OUTSIDE SPHERE RADIUS

TRIGGER1 AAM
TRIGGER2 GUN
TRIGGER3 SMK
TRIGGER4 NON

STRENGTH 8

CRITAOAP  22deg               #CRITICAL AOA POSITIVE
CRITAOAM -6deg               #CRITICAL AOA NEGATIVE

CRITSPED 502kt              #CRITICAL SPEED
MAXSPEED 655kt             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                 #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 1.0                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 1.0                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 23.2m^2                #WING AREA

MXIPTAOA 23.1deg              #MAX INPUT AOA
MXIPTSSA 3.2deg               #MAX INPUT SSA
MXIPTROL 325.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 70kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 90kt                #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

INITIGUN 3000                 #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM

AAMSLOT_ 4.0m -0.05m -2.6m
AAMSLOT_ -4.0m -0.05m -2.6m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 502kt              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 120kt                #LANDING SPEED
REFAOALD 8deg                #LANDING AOA
REFLNRWY 1300m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
