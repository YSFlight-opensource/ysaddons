REM PARAGLIDER for YS FLIGHT SYSTEM 2000
REM Written by Makoto  2004

IDENTIFY "Paraglider"
CATEGORY AEROBATIC

AFTBURNR FALSE                #HAVE AFTERBURNER?
THRAFTBN  0.0t                #THRUST WITH AFTERBURNER

THRMILIT  4.0t                #THRUST AT MILITARY POWER

WEIGHCLN  7.0t                #WEIGHT CLEAN
WEIGFUEL  1kg                 #WEIGHT OF FUEL
WEIGLOAD  0.0t                #WEIGHT OF PAYLOAD
FUELABRN  0.0kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.15kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m -2.25m -0.30m #COCKPIT POSITION
LEFTGEAR -0.19m -3.85m  0.90m #LEFT LANDING GEAR POSITION
RIGHGEAR  0.19m -3.85m  0.90m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -3.85m -0.50m #WHEEL POSITION
ARRESTER  0.00m -3.85m  0.00m
MACHNGUN  0.00m  0.00m  0.00m #MACHINE GUN POSITION
SMOKEGEN  0.00m -2.66m -0.51m #SMOKE GENERATOR POSITION
VAPORPO0  4.49m  2.43m -0.17m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.49m  2.43m -0.17m #VAPOR POSITION (Wings Spread)
HTRADIUS  4.5m                #OUTSIDE SPHERE RADIUS

TRIGGER1 SMK
TRIGGER2 NON
TRIGGER3 NON
TRIGGER4 NON

SMOKEOIL 100.0kg
WEAPONCH SMK

STRENGTH 10

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -20deg               #CRITICAL AOA NEGATIVE

CRITSPED 7.5kt                #CRITICAL SPEED
MAXSPEED 30kt                 #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO -0.2                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO  0.0                 #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP  0.0                 #EFFECT OF FLAP FOR Cl
CDBYFLAP  0.0                 #EFFECT OF FLAP FOR Cd
CDBYGEAR  1.0                 #EFFECT OF GEAR FOR Cd
CDSPOILR  5.0                 #EFFECT OF SPOILER FOR Cd

WINGAREA 16m^2                #WING AREA

MXIPTAOA 40.0deg              #MAX INPUT AOA
MXIPTSSA 10.0deg              #MAX INPUT SSA
MXIPTROL 40.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 5kt                  #MINIMUM MANEUVABLE SPEED
MANESPD2 7.5kt                #FULLY MANEUVABLE SPEED
CPITMANE 1.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 1.0                  #PITCH STABILITY CONSTANT
CYAWMANE 1.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 1.0                  #YAW STABILITY CONSTANT
CROLLMAN 1.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.5                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1 0kt
VGWSPED2 10kt

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

INITIGUN 0                    #INITIAL GUN

RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 30kt                 #CRUISING SPEED
REFACRUS 5000ft               #CRUISING ALTITUDE
REFTCRUS 0.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 15kt                 #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 5m                   #RUNWAY REQUIRED TO STOP
REFTHRLD 0.0                  #LANDING THROTTLE SETTING

AUTOCALC
