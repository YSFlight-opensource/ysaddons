REM FW190-A8
IDENTIFY "Fw190-A8(yellow)"
CATEGORY FIGHTER
AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 2100HP               #PROPELLER ENGINE
WEIGHCLN  3.49t                #WEIGHT CLEAN
WEIGFUEL  0.5t                #WEIGHT OF FUEL
WEIGLOAD  0.5t                #WEIGHT OF PAYLOAD
FUELMILI  0.2kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m   1.11m  -1.45m  #COCKPIT POSITION
INSTPANL user/cep/cep_2/fw190ocp.ist
LEFTGEAR -2.17m -2.79m  0.96m  #LEFT LANDING GEAR POSITION
RIGHGEAR  2.174m -2.79m  0.96m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m  -0.80m -6.76m  #WHEEL POSITION
MACHNGUN  0.15m  0.79m  1.00m  #MACHINE GUN POSITION
MACHNGN2 -0.15m  0.79m  1.00m  #MACHINE2 GUN POSITION
MACHNGN3 -1.20m -0.33m  3.14m
MACHNGN4  1.20m -0.33m  3.14m
MACHNGN5 -2.97m -0.11m  3.29m
MACHNGN6  2.97m -0.11m  3.29m
GUNSIGHT FALSE
GUNINTVL 0.08
SMOKEGEN  0.0m   0.00m -5.50m  #SMOKE GENERATOR POSITION
VAPORPO0  6.23m 0.23m  -0.38m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  6.23m 0.23m  -0.38m  #VAPOR POSITION (Wings Spread)
HTRADIUS  6.5m                 #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  19deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 600km/h              #CRITICAL SPEED
MAXSPEED 670km/h              #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                #HAS VARIABLE GEOMETRY WING?

VGWSPED1 0kt
VGWSPED2 600km/h

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.3                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.7                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 18.3m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 160.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 90kt                 #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED
WEAPONCH GUN

INITIGUN 2000                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB
MAXNBOMB 1

BOMBSLOT 0m -1.00m 0.27m

RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 630km/h              #CRUISING SPEED(*1.2
REFACRUS 13000ft              #CRUISING ALTITUDE
REFTCRUS 0.6                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 90kt                 #LANDING SPEED
REFAOALD 9deg                #LANDING AOA
REFLNRWY 600m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
