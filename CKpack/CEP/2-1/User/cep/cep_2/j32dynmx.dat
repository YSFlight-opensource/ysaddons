REM Remaked Aircraft
REM Masa-AT
REM SAAB J32B LANSEN
REM Swedish Jet Fighter
REM Ver. 2.0
IDENTIFY "J32B_Lansen"
CATEGORY FIGHTER
AFTBURNR TRUE                #HAVE AFTERBURNER?
THRAFTBN  6.5t                #THRUST WITH AFTERBURNER
THRMILIT  4.88t                #THRUST AT MILITARY POWER
WEIGHCLN  8.077t                #WEIGHT CLEAN
WEIGFUEL  3.12t                #WEIGHT OF FUEL
WEIGLOAD  2.5t                #WEIGHT OF PAYLOAD
FUELABRN 2.05kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.35kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  1.015m   1.90m  #COCKPIT POSITION
LEFTGEAR -1.315m -2.075m -1.405m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.315m -2.075m -1.405m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.01m  2.75m  #WHEEL POSITION
MACHNGUN -0.7m -0.3m  4.80m  #MACHINE GUN POSITION
MACHNGN2 0.7m  -0.3m  4.80m  #MACHINE GUN POSITION
SMOKEGEN -0.00m  1.80m -7.885m  #SMOKE GENERATOR POSITION
VAPORPO0 -6.12m  -0.75m -4.05m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1 -6.12m  -0.75m -4.05m  #VAPOR POSITION (Wings Spread)
HTRADIUS  7.3m                #OUTSIDE SPHERE RADIUS

TRIGGER1 AAM
TRIGGER2 GUN
TRIGGER3 SMK
TRIGGER4 NON

STRENGTH 8

CRITAOAP  22deg               #CRITICAL AOA POSITIVE
CRITAOAM -14deg               #CRITICAL AOA NEGATIVE

CRITSPED 965km/h              #CRITICAL SPEED
MAXSPEED 988km/h             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                 #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.7                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.7                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 1.1                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.9                  #EFFECT OF SPOILER FOR Cd

WINGAREA 37.4m^2                #WING AREA

MXIPTAOA 20.0deg              #MAX INPUT AOA
MXIPTSSA 9.0deg               #MAX INPUT SSA
MXIPTROL 82.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 69kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 81kt                #FULLY MANEUVABLE SPEED
CPITMANE 6.5                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 1.9                  #PITCH STABILITY CONSTANT
CYAWMANE 1.8                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 1.9                  #YAW STABILITY CONSTANT
CROLLMAN 2.5                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 3
INITIGUN 90                 #INITIAL GUN
INITIAAM 4                    #INITIAL AAM

HRDPOINT -2.48m -1.1m -1.55m RKT AIM9
HRDPOINT 2.48m -1.1m -1.55m RKT AIM9
HRDPOINT -3.38m -1.1m -2.25m AIM9
HRDPOINT 3.38m -1.1m -2.25m AIM9

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

MAXNMAAM 4
MAXNMAGM 0
MAXNBOMB 0
MAXNMRKT 38

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 965km/h              #CRUISING SPEED
REFACRUS 14000m              #CRUISING ALTITUDE
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 81kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 800m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.6                  #LANDING THROTTLE SETTING

AUTOCALC
