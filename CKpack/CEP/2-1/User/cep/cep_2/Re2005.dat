REM Re.2005_Sagittario  produced by Y'z
IDENTIFY "Re.2005_Sagittario"
CATEGORY WW2FIGHTER
AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR  1475HP              #PROPELLER ENGINE
WEIGHCLN  2.6t                #WEIGHT CLEAN
WEIGFUEL  0.43t                #WEIGHT OF FUEL
WEIGLOAD  0.63t                #WEIGHT OF PAYLOAD
FUELMILI  0.11kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.58m  -1.74m  #COCKPIT POSITION
INSTPANL  user/cep/cep_2/Re2005.ist
LEFTGEAR -1.75m -2.00m  0.60m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.75m -2.00m  0.60m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -0.60m -5.20m  #WHEEL POSITION
GUNSIGHT  FALSE
MACHNGUN  -0.21m 0.42m 1.04m  #MACHINE GUN POSITION
GUNINTVL  0.085
MACHNGN2  0.21m  0.42m 1.04m  #MACHINE GUN POSITION
SMOKEGEN  0.00m  0.00m -6.30m  #SMOKE GENERATOR POSITION
VAPORPO0  5.55m  0.01m  -0.67m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.55m  0.01m  -0.67m  #VAPOR POSITION (Wings Spread)
HTRADIUS  6.30m                #OUTSIDE SPHERE RADIUS
ARRESTER  0.0m  -0.60m -5.40m  #ARRESTING HOCK POSITION

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 7

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 240kt                #CRITICAL SPEED
MAXSPEED 678km/h              #MAXIMUM SPEED

HASSPOIL FALSE                #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?
VGWSPED1 120kt                #VARIABLE GEOMETRY WING SPEED
VGWSPED2 280kt                #VARIABLE GEOMETRY WING SPEED

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.25                 #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.7                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.8                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 20.4m^2              #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 8.0deg               #MAX INPUT SSA
MXIPTROL 90.0deg              #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time??

MANESPD1 50kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 78kt                 #FULLY MANEUVABLE SPEED
CPITMANE 7.85                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 1.8                  #PITCH STABILITY CONSTANT
CYAWMANE 4.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 1.0                  #YAW STABILITY CONSTANT
CROLLMAN 2.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED
WEAPONCH GUN

HRDPOINT  0.90m -0.75m -0.80m B250
HRDPOINT -0.90m -0.75m -0.80m B250

MAXNBOMB 2
LMTBYHDP TRUE

INITIGUN 700                    #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB

NMTURRET 3

REM 20mm Mauser MG151x3 on shaft*1 Wing*2
TURRETPO 0 0.00m -0.12m  1.40m 0.0deg 0.1deg 0.0deg
TURRETPT 0 0deg 0deg 0deg
TURRETHD 0 0deg 0deg 0deg
TURRETAM 0 150
TURRETIV 0 0.081sec
TURRETAR 0
TURRETCT 0 PILOT
TURRETRG 0 800m
TURRETPO 1 -1.88m -0.35m  -0.8m 0.1deg 0.0deg 0.0deg
TURRETPT 1 0deg 0deg 0deg
TURRETHD 1 0deg 0deg 0deg
TURRETAM 1 200
TURRETIV 1 0.081sec
TURRETAR 1
TURRETCT 1 PILOT
TURRETRG 1 800m
TURRETPO 2 1.88m -0.35m  -0.8m -0.1deg 0.0deg 0.0deg
TURRETPT 2 0deg 0deg 0deg
TURRETHD 2 0deg 0deg 0deg
TURRETAM 2 200
TURRETIV 2 0.081sec
TURRETAR 2
TURRETCT 2 PILOT
TURRETRG 2 800m



REM RADARCRS 0.001

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 678km/h              #CRUISING SPEED(*1.2
REFACRUS 8000m                #CRUISING ALTITUDE
REFTCRUS 1.0                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 85kt                 #LANDING SPEED
REFAOALD 7deg                 #LANDING AOA
REFLNRWY 800m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.30                 #LANDING THROTTLE SETTING

AUTOCALC
