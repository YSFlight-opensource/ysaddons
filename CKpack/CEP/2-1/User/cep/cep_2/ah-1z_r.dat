IDENTIFY "AH-1Z_SUPER_COBRA_Rear"
CATEGORY ATTACKER
AIRCLASS HELICOPTER

AFTBURNR FALSE                #HAVE AFTERBURNER?
PROPELLR 3800HP
PROPEFCY 0.65                 #Propeller efficiency
PROPVMIN 20m/s                #Minimum speed that T=P/V becomes valid
WEIGHCLN 5.6t                 #WEIGHT CLEAN
WEIGFUEL  1.2t                #WEIGHT OF FUEL
WEIGLOAD  2.6t                #WEIGHT OF PAYLOAD
FUELABRN 0.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.40kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m   0.95m  3.21m  #COCKPIT POSITION
LEFTGEAR -1.07m -1.28m  3.80m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.07m -1.28m  3.80m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.28m  1.39m  #WHEEL POSITION
MACHNGUN  0.0m 0.0m  0.0m   #MACHINE GUN POSITION
GUNINTVL  0.08
SMOKEGEN  0.0m -0.61m -0.72m   #SMOKE GENERATOR POSITION
VAPORPO0  7.31m 2.62m  1.95m   #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  7.31m 2.62m  1.95m
HTRADIUS 12m                #OUTSIDE SPHERE RADIUS

STRENGTH 8

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 134kt              #CRITICAL SPEED
MAXSPEED 155kt              #MAXIMUM SPEED

HASSPOIL TRUE                #HAS SPOILER?
RETRGEAR FALSE                #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.0                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.0                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 50.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 1m^2                 #WING AREA  1m^2 for a helicopter

MXIPTAOA 0.0deg               #MAX INPUT AOA
MXIPTSSA 0.0deg               #MAX INPUT SSA
MXIPTROL 0.0deg               #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 60kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 120kt                #FULLY MANEUVABLE SPEED
CPITMANE 6.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 6.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 8.0                  #ROLL MANEUVABILITY CONSTANT


TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m 1.0m 0.0m       #Thrust vector 0
TRSTDIR1 0.0m 1.0m 0.0m       #Thrust vector 1
PSTMPTCH 40deg                #Post-Stall Maneuver VPitch
PSTMYAW_ 40deg                #Post-Stall Maneuver VYaw
PSTMROLL 80deg                #Post-Stall Maneuver VRoll


CTLLDGEA FALSE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED


HRDPOINT  2.31m 0.28m 1.32m AIM9
HRDPOINT -2.31m 0.28m 1.32m AIM9
HRDPOINT  2.12m -0.44m 1.32m AGM65 
HRDPOINT -2.12m -0.44m 1.32m AGM65 
HRDPOINT  1.81m -0.44m 1.32m AGM65 
HRDPOINT -1.81m -0.44m 1.32m AGM65 
HRDPOINT  2.12m -0.12m 1.32m AGM65 
HRDPOINT -2.12m -0.12m 1.32m AGM65 

HRDPOINT  1.81m -0.12m 1.32m AGM65 
HRDPOINT -1.81m -0.12m 1.32m AGM65 
HRDPOINT  1.42m -0.53m 1.32m AGM65 
HRDPOINT -1.42m -0.53m 1.32m AGM65 
HRDPOINT  1.11m -0.53m 1.32m AGM65 
HRDPOINT -1.11m -0.53m 1.32m AGM65 
HRDPOINT  1.42m -0.21m 1.32m AGM65 
HRDPOINT -1.42m -0.21m 1.32m AGM65 
HRDPOINT  1.11m -0.21m 1.32m AGM65 
HRDPOINT -1.11m -0.21m 1.32m AGM65 
HRDPOINT  1.96m -0.12m 1.32m RKT
HRDPOINT -1.96m -0.12m 1.32m RKT
HRDPOINT  1.26m -0.53m 1.32m RKT
HRDPOINT -1.26m -0.53m 1.32m RKT


MAXNMAAM 2
MAXNMAGM 16
MAXNMRKT 76


LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

GUNPOWER 1
INITIGUN 0                  #INITIAL GUN
LOADWEPN RKT 38

NMTURRET 4


TURRETPO 0 0.00m -0.61m  5.00m 0deg 5deg 0deg      # Number x y z h p b
TURRETPT 0 -35deg 0deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 0 -80deg 80deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 0 750                              # Ammo
TURRETIV 0 0.1sec                            # Number ShootingInterval
TURRETNM 0 GUN2                          # DNM Node Name
TURRETAR 0                                   # Anti Air Capable
REM TURRETGD 0                                   # Anti Ground Capable
TURRETCT 0 PILOT                           # Controlled by PILOT or GUNNER
TURRETRG 0 2000.0m

TURRETPT 1 0deg 0deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 1 -80deg 80deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 1 0                              # Ammo
TURRETNM 1 GUN1                          # DNM Node Name
TURRETAR 1                                   # Anti Air Capable
TURRETCT 1 PILOT                           # Controlled by PILOT or GUNNER
TURRETRG 1 0.0m

TURRETPT 2 0deg 0deg 0deg                  # Number MinPitch MaxPitch NeutralPitch
TURRETHD 2 -80deg 80deg 0deg               # Number MinHdg MaxHdg NeutralHdg
TURRETAM 2 0                              # Ammo
TURRETIV 2 0.1sec                            # Number ShootingInterval
