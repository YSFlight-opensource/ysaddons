REM YAK38
IDENTIFY "YAK38 Forger"
CATEGORY FIGHTER
AFTBURNR FALSE                #HAVE AFTERBURNER?
THRAFTBN  14.04t                #THRUST WITH AFTERBURNER
THRMILIT  10.5t               #THRUST AT MILITARY POWER
WEIGHCLN  7.39t               #WEIGHT CLEAN
WEIGFUEL  2.0t               #WEIGHT OF FUEL
WEIGLOAD  1.5t                #WEIGHT OF PAYLOAD
FUELABRN  2.0kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.8kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.69m   6.00m  #COCKPIT POSITION
LEFTGEAR -0.99m -1.68m  -0.10m  #LEFT LANDING GEAR POSITION
RIGHGEAR  0.99m -1.68m  -0.10m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -1.90m   5.90m  #WHEEL POSITION
MACHNGUN  1.97m -0.67m   2.10m  #MACHINE GUN POSITION
GUNINTVL 0.06
SMOKEGEN  0.00m  0.00m   0.00m  #SMOKE GENERATOR POSITION
VAPORPO0  3.65m -0.32m  -0.90m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  3.65m -0.32m  -0.90m  #VAPOR POSITION (Wings Spread)
HTRADIUS  7.00m                 #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 8

CRITAOAP  21deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH             #CRITICAL SPEED
MAXSPEED 1.3MACH             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE               #HAS VARIABLE GEOMETRY WING?

TRSTVCTR TRUE                 #HAS THRUST VECTORING CAPABILITY?
TRSTDIR0 0.0m 0.0m 1.0m       #Thrust vector 0
TRSTDIR1 0.0m 1.0m 0.0m       #Thrust vector 1
PSTMPTCH 10deg                #Post-Stall Maneuver VPitch
PSTMYAW_ 10deg                #Post-Stall Maneuver VYaw
PSTMROLL 10deg                #Post-Stall Maneuver VRoll


CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2                  #EFFECT OF SPOILER FOR Cd

WINGAREA 18.5m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 240.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 90kt                 #FULLY MANEUVABLE SPEED
CPITMANE 7.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1 0kt
VGWSPED2 1kt

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 70%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 1
INITIGUN 2000                  #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITIAGM 1                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB

MAXNMAAM 3
MAXNMAGM 2
MAXNBOMB 2
MAXNMRKT 24

AAMSLOT_ -1.96m -0.53m 0.80m
RKTSLOT_ -1.96m -0.53m 0.80m
AAMSLOT_  1.31m -0.5m 1.21m
AAMSLOT_ -1.31m -0.5m 1.21m

BOMBSLOT -1.31m -0.5m 1.21m
BOMBSLOT  1.31m -0.5m 1.21m

AGMSLOT_ -1.31m -0.5m 1.21m
AGMSLOT_  1.31m -0.5m 1.21m



REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 110kt                 #LANDING SPEED
REFAOALD 10deg                 #LANDING AOA
REFLNRWY 200m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
