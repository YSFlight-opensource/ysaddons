IDENTIFY "JAS39_Gripen(FG)"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  8.2t                #THRUST WITH AFTERBURNER
THRMILIT  5.5t                #THRUST AT MILITARY POWER
WEIGHCLN  6.6t                #WEIGHT CLEAN
WEIGFUEL  2.4t                #WEIGHT OF FUEL
WEIGLOAD  4.0t                #WEIGHT OF PAYLOAD
FUELABRN 3.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.25kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m   0.78m  4.75m  #COCKPIT POSITION
LEFTGEAR -1.40m -1.75m -0.30m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.40m -1.75m -0.30m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m  -1.75m  4.86m  #WHEEL POSITION
ARRESTER  0.00m -1.75m -4.50m  #ARRESTING FOOK
MACHNGUN -0.40m -0.50m  4.79m  #MACHINE GUN POSITION
SMOKEGEN -0.00m  0.25m -6.00m  #SMOKE GENERATOR POSITION
VAPORPO0 -4.18m -0.08m -3.90m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1 -4.18m -0.08m -3.90m  #VAPOR POSITION (Wings Spread)
HTRADIUS  8.0m                 #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 RKT

STRENGTH 10

CRITAOAP  23deg               #CRITICAL AOA POSITIVE
CRITAOAM -10deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.0MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?
VGWSPED1 -1kt                 #VARIABLE GEOMETRY WING SPEED
VGWSPED2 -1kt                 #VARIABLE GEOMETRY WING SPEED

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.2                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 1.0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 30m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 300.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 50kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 70kt                 #FULLY MANEUVABLE SPEED
CPITMANE 8.5                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 1.5                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 2.5                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 3
INITIGUN 1000                 #INITIAL GUN
INITIAAM 2                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB
INITRCKT 0                    #INITIAL ROCKET
MAXNMAAM 6                    #MAXIMUM AAM
MAXNMAGM 4                    #MAXIMUM AGM
MAXNBOMB 8                    #MAXIMUM BOMB
MAXNMRKT 38                   #MAXIMUM ROCKET

AAMSLOT_ -4.31m -0.03m -2.07m
AAMSLOT_  4.31m -0.03m -2.07m
AAMSLOT_ -2.86m -0.57m -1.20m
AAMSLOT_  2.86m -0.57m -1.20m
AAMSLOT_ -1.93m -0.57m -0.58m
AAMSLOT_  1.93m -0.57m -0.58m

AGMSLOT_ -2.86m -0.65m -1.20m
AGMSLOT_  2.86m -0.65m -1.20m
AGMSLOT_ -1.93m -0.65m -0.58m
AGMSLOT_  1.93m -0.65m -0.58m

BOMBSLOT -1.78m -0.60m -0.58m
BOMBSLOT  1.78m -0.60m -0.58m
BOMBSLOT -2.08m -0.60m -0.58m
BOMBSLOT  2.08m -0.60m -0.58m
BOMBSLOT -2.71m -0.60m -1.20m
BOMBSLOT  2.71m -0.60m -1.20m
BOMBSLOT -3.01m -0.60m -1.20m
BOMBSLOT  3.01m -0.60m -1.20m

RKTSLOT_ -1.93m -0.55m -0.58m
RKTSLOT_  1.93m -0.55m -0.58m

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 33000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 100kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 350m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.30                 #LANDING THROTTLE SETTING

AUTOCALC
