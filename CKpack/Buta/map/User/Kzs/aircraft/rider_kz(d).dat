REM Rider - Racer Version - Green

REM "rider_kz(d).dat/rider_kz(d).dnm/skater_kz_cockpit.srf/skater_kz_coll.srf/rider_kz(d)_coarse.dnm"
REM    were Created by Kzs.(kazu3613@dc.mbn.or.jp)
REM Copyright 2003 Kzs(kazu3613@dc.mbn.or.jp) all rights reserved.

IDENTIFY "[Kzs] Rider/D"
CATEGORY NORMAL

AFTBURNR FALSE                #HAVE AFTERBURNER?
THRAFTBN  0.00t               #THRUST WITH AFTERBURNER
THRMILIT  0.05t               #THRUST AT MILITARY POWER     [0.10t]
WEIGHCLN  0.15t               #WEIGHT CLEAN
WEIGFUEL  0.01t               #WEIGHT OF FUEL
WEIGLOAD  0.05t               #WEIGHT OF PAYLOAD
FUELABRN  0.00kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.002kg             #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.000m  0.054m  0.120m #COCKPIT POSITION
LEFTGEAR -0.100m -1.000m -0.616m #LEFT LANDING GEAR POSITION
RIGHGEAR  0.100m -1.000m -0.616m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.000m -1.000m  0.616m #WHEEL POSITION
ARRESTER  0.000m -0.950m -0.805m #ARRESTING HOOK POSITION
MACHNGUN  0.000m -0.376m  0.654m #MACHINE GUN POSITION
SMOKEGEN  0.178m -0.373m -0.722m #SMOKE GENERATOR POSITION
VAPORPO0  0.198m -0.259m -0.480m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  0.198m -0.259m -0.480m #VAPOR POSITION (Wings Spread)
HTRADIUS  1.450m                 #OUTSIDE SPHERE RADIUS

STRENGTH 2

CRITAOAP 0deg                 #CRITICAL AOA POSITIVE
CRITAOAM 0deg                 #CRITICAL AOA NEGATIVE

CRITSPED 149kt                #CRITICAL SPEED
MAXSPEED 150kt                #MAXIMUM SPEED                [==277.8km/h]

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR FALSE                #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

VGWSPED1 0kt                  #VARIABLE GEOMETRY WING SPEED
VGWSPED2 1kt                  #VARIABLE GEOMETRY WING SPEED

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.0                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.0                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 1.25                 #EFFECT OF SPOILER FOR Cd

WINGAREA 0.0001m^2            #WING AREA

MXIPTAOA 10.00deg             #MAX INPUT AOA
MXIPTSSA  8.00deg             #MAX INPUT SSA
MXIPTROL 10.00deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 500kt                #MINIMUM MANEUVABLE SPEED
MANESPD2 500kt                #FULLY MANEUVABLE SPEED
CPITMANE 9.99                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 9.99                 #PITCH STABILITY CONSTANT
CYAWMANE 9.99                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB 4.00                 #YAW STABILITY CONSTANT
CROLLMAN 0.01                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA FALSE                #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 0deg 0deg 0deg       #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.00t                #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 1
REM SMOKEOIL 100.0kg
WEAPONCH SMK

INITIGUN 3000                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOM
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

MAXNMAAM 0
MAXNMAGM 0
MAXNBOMB 0
MAXNMRKT 0

AAMVISIB FALSE
AAMSLOT_  0.000m 1.000m  0.000m

AGMVISIB FALSE
AGMSLOT_  0.000m 1.000m  0.000m

RKTVISIB FALSE
RKTSLOT_  0.000m 1.000m  0.000m

RADARCRS 0.001                #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 135kt                #CRUISING SPEED               [140kt]
REFACRUS 100ft                #CRUISING ALTITUDE
REFTCRUS 0.60                 #CRUISING THROTTLE SETTING    [0.50]

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 100km/h              #LANDING SPEED
REFAOALD 10.0deg              #LANDING AOA                  [25.0deg]
REFLNRWY 30m                  #RUNWAY REQUIRED TO STOP
REFTHRLD 0.5                  #LANDING THROTTLE SETTING

AUTOCALC
