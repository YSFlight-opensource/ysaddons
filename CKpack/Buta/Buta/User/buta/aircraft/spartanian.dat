REM kurenainobuta
REM OCT2001 RENEWAL VER
IDENTIFY "Spartanian-Poplin"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 36.0t                #THRUST WITH AFTERBURNER
THRMILIT 22.0t                #THRUST AT MILITARY POWER
WEIGHCLN  14.0t                #WEIGHT CLEAN
WEIGFUEL  8.0t                #WEIGHT OF FUEL
WEIGLOAD  15.0t                #WEIGHT OF PAYLOAD
FUELABRN 8.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.45kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  3.53m  16.50m  #COCKPIT POSITION
LEFTGEAR -3.55m -8.10m  11.7m  #LEFT LANDING GEAR POSITION
RIGHGEAR  3.55m -8.10m  11.7m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -8.10m -18.3m  #WHEEL POSITION
MACHNGUN  0.83m  4.50m  8.80m  #MACHINE GUN POSITION
MACHNGN2 -0.74m  4.50m  8.80m  #MACHINE GUN POSITION
SMOKEGEN  0.0m  -1.40m  0.00m  #SMOKE GENERATOR POSITION
VAPORPO0 -1.30m  1.20m  -20.0m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  1.30m  1.20m  -20.0m  #VAPOR POSITION (Wings Spread)
HTRADIUS  20.0m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 10

CRITAOAP  25deg               #CRITICAL AOA POSITIVE
CRITAOAM -20deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.5MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR FALSE                #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.2                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.0                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 20m^2                #WING AREA

MXIPTAOA 35.0deg              #MAX INPUT AOA
MXIPTSSA 10.0deg               #MAX INPUT SSA
MXIPTROL 200.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 30kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 80kt                 #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA FALSE                #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                 #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED
WEAPONCH GUN

INITIGUN 3000                  #INITIAL GUN
INITIAAM 10                    #INITIAL AAM
INITIAGM 4                    #INITIAL AGM
INITBOMB 2                    #INITIAL BOM

RADARCRS 0.01

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING


REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 110kt                 #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 500m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AAMSLOT_ 1.40m 1.80m 18.50m
AAMSLOT_ -1.40m 1.80m 18.50m
AAMVISIB FALSE
AGMSLOT_ -1.40m 1.00m 18.50m
AGMSLOT_ 1.40m 1.00m 18.50m
AGMVISIB FALSE
BOMBSLOT 0.00m 0.00m 7.00m
BOMVISIB FALSE

AUTOCALC
