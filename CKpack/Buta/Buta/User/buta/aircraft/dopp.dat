REM tobenaibuta no48
IDENTIFY "Dopp"
CATEGORY FIGHTER
AFTBURNR  TRUE                #HAVE AFTERBURNER?
THRAFTBN  16.0t               #THRUST WITH AFTERBURNER
THRMILIT  10.0t               #THRUST AT MILITARY POWER
WEIGHCLN  10.0t               #WEIGHT CLEAN
WEIGFUEL   5.0t               #WEIGHT OF FUEL
WEIGLOAD   3.0t               #WEIGHT OF PAYLOAD
FUELABRN   6.5kg              #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI  0.45kg              #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  2.12m  1.50m  #COCKPIT POSITION
LEFTGEAR -1.00m -1.45m -2.30m #LEFT LANDING GEAR POSITION
RIGHGEAR  1.00m -1.45m -2.30m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.45m  0.90m  #WHEEL POSITION
MACHNGUN  -1.04m 0.82m 0.10m  #MACHINE GUN POSITION
MACHNGN2 -1.04m 0.82m  0.10m  #MACHINE GUN POSITION
SMOKEGEN  0.00m 0.00m  0.00m  #SMOKE GENERATOR POSITION
VAPORPO0  6.05m 1.02m -4.10m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1 -6.05m 1.02m -4.10m  #VAPOR POSITION (Wings Spread)
HTRADIUS  10.0m               #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 25

CRITAOAP  30deg               #CRITICAL AOA POSITIVE
CRITAOAM -25deg               #CRITICAL AOA NEGATIVE

CRITSPED 1.8MACH              #CRITICAL SPEED
MAXSPEED 800km/h              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?
VGWSPED1 249kt                #VARIABLE GEOMETRY WING SPEED
VGWSPED2 250kt                #VARIABLE GEOMETRY WING SPEED

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.2                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.1                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 4.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 35m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 40kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 100kt                #FULLY MANEUVABLE SPEED
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 1.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING
VGWSPED1 5kt
VGWSPED2 100kt

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 5
INITIGUN 1000                 #INITIAL GUN
INITIAAM 12                   #INITIAL AAM
INITIAGM 12                   #INITIAL AGM
INITBOMB 0                    #INITIAL BOMB

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 700km/h              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 110kt                #LANDING SPEED
REFAOALD 15deg                #LANDING AOA
REFLNRWY 400m                 #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AAMSLOT_ -1.16m 1.03m -0.60m
AAMSLOT_ -1.28m 0.82m -0.60m
AAMSLOT_ -0.81m 0.82m -0.60m
AAMSLOT_ -0.92m 1.03m -0.60m
AAMSLOT_ 1.16m 1.03m -0.60m
AAMSLOT_ 1.28m 0.82m -0.60m
AAMSLOT_ 0.81m 0.82m -0.60m
AAMSLOT_ 0.92m 1.03m -0.60m
AAMVISIB FALSE
AGMSLOT_ -1.16m 0.61m -0.60m
AGMSLOT_ -0.92m 0.61m -0.60m
AGMSLOT_ 1.16m 0.61m -0.60m
AGMSLOT_ 0.92m 0.61m -0.60m
AGMVISIB FALSE

AUTOCALC
