REM tobenaibuta-cars1

IDENTIFY "delicar"
CATEGORY NORMAL

AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 0.25t                #THRUST WITH AFTERBURNER
THRMILIT 0.12t                #THRUST AT MILITARY POWER
WEIGHCLN 1.50t                #WEIGHT CLEAN
WEIGFUEL 0.05t                #WEIGHT OF FUEL
WEIGLOAD 0.10t                #WEIGHT OF PAYLOAD
FUELABRN 0.05kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.01kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.58m  1.70m  0.58m #COCKPIT POSITION
LEFTGEAR -0.85m -0.40m -1.50m #LEFT LANDING GEAR POSITION
RIGHGEAR  0.85m -0.40m -1.50m #RIGHT LANDING GEAR POSITION
WHELGEAR  0.00m -0.40m  1.10m #WHEEL POSITION
ARRESTER  0.00m  0.00m  0.00m #ARRESTING HOOK POSITION
MACHNGUN  0.00m  0.29m  2.05m #MACHINE GUN POSITION
SMOKEGEN -0.55m -0.10m -2.70m #SMOKE GENERATOR POSITION
VAPORPO0  0.90m  0.00m -2.70m #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  0.90m  0.00m -2.70m #VAPOR POSITION (Wings Spread)
HTRADIUS  3.00m               #OUTSIDE SPHERE RADIUS

STRENGTH 5

CRITAOAP  6.50deg             #CRITICAL AOA POSITIVE
CRITAOAM -5.00deg             #CRITICAL AOA NEGATIVE

CRITSPED  99kt                #CRITICAL SPEED
MAXSPEED 100kt                #MAXIMUM SPEED    # ==185.2km/h

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR FALSE                #HAS RETRACTABLE LANDING GEAR?
VARGEOMW TRUE                 #HAS VARIABLE GEOMETRY WING?

VGWSPED1 500kt                #VARIABLE GEOMETRY WING SPEED
VGWSPED2 501kt                #VARIABLE GEOMETRY WING SPEED

CLVARGEO -0.99                #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO  0.00                #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP  0.00                #EFFECT OF FLAP FOR Cl
CDBYFLAP  0.00                #EFFECT OF FLAP FOR Cd
CDBYGEAR  0.00                #EFFECT OF GEAR FOR Cd
CDSPOILR  1.25                #EFFECT OF SPOILER FOR Cd

WINGAREA 0.01m^2              #WING AREA

MXIPTAOA  10.0deg             #MAX INPUT AOA
MXIPTSSA   5.0deg             #MAX INPUT SSA
MXIPTROL 180.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 65kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 70kt                 #FULLY MANEUVABLE SPEED
CPITMANE 6.50                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 3.00                 #PITCH STABILITY CONSTANT
CYAWMANE 9.99                 #YAW MANEUVABILITY CONSTANT
CYAWSTAB 4.00                 #YAW STABILITY CONSTANT
CROLLMAN 3.00                 #ROLL MANEUVABILITY CONSTANT

CTLLDGEA FALSE                #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 0deg 0deg 0deg       #ATTITUDE
INITFUEL 100%                 #INITIAL FUEL
INITLOAD 0.00t                #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

GUNPOWER 2
SMOKEOIL 100.0kg
WEAPONCH SMK

INITIGUN 0                    #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITBOMB 0                    #INITIAL BOM
INITRCKT 0                    #INITIAL ROCKET (10kg/each)

MAXNMAAM 0
MAXNMAGM 0
MAXNBOMB 0
MAXNMRKT 5

AAMVISIB FALSE
AAMSLOT_ 0.65m  0.45m  2.00m

AGMVISIB FALSE
AGMSLOT_ -0.65m  0.45m  2.00m

RKTVISIB FALSE
RKTSLOT_ 0.00m  0.45m  2.00m

RADARCRS 0.001                #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 95kt                 #CRUISING SPEED
REFACRUS 10ft                 #CRUISING ALTITUDE
REFTCRUS 0.60                 #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 100km/h              #LANDING SPEED
REFAOALD 10.0deg              #LANDING AOA                  [TailStrikeAngle:27.58 deg]
REFLNRWY 65m                  #RUNWAY REQUIRED TO STOP
REFTHRLD 0.5                  #LANDING THROTTLE SETTING

AUTOCALC
