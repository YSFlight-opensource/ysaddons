REM B-2A SPIRIT
IDENTIFY "[241st]B-2 spirit"
CATEGORY HEAVYBOMBER
AFTBURNR FALSE               #HAVE AFTERBURNER?
THRAFTBN 0.0t                #THRUST WITH AFTERBURNER
THRMILIT 34.5t               #THRUST AT MILITARY POWER
WEIGHCLN 48t                 #WEIGHT CLEAN
WEIGFUEL 91t                #WEIGHT OF FUEL
WEIGLOAD 23t                 #WEIGHT OF PAYLOAD
FUELABRN 0.0kg               #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 1.8kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP -0.55m 0.91m  6.85m  #COCKPIT POSITION
EXCAMERA "CO-PILOT" 0.55m 0.91m  6.85m 0deg 0deg 0deg INSIDE
EXCAMERA "BOMBBAY" 0.0m -0.51m 3.11m 0deg -90deg 0deg OUTSIDE
LEFTGEAR -6.34m -3.06m -0.14m  #LEFT LANDING GEAR POSITION
RIGHGEAR  6.34m -3.06m -0.14m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -3.06m  9.02m  #WHEEL POSITION
ARRESTER  0.0m -2.19m -7.42m
MACHNGUN -0.0m -0.00m  0.00m  #MACHINE GUN POSITION
SMOKEGEN  0.0m -0.10m -10.2m  #SMOKE GENERATOR POSITION
VAPORPO0 26.22m  0.00m  -7.70m  #VAPOR POSITION (Wings Swept back)
VAPORPO1 26.22m  0.00m  -7.70m  #VAPOR POSITION (Wings Spread)
HTRADIUS 20.0m                #OUTSIDE SPHERE RADIUS


STRENGTH 15

CRITAOAP  10deg               #CRITICAL AOA POSITIVE
CRITAOAM -5deg               #CRITICAL AOA NEGATIVE

CRITSPED 764km/h              #CRITICAL SPEED
MAXSPEED 1016km/h             #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.2                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.8                  #EFFECT OF SPOILER FOR Cd

WINGAREA 465m^2              #WING AREA

MXIPTAOA 15.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 30deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 100kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 160kt                #FULLY MANEUVABLE SPEED
CPITMANE 7.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 6.0                  #PITCH STABILITY CONSTANT
CYAWMANE 7.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 1.5                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT


CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW TRUE                 #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m            #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                   #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

REM Slots must come before Inits.
HRDPOINT 1.27m -0.58m 1.0m AGM65
HRDPOINT -1.27m -0.58m 1.0m AGM65
HRDPOINT 1.01m -0.33m 1.0m AGM65
HRDPOINT -1.01m -0.33m 1.0m AGM65
AGMVISIB FALSE
HRDPOINT 1.27m -0.58m 2.50m B500
HRDPOINT -1.27m -0.58m 2.50m B500
HRDPOINT 1.27m -0.58m -0.61m B500
HRDPOINT -1.27m -0.58m -0.61m B500
HRDPOINT 1.01m -0.33m 2.50m B500
HRDPOINT -1.01m -0.33m 2.50m B500
HRDPOINT 1.01m -0.33m -0.61m B500
HRDPOINT -1.01m -0.33m -0.61m B500
HRDPOINT 1.53m -0.33m 2.50m B500
HRDPOINT -1.53m -0.33m 2.50m B500
HRDPOINT 1.53m -0.33m -0.61m B500
HRDPOINT -1.53m -0.33m -0.61m B500

BOMVISIB FALSE
BMBAYRCS 0.3
BOMINBAY TRUE
LMTBYHDP FALSE

INITIGUN 0                    #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
MAXNMAGM  16                   #MAX AGM
MAXNBOMB 80		     #MAX BOMB
INITIAGM 8                   #INITIAL AGM
INITBOMB 40                  #INITIAL BOMB
WEAPONCH AGM



RADARCRS 0.1                  #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.8MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.9                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 150kt                 #LANDING SPEED
REFAOALD 10deg                 #LANDING AOA
REFLNRWY 2000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.7                  #LANDING THROTTLE SETTING

AUTOCALC
