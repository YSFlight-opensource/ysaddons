COCKPITP  0.0m   0.95m  5.18m  #COCKPIT POSITION
LEFTGEAR -1.62m -1.996m -2.3m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.62m -1.996m -2.3m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m  -1.994m  4.0m  #WHEEL POSITION
