REM									#DISABLE  2.26m -0.50m -1.20m	 AIM9 AIM120 AGM65 B500			#HARDPOINT 2 CENTER
REM									#DISABLE -2.51m -0.30m -1.20m	 AIM9 AIM120 AGM65 B500			#HARDPOINT 1 OUT
REM									#DISABLE  2.51m -0.30m -1.20m	 AIM9 AIM120 AGM65 B500			#HARDPOINT 2 OUT
REM									#DISABLE -2.01m -0.30m -1.20m	 AIM9 AIM120 AGM65 B500			#HARDPOINT 1 IN
REM									#DISABLE  2.01m -0.30m -1.20m	 AIM9 AIM120 AGM65 B500			#HARDPOINT 2 IN
