REM									#DISABLE -3.65m -0.50m -1.70m	 AIM9 AIM120 AGM65 B500			#HARDPOINT 4 CENTER
REM									#DISABLE  3.40m -0.35m -1.70m	 AIM9 AIM120 AGM65 B500			#HARDPOINT 3 IN
REM									#DISABLE -3.40m -0.35m -1.70m	 AIM9 AIM120 AGM65 B500			#HARDPOINT 4 IN
REM									#DISABLE  3.90m -0.35m -1.70m	 AIM9 AIM120 AGM65 B500			#HARDPOINT 3 OUT
REM									#DISABLE -3.90m -0.35m -1.70m	 AIM9 AIM120 AGM65 B500			#HARDPOINT 4 OUT

LMTBYHDP TRUE					#Limit Max Num Weapon by Hardpoint

