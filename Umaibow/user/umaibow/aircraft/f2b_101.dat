IDENTIFY "F-2B_ADTW_#101"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  13.3t               #THRUST WITH AFTERBURNER
THRMILIT  7.7t                #THRUST AT MILITARY POWER
THRSTREV 0.3
WEIGHCLN  9.7t                #WEIGHT CLEAN
WEIGFUEL  3.8t                #WEIGHT OF FUEL
WEIGLOAD  6.5t                #WEIGHT OF PAYLOAD
FUELABRN 6.5kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.5kg               #FUEL CONSUMPTION AT MILITARY POWER
GUNINTVL 0.01

COCKPITP  0.0m  0.92m   4.8m  #COCKPIT POSITION
EXCAMERA "CO_P"  0m  0.98m  3.11m 0deg 0deg 0deg INSIDE
LEFTGEAR -1.22m -1.85m -0.15m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.22m -1.85m -0.15m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.85m  4.78m  #WHEEL POSITION
ARRESTER  0.0m -1.365m -5.00m
MACHNGUN -0.76m  0.24m  3.5m  #MACHINE GUN POSITION
SMOKEGEN 0.00m -0.91m -6.15m  #SMOKE GENERATOR POSITION
VAPORPO0  5.53m  0.05m -1.95m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  5.53m  0.05m -1.95m  #VAPOR POSITION (Wings Spread)
HTRADIUS  10.3m                #OUTSIDE SPHERE RADIUS

TRIGGER1 GUN
TRIGGER2 AAM
TRIGGER3 AGM
TRIGGER4 SMK

STRENGTH 9

CRITAOAP  23deg               #CRITICAL AOA POSITIVE
CRITAOAM -13deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.0MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.35                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.4                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 0.5                  #EFFECT OF SPOILER FOR Cd

WINGAREA 35m^2                #WING AREA

MXIPTAOA 25.7deg              #MAX INPUT AOA
MXIPTSSA 7.0deg               #MAX INPUT SSA
MXIPTROL 180.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 90kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 260kt                 #FULLY MANEUVABLE SPEED
CPITMANE 18.3                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 7.8                  #PITCH STABILITY CONSTANT
CYAWMANE 7.5                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 6.0                  #YAW STABILITY CONSTANT
CROLLMAN 7.8                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED


MAXNMAAM 999
MAXNMAGM 4
MAXNBOMB 999
MAXNMRKT 38

HRDPOINT -5.58m 0.06m -0.6m AIM9
HRDPOINT 5.58m 0.06m -0.6m AIM9

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

INITIGUN 512                 #INITIAL GUN
LOADWEPN AIM9 2
LOADWEPN AIM120 0
LOADWEPN AGM65 0

RADARCRS 0.8                  #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.93MACH              #CRUISING SPEED
REFACRUS 25000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 125kt                 #LANDING SPEED
REFAOALD 6deg                 #LANDING AOA
REFLNRWY 1300m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                 #LANDING THROTTLE SETTING

AUTOCALC
