IDENTIFY "[Talon](0)SEPECAT Jaguar GR Mk.1A RAF Sq. 41"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN  10.2t               #THRUST WITH AFTERBURNER
THRMILIT  8.4t                #THRUST AT MILITARY POWER
WEIGHCLN  7.4t                #WEIGHT CLEAN
WEIGFUEL  2.5t                #WEIGHT OF FUEL
WEIGLOAD  5.0t                #WEIGHT OF PAYLOAD
FUELABRN 2.7kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.19kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.00m  0.40m   3.59m  #COCKPIT POSITION
LEFTGEAR -0.71m -2.35m -2.53m  #LEFT LANDING GEAR POSITION
RIGHGEAR  0.71m -2.35m -2.53m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -2.48m  5.3m  #WHEEL POSITION
ARRESTER  0.0m -2.00m -6.86m
MACHNGUN 0.60m  -0.96m  2.50m  #MACHINE GUN POSITION
GUNINTVL 0.04
MACHNGN2 -0.60m  -0.96m  2.50m  #MACHINE GUN POSITION
GUNINTVL 0.04
SMOKEGEN -0.1m -0.65m -7.35m  #SMOKE GENERATOR POSITION
VAPORPO0  4.25m  0.30m -4.34m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  4.25m  0.30m -4.34m  #VAPOR POSITION (Wings Spread)
HTRADIUS  9.0m                #OUTSIDE SPHERE RADIUS


STRENGTH 15

CRITAOAP  28deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.1MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.6                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 24m^2                #WING AREA

MXIPTAOA 19.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 240.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 90kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 140kt                 #FULLY MANEUVABLE SPEED
CPITMANE 8.1                 #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 4.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 5.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.5                  #ROLL MANEUVABILITY CONSTANT

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

REM Slots must come before Inits.
HRDPOINT -2.35m 0.48m -1.24m AIM9
HRDPOINT 2.35m 0.48m -1.24m AIM9
HRDPOINT -2.13m -0.32m 0.05m B250
HRDPOINT 2.13m -0.32m 0.05m B250
HRDPOINT -2.13m -0.32m -1.98m B250 
HRDPOINT 2.13m -0.32m -1.98m B250
HRDPOINT -2.58m -0.32m -1.98m B250 
HRDPOINT 2.58m -0.32m -1.98m B250
HRDPOINT -2.58m -0.32m 0.05m B250 
HRDPOINT 2.58m -0.32m 0.05m B250
HRDPOINT 0.00m -1.36m -0.10m B500

MAXNMAAM 0
MAXNMAGM 0
MAXNBOMB 999
MAXNAAMM 0
MAXNMRKT 999
LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint

GUNPOWER 5
INITIGUN 300                 #INITIAL GUN
INITIAAM 0                    #INITIAL AAM
INITIAGM 0                    #INITIAL AGM
INITAAMM 0

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 90kt                 #LANDING SPEED
REFAOALD 10deg                 #LANDING AOA
REFLNRWY 1500m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.30                 #LANDING THROTTLE SETTING



AUTOCALC
